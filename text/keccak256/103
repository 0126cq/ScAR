"function permit(address owner, address spender, uint value, uint deadline, uint8 v, bytes32 r, bytes32 s) external {        require(deadline >= block.timestamp, 'UniSlurm: EXPIRED');        bytes32 digest = <mask0>;        address recoveredAddress = ecrecover(digest, v, r, s);        require(recoveredAddress != address(0) && recoveredAddress == owner, 'UniSlurm: INVALID_SIGNATURE');        _approve(owner, spender, value);    }"