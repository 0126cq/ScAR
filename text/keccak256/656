"function mint(uint256 amount, bytes32[] memory _proof) public payable {        require(_isPresaleActive && !_isMainSaleActive, \"Presale must be active to mint tokens\");      require(msg.sender == tx.origin, \"mint from contract not allowed\");    require(msg.value >= presale_price * amount, \"incorrect price\");    require(nextId <= maxTotalSupply, \"not enough tokens\");    require(mints[msg.sender] + amount <= MAX_PER_WALLET1, \"mint limit reached\");    bytes32 leaf = <mask0>;    require(_proof.verify(root, leaf), \"invalid proof\");    mints[msg.sender] += amount;    _mint(msg.sender, amount);  }"