"function buyPresale(    bytes memory signature,    uint256 numberOfTokens,    uint256 approvedLimit  ) external payable {    require(      block.timestamp >= saleConfig.preSaleStartTime &&        block.timestamp < saleConfig.publicSaleStartTime,      \"Presale is not active\"    );    require(whitelistSigner != address(0), \"White list signer not yet set\");    require(msg.value == (mintPrice * numberOfTokens), \"Incorrect payment\");    require(      (presaleMinted[msg.sender] + numberOfTokens) <= approvedLimit,      \"Wallet limit exceeded\"    );    bytes32 digest = keccak256(      abi.encodePacked(        \"\\x19\\x01\",        DOMAIN_SEPARATOR,        <mask0>      )    );    address signer = digest.recover(signature);    require(      signer != address(0) && signer == whitelistSigner,      \"Invalid signature\"    );    presaleMinted[msg.sender] = presaleMinted[msg.sender] + numberOfTokens;    mint(msg.sender, numberOfTokens);  }"