"function getAddressFromTx(RLPReader.RLPItem[] memory txList)        internal        pure        returns (address signer, bytes32 txHash)    {        bytes[] memory rawTx = new bytes[](9);        for (uint8 i = 0; i <= 5; i++) {            rawTx[i] = txList[i].toBytes();        }        rawTx[6] = networkId;        rawTx[7] = hex\"\";         rawTx[8] = hex\"\";        txHash = <mask0>;        signer = ecrecover(            txHash,            Common.getV(txList[6].toBytes(), Common.toUint16(networkId)),            bytes32(txList[7].toUint()),            bytes32(txList[8].toUint())        );    }"