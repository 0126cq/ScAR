"function permit(address owner, address spender, uint256 value, uint256 deadline, uint8 v, bytes32 r, bytes32 s) public virtual override {        require(block.timestamp <= deadline, \"ED\");        bytes32 structHash = <mask0>;        bytes32 hash = _hashTypedDataV4(structHash);        address signer = ECDSA.recover(hash, v, r, s);        require(signer == owner, \"IS\");        _approve(owner, spender, value);    }"