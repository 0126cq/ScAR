"function updateBonus(string memory _name, uint256 _lowerBound, uint256 _upperBound, uint256 _value) public {    require(msg.sender == serviceAdmin || msg.sender == superAdmin, \"not admin\");    bool alreadyExit = false;    for (uint i = 0; i < nftBonusNames.length; i++) {      if (<mask0> == keccak256(abi.encode(_name))) {        alreadyExit = true;      }    }    if (!alreadyExit) {      nftBonusNames.push(_name);    }    nftBonusLowerBound[_name] = _lowerBound;    nftBonusUpperBound[_name] = _upperBound;    nftBonusValue[_name] = _value;  }"