"function createSplit(Share[] memory shares) external returns (PercentSplitETH splitInstance) {    bytes32 salt = <mask0>;    address clone = Clones.predictDeterministicAddress(address(this), salt);    splitInstance = PercentSplitETH(payable(clone));    if (!clone.isContract()) {      emit PercentSplitCreated(clone);      Clones.cloneDeterministic(address(this), salt);      splitInstance.initialize(shares);    }  }"