"function permit(address owner, address spender, uint256 amount, uint256 deadline, uint8 v, bytes32 r, bytes32 s) public virtual override {        require(block.timestamp <= deadline, \"ERC20Permit: expired deadline\");        bytes32 structHash = <mask0>;        bytes32 hash = _hashTypedDataV4(structHash);        address signer = ECDSA.recover(hash, v, r, s);        require(signer == owner, \"ERC20Permit: invalid signature\");        _nonces[owner].increment();        _approve(owner, spender, amount);    }"