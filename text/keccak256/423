"function calculateRoot(        bytes32[] memory nodes,        uint256 route,        bytes32 item    ) internal pure returns (bytes32) {        uint256 proofItems = nodes.length;        require(proofItems <= 256);        bytes32 h = item;        for (uint256 i = 0; i < proofItems; i++) {            if (route % 2 == 0) {                h = <mask0>;            } else {                h = keccak256(abi.encodePacked(h, nodes[i]));            }            route /= 2;        }        return h;    }"