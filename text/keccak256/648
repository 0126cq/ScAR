"function emitViaProxy_Approval(address proxyAddr, address owner, address spender, uint value) internal FREEMEM {        (bool success,) = proxyAddr.call(abi.encodePacked(                               uint8(3),                               <mask0>,                               bytes32(uint(uint160(owner))),                               bytes32(uint(uint160(spender))),                               value                          ));        require(success, \"e/log-proxy-fail\");    }"