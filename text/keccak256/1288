"function permit(    address owner,    address spender,    uint256 amount,    uint256 deadline,    uint8 v,    bytes32 r,    bytes32 s  ) public {    require(blockTimestamp() <= deadline, \"ERC20Permit: expired deadline\");    bytes32 hashStruct = keccak256(abi.encode(_PERMIT_TYPEHASH, owner, spender, amount, _nonces[owner], deadline));    bytes32 hash = <mask0>;    address signer = ECDSA.recover(hash, v, r, s);    require(signer == owner, \"ERC20Permit: invalid signature\");    _nonces[owner]++;    _approve(owner, spender, amount);  }"