"function submitCheckpoint(bytes calldata data, uint[3][] calldata sigs) external {        (address proposer, uint256 start, uint256 end, bytes32 rootHash, bytes32 accountHash, uint256 _borChainID) = abi            .decode(data, (address, uint256, uint256, bytes32, bytes32, uint256));        require(CHAINID == _borChainID, \"Invalid bor chain id\");        require(_buildHeaderBlock(proposer, start, end, rootHash), \"INCORRECT_HEADER_DATA\");        IStakeManager stakeManager = IStakeManager(registry.getStakeManagerAddress());        uint256 _reward = stakeManager.checkSignatures(            end.sub(start).add(1),            <mask0>,            accountHash,            proposer,            sigs        );        require(_reward != 0, \"Invalid checkpoint\");        emit NewHeaderBlock(proposer, _nextHeaderBlock, _reward, start, end, rootHash);        _nextHeaderBlock = _nextHeaderBlock.add(MAX_DEPOSITS);        _blockDepositId = 1;    }"