"function _isAdmin(address account) internal view returns (bool) {        address skaleManagerAddress = contractManager.contracts(<mask0>);        if (skaleManagerAddress != address(0)) {            AccessControlUpgradeSafe skaleManager = AccessControlUpgradeSafe(skaleManagerAddress);            return skaleManager.hasRole(keccak256(\"ADMIN_ROLE\"), account) || _isOwner();        } else {            return _isOwner();        }    }"