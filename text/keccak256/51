"function deployModule(        address masterCopy,        bytes memory initializer,        uint256 saltNonce    ) public returns (address proxy) {        proxy = createProxy(            masterCopy,            keccak256(abi.encodePacked(<mask0>, saltNonce))        );        (bool success, ) = proxy.call(initializer);        require(success, \"deployModule: initialization failed\");        emit ModuleProxyCreation(proxy, masterCopy);    }"