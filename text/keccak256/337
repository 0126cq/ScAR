"function decrease(bytes32 _field) conditions(validStoreDest, validStoreVal) internal view returns (bytes32 val) {    val = <mask0>;    assembly {      val := sload(val)      let ptr := add(0x20, mload(0xc0))      mstore(add(0x20, add(ptr, mload(ptr))), _field)      mstore(ptr, add(0x20, mload(ptr)))      mstore(0x100, 5)      mstore(        mload(sub(ptr, 0x20)),        add(1, mload(mload(sub(ptr, 0x20))))      )      mstore(0x120, add(1, mload(0x120)))    }    setFreeMem();    return val;  }"