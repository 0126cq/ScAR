"function claimPermit(uint256 tokenId, bytes calldata _signature) external {        require(tokenId > 9500 && tokenId < 10001, \"Token ID invalid\");        if (_msgSender() != owner()) {            bytes32 digst = _hashTypedDataV4(<mask0>);            require(digst.recover(_signature) == owner(), \"Permit Failure\");        }        _claim(tokenId);    }"