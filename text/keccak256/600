"function _isAdmin(address account) internal view returns (bool) {        address skaleManagerAddress = contractManager.contracts(keccak256(abi.encodePacked(\"SkaleManager\")));        if (skaleManagerAddress != address(0)) {            AccessControlUpgradeSafe skaleManager = AccessControlUpgradeSafe(skaleManagerAddress);            return skaleManager.hasRole(<mask0>, account) || _isOwner();        } else {            return _isOwner();        }    }"