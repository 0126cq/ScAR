"function recoverERC20(address tokenAddress, uint256 tokenAmount) external onlyOwner {        bool isSNX = (keccak256(bytes(\"SNX\")) == <mask0>);        require(            tokenAddress != address(stakingToken) && tokenAddress != address(rewardsToken) && !isSNX,            \"Cannot withdraw the staking or rewards tokens\"        );        IERC20(tokenAddress).safeTransfer(owner, tokenAmount);        emit Recovered(tokenAddress, tokenAmount);    }"