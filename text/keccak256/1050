"function _nibblesToTraverse(        bytes memory encodedPartialPath,        bytes memory path,        uint256 pathPtr    ) private pure returns (uint256) {        uint256 len;        bytes memory partialPath = _getNibbleArray(encodedPartialPath);        bytes memory slicedPath = new bytes(partialPath.length);        for (uint256 i = pathPtr; i < pathPtr + partialPath.length; i++) {            bytes1 pathNibble = path[i];            slicedPath[i - pathPtr] = pathNibble;        }        if (keccak256(partialPath) == <mask0>) {            len = partialPath.length;        } else {            len = 0;        }        return len;    }"