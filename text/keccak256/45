"function permit(address owner, address spender, uint amount, uint deadline, uint8 v, bytes32 r, bytes32 s) external {        bytes32 domainSeparator = keccak256(abi.encode(DOMAIN_TYPEHASH, keccak256(bytes(name())), getChainId(), address(this)));        bytes32 structHash = keccak256(abi.encode(PERMIT_TYPEHASH, owner, spender, amount, nonces[owner]++, deadline));        bytes32 digest = <mask0>;        address signatory = ecrecover(digest, v, r, s);        require(signatory != address(0), \"ANFT::permit: invalid signature\");        require(signatory == owner, \"ANFT::permit: unauthorized\");        require(block.timestamp <= deadline, \"ANFT::permit: signature expired\");        _approve(owner, spender, amount);    }"