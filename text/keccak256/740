"function delegateBySig(        address delegatee,        uint nonce,        uint expiry,        uint8 v,        bytes32 r,        bytes32 s    )        external    {        bytes32 structHash = keccak256(            abi.encode(                DELEGATION_TYPEHASH,                delegatee,                nonce,                expiry            )        );        bytes32 digest = <mask0>;        address signatory = ecrecover(digest, v, r, s);        require(signatory != address(0), \"YAM::delegateBySig: invalid signature\");        require(nonce == nonces[signatory]++, \"YAM::delegateBySig: invalid nonce\");        require(now <= expiry, \"YAM::delegateBySig: signature expired\");        return _delegate(signatory, delegatee);    }"