"function metaDelegateCall(    address to, bytes calldata data, bytes calldata signature, bytes calldata unsignedData  ) external payable onlyDelegateCallable {    address signer = _recoverSigner(      <mask0>,      signature    );    if (proxyOwner() != signer) {      revert NotOwner(signer);    }    bytes memory callData = abi.encodePacked(data, unsignedData);    assembly {      let result := delegatecall(gas(), to, add(callData, 0x20), mload(callData), 0, 0)      returndatacopy(0, 0, returndatasize())      switch result      case 0 {        revert(0, returndatasize())      }      default {        return(0, returndatasize())      }    }  }"