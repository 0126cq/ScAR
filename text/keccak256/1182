"function getRandomSaleId(uint64 _tier)        internal        view        returns (uint _saleId)    {        uint[] memory listSales = new uint[](PetSales.length);        uint saleLength;        for (uint i=0; i<PetSales.length; i++) {           if(PetSales[i].status == SaleStatus.Ongoing && PetSales[i].tier == _tier){               listSales[saleLength] = i;               saleLength++;           }        }        uint randomHash = uint(<mask0>);        uint randomNumber = randomHash % saleLength;        return listSales[randomNumber];    }"