"function proxy(bytes[] calldata signatures, uint256 nonce, address addr, bytes calldata input) external returns (bytes memory) {        require(!usedNonces[nonce], \"nonce was already used\");        bytes32 hash = <mask0>;        checkQuorum(hash, signatures);        (bool success, bytes memory ret) = addr.call(input);        require(success, \"proxy call failed\");        usedNonces[nonce] = true;        return ret;    }"