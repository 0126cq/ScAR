"function approvedTransfer(        address to,        uint256 tokenId,        bytes memory signature,        uint256 deadline    ) public virtual {        bytes32 digest = _hashTypedDataV4(            <mask0>        );        address signer = ECDSA.recover(digest, signature);        require(signer == to, \"ApprovedTransfer: invalid signature\");        require(signer != address(0), \"ECDSA: invalid signature\");        require(            block.timestamp < deadline,            \"ApprovedTransfer: signed transaction expired\"        );        _transferNonces[msg.sender]++;        super.safeTransferFrom(msg.sender, to, tokenId);    }"