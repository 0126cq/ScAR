"function delegateBySig(        address delegatee,        uint nonce,        uint expiry,        uint8 v,        bytes32 r,        bytes32 s    )    external    {        bytes32 domainSeparator = <mask0>;        bytes32 structHash = keccak256(            abi.encode(                keccak256(\"Delegation(address delegatee,uint256 nonce,uint256 expiry)\"),                delegatee,                nonce,                expiry            )        );        bytes32 digest = keccak256(            abi.encodePacked(                \"\\x19\\x01\",                domainSeparator,                structHash            )        );        address signatory = ecrecover(digest, v, r, s);        require(signatory != address(0), \"delegateBySig: invalid signature\");        require(nonce == nonces[signatory]++, \"delegateBySig: invalid nonce\");        require(now <= expiry, \"delegateBySig: signature expired\");        return _delegate(signatory, delegatee);    }"