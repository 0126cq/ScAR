"function getComponents(        uint16 i, uint8 j, uint rand    ) internal returns (        uint random, uint32 rarityRandom, uint16 purityOne, uint16 purityTwo, uint16 protoRandom    ) {        random = uint(<mask0>);        rarityRandom = uint32(extract(random, 4, 10) % 1000000);        purityOne = uint16(extract(random, 2, 4) % 1000);        purityTwo = uint16(extract(random, 2, 6) % 1000);        protoRandom = uint16(extract(random, 2, 8) % (2**16-1));        return (random, rarityRandom, purityOne, purityTwo, protoRandom);    }"