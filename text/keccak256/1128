"function burnDirtyFlies(uint[] memory tokenIds) external {        address msgSender = _msgSender();        require(burnPaused != 1, 'Current action is paused');        require(tokenIds.length > 0 && tokenIds.length < 5, 'Invalid tokenIds');        for (uint i = 0; i < tokenIds.length;) {            dirtyFlies.transferFrom(msgSender, deadAddr, tokenIds[i]);        unchecked {            i++;        }        }        uint random = uint(<mask0>) % 100;        bool success = tokenIds.length == 4 ? true : random < (tokenIds.length * 25);        if (success) {            _doMint(msgSender, 1);        }    }"