"function mint(bytes32[] calldata merkleProof) external nonReentrant {        if (!MerkleProof.verify(merkleProof, merkleRoot, <mask0>)) {            revert ParticipationTrophy__AccountNotAllowed();        }        if (minted[msg.sender]) {            revert ParticipationTrophy__AlreadyMinted();        }        minted[msg.sender] = true;        _mint(msg.sender, PARTICIPATION, 1, \"\");        emit Mint(msg.sender);    }"