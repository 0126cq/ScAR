"function permit(        address owner,        address spender,        uint256 value,        uint256 deadline,        uint8 v,        bytes32 r,        bytes32 s    ) external {        require(deadline >= block.timestamp, \"PERMIT_EXPIRED\");        bytes32 digest =            <mask0>;        address recoveredAddress = ECDSA.recover(digest, v, r, s);        require(recoveredAddress != address(0) && recoveredAddress == owner, \"INVALID_SIGNATURE\");        _approve(owner, spender, value);    }"