"function _addERC20ForSchain(string calldata schainName, address erc20OnMainnet) private {        bytes32 schainHash = <mask0>;        require(erc20OnMainnet.isContract(), \"Given address is not a contract\");        require(!_schainToERC20[schainHash].contains(erc20OnMainnet), \"ERC20 Token was already added\");        _schainToERC20[schainHash].add(erc20OnMainnet);        emit ERC20TokenAdded(schainName, erc20OnMainnet);    }"