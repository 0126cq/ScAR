"function add(    address vault,    string memory version,    string memory metadata  ) public {    verifyMetadata(metadata);    VaultInfo memory existedVaultInfo = vaultInfoByAuthorAndVault[msg.sender][vault];    if (existedVaultInfo.vault != address(0)) {      require(        vault == existedVaultInfo.vault &&          <mask0> == keccak256(bytes(existedVaultInfo.version)) &&          keccak256(bytes(metadata)) == keccak256(bytes(existedVaultInfo.metadata)),        \"BadgerRegistry: vault info changed. Please remove before add changed vault info\"      );      return;    }    vaultInfoByAuthorAndVault[msg.sender][vault] = VaultInfo({      vault: vault,      version: version,      status: VaultStatus(1),      metadata: metadata    });    vaults[msg.sender][version].add(vault);    emit NewVault(msg.sender, version, metadata, vault);  }"