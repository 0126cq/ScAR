"function permit(address owner, address spender, uint256 amount, uint256 deadline, uint8 v, bytes32 r, bytes32 s) external virtual override {        require(deadline >= block.timestamp, \"ERC20Permit: expired deadline\");        bytes32 hashStruct = keccak256(            abi.encode(                PERMIT_TYPEHASH,                owner,                spender,                amount,                nonces[owner]++,                deadline            )        );        bytes32 hash = <mask0>;        address signer = ecrecover(hash, v, r, s);        require(            signer != address(0) && signer == owner,            \"ERC20Permit: invalid signature\"        );        _setAllowance(owner, spender, amount);    }"