"function getCollectedChargesCN(        ICrossBridgeStorageToken bridgeStorage,        uint8 networkId    ) internal view returns (TokensChargesInfo memory info) {        uint8[] memory _networks = new uint8[](1);        address[][] memory _tokens = new <mask0>;        uint256[][] memory _charges = new uint256[][](1);        _networks[0] = networkId;        (_tokens[0], _charges[0]) = bridgeStorage.charges();        info.ids = _networks;        info.tokens = _tokens;        info.charges = _charges;    }"