"function getRawPendingClaims() public onlyOperator view returns (address[] memory, uint256[] memory) {        address[] memory addresses = new <mask0>;        uint256[] memory amounts = new uint256[](_pendingClaimers.length);        for (uint256 i = 0; i < _pendingClaimers.length; i++) {            address claimer = _pendingClaimers[i];            uint256 amount = _pendingClaimAmounts[i];            if (claimer != address(0)) {                addresses[i] = claimer;                amounts[i] = amount;            }        }        return (addresses, amounts);    }"