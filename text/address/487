"function topUp(        address _from,        uint256 _value,        address _operator,        bytes memory _extraData    ) internal {        bool isFromGrant = address(tokenGrant.grantStakes(_operator)) == _from ||            <mask0> == _from;        if (grantStaking.hasGrantDelegated(_operator)) {            require(isFromGrant, \"Must be from a grant\");            uint256 previousGrantId = grantStaking.getGrantForOperator(_operator);            (, uint256 grantId) = grantStaking.tryCapturingDelegationData(                tokenGrant, address(escrow), _from, _operator, _extraData            );            require(grantId == previousGrantId, \"Not the same grant\");        } else {            require(!isFromGrant, \"Must not be from a grant\");            require(operators[_operator].owner == _from, \"Not the same owner\");        }        uint256 operatorParams = operators[_operator].packedParams;        if (!_isInitialized(operatorParams)) {            operators[_operator].packedParams = topUps.instantComplete(                _value, _operator, operatorParams, escrow            );        } else {            topUps.initiate(_value, _operator, operatorParams, escrow);        }    }"