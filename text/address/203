"function pop(address[] memory A, uint256 index)        internal        pure        returns (address[] memory, address)    {        uint256 length = A.length;        require(index < A.length, \"Index must be < A length\");        address[] memory newAddresses = new <mask0>;        for (uint256 i = 0; i < index; i++) {            newAddresses[i] = A[i];        }        for (uint256 j = index + 1; j < length; j++) {            newAddresses[j - 1] = A[j];        }        return (newAddresses, A[index]);    }"