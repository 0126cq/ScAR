"function chainIdMappingTokenMappeds(address tokenOrMappingToken) virtual external view returns (uint[] memory chainIds, address[] memory mappingTokenMappeds_) {        (, address token) = mainChainIdTokens(tokenOrMappingToken);        if(token == address(0))            token = tokenOrMappingToken;        uint N = 0;        for(uint i=0; i<supportChainCount(); i++)            if(mappingTokenMappeds[token][supportChainIds[i]] != address(0))                N++;        chainIds = new uint[](N);        mappingTokenMappeds_ = new <mask0>;        uint j = 0;        for(uint i=0; i<supportChainCount(); i++) {            uint chainId = supportChainIds[i];            address mappingTokenMapped = mappingTokenMappeds[token][chainId];            if(mappingTokenMapped != address(0)) {                chainIds[j] = chainId;                mappingTokenMappeds_[j] = mappingTokenMapped;                j++;            }        }    }"