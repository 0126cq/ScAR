"function beforeTokenTransfer(address from, address to, uint256 amount) external override onlyControlledToken(msg.sender) {    if (from != address(0)) {      uint256 fromBeforeBalance = IERC20(msg.sender).balanceOf(from);      uint256 newCreditBalance = _calculateCreditBalance(from, msg.sender, fromBeforeBalance, 0);      newCreditBalance = _applyCreditLimit(msg.sender, fromBeforeBalance.sub(amount), newCreditBalance);      _updateCreditBalance(from, msg.sender, newCreditBalance);    }    if (to != address(0)) {      _accrueCredit(to, msg.sender, IERC20(msg.sender).balanceOf(to), 0);    }    if (from != address(0) && <mask0> != address(0)) {      prizeStrategy.beforeTokenTransfer(from, to, amount, msg.sender);    }  }"