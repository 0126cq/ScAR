"function _buyInvestmentTokenWithWant(uint256 _amount) internal {        if (_amount == 0 || <mask0> == address(want)) {            return;        }        _checkAllowance(address(router), address(want), _amount);        router.swapTokensForExactTokens(            _amount,            type(uint256).max,            getTokenOutPath(address(want), address(investmentToken)),            address(this),            now        );    }"