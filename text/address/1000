"function estimateBatchAssetPriceInErc20(bytes memory data) external view returns(address[] memory erc20Addrs, uint256[] memory amounts) {        address[] memory nftAddrs;        uint256[] memory assetIds;        (nftAddrs, assetIds) = abi.decode(            data,            (address[],uint256[])        );        erc20Addrs = new <mask0>;        amounts = new uint256[](assetIds.length);        for (uint256 i = 0; i < nftAddrs.length; i++) {            erc20Addrs[i] = ACCEPTED_TOKEN;            amounts[i] = estimateAssetPriceInErc20(nftAddrs[i], assetIds[i]);        }    }"