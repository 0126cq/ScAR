"function addLayer(uint256 _layerIndex, TraitDTO[] memory traits)            public            onlyOwner        {            require(TIERS[_layerIndex].length == traits.length, \"Traits size does not match tiers for this index\");            require(traits.length < 100, \"There cannot be over 99 traits per layer\");            address[] memory dataPointers = new <mask0>;            for (uint256 i = 0; i < traits.length; i++) {                dataPointers[i] = SSTORE2.write(traits[i].data);                _traitDetails[_layerIndex][i] = Trait(traits[i].name, traits[i].mimetype);            }            _traitDataPointers[_layerIndex] = dataPointers;            return;        }"