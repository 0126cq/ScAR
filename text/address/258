"function getUniPrice(IUniswapV2Pair unipair) public view returns (uint256) {    (uint112 r0, uint112 r1, ) = unipair.getReserves();    uint256 total = 0;    if (unipair.token0() == weth) {      total = uint256(r0) * 2;    } else {      total = uint256(r1) * 2;    }    uint256 singlePriceInWeth = 1e18 * total / unipair.totalSupply();    address[] memory p = new <mask0>;    p[0] = weth;    p[1] = usdc;    uint256[] memory prices = IUniswapRouterV2(oracle).getAmountsOut(1e18, p);    return prices[1] * singlePriceInWeth / 1e18;   }"