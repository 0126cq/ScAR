"function newUToken(address _uToken2, address _multiplier2) external onlyPlatformWallet returns(bool) {        require(uToken2 == address(0) && <mask0> == address(0), \"already migrated to LQDY\");        require(_uToken2 != address(0x0) && _multiplier2 != address(0x0), \"cannot set the zero address\");        require(Address.isContract(_multiplier2), \"multiplier must be a smart contract address\");        uToken2 = _uToken2;        multiplier2 = Multiplier(_multiplier2);        emit NewUToken(msg.sender, _uToken2, _multiplier2);        return true;    }"