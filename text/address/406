"function mergeTokenArr(address[] memory supplyTokens, address[] memory borrowTokens)        internal        view        returns (address[] memory ctokens, bool isBorrow, bool isSupply)    {         uint _supplyLen = supplyTokens.length;        uint _borrowLen = borrowTokens.length;        uint _totalLen = add(_supplyLen, _borrowLen);        ctokens = new <mask0>;        isBorrow;        isSupply;        if(_supplyLen > 0) {            for (uint i = 0; i < _supplyLen; i++) {                ctokens[i] = instaMapping.cTokenMapping(supplyTokens[i]);            }            isSupply = true;        }        if(_borrowLen > 0) {            for (uint i = 0; i < _borrowLen; i++) {                ctokens[_supplyLen + i] = instaMapping.cTokenMapping(borrowTokens[i]);            }            isBorrow = true;        }    }"