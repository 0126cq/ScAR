"function getRawPendingStakes() public onlyOperator view returns (address[] memory, uint256[] memory) {        address[] memory addresses = new <mask0>;        uint256[] memory amounts = new uint256[](_pendingStakers.length);        for (uint256 i = 0; i < _pendingStakers.length; i++) {            address staker = _pendingStakers[i];            if (staker != address(0)) {                addresses[i] = staker;                amounts[i] = _pendingUserStakes[staker];            }        }        return (addresses, amounts);    }"