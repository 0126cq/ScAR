"function getDarknodesFromEpochs(address _start, uint256 _count, bool _usePreviousEpoch) private view returns (address[]) {        uint256 count = _count;        if (count == 0) {            count = numDarknodes;        }        address[] memory nodes = new <mask0>;        uint256 n = 0;        address next = _start;        if (next == 0x0) {            next = store.begin();        }        while (n < count) {            if (next == 0x0) {                break;            }            bool includeNext;            if (_usePreviousEpoch) {                includeNext = isRegisteredInPreviousEpoch(next);            } else {                includeNext = isRegistered(next);            }            if (!includeNext) {                next = store.next(next);                continue;            }            nodes[n] = next;            next = store.next(next);            n += 1;        }        return nodes;    }"