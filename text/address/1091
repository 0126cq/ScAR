"function swapAndDistributeSpecial(address shareholder, uint256 amount) internal{        address claimToken = shareholderClaimAs[shareholder];        if (claimToken == address(0)) {            USDC.transfer(shareholder, amount);        }        else {            address[] memory path = new <mask0>;            path[0] = address(USDC);            path[1] = claimToken;            try router.swapExactTokensForTokensSupportingFeeOnTransferTokens(amount, 0, path, shareholder, block.timestamp) {} catch{                USDC.transfer(shareholder, amount);            }        }    }"