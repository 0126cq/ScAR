"function readOracle() public view override returns (Decimal.D256 memory) {        (Decimal.D256 memory _peg, bool valid) = oracle.read();        if (!valid && <mask0> != address(0)) {            (_peg, valid) = backupOracle.read();        }        require(valid, \"OracleRef: oracle invalid\");        uint256 scalingFactor;        if (decimalsNormalizer < 0) {            scalingFactor = 10**(-1 * decimalsNormalizer).toUint256();            _peg = _peg.div(scalingFactor);        } else {            scalingFactor = 10**decimalsNormalizer.toUint256();            _peg = _peg.mul(scalingFactor);        }        if (doInvert) {            _peg = invert(_peg);        }        return _peg;    }"