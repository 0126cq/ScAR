"function getRewardsBalance(address user) external view returns(uint amt) {        address[] memory assets = new <mask0>;        assets[0] = _bendDebtWETH_;        for(uint i=0; i<neoA.length; i++) {            NEO neo = NEO(neoA[i]);            address nft = neo.nft();            for(uint j=0; j<neo.balanceOf(user); j++) {                address nbp = nbps[nft][neo.tokenOfOwnerByIndex(user, j)];                amt = amt.add(IBendIncentives(_bendIncentives_).getRewardsBalance(assets, nbp));            }        }    }"