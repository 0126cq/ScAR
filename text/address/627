"function updateRegistry() public {        require(msg.sender == owner || !onlyOwnerCanUpdateRegistry, \"ERR_ACCESS_DENIED\");        IContractRegistry newRegistry = IContractRegistry(addressOf(CONTRACT_REGISTRY));        require(newRegistry != registry && <mask0> != address(0), \"ERR_INVALID_REGISTRY\");        require(newRegistry.addressOf(CONTRACT_REGISTRY) != address(0), \"ERR_INVALID_REGISTRY\");        prevRegistry = registry;        registry = newRegistry;    }"