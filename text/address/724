"function publishCollection(uint256 collectionId_, address[] calldata path, uint256 amountInMax_, uint256 deadline_) external {        Collection storage collection = allCollections[collectionId_];        require(collection.owner == _msgSender(), \"Only owner can publish\");        uint256 actualSize = nftsByCollectionId[collectionId_].length;        require(actualSize >= minimumCollectionSize, \"Not enough boxes\");        collection.size = actualSize;          collection.averagePrice = collection.totalPrice.add(actualSize.sub(1)).div(actualSize);        collection.publishedAt = now;        uint256 count = randomnessCount(actualSize);        uint256 times = actualSize.add(count).sub(1).div(count);          if (linkCost > 0 && <mask0> != address(0)) {            buyLink(times, path, amountInMax_, deadline_);        }        collection.timesToCall = times;        emit PublishCollection(_msgSender(), collectionId_);    }"