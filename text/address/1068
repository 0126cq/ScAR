"function _mintTimelock(address user, uint256 amount, uint256 timestamp) internal {    address[] memory users = new <mask0>;    users[0] = user;    _sweepTimelockBalances(users);    timelockTotalSupply = timelockTotalSupply.add(amount);    _timelockBalances[user] = _timelockBalances[user].add(amount);    _unlockTimestamps[user] = timestamp;    if (timestamp <= _currentTime()) {      _sweepTimelockBalances(users);    }  }"