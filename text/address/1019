"function checkAaveStakeReward(address aaveAccount)        public        view        returns (address[] memory atokens, uint256 rewards)    {        uint256 assetsCount;        (bool[] memory collateral, ) = IAaveV2Resolver(aaveResolver)            .getConfiguration(aaveAccount);        address[] memory reservesList = IAaveV2Resolver(aaveResolver)            .getReservesList();        for (uint256 i = 0; i < reservesList.length; i++) {            if (collateral[i] == true) {                assetsCount = assetsCount + 1;            }        }        address[] memory assets = new address[](assetsCount);        uint256 j;        for (uint256 i = 0; i < reservesList.length; i++) {            if (collateral[i] == true) {                assets[j] = reservesList[i];                j = j + 1;            }        }        uint256 arrLength = 2 * assets.length;        address[] memory _atokens = new <mask0>;        AaveProtocolDataProvider aaveData = AaveProtocolDataProvider(            aaveDataProvider        );        for (uint256 i = 0; i < assets.length; i++) {            (_atokens[2 * i], , _atokens[2 * i + 1]) = aaveData                .getReserveTokensAddresses(assets[i]);        }        uint256 _rewards = AaveStakedTokenIncentivesController(            aaveIncentivesAddress        ).getRewardsBalance(_atokens, aaveAccount);        return (_atokens, _rewards);    }"