"function bind() public nonReentrant {        require(!investors[msg.sender], \"!You've tied it up.\");        investors[msg.sender] = true;        uint256 amount = _calculate(msg.sender);        require(amount > 0, \"!amount\");        address[] memory recipients = new <mask0>;        uint256[] memory amounts = new uint256[](1);        recipients[0] = msg.sender;        amounts[0] = amount;        lockedSupply = lockedSupply.add(amount);        ILendFlareTokenLocker(lendFlareTokenLocker).fund(recipients, amounts);        emit Bind(msg.sender, amount);    }"