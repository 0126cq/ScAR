"function validateSignatures(bytes32 _ethHash, bytes[] calldata _signatures) internal view {        address[] memory signers = new <mask0>;        for (uint256 i = 0; i < _signatures.length; i++) {            address signer = ECDSA.recover(_ethHash, _signatures[i]);            require(isMember(signer), \"Governance: invalid signer\");            for (uint256 j = 0; j < i; j++) {                require(signer != signers[j], \"Governance: duplicate signatures\");            }            signers[i] = signer;        }    }"