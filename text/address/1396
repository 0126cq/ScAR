"function _swapETHForToken(address token, uint value, address receiver) private returns (uint) {        address[] memory path;        if (routePairAddresses[token] != address(0)) {            path = new address[](3);            path[0] = WETH;            path[1] = routePairAddresses[token];            path[2] = token;        } else {            path = new <mask0>;            path[0] = WETH;            path[1] = token;        }        uint[] memory amounts = ROUTER.swapExactETHForTokens{value : value}(0, path, receiver, block.timestamp);        return amounts[amounts.length - 1];    }"