"function deconstruct(bytes calldata encoded, uint8 v, bytes32 r, bytes32 s) public whenNotPaused {        require(phase == PHASE.DECONSTRUCT, \"wrong phase\");        bytes32 hash = keccak256(encoded);        address signer = ecrecover(hash, v, r, s);        require (ticket_signer == signer, \"wrong signer\");        (uint256 id, uint256 traits, uint256 version)  = abi.decode(encoded, (uint256, uint256, uint256));        require(ownerOf(id)==msg.sender, \"not owner\");        require(dolls[id].status != DOLL_STATUS.DECONSTRUCTED, string(abi.encode(\"not constructed \", Strings.<mask0>)));        require(version==dolls[id].version, \"wrong version\");        dolls[id].status = DOLL_STATUS.DECONSTRUCTED;        string memory empty;        dolls[id].reconstructedLink = empty;        traits_contract.mintTraits(msg.sender, traits);    }"