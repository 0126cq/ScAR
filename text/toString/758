"function uri(uint256 tokenId) public view virtual returns (string memory) {        require(_exists(tokenId));        string memory _tokenURI = _tokenURIs[tokenId];        string memory base = tokenURIPrefix;        if (bytes(base).length == 0) {            return _tokenURI;        }        if (bytes(_tokenURI).length > 0) {            return string(abi.encodePacked(base, _tokenURI));        }        return string(abi.encodePacked(base, tokenId.<mask0>));    }"