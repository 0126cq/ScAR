"function fetchUserInventory(address wallet) public view returns (string memory){        uint256 ownedTokenCount = balanceOf(wallet);        string memory json;        uint256 tokenId;        uint8 itemId;        json = \"[\";        for (uint256 i = 0; i < ownedTokenCount; i++){            tokenId = tokenOfOwnerByIndex(wallet, i);            itemId = getMrId(tokenId);            json = string(abi.encodePacked(json, \"{\\\"tokenId\\\":\\\"\", tokenId.<mask0>, \"\\\",\\\"tokenSellState\\\":\\\"\",               getUserNftSellState(tokenId).toString(), \"\\\",\\\"itemId\\\":\\\"\", itemId.toString(), \"\\\", \\\"price\\\":\\\"\", getUserMrPrice(tokenId).toString(),               \"\\\",\\\"rate\\\":\\\"\", mrInfos[itemId].mrRate.toString(),\"\\\",\\\"metaDataURI\\\":\\\"\", tokenURI(tokenId), \"\\\"}\"));            if(i != ownedTokenCount-1)                json = string(abi.encodePacked(json, \",\"));        }        json = string(abi.encodePacked(json, \"]\"));        return json;    }"