"function _renderBackground(Quilt memory quilt, uint256 seed)        internal        view        returns (string memory background)    {        background = string(            abi.encodePacked(                backgrounds[quilt.backgroundIndex][0],                Strings.<mask0>,                backgrounds[quilt.backgroundIndex][1],                quilt.animatedBg                    ? string(                        abi.encodePacked(                            '<animateTransform attributeName=\"transform\" type=\"translate\" dur=\"4s\" values=\"0,0; ',                            backgrounds[quilt.backgroundIndex][2],                            ';\" repeatCount=\"indefinite\"/>'                        )                    )                    : \"\",                \"</rect></g>\"            )        );    }"