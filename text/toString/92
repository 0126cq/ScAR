"function checkURI(uint256 _tokenId) public view returns (string memory) {        require(_baseSet == true, \"HRF: CID has not been set\");        require(_baseLock == false, \"HRF: URI has been set\");        if (_jsonExtension == true) {            return string(abi.encodePacked(_baseUri(), _extendedBaseUri, \"/\", toString(_tokenId), \".json\"));        } else {            return string(abi.encodePacked(_baseUri(), _extendedBaseUri, \"/\", <mask0>));        }    }"