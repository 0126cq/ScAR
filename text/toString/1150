"function _renderShadow(Quilt memory quilt) internal pure returns (string memory shadow) {        shadow = string(            abi.encodePacked(                '<g filter=\"url(#f)\"><rect x=\"8\" y=\"8\" width=\"',                Strings.toString(quilt.quiltAbsW),                '\" height=\"',                Strings.toString(quilt.quiltAbsH),                '\" rx=\"',                Strings.<mask0>,                '\" fill=\"url(#c1)\"/>',                quilt.hovers                    ? '<animateTransform attributeName=\"transform\" type=\"scale\" additive=\"sum\" dur=\"4s\" values=\"1 1; 1.005 1.02; 1 1;\" calcMode=\"spline\" keySplines=\"0.45, 0, 0.55, 1; 0.45, 0, 0.55, 1\" repeatCount=\"indefinite\"/>'                    : \"\",                \"</g>\"            )        );    }"