"function getRequirements(address[] memory titleAddresses, uint256[] memory titleMinAssets) public view returns (string memory) {        string memory description = '';        for (uint i=0; i<titleAddresses.length-1; i++) {            description = string(abi.encodePacked(description, titleMinAssets[i].toString(), 'x ', IERC721Metadata(titleAddresses[i]).name(), ', '));        }        description = string(abi.encodePacked(description, titleMinAssets[titleAddresses.length-1].<mask0>, 'x ', IERC721Metadata(titleAddresses[titleAddresses.length-1]).name()));        return description;    }"