"function getShapeSVG(uint shape_key, uint shape_color, uint i) public view returns (string memory)     {        string memory svg_output;          string memory traitSvg;                        string memory colorName;        uint color;        uint shapeKey;        color = shape_color;        colorName = colors[color];        shapeKey = shape_key;        traitSvg = shapes[shapeKey].svg;        svg_output = string(abi.encodePacked(            '<style>.e',            toString(i),            ' { fill: ',            colorName,            '; opacity:0.',            <mask0>,            '; }</style>'));        svg_output = string(abi.encodePacked(            svg_output,            '<path class=\"e',            toString(i),            '\" ',            traitSvg));                return svg_output;    }"