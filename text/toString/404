"function fetchItemFromShop() public view returns (string memory){        uint256 itemKindCount = getMrItemKindCount();        string memory json;        json = \"[\";        for (uint8 i = 0; i < itemKindCount; i++){            json = string(abi.encodePacked(json, \"{\\\"mrId\\\":\\\"\", mrInfos[i].mrId.toString(), \"\\\", \\\"mrMetaDataURI\\\":\\\"\", mrInfos[i].mrMetaDataURI, \"\\\", \\\"mrPrice\\\":\\\"\", mrInfos[i].mrPrice.<mask0>,               \"\\\", \\\"currency\\\":\\\"\", mrInfos[i].currency.toString(), \"\\\", \\\"rate\\\":\\\"\", mrInfos[i].mrRate.toString(),\"\\\", \\\"curCount\\\":\\\"\", getMrMintedTokenCount(i).toString(), \"\\\", \\\"maxCount\\\":\\\"\", mrInfos[i].maxCount.toString(), \"\\\"}\"));            if(i != itemKindCount-1)              json = string(abi.encodePacked(json, \",\"));        }        json = string(abi.encodePacked(json, \"]\"));        return json;    }"