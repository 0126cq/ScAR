"function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {        require(_exists(tokenId), \"URI query for nonexistent token\");        string memory a = toString(_donated[tokenId] / 10 ** 18);        string memory b = toString(_donated[tokenId] % 10 ** 18);        while (bytes(b).length < 18) b = string(abi.encodePacked(\"0\",b));        while (bytes(b).length > 1 && bytes(b)[bytes(b).length-1] == \"0\") b = substring(b,0,bytes(b).length-1);        string memory donatedVal = string(abi.encodePacked(a,\".\",b));        string memory json = Base64.encode(            bytes(                string(                    abi.encodePacked(                        '{\"name\": \"Message in memory of Putin War victims #',                         <mask0>,                         '\",',                        '\"description\": \"This message was minted as a tribute to Putin War victims. It reveals the day Putin is gone. May his victims and the truth never be forgotten.\",',                         '\"image\": \"',                        getSvg(tokenId),                        '\",\"attributes\": [{\"trait_type\": \"donated\", \"value\":\"',                        donatedVal,                        ' ETH',                        '\"}]}'                    )                )            )        );        return string(abi.encodePacked(\"data:application/json;base64,\", json));    }"