"function getRequiredLootIdxMultiplicity(uint256 tokenId, uint256 requiredLootIdx) public pure returns (uint256 multiplicity) {        if(getRequiredLootIdxMask(tokenId) & (1 << requiredLootIdx) == 0) return 0;        uint256 r = random(string(abi.encodePacked(\"Multiplicity\", Strings.<mask0>))) % 100;        if(     r < 60) return 1;        else if(r < 90) return 2;        else            return 3;    }"