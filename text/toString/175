"function ogsTraits(string[] memory names)        public        pure        returns (string memory)    {        string memory traitsDefinitions = string(abi.encodePacked(            '{',                '\"trait_type\": \"OGs\",',                '\"value\": ', names.length.<mask0>,            '}'        ));        if (names.length > 0) {            for (uint256 i = 0; i < names.length; i++) {                string memory name = names[i];                traitsDefinitions = string(abi.encodePacked(                    traitsDefinitions,                    ',',                    traitDefinition('OG', name)                ));            }        }        return traitsDefinitions;    }"