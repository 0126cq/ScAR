"function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {        require(_exists(tokenId), \"nonexistent token\");        if (phase==PHASE.PREREVEAL) {            return prerevealURI;        } else if (dolls[tokenId].status==DOLL_STATUS.ORIGINAL) {            return string(abi.encodePacked(revealURIBase, \"/\", Strings.<mask0>));        } else if (dolls[tokenId].status==DOLL_STATUS.DECONSTRUCTED) {            return string(abi.encodePacked(deconstructionURIBase, \"/\", Strings.toString(tokenId)));        } else {            return dolls[tokenId].reconstructedLink;        }    }"