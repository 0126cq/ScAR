"function _renderPatchSlots(Quilt memory quilt)        internal        pure        returns (string memory renderedSlots)    {        for (uint256 i = 0; i < quilt.totalPatchCount; i++) {            uint256 col = i % quilt.quiltW;            uint256 row = i / quilt.quiltW;            uint256 x = 64 * col;            uint256 y = 64 * row;            renderedSlots = string(                abi.encodePacked(                    renderedSlots,                    '<mask id=\"s',                    Strings.toString(col),                    Strings.<mask0>,                    '\"><rect x=\"',                    Strings.toString(x),                    '\" y=\"',                    Strings.toString(y),                    '\" width=\"64\" height=\"64\" fill=\"white\"/></mask>'                )            );        }    }"