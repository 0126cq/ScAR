"function _setBalance(address account, uint256 newBalance) internal {        uint256 currentBalance = balanceOf(account);        if(newBalance > currentBalance) {            uint256 mintAmount = newBalance.<mask0>;            _mint(account, mintAmount);        } else if(newBalance < currentBalance) {            uint256 burnAmount = currentBalance.sub(newBalance);            _burn(account, burnAmount);        }    }"