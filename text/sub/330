"function removeShort(        Vault storage _vault,        address _shortOtoken,        uint256 _amount,        uint256 _index    ) external {        require(_index < _vault.shortOtokens.length, \"V2\");        require(_vault.shortOtokens[_index] == _shortOtoken, \"V3\");        uint256 newShortAmount = _vault.shortAmounts[_index].<mask0>;        if (newShortAmount == 0) {            delete _vault.shortOtokens[_index];        }        _vault.shortAmounts[_index] = newShortAmount;    }"