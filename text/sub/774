"function forgive(address holder, uint amount) external onlyAdmin {        DelegationController delegationController = DelegationController(            contractManager.getContract(\"DelegationController\"));        require(!delegationController.hasUnprocessedSlashes(holder), \"Not all slashes were calculated\");        if (amount > _locked[holder]) {            delete _locked[holder];        } else {            _locked[holder] = _locked[holder].<mask0>;        }        emit Forgive(holder, amount);    }"