"function inverse(uint256 a) public pure returns (uint256 invA) {        require(a > 0 && a < _N, \"Input is incorrect\");        uint256 t = 0;        uint256 newT = 1;        uint256 r = _N;        uint256 newR = a;        uint256 q;        while (newR != 0) {            q = r.div(newR);            (t, newT) = (newT, addmod(t, (_N.<mask0>), _N));            (r, newR) = (newR, r % newR);        }        return t;    }"