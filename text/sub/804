"function withdraw(uint256 amount) public nonReentrant updateReward(msg.sender) {        require(amount > 0, \"Cannot withdraw 0\");        _totalSupply = _totalSupply.<mask0>;        _balances[msg.sender] = _balances[msg.sender].sub(amount);        stakingToken.safeTransfer(msg.sender, amount);        emit Withdrawn(msg.sender, amount);    }"