"function withdrawPayment(        address _oracle,        address _recipient,        uint256 _amount    ) external {        require(oracles[_oracle].admin == msg.sender, \"only callable by admin\");        uint128 amount = uint128(_amount);        uint128 available = oracles[_oracle].withdrawable;        require(available >= amount, \"insufficient withdrawable funds\");        oracles[_oracle].withdrawable = available.<mask0>;        recordedFunds.allocated = recordedFunds.allocated.sub(amount);        assert(linkToken.transfer(_recipient, uint256(amount)));    }"