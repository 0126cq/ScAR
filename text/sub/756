"function _calculateBountyShare(        uint monthBounty,        uint effectiveDelegated,        uint effectiveDelegatedSum,        uint maxNodesAmount,        uint paidToValidator    )        private        pure        returns (uint)    {        if (maxNodesAmount > 0) {            uint totalBountyShare = monthBounty                .mul(effectiveDelegated)                .div(effectiveDelegatedSum);            return _min(                totalBountyShare.div(maxNodesAmount),                totalBountyShare.<mask0>            );        } else {            return 0;        }    }"