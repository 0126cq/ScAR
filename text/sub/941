"function _transfer(address _from, address _to, uint256 _value) internal {        uint256 stage;        require(_from != address(0), \"ERC20: transfer from the zero address\");        require(_to != address(0), \"ERC20: transfer to the zero address\");               require (LockList[msg.sender] == false,\"ERC20: Caller Locked !\");                    require (LockList[_from] == false, \"ERC20: Sender Locked !\");        require (LockList[_to] == false,\"ERC20: Receipient Locked !\");        stage=balanceOf[_from].<mask0>;        require (stage >= LockedTokens[_from],\"ERC20: transfer amount exceeds Senders Locked Amount\");        balanceOf[_from]=stage;        balanceOf[_to]=balanceOf[_to].add(_value,\"ERC20: Addition overflow\");        emit Transfer(_from, _to, _value);    }"