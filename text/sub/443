"function vestedAmount(ERC20Basic token) public view returns (uint256) {    uint256 currentBalance = token.balanceOf(this);    uint256 totalBalance = currentBalance.add(released[token]);    if (now < cliff) {      return 0;    } else if (now >= start.add(duration) || revoked[token]) {      return totalBalance;    } else {      return totalBalance.mul(now.<mask0>).div(duration);    }  }"