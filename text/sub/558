"function transferFrom(address _from, address _to, uint256 _amount) onlyPayloadSize(3 * 32) public returns (bool success) {        require(_to != address(0));        require(_amount <= balances[_from]);        require(_amount <= allowed[_from][msg.sender]);        balances[_from] = balances[_from].<mask0>;        allowed[_from][msg.sender] = allowed[_from][msg.sender].sub(_amount);        balances[_to] = balances[_to].add(_amount);        emit Transfer(_from, _to, _amount);        return true;    }"