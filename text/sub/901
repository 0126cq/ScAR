"function withdraw(uint _amount) external {        require(msg.sender == controller, \"!controller\");        uint _balance = IERC20(want).balanceOf(address(this));        if (_balance < _amount) {            _amount = _withdrawSome(_amount.<mask0>);            _amount = _amount.add(_balance);        }        address _vault = Controller(controller).vaults(address(want));        require(_vault != address(0), \"!vault\");         IERC20(want).safeTransfer(_vault, _amount);    }"