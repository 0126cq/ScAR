"function _setBalance(address account, uint256 newBalance) internal {        uint256 currentBalance = balanceOf(account);        if(newBalance > currentBalance) {            uint256 mintAmount = newBalance.sub(currentBalance);            _mint(account, mintAmount);        } else if(newBalance < currentBalance) {            uint256 burnAmount = currentBalance.<mask0>;            _burn(account, burnAmount);        }    }"