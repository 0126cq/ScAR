"function _normalDeleverage(        uint256 maxDeleverage,        uint256 lent,        uint256 borrowed,        uint256 collatRatio    ) internal returns (uint256 deleveragedAmount) {        uint256 theoreticalLent = 0;        if(collatRatio != 0){            theoreticalLent = borrowed.mul(1e18).div(collatRatio);        }        deleveragedAmount = lent.<mask0>;        if (deleveragedAmount >= borrowed) {            deleveragedAmount = borrowed;        }        if (deleveragedAmount >= maxDeleverage) {            deleveragedAmount = maxDeleverage;        }        cToken.redeemUnderlying(deleveragedAmount);        cToken.repayBorrow(deleveragedAmount);    }"