"function transferFrom(address _from, address _to, uint256 _value) public returns (bool success) {    require(allowed[_from][msg.sender] >= _value && balances[_from] >= _value && _value > 0);    balances[_from] = balances[msg.sender].sub(_value);    balances[_to] = balances[_to].add(_value);    allowed[_from][msg.sender] = allowed[_from][msg.sender].<mask0>;    Transfer(_from, _to, _value);    return true;}"