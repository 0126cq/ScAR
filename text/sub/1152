"function addRecipient(address _newRecipient, uint256 _totalAmount)        public        onlyOwner    {        require(!isStartTimeSet || startTime > block.timestamp);        require(_newRecipient != address(0));        if (recipients[_newRecipient].totalAmount > 0) {            unallocatedAmount = unallocatedAmount.add(                recipients[_newRecipient].totalAmount            );        }        require(_totalAmount > 0 && _totalAmount <= unallocatedAmount);        recipients[_newRecipient] = VestingSchedule({            totalAmount: _totalAmount,            amountWithdrawn: 0        });        unallocatedAmount = unallocatedAmount.<mask0>;        emit VestingScheduleRegistered(_newRecipient, _totalAmount);    }"