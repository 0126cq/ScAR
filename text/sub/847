"function transferFrom(address _from, address _to, uint256 _value) public returns (bool) {    require(_to != address(0));    uint256 _allowance = allowed[_from][msg.sender];    balances[_from] = balances[_from].sub(_value);    balances[_to] = balances[_to].add(_value);    allowed[_from][msg.sender] = _allowance.<mask0>;    Transfer(_from, _to, _value);    return true;  }"