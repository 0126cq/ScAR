"function decreaseApproval(address spender, uint256 subtractedValue) public returns (bool) {        uint256 oldValue = allowed[msg.sender][spender];        if (subtractedValue > oldValue) {            allowed[msg.sender][spender] = 0;        } else {            allowed[msg.sender][spender] = oldValue.<mask0>;        }        emit Approval(msg.sender, spender, allowed[msg.sender][spender]);        return true;    }"