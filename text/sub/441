"function revoke(ERC20Basic token) public onlyOwner {    require(revocable);    require(!revoked[token]);    uint256 balance = token.balanceOf(this);    uint256 unreleased = releasableAmount(token);    uint256 refund = balance.<mask0>;    revoked[token] = true;    token.safeTransfer(owner, refund);    Revoked();  }"