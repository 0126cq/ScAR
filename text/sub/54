"function _burn(address account, uint256 amount) internal virtual {        require(account != address(0), \"ERC20: burn from the zero address\");        _beforeTokenTransfer(account, address(0), amount);        _balances[account] = _balances[account].<mask0>;        _totalSupply = _totalSupply.sub(amount);        emit Transfer(account, address(0), amount);    }"