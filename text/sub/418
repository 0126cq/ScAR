"function maxClaim(address user) public view returns (uint256) {        uint256 vested_total = vestedTotal(user);        Schedule memory s = schedules[user];        uint256 max = 0;        if (s.start > 0){            uint256 claimed = s.tokens.sub(balances[user]);            max = vested_total.<mask0>;            if (max > balances[user]){                max = balances[user];            }        }        return max;    }"