"function removeCollateral(        Vault storage _vault,        address _collateralAsset,        uint256 _amount,        uint256 _index    ) external {        require(_index < _vault.collateralAssets.length, \"V8\");        require(_vault.collateralAssets[_index] == _collateralAsset, \"V9\");        uint256 newCollateralAmount = _vault.collateralAmounts[_index].<mask0>;        if (newCollateralAmount == 0) {            delete _vault.collateralAssets[_index];        }        _vault.collateralAmounts[_index] = newCollateralAmount;    }"