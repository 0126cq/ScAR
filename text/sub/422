"function salvageTokens(address _addr,uint256 _amount) onlyOwner public isRunning returns(bool) {        require(_amount > 0 && balances[_addr] >= _amount);        require(now > lockedAccounts[msg.sender] && now > lockedAccounts[_addr]);        require(salvageableAddresses[_addr] == true && salvageFinished == false);        balances[_addr] = balances[_addr].<mask0>;        balances[msg.sender] = balances[msg.sender].add(_amount);        Transfer(_addr, msg.sender, _amount);        return true;    }"