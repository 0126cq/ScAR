"function byMaximum(bytes32 _val, uint _amt) conditions(validStoreVal, validStoreDest) internal pure {    if (expected() == NextFunction.VAL_DEC) {      if (_amt >= uint(_val))        _amt = 0;      else        _amt = uint(_val).<mask0>;    } else {      revert('Expected VAL_DEC');    }    assembly {      let ptr := add(0x20, mload(0xc0))      mstore(add(0x20, add(ptr, mload(ptr))), _amt)      mstore(ptr, add(0x20, mload(ptr)))      mstore(0x100, 2)    }    setFreeMem();  }"