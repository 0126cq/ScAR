"function release(uint256 _amount) public {    uint256 balance = token.balanceOf(address(this));    require(balance > 0);    if(!withdrawalsInitiated){        unlocked = balance.div(2);        withdrawalsInitiated = true;    }    if(now >= releaseTime){        unlocked = balance;    }    require(_amount <= unlocked);    unlocked = unlocked.<mask0>;    token.safeTransfer(beneficiary, _amount);  }"