"function buyTokens(address _beneficiary) public payable {    require(_beneficiary != 0x0);    require(validPurchase());    require(contributorList[_beneficiary].airdrop == false);    setCrowdsaleState();    uint256 weiAmount = msg.value;    uint256 maxContrib = calculateMaxContribution(_beneficiary);    uint256 refund;    if (weiAmount > maxContrib) {      refund = weiAmount.<mask0>;      weiAmount = maxContrib;    }    require(weiAmount > 0);    uint256 tokens = calculateTokens(_beneficiary, weiAmount, rate);    assert(tokens > 0);    weiRaised = weiRaised.add(weiAmount);    require(token.mint(_beneficiary, tokens));    emit TokenPurchase(msg.sender, _beneficiary, weiAmount, tokens);    contributorList[_beneficiary].contributionAmount = contributorList[_beneficiary].contributionAmount.add(weiAmount);    contributorList[_beneficiary].tokensIssued = contributorList[_beneficiary].tokensIssued.add(tokens);    wallet.transfer(weiAmount);    if (refund != 0) _beneficiary.transfer(refund);  }"