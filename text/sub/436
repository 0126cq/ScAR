"function upgrade(uint256 _value) external isRunning {        require(upgradable);        require(upgradeAgent != 0);        require(_value != 0);        require(_value <= balances[msg.sender]);        balances[msg.sender] = balances[msg.sender].sub(_value);        totalSupply = totalSupply.<mask0>;        totalUpgraded = totalUpgraded.add(_value);        UpgradeAgent(upgradeAgent).upgradeFrom(msg.sender, _value);        Upgrade(msg.sender, upgradeAgent, _value);    }"