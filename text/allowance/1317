"function _callParaswap(        IERC20 fromToken,        uint256 amount,        bytes memory callData    )        internal    {        uint256 ethAmountToTransfert = 0;        if (fromToken.isETH()) {            require(address(this).balance >= amount, \"ETH balance is insufficient\");            ethAmountToTransfert = amount;        } else {            uint256 approvedAmount = fromToken.<mask0>;            if (approvedAmount < amount) {                fromToken.safeIncreaseAllowance(paraswapProxy, amount - approvedAmount);            }        }        (bool success,) = augustusSwapper.call{ value: ethAmountToTransfert }(callData);        require(success, \"Paraswap execution failed\");    }"