"function safeApprove(        ERC20 token,        address spender,        uint256 value,        string memory location    ) internal {        require(            (value == 0) || (token.<mask0> == 0),            string(abi.encodePacked(\"SafeERC20: bad approve call from \", location))        );        callOptionalReturn(            token,            abi.encodeWithSelector(token.approve.selector, spender, value),            \"approve\",            location        );    }"