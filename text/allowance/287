"function safeApproveMax(        ERC20 token,        address spender,        uint256 amount,        string memory location    ) internal {        uint256 allowance = ERC20(token).<mask0>;        if (allowance < amount) {            if (allowance > 0) {                safeApprove(token, spender, 0, location);            }            safeApprove(token, spender, type(uint256).max, location);        }    }"