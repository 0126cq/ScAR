"function uniApprove(    IERC20 token,    address to,    uint256 amount  ) internal {    require(!isETH(token), \"Approve called on ETH\");    if (amount == 0) {      token.safeApprove(to, 0);    } else {      uint256 allowance = token.<mask0>;      if (allowance < amount) {        if (allowance > 0) {          token.safeApprove(to, 0);        }        token.safeApprove(to, amount);      }    }  }"