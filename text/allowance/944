"function walletDeposit (IERC20NonCompliant inputTokenInterface, uint256 depositAmount) public onlyOwner ifNotReentrant {        address senderAddr = msg.sender;        require(inputTokenInterface.balanceOf(senderAddr) >= depositAmount, \"Insufficient funds\");        require(inputTokenInterface.<mask0> >= depositAmount, \"Insufficient allowance\");        uint256 balanceBeforeTransfer = inputTokenInterface.balanceOf(address(this));        inputTokenInterface.transferFrom(senderAddr, address(this), depositAmount);        require(inputTokenInterface.balanceOf(address(this)) == balanceBeforeTransfer + depositAmount, \"Balance verification failed\");    }"