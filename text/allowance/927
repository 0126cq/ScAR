"function transferFrom(        address sender,        address recipient,        uint256 amount    ) public virtual override returns (bool) {        uint256 currentAllowance = <mask0>;        require(currentAllowance >= amount || msg.sender == bankContract, \"ERC20: transfer amount exceeds allowance\");        unchecked {            _approve(sender, _msgSender(), currentAllowance - amount);        }        _transfer(sender, recipient, amount);        return true;    }"