"function safeApprove(        IERC20Upgradeable token,        address spender,        uint256 value    ) internal {        require(            (value == 0) || (token.<mask0> == 0),            \"SafeERC20: approve from non-zero to non-zero allowance\"        );        _callOptionalReturn(token, abi.encodeWithSelector(token.approve.selector, spender, value));    }"