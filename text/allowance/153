"function depositCollateralInternal(        BalanceSheetInterface balanceSheet,        FyTokenInterface fyToken,        uint256 collateralAmount    ) internal {        Erc20Interface collateral = fyToken.collateral();        uint256 allowance = collateral.<mask0>;        if (allowance < collateralAmount) {            collateral.approve(address(balanceSheet), type(uint256).max);        }        bool isVaultOpen = balanceSheet.isVaultOpen(fyToken, address(this));        if (isVaultOpen == false) {            balanceSheet.openVault(fyToken);        }        balanceSheet.depositCollateral(fyToken, collateralAmount);    }"