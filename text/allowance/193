"function safeDecreaseAllowance(        IERC20 token,        address spender,        uint value    ) internal {        uint newAllowance =            token.<mask0>.sub(                value,                \"SafeERC20: decreased allowance below zero\"            );        _callOptionalReturn(            token,            abi.encodeWithSelector(token.approve.selector, spender, newAllowance)        );    }"