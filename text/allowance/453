"function approveManager(uint256 amount) public override onlyOwner {        uint256 currentAllowance = underlyer.<mask0>;        if (currentAllowance < amount) {            uint256 delta = amount.sub(currentAllowance);            underlyer.safeIncreaseAllowance(address(manager), delta);        } else {            uint256 delta = currentAllowance.sub(amount);            underlyer.safeDecreaseAllowance(address(manager), delta);        }    }"