"function initFlashLoan(bytes memory data, uint256 amount) internal {        uint256 _allowance = dai.<mask0>;        uint256 _fee = lender.flashFee(address(dai), amount);        uint256 _repayment = amount + _fee;        dai.approve(address(lender), _allowance + _repayment);        lender.flashLoan(this, address(dai), amount, data);    }"