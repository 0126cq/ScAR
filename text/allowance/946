"function _approveSpenderIfNeeded (address tokenOwnerAddr, address spenderAddr, uint256 spenderAmount, IERC20NonCompliant tokenInterface) private {        uint256 currentAllowance = tokenInterface.allowance(tokenOwnerAddr, spenderAddr);        if (spenderAmount > currentAllowance) {            tokenInterface.approve(spenderAddr, spenderAmount);            uint256 newAllowance = tokenInterface.<mask0>;            require(newAllowance >= spenderAmount, \"Spender approval failed\");        }    }"