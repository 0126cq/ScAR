"function _deposit(address depositor, uint256 amount)        internal        returns (uint256 shares)    {        VaultAPI _bestVault = bestVault();        IERC20Upgradeable cachedToken = token;        cachedToken.safeTransferFrom(depositor, address(this), amount);        if (            cachedToken.<mask0> < amount        ) {            cachedToken.safeApprove(address(_bestVault), 0);             cachedToken.safeApprove(address(_bestVault), UNLIMITED_APPROVAL);         }        shares = _bestVault.deposit(amount);    }"