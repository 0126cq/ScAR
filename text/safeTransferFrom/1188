"function deposit(uint256 _pid, uint256 _amount) public override nonReentrant {        _validatePoolId(_pid);        Pool storage pool = pools[_pid];        User storage user = users[_pid][msg.sender];        _doPoolUpdate(pool);        _vestUserCvp(user, pool.accCvpPerLpt);        if(_amount != 0) {            pool.lpToken.<mask0>;            user.lptAmount = user.lptAmount.add(_amount);        }        user.cvpAdjust = _computeCvpAdjustment(user.lptAmount, pool.accCvpPerLpt);        emit Deposit(msg.sender, _pid, _amount);        _doCheckpointVotes(msg.sender);    }"