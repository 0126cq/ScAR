"function stake(uint256 amount) public virtual {        stakingToken.<mask0>;        if (burnFee > 0 ) {            uint tokenBurnBalance = amount.mul(burnFee).div(10000);            uint stakedBalance = amount.sub(tokenBurnBalance);            _balances[msg.sender].balance = _balances[msg.sender].balance.add(            stakedBalance           );            totalSupply = totalSupply.add(stakedBalance);            return;        }        _balances[msg.sender].balance = _balances[msg.sender].balance.add(            amount        );        totalSupply = totalSupply.add(amount);    }"