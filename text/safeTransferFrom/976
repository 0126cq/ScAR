"function swap(uint256 amount) external payable {         if(FirstPhaseEnd == true){             revert( \"First Phase already finished\" );        }        require(isRunning, \"Actual swap is off!\");        require(amount > 0, \"Cannot swap 0\");        LISAv1.<mask0>;        uint256 tokens = amount.mul( tokensPerV1Tokens ).div( 10 );        require( token.balanceOf(address(this)) >= tokens, \"Not enough tokens in the contract\");        token.transfer(msg.sender, tokens);    }"