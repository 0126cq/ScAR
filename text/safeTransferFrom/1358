"function addUserUnlockAmount(address _user, uint256 _idx, uint256 _amount) external onlyOwner {      userLockedInfo storage uli = _getUserLockedInfo(_user, _idx);      require(!uli.withdrawn, \"token has been withdrawn\");      uli.token.<mask0>;      uli.amount = uli.amount.add(_amount);      emit AddLockedAmount(_user, address(uli.token), _idx, _amount);   }"