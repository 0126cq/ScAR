"function zapStake(uint256 cookAmount, address userAddress)        external        override    {        checkMiningPaused();        ensureAddrNotBlacklisted(userAddress);        checkPoolStakeCapLimit(cookAmount);        checkPerAddrStakeLimit(cookAmount, userAddress);        updateStakeStates(cookAmount, userAddress);        cook().<mask0>;        cookBalanceCheck();        emit ZapCook(userAddress, cookAmount);    }"