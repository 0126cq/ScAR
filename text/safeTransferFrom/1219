"function reserveWithToken(address _tokenAddress, uint256 _tokenAmount)        external    {        IERC20 token = IERC20(_tokenAddress);        token.<mask0>;        token.approve(address(uniswapRouter), _tokenAmount);        address[] memory _path = preparePath(_tokenAddress);        uint256[] memory amounts = uniswapRouter.swapExactTokensForETH(            _tokenAmount,            minInvest,            _path,            address(this),            block.timestamp        );        _reserve(msg.sender, amounts[1]);    }"