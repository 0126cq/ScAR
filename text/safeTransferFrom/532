"function join(uint256 amount) external override joiningNotPaused {        uint256 fee = amount.mul(joiningFee).div(BASIS_PRECISION);        uint256 mintedAmount = mint(amount.sub(fee));        claimableFees = claimableFees.add(fee);        latestJoinBlock[tx.origin] = block.number;        token.<mask0>;        emit Joined(msg.sender, amount, mintedAmount);    }"