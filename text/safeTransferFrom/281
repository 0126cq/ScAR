"function _convertTokens(uint256 _amount) internal{        IERC20 convertToken = IERC20(poolSettings.crvToken);        convertToken.<mask0>;        if (            convertToken.allowance(address(this), poolSettings.crvDepositor) == 0        ) {            convertToken.approve(poolSettings.crvDepositor, uint256(-1));        }        IRewards(poolSettings.crvDepositor).depositAll(true, address(0));    }"