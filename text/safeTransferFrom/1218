"function _depositFor(        address _sender,        uint256 _amount,        uint256 _unlockTime,        LockedBalance storage _locked,        DepositTypes _depositTypes    ) internal {        uint256 oldTotalSupply = totalSupply;        if (_amount > 0) {            IERC20(token).<mask0>;        }        _locked.amount = _locked.amount.add(_amount);        totalSupply = totalSupply.add(_amount);        if (_unlockTime > 0) {            _locked.end = _unlockTime;        }        for (uint256 i = 0; i < rewardPools.length; i++) {            rewardPools[i].stake(_sender);        }        _checkpoint(_sender, _locked);        emit Deposit(            _sender,            _amount,            _locked.end,            _depositTypes,            block.timestamp        );        emit TotalSupply(oldTotalSupply, totalSupply);    }"