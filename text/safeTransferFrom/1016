"function addCardMintedWithAmount(uint256 cardId, uint256 amount) public onlyOwner {        require(momijiToken.balanceOf(msg.sender, cardId) >= amount, \"You don't have enough Cards\");        momijiToken.<mask0>;        cardMintedAmountWithId[cardId] = cardMintedAmountWithId[cardId].add(amount);        amountWithId[cardId] = amountWithId[cardId].add(amount);        for (uint256 i = 0; i < amount; i ++) {            _prizePool[cardAmount + i] = cardId;        }        cardAmount = cardAmount.add(amount);        emit AddCardMinted(cardId, amount, cardAmount);    }"