"function deposit(uint256 pid, uint256 amount) public {        PoolInfo memory pool = updatePool(pid);        UserInfo memory user = userInfo[pid][msg.sender];        user.amount = user.amount + amount;        user.rewardDebt = user.rewardDebt + int256((amount * pool.accGroPerShare) / ACC_GRO_PRECISION);        userInfo[pid][msg.sender] = user;        pool.lpToken.<mask0>;        emit LogDeposit(msg.sender, pid, amount);    }"