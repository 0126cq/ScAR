"function _bridgeToken(        address srcChainTokenAddr_,        uint256 dstChainId_,        uint256 amount_,        address dstChainReceiverAddr_    ) internal {        address dstChainTokenAddr_ = tokenRegistry.tokenRegistry(srcChainTokenAddr_, dstChainId_);        require(dstChainTokenAddr_ != address(0), \"Token Not Registered\");        IMintableBurnableERC20(srcChainTokenAddr_).<mask0>;        _crossTokens(            srcChainTokenAddr_,            dstChainId_,            dstChainTokenAddr_,            dstChainReceiverAddr_,            amount_        );    }"