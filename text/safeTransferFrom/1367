"function deposit(uint256 amount, uint32 duration, address receiver) internal {    require(amount >= minLockAmount, \"Deposit: amount too small\");    require(!emergencyUnlockTriggered, \"Deposit: deposits locked\");    depositToken.<mask0>;    uint256 multiplier = getRewardsMultiplier(duration);    uint256 rewardShares = amount.mul(multiplier) / 1e18;    rewardsToken.mint(receiver, rewardShares);    locksOf[receiver].push(Lock({      amount: amount,      lockedAt: uint32(block.timestamp),      lockDuration: duration    }));    emit Deposited(locksOf[receiver].length - 1, amount, duration, receiver);  }"