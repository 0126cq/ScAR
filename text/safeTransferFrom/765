"function buyToken(uint256 _id) external payable onlyNotPaused  {      Nft memory nft = tokens[_id];      require(nft.isForSale, \"Not for sale\");      require(msg.value >= nft.minPrice, \"Minimum price not met\");      require(msg.sender != nft.owner, \"Already owner\");      nft.isForSale = false;      nft.minPrice = 0;      tokens[_id] = nft;      nft.owner.transfer(msg.value);      this.<mask0>;       setApprovalForAll(contractAddress, false);      emit BuyToken(_id, msg.value);    }"