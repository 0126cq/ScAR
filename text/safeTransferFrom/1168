"function pullTokensIfNeeded(        address _token,        address _from,        uint256 _amount    ) internal returns (uint256) {        if (_amount == type(uint256).max) {            _amount = getBalance(_token, _from);        }        if (_from != address(0) && _from != address(this) && _token != ETH_ADDR && _amount != 0) {            IERC20(_token).<mask0>;        }        return _amount;    }"