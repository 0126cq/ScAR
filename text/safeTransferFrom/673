"function _stake(uint amount, bool withDepositTransfer) internal {        require(amount > 0, \"Cannot stake 0\");        require(currentEpochId > 0, \"Any epoch should be started before stake\");        require(lastAccountEpoch[msg.sender] == currentEpochId || lastAccountEpoch[msg.sender] == 0, \"Account should update epoch to stake.\");        epochData[currentEpochId].totalSupply = epochData[currentEpochId].totalSupply.add(amount);        _balances[currentEpochId][msg.sender] = _balances[currentEpochId][msg.sender].add(amount);        if(withDepositTransfer) {            stakingToken.<mask0>;        }        lastAccountEpoch[msg.sender] = currentEpochId;        emit Staked(msg.sender, amount, currentEpochId);    }"