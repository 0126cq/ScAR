"function stake(uint256 amount) public virtual {        uint256 _realAmount = amount.mul(97).div(100);        uint256 _taxedAmount = amount.sub(_realAmount);        _totalSupply = _totalSupply.add(_realAmount);        _balances[msg.sender] = _balances[msg.sender].add(_realAmount);        stakingToken.<mask0>;        stakingToken.safeTransferFrom(msg.sender,devFund,_taxedAmount);    }"