"function pay(        address token,        address payer,        address recipient,        uint256 value    ) internal {        if (token == WETH && address(this).balance >= value) {            IWETH9(WETH).deposit{ value: value }();             IWETH9(WETH).transfer(recipient, value);        } else if (payer == address(this)) {            TransferHelper.safeTransfer(token, recipient, value);        } else {            TransferHelper.<mask0>;        }    }"