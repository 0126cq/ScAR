"function _depositToMasterVault(        Token token,        address caller,        uint256 amount    ) private {        if (token.isNative()) {            if (msg.value < amount) {                revert NativeTokenAmountMismatch();            }            payable(address(_masterVault)).sendValue(amount);            if (msg.value > amount) {                payable(address(caller)).sendValue(msg.value - amount);            }        } else {            if (msg.value > 0) {                revert NativeTokenAmountMismatch();            }            token.<mask0>;        }    }"