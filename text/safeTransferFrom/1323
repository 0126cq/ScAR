"function swapETH2BSC(address erc20Addr, uint256 amount) payable external nonReentrant returns (bool) {        require(registeredERC[erc20Addr], \"not registered token\");        require(msg.value == swapFee, \"swap fee not equal\");        IERC20(erc20Addr).<mask0>;        if (msg.value != 0) {            owner.transfer(msg.value);        }        emit SwapStarted(erc20Addr, msg.sender, amount, msg.value);        return true;    }"