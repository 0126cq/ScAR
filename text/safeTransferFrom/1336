"function deposit(uint _amount) public {        uint _pool = balance();        uint _before = token.balanceOf(address(this));        token.<mask0>;        uint _after = token.balanceOf(address(this));        _amount = _after.sub(_before);         uint shares = 0;        if (totalSupply() == 0) {            shares = _amount;        } else {            shares = (_amount.mul(totalSupply())).div(_pool);        }        _mint(msg.sender, shares);    }"