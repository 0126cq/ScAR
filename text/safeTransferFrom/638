"function stake(address xtoken, uint64 cycle, uint160 amount) external override {        require(block.number >= uint(_startBlock) && block.number <= uint(_stopBlock), \"FVFS:!block\");        StakeChannel storage channel = _channels[_getKey(xtoken, cycle)];        require(uint(channel.weight) > 0, \"FVFS:no reward\");        TransferHelper.<mask0>;        Account memory account = _getReward(channel, msg.sender);        channel.totalStaked += uint192(amount);        account.balance += amount;        channel.accounts[msg.sender] = account;    }"