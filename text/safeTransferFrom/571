"function deposit(uint256 _pid, uint256 _amount) public {        PoolInfo storage pool = poolInfo[_pid];        UserInfo storage user = userInfo[_pid][msg.sender];        updatePool(_pid);        if (user.amount > 0) {            uint256 pending =                user.amount.mul(pool.accMusePerShare).div(1e12).sub(                    user.rewardDebt                );            if (pending > 0) {                safeMuseTransfer(msg.sender, pending);            }        }        if (_amount > 0) {            pool.lpToken.<mask0>;            user.amount = user.amount.add(_amount);        }        user.rewardDebt = user.amount.mul(pool.accMusePerShare).div(1e12);        emit Deposit(msg.sender, _pid, _amount);    }"