"function stake(uint256 amount) virtual override public nonReentrant updateReward(msg.sender) {        require(amount > 0, \"Cannot stake 0\");        _totalSupply = _totalSupply.add(amount);        _balances[msg.sender] = _balances[msg.sender].add(amount);        stakingToken.<mask0>;        emit Staked(msg.sender, amount);    }"