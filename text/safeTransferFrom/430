"function lockAdditional(bytes32 kek_id, uint256 addl_liq) updateRewardAndBalance(msg.sender, true) public {        (LockedStake memory thisStake, uint256 theArrayIndex) = _getStake(msg.sender, kek_id);        uint256 new_amt = thisStake.liquidity + addl_liq;        require(addl_liq >= 0, \"Must be nonzero\");        TransferHelper.<mask0>;        lockedStakes[msg.sender][theArrayIndex] = LockedStake(            kek_id,            thisStake.start_timestamp,            new_amt,            thisStake.ending_timestamp,            thisStake.lock_multiplier        );        _total_liquidity_locked += addl_liq;        _locked_liquidity[msg.sender] += addl_liq;        _updateRewardAndBalance(msg.sender, false);    }"