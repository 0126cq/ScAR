"function burnSingleERC721Capsule(address _capsule, uint256 _id)        external        nonReentrant        onlyValidCapsuleCollections(_capsule)    {        SingleERC721Capsule memory _capsuleData = singleERC721Capsule[_capsule][_id];        require(_capsuleData.tokenAddress != address(0), Errors.NOT_ERC721_CAPSULE_ID);        address heldTokenAddress = _capsuleData.tokenAddress;        uint256 tokenId = _capsuleData.id;        delete singleERC721Capsule[_capsule][_id];        string memory uri = ICapsule(_capsule).tokenURI(_id);        ICapsule(_capsule).burn(_msgSender(), _id);        IERC721(heldTokenAddress).<mask0>;        emit SingleERC721CapsuleBurnt(_msgSender(), _capsule, heldTokenAddress, tokenId, uri);    }"