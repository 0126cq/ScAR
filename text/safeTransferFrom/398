"function _charge(uint256 _amount) internal nonReentrant {        bool success = true;        uint256 shouldChargedETH;        if(address(chargeToken) != address(0)){            uint256 balance = chargeToken.balanceOf(msg.sender);            require(balance >= _amount, \"StarBlockCollection: not enough token!\");            chargeToken.<mask0>;        }else{            require(msg.value >= _amount, \"StarBlockCollection: not enough ETH!\");            shouldChargedETH = _amount;        }        if (msg.value > shouldChargedETH) {            success = _transferETH(payable(msg.sender), msg.value - shouldChargedETH);        }        require(success, \"StarBlockCollection: charge failed!\");    }"