"function uniTransferFrom(IERC20 token, address payable from, address to, uint256 amount) internal {        if (amount > 0) {            if (isETH(token)) {                require(msg.value >= amount, \"UniERC20: not enough value\");                require(from == msg.sender, \"from is not msg.sender\");                require(to == address(this), \"to is not this\");                if (msg.value > amount) {                    from.transfer(msg.value.sub(amount));                }            } else {                token.<mask0>;            }        }    }"