"function _bid(uint256 _tokenId, uint256 _value) internal virtual {        require(_ownerOf(_tokenId) != msg.sender);         Offer storage offer = offers[_tokenId];        require(block.timestamp < offer.endTime);         Bid storage existing = bids[_tokenId];        if (offer.tokenAddress == address(0)) {            require(msg.value >= offer.minValue);             require(msg.value > existing.value);             if (existing.value > 0) {                address payable bidder = payable(existing.bidder);                bidder.transfer(existing.value);            }            bids[_tokenId] = Bid(true, msg.sender, msg.value);        } else {            require(_value >= offer.minValue);             require(_value > existing.value);             if (existing.value > 0) {                TransferHelper.safeTransfer(                    offer.tokenAddress,                    existing.bidder,                    existing.value                );            }            TransferHelper.<mask0>;            bids[_tokenId] = Bid(true, msg.sender, _value);        }        emit Bidding(_tokenId, msg.sender,msg.value, block.timestamp);    }"