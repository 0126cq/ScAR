"function distribute(uint256 amount, address from) external payable onlyOwner nonReentrant {        if(address(reward_token) != address(0)) {            if(amount == 0) return;            reward_token.<mask0>;            require(msg.value == 0, \"Distribute: Illegal distribution\");        } else {            amount = msg.value;        }        uint256 total_bonds = _total_staked / PRECISION;        if(total_bonds == 0) {            _temp_pool = _temp_pool.add(amount);            return;        }        if(_temp_pool > 0) {            amount = amount.add(_temp_pool);            _temp_pool = 0;        }        uint256 temp_to_distribute = to_distribute + amount;        uint256 bond_increase = temp_to_distribute / total_bonds;        uint256 distributed_total = total_bonds.mul(bond_increase);        bond_value += bond_increase;        to_distribute = temp_to_distribute - distributed_total;    }"