"function universalTransferFrom(        IERC20 token,        address from,        address to,        uint256 amount    )        internal    {        if (amount == 0) {            return;        }        if (isETH(token)) {            require(from == msg.sender && msg.value >= amount, \"Wrong useage of ETH.universalTransferFrom()\");            if (to != address(this)) {                payable(to).transfer(amount);            }        } else {            token.<mask0>;        }    }"