"function stake(uint256[] memory _tokenIds) public {    require(_tokenIds.length > 0, \"no token IDs sent\");    for (uint256 i = 0; i < _tokenIds.length; i++) {      uint256 tokenId = _tokenIds[i];      ownerOf[tokenId] = msg.sender;      stakedTokens[msg.sender].add(tokenId);      stakedAt[tokenId] = block.timestamp;      erc721.<mask0>;      emit Staked(msg.sender, tokenId, stakedAt[tokenId]);    }  }"