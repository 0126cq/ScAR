"function deposit(uint256 _pid, uint256 _amount) public {        PoolInfo storage pool = poolInfo[_pid];        UserInfo storage user = userInfo[_pid][msg.sender];        updatePool(_pid);        if (user.amount > 0) {            uint256 pending =                user.amount.mul(pool.accEswPerShare).div(1e12).sub(user.rewardDebt).add(user.remainingEswTokenReward);            user.remainingEswTokenReward = safeRewardTransfer(msg.sender, pending);        }        pool.stakingToken.<mask0>;        user.amount = _amount.add(user.amount);        pool.stakingTokenTotalAmount = _amount.add(pool.stakingTokenTotalAmount);        user.rewardDebt = user.amount.mul(pool.accEswPerShare).div(1e12);        emit Deposit(msg.sender, _pid, _amount);    }"