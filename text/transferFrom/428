"function distributeBonuses(uint _amount) public onlyDistributionSource returns (uint) {        ERC20Interface _bonusToken = ERC20Interface(bonusToken);        if (_bonusToken.allowance(msg.sender, address(this)) < _amount) {            return _emitError(PROFITEROLE_ERROR_INSUFFICIENT_DISTRIBUTION_BALANCE);        }        if (!_bonusToken.<mask0>) {            return _emitError(PROFITEROLE_ERROR_TRANSFER_ERROR);        }        if (firstDepositDate == 0) {            firstDepositDate = now;        }        uint _lastDepositDate = lastDepositDate;        if (_lastDepositDate != 0) {            distributionDeposits[_lastDepositDate].nextDepositDate = now;        }        lastDepositDate = now;        distributionDeposits[now] = Deposit(_amount, _amount, 0);        Treasury(treasury).addDistributionPeriod();        DepositPendingAdded(_amount, msg.sender, now);        return OK;    }"