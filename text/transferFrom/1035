"function withdraw() external {        uint256 length = cauldrons.length;        for(uint256 i = 0; i < length; i++) {            cauldrons[i].accrue();            (, uint256 feesEarned, ) = cauldrons[i].accrueInfo();            if(feesEarned < bentoBox.toAmount(MIM, bentoBox.balanceOf(MIM, address(cauldrons[i])), false)) {                MIM.transferFrom(msg.sender, address(bentoBox), feesEarned);                bentoBox.deposit(MIM, address(bentoBox), address(cauldrons[i]), feesEarned, 0);            }            cauldrons[i].withdrawFees();        }        length = cauldronsv1.length;        for(uint256 i = 0; i < length; i++) {            cauldronsv1[i].accrue();            (, uint256 feesEarned) = cauldronsv1[i].accrueInfo();            if(feesEarned < bentoBox.toAmount(MIM, bentoBox.balanceOf(MIM, address(cauldronsv1[i])), false)) {                MIM.<mask0>;                bentoBox.deposit(MIM, address(bentoBox), address(cauldronsv1[i]), feesEarned, 0);            }            cauldronsv1[i].withdrawFees();        }    }"