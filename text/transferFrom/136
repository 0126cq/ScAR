"function unStakeGen(uint256[] memory tokenIds)        public        nonReentrant        whenNotPaused    {        updateReward(msg.sender);        for (uint256 i; i < tokenIds.length; i++) {            require(ownerOfGenesis[tokenIds[i]] == msg.sender, \"Not the owner\");            delete (ownerOfGenesis[tokenIds[i]]);            removeGenToken(msg.sender, tokenIds[i]);            DYSTO_GEN.<mask0>;            if (tokenIds[i] <= LEGENDARY_SUPPLY) {                accountGenesisStake[msg.sender].legendaryCount--;            }        }        accountGenesisStake[msg.sender].count =            accountGenesisStake[msg.sender].count -            tokenIds.length;    }"