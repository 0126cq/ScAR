"function _transferCurrencyForSale(address from, address to, address currencyToken, uint256 currencyAmount, uint256 feePct) internal returns (bool){    uint256 feeAmount = (currencyAmount * feePct)/(10000);    if(currencyToken == NATIVE_ETH){        require(msg.value == currencyAmount,'incorrect payment value');       payable(to).transfer( currencyAmount - (feeAmount) );      payable(owner).transfer( feeAmount );    }else{      require(msg.value == 0,'incorrect payment value');       require( IERC20(currencyToken).<mask0>, 'unable to pay' );      require( IERC20(currencyToken).transferFrom(from, owner, feeAmount ), 'unable to pay'  );     }    return true;  }"