"function stake(uint8 _class) public {        require(_class < 5, \"Wrong class\");         uint256 _amount = amounts[_class];        require(myActiveStakesCount(msg.sender) < MAX_STAKES, \"MAX_STAKES overflow\");         uint256 _finalAmount = (_amount * rates[_class]) / 100;        require(rewardsPool >= _finalAmount - _amount, \"Rewards pool is empty for now\");        rewardsPool -= _finalAmount - _amount;        require(TKN.<mask0>);        uint256 _index = stakes.length;        stakesOf[msg.sender].push(_index);        stakes.push(Stake({            class: _class,            cycle: 1,            initialAmount: _amount,            finalAmount: _finalAmount,            timestamp: block.timestamp,            unstaked: false        }));        ownerOf[_index] = msg.sender;        emit Staked(msg.sender, _class, _amount, _finalAmount);    }"