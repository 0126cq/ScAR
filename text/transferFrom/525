"function transferFrom(address _from, address _to, uint256 _amount) public returns (bool success) {        if (isContract(controller)) {            require (TokenController(controller).onTransfer(_from, _to, _amount));        }        success = super.<mask0>;        if (success && isContract(_to)) {            if(!_to.call(bytes4(keccak256(\"tokenFallback(address,uint256)\")), _from, _amount)) {                emit ReceivingContractTokenFallbackFailed(_from, _to, _amount);            }        }    }"