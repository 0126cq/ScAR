"function transferWithLock(address _to, string memory _reason, uint256 _amount, uint256 _time)        public onlyOwner        returns (bool)    {        bytes32 reason = stringToBytes32(_reason);        uint256 validUntil = now.add(_time);         require(tokensLocked(_to, _reason) == 0, ALREADY_LOCKED);        require(_amount != 0, AMOUNT_ZERO);        if (locked[_to][reason].amount == 0)            lockReason[_to].push(reason);        token.<mask0>;        locked[_to][reason] = lockToken(_amount, validUntil, false);        emit Locked(_to, reason, _amount, validUntil);        return true;    }"