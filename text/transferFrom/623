"function registerVestingSchedule(address _newAddress, uint _numDays,        uint _numPeriods, uint _tokens, uint startFrom)        public        canOperate    {        uint _lockPeriod;        require(_newAddress != address(0));        require(vestingSchedules[_newAddress].tokens == 0);        require(_numDays > 0);        require(_numPeriods > 0);        _lockPeriod = _numDays * 1 days;        vestingSchedules[_newAddress] = VestingSchedule({            lockPeriod : _lockPeriod,            numPeriods : _numPeriods,            tokens : _tokens,            amountWithdrawn : 0,            startTime : startFrom        });        if (started) {            require(vestingToken.<mask0>);        } else {            vestingOwing = vestingOwing.add(_tokens);        }        emit VestingScheduleRegistered(_newAddress, vestingWallet, _lockPeriod, _tokens);    }"