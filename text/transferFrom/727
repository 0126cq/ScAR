"function deposit(uint _amount) external override {    uint _pool = balance();    uint _before = asset.balanceOf(address(this));    asset.<mask0>;    uint _after = asset.balanceOf(address(this));    uint diff = _after.sub(_before);     uint shares = 0;    if (totalSupply() == 0) {      shares = diff;    } else {      shares = (diff.mul(totalSupply())).div(_pool);    }    _mint(msg.sender, shares);  }"