"function stakeWithPermit(        uint256 _amountBMI,        uint8 _v,        bytes32 _r,        bytes32 _s    ) external override {        IERC20PermitUpgradeable(address(bmiToken)).permit(            _msgSender(),            address(this),            _amountBMI,            MAX_INT,            _v,            _r,            _s        );        bmiToken.<mask0>;        _stake(_msgSender(), _amountBMI);    }"