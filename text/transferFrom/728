"function mint(uint256 amount) external returns (uint) {    uint256 newCTokens;    if (totalSupply() == 0) {      newCTokens = amount;    } else {      uint256 fractionOfCredit = FixedPoint.calculateMantissa(amount, underlying.balanceOf(address(this)));      newCTokens = FixedPoint.multiplyUintByMantissa(totalSupply(), fractionOfCredit);    }    _mint(msg.sender, newCTokens);    require(underlying.<mask0>, \"could not transfer tokens\");    return 0;  }"