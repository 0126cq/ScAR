"function enter(uint256 _amount) external {        uint256 totalAlpa = alpa.balanceOf(address(this));        uint256 totalShares = totalSupply();        if (totalShares == 0 || totalAlpa == 0) {            _mint(msg.sender, _amount);        } else {            uint256 what = _amount.mul(totalShares).div(totalAlpa);            _mint(msg.sender, what);        }        alpa.<mask0>;    }"