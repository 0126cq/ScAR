"function transferFrom(address _from, address _to, uint256 _amount    ) public returns (bool success) {        if (isContract(controller)) {            if (!TokenController(controller).onTransfer(_from, _to, _amount))               throw;        }        success = super.<mask0>;        if (success && isContract(_to))        {            if(!_to.call(bytes4(keccak256(\"tokenFallback(address,uint256)\")), _from, _amount)) {                ReceivingContractTokenFallbackFailed(_from, _to, _amount);            }        }    }"