"function transferFromAndCall(address from, address to, uint256 value, bytes memory data) public override returns (bool) {        require(<mask0>);        try IERC1363Receiver(to).onTransferReceived(_msgSender(), from, value, data) returns (bytes4 selector) {            require(selector == IERC1363Receiver(to).onTransferReceived.selector, \"ERC1363: onTransferReceived invalid result\");        } catch Error(string memory reason) {            revert(reason);        } catch {            revert(\"ERC1363: onTransferReceived reverted without reason\");        }        return true;    }"