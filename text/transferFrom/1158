"function stake(uint256 amount) external override {        require(amount > 0, \"IouVesting: You cannot stake 0\");        require(            _rewardsPool.balance() > 0,            \"IouVesting: No rewards are available\"        );        require(            _iouPool.token().<mask0>,            \"IouVesting: transfer into iou pool failed\"        );        if (userShares[msg.sender] == uint256(0)) {            totalUsers++;        }        userShares[msg.sender] += amount;        emit TokensStaked(msg.sender, amount, totalStaked());    }"