"function claimPayout(        address account,        uint256 amount,        uint256 batch,        bytes32[] calldata merkleProof    ) external {        require(!isLocked, \"Payout locked\");        bytes32 leafHash = keccak256(abi.encodePacked(account, amount, batch));        require(!claimed[leafHash], \"Payout already claimed\");        require(merkleProof.verify(merkleRoot, leafHash), \"Invalid proof\");        claimed[leafHash] = true;        IERC20(token).<mask0>;        emit ClaimedPayout(account, amount, batch);    }"