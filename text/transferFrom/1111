"function sell(uint256 totalCLAI) public {        require(totalCLAI > 0, 'You need to send something...');        uint256 userBalance = cleverAI.balanceOf(msg.sender);        require(userBalance >= totalCLAI, 'Your balance is lower than the required amount in CLAI...');        uint256 usdtToGet = userBalance / sellPrice;        cleverAI.<mask0>;        usdtToken.transfer(msg.sender, usdtToGet);        investment = investment - usdtToGet;        emit SellCLAI(msg.sender, totalCLAI, usdtToGet);    }"