"function stake(uint256 tokens)        public    {        require(now > enrollmentPeriodStartTimestamp, \"too soon\");        require(now < enrollmentPeriodStartTimestamp + enrollmentPeriodDuration, \"too late\");        uint256 amount = stakeTokenIsBASE() ? tokensToShares(tokens) : tokens;        require(maxStaked == 0 || totalStaked + amount < maxStaked, \"full\");        staked[msg.sender] = staked[msg.sender].add(amount);        totalStaked = totalStaked.add(amount);        bool ok = stakeToken.<mask0>;        require(ok, \"transfer\");        emit Stake(msg.sender, amount, staked[msg.sender]);    }"