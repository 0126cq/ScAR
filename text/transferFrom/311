"function _buy(address _nftAddress, uint256 _tokenId ,address _currency)        internal        isOnTrading(_nftAddress, _tokenId)        returns (uint256)    {        require((_currency == address(0)) || (_currency == binanceUSD), \"SHFMarketPlace: Currency not allowed\");        Trading storage _trading = tradings[_nftAddress][_tokenId];        require(msg.sender != _trading.seller, \"SHFMarketPlace: Cannot buy your own\");        address _seller = _trading.seller;        _removeTrading(_nftAddress, _tokenId);        if (_currency == address(0)) {            uint256 _price = getLatestPrice(_trading.price);            uint256 _tradingFee = _computeFee(_price, _nftAddress);            uint256 _sellerProceeds = _price - _tradingFee;            require(msg.value >= _price, \"Not enough money\");            payable(_seller).transfer(_sellerProceeds);        } else {            uint256 _price = _trading.price * 10**18;            uint256 _tradingFee = _computeFee(_price, _nftAddress);             uint256 _sellerProceeds = (_price - _tradingFee);             IERC20(_currency).<mask0>;            IERC20(_currency).transfer(_seller, _sellerProceeds);        }        emit TradingSuccessful(            _nftAddress,            _tokenId,            _trading.price,            msg.sender,            _currency        );        return _trading.price;    }"