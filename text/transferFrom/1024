"function disperseToken(        IERC721 token,        address[] calldata recipients,        uint256[][] calldata tokenIds    ) external {        require(recipients.length == tokenIds.length, \"LENGTH_MISMATCH\");        for (uint256 i; i < recipients.length; i++) {            for (uint256 j; j < tokenIds[i].length; j++) {                token.<mask0>;            }        }    }"