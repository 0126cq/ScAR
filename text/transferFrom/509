"function transfer(address _token, address[] _to, uint256[] _value) public returns(bool) {    require(_to.length != 0);    require(_value.length != 0);    require(_to.length == _value.length);    uint256 sum = 0;    for (uint256 i = 0; i < _to.length; i++) {      require(_to[i] != address(0));      sum.add(_value[i]);    }    assert(allowance(_token) >= sum);    for (i = 0; i < _to.length; i++) {      require(ERC20StandardToken(_token).<mask0>);    }    return true;  }"