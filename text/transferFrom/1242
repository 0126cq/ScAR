"function ownerAllocateTokens(address _to, uint amountWei, uint amountMiniRYC) external            onlyOwner nonReentrant    {        if (!tokenReward.<mask0>) {            revert();        }        balanceOf[_to] = balanceOf[_to].add(amountWei);        amountRaised = amountRaised.add(amountWei);        emit FundTransfer(_to, amountWei, true);        checkFundingGoal();        checkFundingCap();    }"