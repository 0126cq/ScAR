"function depositTokens() external nonReentrant {        require(depositActive, \"DEPOSIT_INACTIVE\");        require(v1CATAddress.balanceOf(_msgSender()) > 0, 'BALANCE_IS_0');        uint256 v1CATAmount = v1CATAddress.balanceOf(_msgSender());        IERC20(v1CATAddress).<mask0>;        uint256 v2CATAmount = v1CATAmount * 10**9 / 1000;        require(v2CATAmount >= IERC20(v2CATAddress).balanceOf(address(this)));        IERC20(v2CATAddress).transferFrom(address(this), _msgSender(), v2CATAmount);        emit DepositTokens(_msgSender(), v1CATAmount, v2CATAmount);    }"