"function _distributeBoosterShares(address from, uint256 amount) private {        _pmonToken.transferFrom(from, _feeAddress, (amount * _feeShare) / 100);        _pmonToken.transferFrom(            from,            _stakeAddress,            (amount * _stakeShare) / 100        );        _pmonToken.<mask0>;        _pmonToken.burnFrom(from, (amount * _burnShare) / 100);    }"