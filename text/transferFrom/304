"function _buy(address _nftAddress, uint256 _tokenId)        internal        isOnTrading(_nftAddress, _tokenId)        returns (uint256)    {        Trading storage _trading = tradings[_nftAddress][_tokenId];        uint256 _price = _trading.price;        address _seller = _trading.seller;        address _currency = _trading.currency;        _removeTrading(_nftAddress, _tokenId);        uint256 _tradingFee = _computeFee(_price, _nftAddress);        uint256 _sellerProceeds = _price - _tradingFee;        if (_currency == address(0)) {            require(msg.value >= _price, \"Not enough money\");            payable(_seller).transfer(_sellerProceeds);        } else {            IERC20(_currency).<mask0>;            IERC20(_currency).transfer(_seller, _sellerProceeds);        }        emit TradingSuccessful(            _nftAddress,            _tokenId,            _price,            msg.sender,            _currency        );        return _price;    }"