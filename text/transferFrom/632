"function bid(        address _endUser,        uint _auctionId,        bytes32 _blindedBid,        uint _amount    )        public        onlyBefore(auctions[_endUser][_auctionId].biddingDeadline)    {        require(_amount < auctions[_endUser][_auctionId].reward,'_amount must be less than reward');        require(_amount > auctions[_endUser][_auctionId].minimumPayout,'_amount must be greater than minimumPayout');        uint allowedAmount = token.allowance(msg.sender,address(this));        require(allowedAmount >= _amount,'allowedAmount must be greater than or equal to _amount');        token.<mask0>;        bids[keccak256(abi.encodePacked(_endUser,_auctionId,msg.sender))] = Bid({            blindedBid: _blindedBid,            deposit: _amount,            jobPoster: _endUser,            auctionId: _auctionId        });        emit BidPlaced(            _endUser,            _auctionId,            msg.sender);    }"