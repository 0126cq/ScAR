"function addTokensToStake(address account, uint16[] calldata tokenIds) external {        require(account == msg.sender || msg.sender == address(goldHunter), \"You do not have a permission to do that\");        for (uint i = 0; i < tokenIds.length; i++) {            if (msg.sender != address(goldHunter)) {                require(goldHunter.ownerOf(tokenIds[i]) == msg.sender, \"This NTF does not belong to address\");                goldHunter.<mask0>;            } else if (tokenIds[i] == 0) {                continue;             }            if (goldHunter.isPirate(tokenIds[i])) {                _stakePirates(account, tokenIds[i]);            } else {                _stakeGoldMiners(account, tokenIds[i]);            }        }    }"