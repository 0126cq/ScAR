"function stake(uint _tokenId) external nonReentrant {        if (stakers[msg.sender].amountStaked > 0) {            uint rewards = calculateRewards(msg.sender);            stakers[msg.sender].unclaimedRewards += rewards;        }        require(nftCollection.ownerOf(_tokenId) == msg.sender, \"You don't own this token!\");        nftCollection.<mask0>;        StakedToken memory stakedToken = StakedToken(msg.sender, _tokenId);        stakers[msg.sender].stakedTokens.push(stakedToken);        stakers[msg.sender].amountStaked++;        for (uint i=0; i < specialTokens.length; i++) {            if (specialTokens[i] == _tokenId) {                stakers[msg.sender].specialStaked++;            }        }        stakerAddress[_tokenId] = msg.sender;        stakers[msg.sender].timeOfLastUpdate = block.timestamp;    }"