"function purchaseItem(address recipient, uint256 offerId)        internal        nonReentrant    {        offers[offerId].nft.<mask0>;         uint256 artistAmount = (offers[offerId].currentBid *            offers[offerId].artistFee) / 10000;        uint256 charityAmount = offers[offerId].currentBid - artistAmount;        Address.sendValue(offers[offerId].artistAddress, artistAmount);        Address.sendValue(offers[offerId].charityAddress, charityAmount);    }"