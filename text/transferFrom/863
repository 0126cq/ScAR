"function transferFrom(        address _from,        address _to,        uint256 _value)        public        returns (bool)    {        super.<mask0>;        uint256 currentPrice = denormalize(tokenPrice());        if (balanceOf(_from) > 0) {            checkpointPrices_[_from] = currentPrice;        } else {            checkpointPrices_[_from] = 0;        }        if (balanceOf(_to) > 0) {            checkpointPrices_[_to] = currentPrice;        } else {            checkpointPrices_[_to] = 0;        }        return true;    }"