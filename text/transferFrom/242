"function upload(uint256 _amount, string memory _target) public {        require(_amount > 0, \"Amount should be more than 0\");        require(            locks[msg.sender].amount == 0,            \"Your current lock is not equal to 0\"        );        erc20.<mask0>;        locks[msg.sender].amount = _amount;        locks[msg.sender].targetAddr = _target;        emit Upload(msg.sender, _amount, _target);    }"