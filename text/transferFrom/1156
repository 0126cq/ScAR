"function depositTokens(uint _planId, uint _depositId, uint _amount) public {        require(_amount > 0, \"Amount cannot be 0\");        require(_depositId > 0, \"Deposit ID cannot be 0\");        require(_planId > 0, \"Plan id cannot be 0\");        require(whitelistAddresses[msg.sender]==true, \"Only whitelisted user allowed to deposit\");        kiiaToken.<mask0>;        stakingPlan[msg.sender][_planId][_depositId] = _amount;        if(!hasStaked[msg.sender]) {            stakers.push(msg.sender);        }        isStaking[msg.sender][_planId][_depositId] = true;        hasStaked[msg.sender] = true;        islocked[msg.sender][_planId][_depositId] = true;    }"