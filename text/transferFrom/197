"function contributeToPresaleWithUSDC(uint256 amount) public {        require(amount >= minimum,\"Minimum amount not met\");        require(amount <= maximum,\"Maximum amount exceeded\");        USD.<mask0>;        if(presaleContributions[msg.sender] == 0) contributorByID[totalContributors] = msg.sender;        presaleContributions[msg.sender] += amount;        totalContributionAmount += amount;        totalContributors++;    }"