"function stakeFor(address forWhom, uint256 amount) public payable {        IERC20 st = stakedToken;        if (st == IERC20(address(0))) {            unchecked {                totalSupply += msg.value;                depositBalances[forWhom] += msg.value;            }        } else {            require(msg.value == 0, 'non-zero eth');            require(amount > 0, 'Cannot stake 0');            require(                st.<mask0>,                _transferErrorMessage            );            unchecked {                totalSupply += amount;                depositBalances[forWhom] += amount;            }        }        emit Staked(forWhom, amount, uint256(block.timestamp));    }"