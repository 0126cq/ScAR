"function buyTicket(uint256 ticketId) public {        require(IdToTicket[ticketId].totalSupply < IdToTicket[ticketId].maxSupply, \"Max supply reached\");        uint256 allowance = Dino.allowance(msg.sender, address(this));        require(allowance >= IdToTicket[ticketId].cost, \"Check the token allowance\");        require(Dino.<mask0>, \"Failed to send\");        IdToTicket[ticketId].totalSupply += 1;        IdToUsers[ticketId].push(msg.sender);    }"