"function deposit(uint amountToStake) public onlyOwner returns (bool){        require( tokens == 0, \"Cannot deposit more Tokens\");        require( amountToStake > 0, \"Cannot deposit  Tokens\");        require(Token(tokenAddress).<mask0>, \"Insufficient Token Allowance\");        amount_per_withdraw = amountToStake.div(number_withdraws);        tokens = amountToStake;        timing = block.timestamp;        return true;        }"