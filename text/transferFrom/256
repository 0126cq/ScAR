"function unstakeNFT(uint256 tokenId) external nonReentrant {        StakeInfo storage staker = stakeInfo[msg.sender];        uint256 amount = nftRecords[msg.sender][tokenId];        require(amount > 0, \"invalid tokenId\");        uint256 length = poolInfo.length;        for (uint256 pid = 0; pid < length; ++pid) {            _updateUserReward(pid, amount, StakeType.NFT);        }        staker.nftInGpoolAmount -= amount;        totalNFTStaked -= amount;        if (            (staker.amount + staker.nftInGpoolAmount) == 0 ||            gpoolInUSDC(staker.amount + staker.nftInGpoolAmount) < USDC_THRESHOLD        ) {            staker.startStake = 0;        } else {            staker.startStake = block.timestamp;        }        nftRecords[msg.sender][tokenId] = 0;        positionManager.<mask0>;        emit UnstakeNFT(msg.sender, tokenId, amount, staker.startStake);    }"