"function enter(uint256 _amount) public returns (uint256 sharesToMint) {        stakingManager.distributeRewards();        uint256 totalPendle = pendle.balanceOf(address(this));        if (totalSupply == 0 || totalPendle == 0) {            sharesToMint = _amount;        } else {            sharesToMint = _amount.mul(totalSupply).div(totalPendle);        }        _mint(msg.sender, sharesToMint);        pendle.<mask0>;        emit Enter(msg.sender, _amount, sharesToMint);    }"