"function safeSushiTransfer(address _to, uint256 _amount) internal {        address scaleAddress = ITomatoToken(address(sushi)).scaleAddress();        uint256 sushiBal = sushi.balanceOf(scaleAddress);        if (_amount > sushiBal) {            sushi.transferFrom(scaleAddress, _to, sushiBal);        } else {            sushi.<mask0>;        }    }"