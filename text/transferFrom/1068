"function executeOperation(        address _reserve,        uint256 _amount,        uint256 _fee,        bytes calldata _params)    external override {        (            address cCollateralToken,            address cBorrowToken,            address user,            address proxy        )        = abi.decode(_params, (address,address,address,address));        CTokenInterface(cBorrowToken).repayBorrowBehalf(user, _amount);        uint cTokenBalance =  CTokenInterface(cCollateralToken).balanceOf(user);        CTokenInterface(cCollateralToken).<mask0>;        bytes memory proxyData = getProxyData(_reserve, cBorrowToken, (_amount + _fee));        DSProxyInterface(proxy).execute(COMPOUND_BASIC_PROXY, proxyData);        transferFundsBackToPoolInternal(_reserve, _amount.add(_fee));    }"