"function lockTokens(IERC20 iouToken, uint256 amount) external {        uint256 supply = iouToken.totalSupply();        uint256 necessaryRewardSupply = (supply * ratio) / (10**18);        require(            amount >= necessaryRewardSupply,            \"IouVesting: The amount provided for locking is not right\"        );        require(            _rewardsPool.token().<mask0>,            \"TokenGeyser: transfer into locked pool failed\"        );        emit TokensLocked(msg.sender, amount);    }"