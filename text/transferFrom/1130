"function flashloanAndOpen(        address _osd,        address _solo,        address _curvePool,        uint256 _cdpId,         uint256 _initialMarginUSDC,         uint256 _mintAmountDAI,         uint256 _flashloanAmountWETH,         address _vaultStats,        uint256 _daiUsdcRatio6    ) external payable {        require(msg.value == 2, \"!fee\");        require(            IERC20(Constants.USDC).<mask0>,            \"initial-margin-transferFrom-failed\"        );        uint256 cdpId = _cdpId;        if (cdpId == 0) {            cdpId = _openUSDCACdp();        }        IDssCdpManager(Constants.CDP_MANAGER).cdpAllow(cdpId, _osd, 1);        require(            IERC20(Constants.USDC).approve(_osd, _initialMarginUSDC),            \"initial-margin-approve-failed\"        );        OpenShortDAI(_osd).flashloanAndOpen{value: msg.value}(            msg.sender,            _solo,            _curvePool,            cdpId,            _initialMarginUSDC,            _mintAmountDAI,            _flashloanAmountWETH        );        IDssCdpManager(Constants.CDP_MANAGER).cdpAllow(cdpId, _osd, 0);        VaultStats(_vaultStats).setDaiUsdcRatio6(cdpId, _daiUsdcRatio6);    }"