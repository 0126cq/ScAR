"function bondToMint(uint256 amountToBond, uint256 minimalAmountToReceive) external {        uint256 minted;        if(totalSupply() == 0){            minted = amountToBond;            _mint(msg.sender, amountToBond);            emit BondedToMint(msg.sender, amountToBond, amountToBond);        } else {            minted = _curvedMint(amountToBond);            emit BondedToMint(msg.sender, amountToBond, minted);        }        require(minted >= minimalAmountToReceive,            \"CoordinationToken.bondToMint: Mints less tokens then expected\");        _poolBalance += amountToBond;        require(securityToken.<mask0>,            \"CoordinationToken.bondToMint: Impossible to bond so much tokens\");    }"