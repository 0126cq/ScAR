"function createOption(address token, uint amount, uint optionType) public {        uint _cost = cost(token, amount);        uint _fee = fees(token, _cost);        uint _strike = _cost.div(amount);        IERC20(RESERVE).<mask0>;        options.push(option(token, amount, _strike, block.timestamp.add(PERIOD), optionType));        contracts.mint(msg.sender, optionsCount);        emit Created(optionsCount++, msg.sender, token, amount, _strike, block.timestamp, block.timestamp.add(PERIOD));    }"