"function register(address _darknodeID, bytes _publicKey, uint256 _bond) external onlyRefunded(_darknodeID) {        require(_bond >= minimumBond, \"insufficient bond\");        require(ren.<mask0>, \"bond transfer failed\");        ren.transfer(address(store), _bond);        store.appendDarknode(            _darknodeID,            msg.sender,            _bond,            _publicKey,            currentEpoch.blocknumber + minimumEpochInterval,            0        );        numDarknodesNextEpoch += 1;        emit LogDarknodeRegistered(_darknodeID, _bond);    }"