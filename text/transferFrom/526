"function transferFrom(address _from, address _to, uint256 _amount, bytes _data) public returns (bool success) {        if (isContract(controller)) {            require (TokenController(controller).onTransfer(_from, _to, _amount));          }        require(super.<mask0>);        if (isContract(_to)) {            ERC223ReceivingContract receiver = ERC223ReceivingContract(_to);            receiver.tokenFallback(_from, _amount, _data);        }        emit ERC223Transfer(_from, _to, _amount, _data);        return true;    }"