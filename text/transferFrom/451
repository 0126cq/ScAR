"function walletDeposit (IERC20NonCompliant inputTokenInterface, uint256 depositAmount) public onlyOwner ifNotReentrant {        address senderAddr = msg.sender;        require(inputTokenInterface.balanceOf(senderAddr) >= depositAmount, \"Insufficient funds\");        require(inputTokenInterface.allowance(senderAddr, address(this)) >= depositAmount, \"Insufficient allowance\");        uint256 balanceBeforeTransfer = inputTokenInterface.balanceOf(address(this));        inputTokenInterface.<mask0>;        require(inputTokenInterface.balanceOf(address(this)) == balanceBeforeTransfer + depositAmount, \"Balance verification failed\");    }"