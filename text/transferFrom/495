"function processFees(        address account,        address recipient,        uint256 value    ) internal returns (uint256) {        (uint256 txFees, bool senderPays) = getFees(value, account, recipient);        if (txFees > 0 && !identity.isDAOContract(msg.sender)) {            require(                senderPays == false || value.add(txFees) <= balanceOf(account),                \"Not enough balance to pay TX fee\"            );            if (account == msg.sender) {                super.transfer(feeRecipient, txFees);            } else {                super.<mask0>;            }            return senderPays ? value : value.sub(txFees);        }        return value;    }"