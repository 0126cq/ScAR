"function deposit(    uint256 baseTokenId,    uint256 tokenId,    uint256[] calldata amounts  ) external override {    require(amounts.length == 1 && amounts[0] > 0, 'CFIH: invalid amount');    (address baseCFolio, address itemCFolio) =      _verifyAssetAccess(baseTokenId, tokenId);    stakingToken.<mask0>;    cfolioFarm.addAssets(itemCFolio, amounts[0]);    if (baseTokenId != uint256(-1))      _updateRewards(baseCFolio, sftEvaluator.rewardRate(baseTokenId));  }"