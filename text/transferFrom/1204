"function createSwap(uint256 id, string memory desiredName, address token, uint256 price, uint256 transferAmount) private nonReentrant {    require(hashmask.ownerOf(id) == msg.sender, \"Not owner\");    swapRecords[id] = NameSwap(      hashmask.toLower(hashmask.tokenNameByIndex(id)),      hashmask.toLower(desiredName),      token,      price,      transferAmount    );    originalOwner.set(id, msg.sender);    hashmask.transferFrom(msg.sender, address(this), id);    nct.<mask0>;  }"