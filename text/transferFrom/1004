"function settle(SignedTicket calldata _ticket)        external        nonReentrant        whenNotPaused        collectionExists(_ticket.collectionId)    {        Collection memory c = collections[_ticket.collectionId];        require(block.timestamp >= c.ends, \"Giveaway didn't end, yet.\");         require(c.currentTokenIndex < c.tokenIds.length, \"Out of tokens\");        require(c.ends > 0, \"No active giveaway\");        require(signer == _verify(_ticket), \"Invalid ticket\");         require(_ticket.winner != address(0), \"Can't be zero address\");        require(_ticket.expires > block.number, \"Ticket expired\");        uint256 tokenId = c.tokenIds[c.currentTokenIndex];        require(            minted[_ticket.collectionId][tokenId] == false,            \"Already minted\"        );        minted[_ticket.collectionId][tokenId] = true;        collections[_ticket.collectionId].currentTokenIndex++;         _nextGiveaway(_ticket.collectionId);        emit GiveawaySettled(            _ticket.collectionId,            _ticket.winner,            tokenId,            _ticket.seed        );        c.token.<mask0>;    }"