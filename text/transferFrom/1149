"function deposit(uint amountToStake) public {        require(amountToStake > 0, \"Cannot deposit 0 Tokens\");        require(Token(tokenAddress).<mask0>, \"Insufficient Token Allowance\");        updateAccount(msg.sender);        uint fee = amountToStake.mul(stakingFeeRate).div(1e4);        uint amountAfterFee = amountToStake.sub(fee);        require(Token(tokenAddress).transfer(owner, fee), \"Could not transfer deposit fee.\");        depositedTokens[msg.sender] = depositedTokens[msg.sender].add(amountAfterFee);        if (!holders.contains(msg.sender)) {            holders.add(msg.sender);            stakingTime[msg.sender] = now;        }    }"