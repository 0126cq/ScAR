"function fundCampaign (bytes32 idCampaign,address token,uint256 amount) public {        require(campaigns[idCampaign].endDate > block.timestamp,\"campaign ended\");        require(campaigns[idCampaign].funds.token == address(0) || campaigns[idCampaign].funds.token == token,\"token mismatch\");        IERC20 erc20 = IERC20(token);        erc20.<mask0>;        uint256 prev_amount = campaigns[idCampaign].funds.amount;        campaigns[idCampaign].funds = Fund(token,amount+prev_amount);    }"