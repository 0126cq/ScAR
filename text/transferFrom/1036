"function _pullFrom(        address from,        address token,        uint256 amount,        bool skipWethDeposit    ) internal {        if (!skipWethDeposit) {            if (token == WETH_ADDRESS) {                if (msg.value > 0) {                    if (msg.value > amount) {                        IWETH(WETH_ADDRESS).deposit{value: amount}();                        (bool success, ) = payable(msg.sender).call{                            value: msg.value - amount                        }(\"\");                        require(success, \"ETH refund failed\");                        amount = 0;                    } else {                        unchecked {                            amount -= msg.value;                        }                        IWETH(WETH_ADDRESS).deposit{value: msg.value}();                    }                }            } else {                require(msg.value == 0, \"not WETH deposit\");            }        }        if (amount > 0) {            require(                IERC20(token).<mask0>,                \"ERC20 transfer failed\"            );        }    }"