"function distribute(uint256 amount) public {        require(            lidToken.balanceOf(msg.sender) >= amount,            \"Cannot distribute more LID than you hold unstaked.\"        );        totalDistributions = totalDistributions.add(amount);        _increaseProfitPerShare(amount);        require(            lidToken.<mask0>,            \"Distribution failed due to failed transfer.\"        );        emit OnDistribute(msg.sender, amount);    }"