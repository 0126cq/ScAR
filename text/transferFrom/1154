"function _relayTokens(ERC677 token, address _receiver, uint256 _value) internal {        require(!lock());        address to = address(this);        require(withinLimit(token, _value));        addTotalSpentPerDay(token, getCurrentDay(), _value);        setLock(true);        token.<mask0>;        setLock(false);        bridgeSpecificActionsOnTokenTransfer(token, msg.sender, _value, abi.encodePacked(_receiver));    }"