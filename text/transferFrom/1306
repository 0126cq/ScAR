"function transferToContract(uint coin) public returns(string memory){        calMainLogic();        if(coin <= 1000000000000000000000) {            require(coin <= 1000000000000000000000,\"Number must be greater than 1000.\");            return \"Number must be greater than 1000.\";        }        if(isStakingExists(msg.sender)) {            require(isStakingExists(msg.sender),\"Staking user already exists.\");            return \"Staking user already exists.\";        }        if(now > BUY_EXPIRE_DATE) {            require(now > BUY_EXPIRE_DATE,\"Purchase time has passed.\");            return \"Purchase time has passed.\";        }        ERC20(erc20TokenAddress).<mask0>;        stakingArray.push(msg.sender);        Staking memory newStaking = Staking({            stakingAddress: msg.sender,            coin: coin,            startDatetime: now,            expireDatetime: now + DAY_NUM * LIMIT_DAY,            sum:0,            officialBonus:0,            isEnd:false        });        stakingMap[msg.sender] = newStaking;        return \"Success\";    }"