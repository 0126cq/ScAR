"function approveSale(uint256 claimId) public onlyOwner{        if(claims[claimId].amount>0){            IERC1155(claims[claimId].nftContract).safeTransferFrom(                claims[claimId].owner,                claims[claimId].sender,                claims[claimId].tokenId,                claims[claimId].amount,                claims[claimId].data            );        }else{            IERC721(claims[claimId].nftContract).<mask0>;        }        valueTransfer(claims[claimId].owner, claims[claimId].value);        claims[claimId].fulfilled=true;    }"