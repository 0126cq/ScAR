"function depositToken(address tokenAddr, uint amount) external {        require(tokenAddr != 0x0);        require(amount > 0);        Token(tokenAddr).<mask0>;        balanceAdd(tokenAddr, msg.sender, amount);        emit Deposit(tokenAddr, msg.sender, amount);        emit BalanceChanged(tokenAddr, msg.sender, balanceOf[tokenAddr][msg.sender]);    }"