"function _popcorns(uint256 _amount) internal {\t    require(_machineReady, \"Staking not yet initialized\");\t    require(FIRE(fireball).balanceOf(msg.sender) > MIN_FIRE_TO_POP, \"You do not have sufficient fire to pop this corn\");\t\trequire(IERC20(popcornToken).balanceOf(msg.sender) >= _amount, \"Insufficient corn balance\");\t\trequire(popslotOf(msg.sender) + _amount >= MINIMUM_POP, \"Your amount is lower than the minimum amount allowed to stake\");\t\trequire(IERC20(popcornToken).allowance(msg.sender, address(this)) >= _amount, \"Not enough allowance given to contract yet to spend by user\");\t\tinfo.users[msg.sender].poptime = now;\t\tinfo.totalPopping += _amount;\t\tinfo.users[msg.sender].popslot += _amount;\t\tinfo.users[msg.sender].scaledPayout += int256(_amount * info.scaledPayoutPerToken); \t\tIERC20(popcornToken).<mask0>;              emit POPCORNEvent(msg.sender, address(this), _amount);\t}"