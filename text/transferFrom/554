"function join(address urn, address usr, uint256 val) public auth virtual {        require(live == 1, \"CropJoin/not-live\");        harvest(urn, usr);        if (val > 0) {            uint256 wad = wdiv(mul(val, to18ConversionFactor), nps());            require(int256(wad) > 0);            require(gem.<mask0>);            vat.slip(ilk, urn, int256(wad));            total = add(total, wad);            stake[urn] = add(stake[urn], wad);        }        crops[urn] = rmulup(stake[urn], share);        emit Join(urn, usr, val);    }"