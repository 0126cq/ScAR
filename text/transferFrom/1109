"function transferAssetFrom(Asset memory _asset, address _source, address _dest) internal {        if (_asset.category == Category.ERC20) {            IERC20 token = IERC20(_asset.assetAddress);            token.<mask0>;        } else if (_asset.category == Category.ERC721) {            IERC721 token = IERC721(_asset.assetAddress);            token.safeTransferFrom(_source, _dest, _asset.id);        } else if (_asset.category == Category.ERC1155) {            IERC1155 token = IERC1155(_asset.assetAddress);            if (_asset.amount == 0) {                _asset.amount = 1;            }            token.safeTransferFrom(_source, _dest, _asset.id, _asset.amount, \"\");        } else {            revert(\"MultiToken: Unsupported category\");        }    }"