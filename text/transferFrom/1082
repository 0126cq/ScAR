"function stakeGen0(uint16 [] calldata data) external{    uint16 _number= uint16(data.length );    require(_number > 0 , \"Invalid Number\");    uint16 tokens=uint16(stakeOwners[msg.sender].gen0.tokens.length);    if(tokens > 0){      stakeOwners[ msg.sender].gen0.rewards = calculateGen0Reward(msg.sender);    }      stakeOwners[ msg.sender].gen0.rewardStartTime = uint64(block.timestamp);      totalgen0NFTStaked += _number;      storeGen0Tokens(_number , data);      for(uint16 i ; i< _number ; i++)    {         require(genesis0.ownerOf(data[i]) == msg.sender, \"Not the owner\");    genesis0.<mask0>;    }    delete tokens; }"