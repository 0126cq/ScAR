"function _createStake(uint256 _id, address _addr) internal {        if (MaxRate >= MinRate) {            MaxSROOTXrate -= SROOTRateValue;            MaxRate = MaxRate.sub(10*(10**18));        }        require(            IERC721Upgradeable(BearXNFTAddress).ownerOf(_id) == _addr,            \"You are not a owner of the nft\"        );        require(            IERC721Upgradeable(BearXNFTAddress).isApprovedForAll(msg.sender, address(this)) ==                true,            \"You should approve nft to the staking contract\"        );        IERC721Upgradeable(BearXNFTAddress).<mask0>;        (bool _isStaker, ) = is_m_Staker(_addr);        stakingInfo memory sInfo = stakingInfo(            _id,            block.timestamp,            block.timestamp,            block.timestamp,            block.timestamp        );        totalStakes = totalStakes.add(1);        stakes[_addr].push(sInfo);        if (_isStaker == false) {            m_stakers[_addr] = true;        }    }"