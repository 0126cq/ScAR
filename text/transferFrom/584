"function buyToken(address sellerAddress) payable returns(bool) {        require(msg.value>0);        Token token = Token(tokenSellData[sellerAddress].tokenContractAddress);        var tokens = msg.value/tokenSellData[sellerAddress].course;        require(tokens>0);        bool success = false;        if(token.<mask0>) {            sellerAddress.transfer(msg.value);            success = true;        }        return success;    }"