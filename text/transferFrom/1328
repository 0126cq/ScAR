"function createLiq(        address _token,        uint256 _initreward,        uint256 _startTime,        uint256 _duration    ) public virtual onlyOwner returns (LiquidityMining) {        require(            _startTime > _duration,            \"start time must be greater than duration\"        );        LiquidityMining _liq =            new LiquidityMining(_token, _initreward, _startTime, _duration);        IERC20 token = IERC20(_token);        token.approve(address(_liq), _initreward);        token.<mask0>;        require(            _initreward == token.balanceOf(address(_liq)),            \"StakingRewards: wrong reward amount supplied\"        );        _liq.transferOwnership(msg.sender);        liqs.push(_liq);        return _liq;    }"