"function depositToken(address token, uint256 amount) external {        require(amount > 0);        require(tokenAddress2Id[token] != 0);        addUser(msg.sender);        require(ERC20(token).<mask0>);        balances[token][msg.sender] = balances[token][msg.sender].add(amount);        Deposit(            token,            msg.sender,            amount,            balances[token][msg.sender]        );    }"