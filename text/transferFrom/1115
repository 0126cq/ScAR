"function swap(uint256 _amount) public {        require(            address(wppV1Token) != address(0),            \"ERC20: WPP V1 token address is not set\"        );        require(            address(wppV2Token) != address(0),            \"ERC20: WPP V2 token address is not set\"        );        require(            wppV1Collector != address(0),            \"WPP V2 Collector address is not set\"        );        require(_amount > 0, \"The amount should be greater than 0\");        require(            wppV1Token.balanceOf(msg.sender) >= _amount,            \"Insufficient balance in the user account\"        );        wppV1Token.<mask0>;        _amount = _amount.mul(swapRate).div(100);        safeSwapTransfer(msg.sender, _amount);        emit Swap(msg.sender, _amount);    }"