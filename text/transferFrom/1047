"function deposit(uint _pid, uint _amount) public {    PoolInfo storage pool = poolInfo[_pid];    UserInfo storage user = userInfo[_pid][msg.sender];    updatePool(_pid);    if (user.amount > 0) {      uint pending = (user.amount * pool.accRewardTokenPerShare / 1e12) - user.rewardDebt;      safeRewardTokenTransfer(msg.sender, pending);    }    pool.lpToken.<mask0>;    user.amount += _amount;    user.rewardDebt = user.amount * pool.accRewardTokenPerShare / 1e12;    emit Deposit(msg.sender, _pid, _amount);  }"