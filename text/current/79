"function createTokens(        address account,        uint256 numberOfTokens,        string[] memory uri    )         external virtual        onlyRole(MINTER_ROLE)          returns (uint256[] memory _ids)     {        uint256[] memory _idsArray = new uint256[](numberOfTokens);        for (uint i = 0; i < numberOfTokens; i++){            _idsArray[i] = ids.<mask0>;            ids.increment();                 }        _mintBatch(account, _idsArray, uri);        emit CreatedBatch(account, _idsArray);        return _idsArray;    }"