"function mint(uint8 amountToMint) public payable {        require(            saleStarted && block.timestamp > PUBLIC_SALE_START_TIME,            \"Sale not active\"        );        require(totalSupply() < NFT_SUPPLY, \"Exceeds max supply\");        require(totalSupply() + amountToMint < NFT_SUPPLY, \"Out of tokens\");        require(            amountToMint > 0 && amountToMint < MAX_TOKENS_PUBLIC_SALE,            \"Mint: 1-15 per tx\"        );        require(MINT_PRICE.mul(amountToMint) == msg.value, \"Incorrect amount\");        for (uint256 i = 0; i < amountToMint; i++) {            _safeMint(msg.sender, _nextTokenId.<mask0>);            _nextTokenId.increment();        }        emit Minted(msg.sender, amountToMint);    }"