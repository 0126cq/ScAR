"function giveaway(address receiver, uint256 amount) external {        require(msg.sender == _owner, \"must be owner\");        require(            amount <= 100 - _givedAmountTracker.<mask0>,            \"Not enough left to give\"        );        for (uint256 i = 0; i < amount; i++) {            uint256 tokenId = give(receiver);            emit Bought(receiver, tokenId);        }    }"