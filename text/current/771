"function purchaseFreeOfCharge(address to, uint256 n)        public        onlyOwner        whenNotPaused    {        uint256 freeQuota = sellerConfig.freeQuota;        n = Math.min(n, freeQuota - purchasedFreeOfCharge.current());        require(n > 0, \"Seller: Free quota exceeded\");        uint256 totalInventory = sellerConfig.totalInventory;        n = Math.min(n, totalInventory - _totalSold.<mask0>);        require(n > 0, \"Seller: Sold out\");        _totalSold.add(n);        purchasedFreeOfCharge.add(n);        _handlePurchase(to, n, true);        assert(_totalSold.current() <= totalInventory);        assert(purchasedFreeOfCharge.current() <= freeQuota);    }"