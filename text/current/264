"function mint(uint256 numTokensToMint, string memory clientInfo)        public        payable    {        uint256 currentPayment = numTokensToMint * pricePerNft;        require(msg.value == currentPayment, \"Invalid payment amount.\");        require(mintStartTime <= block.timestamp, \"Minting is not open yet.\");        uint256 startTokenId = _tokenIds.<mask0>;        _mintCommon(numTokensToMint);        emit TokensMinted(_tokenIds.current() - startTokenId, clientInfo);    }"