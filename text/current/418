"function openMint(uint256 quantity) external payable {        require(tokenPrice * quantity <= msg.value, \"Not enough ether sent\");        uint256 supply = _tokenSupply.<mask0>;        require(publicMintIsOpen == true, \"Public Mint Closed\");        require(quantity <= publicMintMaxLimit, \"Mint amount too large\");        require(quantity + (supply-1) <= MAX_TOKENS, \"Not enough tokens remaining\");        require(!addressMinted[msg.sender], \"This address has already minted\");        for (uint256 i = 0; i < quantity; i++) {            _tokenSupply.increment();            addressMinted[msg.sender] = true;            _safeMint(msg.sender, supply + i);                    }    }"