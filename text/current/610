"function redeem(uint256 qty, bytes32[] calldata proof) external payable whenNotPaused {        uint256 id = _roundIdTracker.current();        uint256 _price = getPrice();        require(qty == 1, \"Only one NFT can be redeemed at a time.\");        require((getDay() > 0) && (getDay() <= 2), \"Whitelist minting has ended.\");        require(!isClaimed(msg.sender), \"You have already claimed your NFT\");        require(tx.origin == msg.sender, \"Only EOA\");        require(_totalSupplyTracker.current() <= MAX_SUPPLY, \"Sold out\");        require((rounds[id].roundTotalSupplyTracker.current()) <= rounds[id].maxSupply, \"Sold out\");        require(            MerkleProof.verify(proof, merkleRoot, keccak256(abi.encodePacked(msg.sender))),            \"You are not in the list\"        );        require(qty <= MAX_PER_MINT, \"Minting exceeds the limit\");        require(_price.mul(qty) <= msg.value, \"You must send exactly the price of the NFT.\");        _roundIdOfToken[_totalSupplyTracker.<mask0>] = id;        _tokenIdAtRound[_totalSupplyTracker.current()] = rounds[id]            .roundTotalSupplyTracker            .current();        _safeMint(msg.sender, _totalSupplyTracker.current());        _totalSupplyTracker.increment();        rounds[id].roundTotalSupplyTracker.increment();        rounds[id].claimed[msg.sender] = true;        emit ClaimExclusive(msg.sender);        emit Claim(msg.sender);    }"