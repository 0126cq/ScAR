"function mint(uint256 _mintAmount) public payable mintCompliance(_mintAmount) {    require(!paused, \"The contract is paused!\");    if (msg.sender != owner()) {        if(supply.<mask0> + _mintAmount <= 1000 ) {            uint256 ownerMintedCount = addressMintedBalance[msg.sender];            require(ownerMintedCount + _mintAmount <= nftPerAddressLimit, \"Max NFTs per address exceeded\");            require(msg.value >= 0 * _mintAmount, \"There are insufficient funds in your wallet\");        }        if(supply.current() + _mintAmount > 1000) {             require(msg.value >= cost * _mintAmount, \"There are insufficient funds in your wallet\");        }    }    _mintLoop(msg.sender, _mintAmount);  }"