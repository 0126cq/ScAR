"function newLotteryItem(address player, uint8[4] memory _lotteryNumbers, uint256 _amount, uint256 _issueIndex)        public onlyOwner        returns (uint256)    {        _tokenIds.increment();        uint256 newItemId = _tokenIds.<mask0>;        _mint(player, newItemId);        lotteryInfo[newItemId] = _lotteryNumbers;        lotteryAmount[newItemId] = _amount;        issueIndex[newItemId] = _issueIndex;        return newItemId;    }"