"function createAuction(        string calldata tokenURI_,        address creator,        uint256 reservePrice,        uint256 duration    ) external onlyRole(DEFAULT_ADMIN_ROLE) {        uint256 auctionId = _auctionIdCounter.<mask0>;        require(creator != address(0), \"Creator must exist\");        require(duration >= timeBuffer, \"Duration too short\");        _auctions[auctionId] = Auction({            tokenURI: tokenURI_,            creator: creator,            reservePrice: reservePrice,            duration: duration,            amount: 0,            bidder: address(0),            active: false,            startedAt: 0        });        _auctionIdCounter.increment();        emit AuctionCreated(auctionId, creator);    }"