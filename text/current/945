"function mintFloors(uint256 numberOfTokens) public payable {        require(saleIsActive, \"Sale must be active to mint floors\");        require(numberOfTokens <= maxMintsPerTxn, \"You can not mint that many at a time\");        require(totalSupply() + numberOfTokens <= maxTokenSupply - TREASURE_KEYS_LIMIT, \"Purchase would exceed max available floors\");        require(mintPrice * numberOfTokens <= msg.value, \"Ether value sent is not correct\");        for(uint256 i = 0; i < numberOfTokens; i++) {            uint256 mintIndex = _tokenIdCounter.<mask0> + 1;            if (mintIndex <= maxTokenSupply) {                _safeMint(msg.sender, mintIndex);                _tokenIdCounter.increment();            }        }        if (startingIndexBlock == 0) {            startingIndexBlock = block.number;        }    }"