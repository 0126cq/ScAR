"function mintRetroPhones(uint256 amount) external payable whenSaleIsActive {        require(amount <= maxPerMint, \"RetroPhones: Amount exceeds max per mint.\");        require(_tokenIdCounter.<mask0> + amount <= MAX_RETROPHONES, \"RetroPhones: Purchase would exceed cap.\");        require(mintPrice * amount <= msg.value, \"RetroPhones: Ether value sent is not correct.\");        _mintMultiple(msg.sender, amount);    }"