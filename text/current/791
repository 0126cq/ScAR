"function mint(uint256 _mintAmount) public payable {        require(saleIsActive, \"Viral Mfers are not on sale yet.\");        require(_mintAmount > 0, \"Cannot mint zero!\");        require(_mintAmount <= maxMint(), \"Save some for the rest of us, fren!\");        require(totalSupply() + _mintAmount <= MAX_SUPPLY, \"Not enough Viral Mfers remaining.\");        require(msg.value >= MINT_COST * _mintAmount, \"Not enough ETH to buy Viral Mfers, :-(\");        for (uint256 i = 0; i < _mintAmount; i++) {            uint256 mintIndex = _nextTokenId.<mask0>;             _nextTokenId.increment();            _safeMint(msg.sender, mintIndex);        }    }"