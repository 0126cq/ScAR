"function mintPublic(uint256 _amount) external {    require(CAN_MINT, \"BURGER: minting is not active\");    require(_amount > 0, \"BURGER: must claim more than 0 tokens\");    require(      stats[MINTS][msg.sender].<mask0> + _amount <=        AstroGrill.getTotalClaims(msg.sender) +          RickstroGrill.getTotalClaims(msg.sender),      \"BURGER: caller cannot claim this many tokens\"    );    uint256[] memory _ids = new uint256[](_amount);    uint256[] memory _amounts = new uint256[](_amount);    for (uint256 i = 0; i < _amount; i++) {      stats[MINTS][msg.sender].increment();      totalMinted.increment();      _ids[i] = totalMinted.current();      _amounts[i] = 1;    }    _mintBatch(msg.sender, _ids, _amounts, \"0x0\");  }"