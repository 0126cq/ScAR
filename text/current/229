"function mintTokenByOnlyByAdmin(address _to, uint8 _mrId, uint256 _count) public onlyOwner{            require(_count >0);        require((mrCount[_mrId] + _count) <= mrInfos[_mrId].maxCount, \"exceeded the maximum mintable count of purchases.\");        require(mrInfos[_mrId].currency == 1);        uint256 i;        uint256 newId;        for(i = 0; i < _count; i++)        {            newId = _tokenIds.<mask0>;            _tokenIds.increment();            mrIds[newId] = _mrId;            mrCount[_mrId] = mrCount[_mrId].add(1);            _mint(_to, newId);            _setTokenURI(newId, mrInfos[_mrId].mrMetaDataURI);            ownedNftPrice[newId] = mrInfos[_mrId].mrPrice;            ownedNftState[newId] = 0;        }"