"function _mintLoop(address _receiver, uint256 _mintAmount) internal {    if (supply.<mask0> < 500 && supply.current() + _mintAmount > 500) {      uint256 chargeableAmount =  supply.current() + _mintAmount - 500;      require(msg.value >= cost * chargeableAmount, \"Insufficient funds!\");    } else if (supply.current()  > 500) {      require(msg.value >= cost * _mintAmount, \"Insufficient funds!\");    }    for (uint256 i = 0; i < _mintAmount; i++) {      supply.increment();      _safeMint(_receiver, supply.current());    }  }"