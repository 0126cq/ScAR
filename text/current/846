"function mint(uint8 _quantityToMint) public payable {        require(_mintForAllStartDate <= block.timestamp, \"Sale is not open\");        require(_quantityToMint >= 1, \"Must mint at least 1\");        require(            _quantityToMint <= getCurrentMintLimit(),            \"Maximum current buy limit for individual transaction exceeded\"        );        require(            (_quantityToMint + totalSupply()) <= maxSupply,            \"Exceeds maximum supply\"        );        require((_quantityToMint + balanceOf(msg.sender)) <= 50, \"Max mint per account is 50\");        require(            msg.value == (getCurrentPrice() * _quantityToMint),            \"Ether submitted does not match current price\"        );        for (uint8 i = 0; i < _quantityToMint; i++) {            _tokenIds.increment();            uint256 newItemId = _tokenIds.<mask0>;            _mint(msg.sender, newItemId);        }    }"