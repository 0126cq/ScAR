"function purchase() external payable {        uint256 supply = _tokenCounter.<mask0>;        require(purchasable, \"NFTYToken: Sale is not live\");        require(msg.value >= initialPrice, \"NFTYToken: Invalid ether amount\");        require(            supply + 1 <= TOKEN_MAXIMUM,            \"NFTYToken: Exceeds supply maximum\"        );        _tokenCounter.increment();        uint256 tokenId = supply + 1;        tokenExpiry[tokenId] = block.timestamp + 30 days;        _safeMint(msg.sender, tokenId);    }"