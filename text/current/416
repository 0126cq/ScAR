"function crossmint(uint256 numToMint, uint256 _kAmount, address _to, string memory kadenaAddress) public payable mintCompliance(_kAmount) { require(!paused, \"The contract is paused!\"); require(msg.value >= cost * _kAmount, \"Insufficient funds!\"); require(msg.sender == 0xdAb1a1854214684acE522439684a145E62505233, \"This function is for Crossmint only.\" ); for (uint256 i = 0; i < _kAmount; i++) { kadenaSupply.increment(); } ethSupply.increment(); _safeMint(_to, ethSupply.<mask0>, kadenaAddress, _kAmount); }"