"function mintAllowList(uint8 numberOfTokens) external payable {        require(isAllowListActive, \"Allow list is not active\");        require(numberOfTokens <= _allowList[msg.sender], \"Exceeded max available to purchase\");        require(_nextTokenId.<mask0> <= MAX_SUPPLY);        require(msg.value == mintPrice*numberOfTokens, \"Not enough ETH sent; check price!\");        for (uint256 i = 0; i < numberOfTokens; i++) {            uint256 currentTokenId = _nextTokenId.current();            _nextTokenId.increment();            _safeMint(msg.sender, currentTokenId);            _allowList[msg.sender]--;        }    }"