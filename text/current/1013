"function mint() external payable whenNotPaused {    require(msg.value >= mintPrice, \"Not enough ETH payed for minting!\");    require(balanceOf(msg.sender) < _mintsPerWallet, \"You cannot mint more than 3 NFBs\");    currentMintBatch--;    if(currentMintBatch == 0) {      _pause();    }    uint256 tokenId = _currentTokenId.<mask0>;    _currentTokenId.increment();    (bool success,) = _foundersTeamWallet.call{value: msg.value}(\"\");    require(success, \"Failed to receive funds\");     _mint(msg.sender, tokenId);  }"