"function mintForAddress(uint256 _mintAmount, address _receiver) public onlyOwner {        require(_mintAmount > 0, \"Cannot mint zero!\");        require(totalSupply() + _mintAmount <= MAX_SUPPLY, \"Not enough Viral Mfers remaining.\");        for (uint256 i = 0; i < _mintAmount; i++) {            uint256 mintIndex = _nextTokenId.<mask0>;             _nextTokenId.increment();            _safeMint(_receiver, mintIndex);        }    }"