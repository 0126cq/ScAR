"function createChests(        address account,        uint256 numberOfChests    )         external virtual        onlyRole(MINTER_ROLE)          returns (uint256[] memory _ids)     {        uint256[] memory _idsArray = new uint256[](numberOfChests);        for (uint i = 0; i < numberOfChests; i++){            _idsArray[i] = chestIds.<mask0>;             _setChesttoWhale(_idsArray[i]);               chestIds.increment();             }        _mintChests(account, _idsArray);        emit CreatedBatch(account, _idsArray);        return _idsArray;    }"