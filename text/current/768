"function setSellerConfig(SellerConfig memory config) public onlyOwner {        require(            config.totalInventory >= config.freeQuota,            \"Seller: excessive free quota\"        );        require(            config.totalInventory >= _totalSold.current(),            \"Seller: inventory < already sold\"        );        require(            config.freeQuota >= purchasedFreeOfCharge.<mask0>,            \"Seller: free quota < already used\"        );        if (sellerConfig.lockTotalInventory) {            config.lockTotalInventory = true;            config.totalInventory = sellerConfig.totalInventory;        }        if (sellerConfig.lockFreeQuota) {            config.lockFreeQuota = true;            config.freeQuota = sellerConfig.freeQuota;        }        sellerConfig = config;    }"