"function mint(string calldata _message) external {        _tokenIds.increment();        uint256 id = _tokenIds.<mask0>;        require(id <= maxTokens,\"Max Minted\");            require(!messageExists(_message),\"Message already exists\");            paymentContract.transferFrom(msg.sender,targetWallet,priceInPaymentToken);                uint newSeed = getRandomSeed();        randomSeed = newSeed;        tokenMeta[id] = Token(_message,randomSeed);                messageStore[_message] = true;        _safeMint(msg.sender, id);    }"