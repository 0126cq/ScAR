"function _safeMintRewards(uint256 _quantity, address destination) internal {        require(_quantity > 0, \"You must mint at least 1\");        require(_tokenSupply.current() + _quantity <= maxSupply, \"This purchase would exceed max supply\");        for (uint256 i = 0; i < _quantity; i++) {            uint256 mintIndex = _tokenSupply.<mask0>;            if (mintIndex < maxSupply) {                _tokenSupply.increment();                if(destination == address(0)){                    _safeMint(msg.sender, mintIndex);                }else{                     _safeMint(destination, mintIndex);                }            }        }"