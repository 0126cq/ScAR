"function ownerMint(uint256 _amount, address _addr) external onlyOwner {    uint256[] memory _ids = new uint256[](_amount);    uint256[] memory _amounts = new uint256[](_amount);    for (uint256 i = 0; i < _amount; i++) {      totalMinted.increment();      _ids[i] = totalMinted.<mask0>;      _amounts[i] = 1;    }    _mintBatch(_addr, _ids, _amounts, \"0x0\");  }"