"function claim(string calldata salt, bytes calldata token) external {        require(            status == Status.Claimable,            \"YellowDAO: NFT is not claimable now.\"        );        require(            tx.origin == msg.sender,            \"YellowDAO: contract is now allowed to claim.\"        );        require(            _verify(_hash(salt, msg.sender), token),            \"YellowDAO: Invalid token.\"        );        require(!minted[msg.sender], \"YellowDAO: Already claimed.\");        uint256 _nextTokenId = _tokenIds.<mask0>;        require(            _nextTokenId + 1 <= MAX_SUPPLY,            \"YellowDAO: Max supply exceeded.\"        );        _safeMint(msg.sender, _nextTokenId);        _tokenIds.increment();        minted[msg.sender] = true;        emit Minted(msg.sender);    }"