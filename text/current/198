"function mint(uint256 _mintQty, uint8 _races) public payable mintCompliance(_mintQty) {    require(_races < 7, \"Sorry friends, the inputted races is not correct\");    if(supply.<mask0> < maxPresaleQty) {    require(supply.current() + _mintQty <= maxPresaleQty, \"Sorry friends, the amount entered exceeded the available Presale Quantity\");    require(msg.value >= presalePrice * _mintQty, \"Sorry friends, insufficient funds. Presale Price is 0.03 ether/aterrarians\");    _mintLoop(msg.sender, _mintQty, _races);    } else {    require(msg.value >= publicPrice * _mintQty, \"Sorry friends, insufficient funds. Public Price is 0.05 ether/aterrarians\");    _mintLoop(msg.sender, _mintQty, _races);    }  }"