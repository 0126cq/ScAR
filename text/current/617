"function getPrice() public view returns (uint256) {        uint256 id = _roundIdTracker.<mask0>;        uint256 currentDay = getDay();        if (currentDay == 0) {            return rounds[id].exclusiveSalePrice;        } else if (currentDay == 1) {            return rounds[id].exclusiveSalePrice;        } else if (currentDay == 2) {            return rounds[id].exclusiveSalePrice;        } else {            return rounds[id].publicSalePrice;        }    }"