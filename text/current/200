"function _mintLoop(address _receiver, uint256 _mintQty, uint8 _races) internal {    for (uint256 i = 0; i < _mintQty; i++) {      supply.increment();      if (_races == 0){        require(fungusStart <= fungusMax, \"Sorry friends, the amount of available Fungus is inadaquate\");        _safeMint(_receiver, fungusStart);        fungusStart++;      }       else if (_races == 1){        require(qqStart <= qqMax, \"Sorry friends, the amount of available Quirkyquarks is inadaquate\");        _safeMint(_receiver, qqStart);        qqStart++;      }       else if (_races == 2){        require(piscesStart <= piscesMax, \"Sorry friends, the amount of available Piscerian is inadaquate\");        _safeMint(_receiver, piscesStart);        piscesStart++;      }       else if (_races == 3){        require(bejanianStart <= bejanianMax, \"Sorry friends, the amount of available Bejanian is inadaquate\");        _safeMint(_receiver, bejanianStart);        bejanianStart++;      }       else if (_races == 4){        require(dollyStart <= dollyMax, \"Sorry friends, the amount of available Dolly is inadaquate\");        _safeMint(_receiver, dollyStart);        dollyStart++;      }       else if (_races == 5){        require(plantaeStart <= plantaeMax, \"Sorry friends, the amount of available Plantae is inadaquate\");        _safeMint(_receiver, plantaeStart);        plantaeStart++;      }       else {        require(devStart <= devMax, \"The amount of Dev Mint is inadaquate\");        require(_receiver == owner(), \"Sorry friends, you are not the dev team\");        _safeMint(_receiver, supply.<mask0>);        devStart++;      }    }  }"