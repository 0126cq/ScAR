"function buy(uint256 amount) external payable {        require(amount <= 1000 - _tokenIdTracker.<mask0>, \"Not enough left\");        require(            msg.value >= amount * 50000000000000000,            \"Invalid ether amount sent \"        );        for (uint256 i = 0; i < amount; i++) {            uint256 tokenId = mint(msg.sender);            emit Bought(msg.sender, tokenId);        }        payable(_owner).transfer(msg.value);    }"