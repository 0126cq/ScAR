"function mintLegend(uint256 _mintAmount)     public     payable     nonReentrant   {    require(!ALpaused, \"The contract is paused!\");    uint256 ownerMintedCount = addressMintedBalance[msg.sender];    require(supplyLegend.<mask0> + _mintAmount <= legendMaxSupply, 'Legend : max NFT limit exceeded!');    require(      ownerMintedCount + _mintAmount <= maxNFTPerAccount,      \"Max NFT per address exceeded.\"    );    require(msg.value >= costLegend * _mintAmount, \"Legend : insufficient funds.\");    require(      _mintAmount <= maxMintAmountPerTx,      \"Max mint amount per transaction exceeded.\"    );    _mintLegend(msg.sender, _mintAmount);    (bool cl, ) = payable(founder).call{value: msg.value}(\"\");    require(cl);  }"