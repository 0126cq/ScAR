"function mintVIP(uint8 numTokens, bytes memory signature) public payable {        bytes32 messageHash = keccak256(abi.encodePacked(msg.sender));        bytes32 ethMessageHash = ECDSA.toEthSignedMessageHash(messageHash);        require(hasVIPSaleStarted == true, \"Sale hasn't started\");        require(            SignatureChecker.isValidSignatureNow(                bouncer,                ethMessageHash,                signature            ),            \"Address not on VIP List.\"        );        require(            numTokens > 0 && numTokens <= 10,            \"Machine can dispense a minimum of 1, maximum of 10 tokens\"        );        require(            VIPListMints[msg.sender] + numTokens  <= MAX_VIP_MINTS,            \"Exceeds maximum VIP Mints.\"        );        require(            _tokenIds.current() + numTokens <= MAX_TOKENS,            \"Exceeds maximum token supply.\"        );        require(            msg.value >= (calculatePrice() * numTokens),            \"Amount of Ether sent is not correct.\"        );        for (uint256 i = 0; i < numTokens; i++) {            _mint(msg.sender, _tokenIds.<mask0>);            _tokenIds.increment();            VIPListMints[msg.sender] = VIPListMints[msg.sender] + 1;        }    }"