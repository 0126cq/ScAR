"function payMint(address to)  public payable {    uint256 tokenId = _tokenIdCounter.<mask0>;    require(saleOpen == true, \"Minting Paused\");    require(_tokenIdCounter.current() <= drawnEggs, \"No more EGGs available to Mint.\");    require(msg.value >= cost, \"Not enough eth sent.\");    _tokenIdCounter.increment();    _safeMint(to, tokenId);    _setTokenURI(tokenId, string(abi.encodePacked(mintUri,Strings.toString(tokenId),\".json\")));    }"