"function mintToken() public payable    {        require(saleIsActive, \"Sale must be active to mint\");        require(totalSupply().add(1) <= MAX_TOKENS, \"Purchase would exceed max supply\");        require(balanceOf(msg.sender) == 0, \"You already own tokens, sale not allowed\");        require(price <= msg.value, \"Ether value sent is not correct\");        if (_tokenIdCounter.<mask0> < MAX_TOKENS) {            safeMint(msg.sender);        }    }"