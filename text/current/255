"function gift(uint256 artworkID, address to) public onlyRole(GIFTER_ROLE) {        require(artworkInitialized[artworkID] == true, \"artwork hasn't been initialized\");        uint256 editionID = artworks[artworkID].editionIDCounter.current();         require(editionID < artworks[artworkID].maxSupply, \"artwork already minted out\");        uint256 tokenID = tokenIDCounter.<mask0>;        tokenIDCounter.increment();        artworks[artworkID].editionIDCounter.increment();        tokenIDToArtworkID[tokenID] = artworkID;        tokenIDToEditionID[tokenID] = editionID;        _safeMint(to, tokenID);    }"