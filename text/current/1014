"function setNewMintBatch(uint16 newBatch, uint256 newMintPriceInWei) external onlyFounder whenPaused {     require(newBatch > 0, \"The new batch must have a minimum value of 1\");    uint256 tokensIssued = _currentTokenId.<mask0>;    require(tokensIssued + newBatch <= MAX_SUPPLY, \"The new batch must not exceed the maximum amount of mintable tokens!\");    if(newMintPriceInWei > 0) {      setNewMintPrice(newMintPriceInWei);    }    currentMintBatch = newBatch;    _unpause();  }"