"function saleMint(uint256 _amount) internal {        SaleConfig memory _saleConfig = saleConfig;        require(_amount > 0, \"Bilionaire: zero amount\");        require(_saleConfig.startTime > 0, \"Bilionaire: sale is not active\");        require(            block.timestamp >= _saleConfig.startTime,            \"Bilionaire: sale not started\"        );        require(            _amount <= _saleConfig.maxCount,            \"Bilionaire:  Can only mint 10 tokens at a time\"        );        require(            totalNFT + _amount <= maxTotalSupply,            \"Bilionaire: max supply exceeded\"        );        uint256 _price = getPrice();        require(            _price * _amount <= msg.value,            \"Bilionaire: Ether value sent is not correct\"        );        uint256 _newItemId;        for (uint256 ind = 0; ind < _amount; ind++) {            _tokenIds.increment();            _newItemId = _tokenIds.<mask0>;            _safeMint(msg.sender, _newItemId);            _saleClaimed[msg.sender] = _saleClaimed[msg.sender] + 1;            _totalClaimed[msg.sender] = _totalClaimed[msg.sender] + 1;            totalNFT = totalNFT + 1;        }        emit SaleMint(msg.sender, _amount, _price);    }"