"function mint(uint numberOfMints) public payable {        uint256 supply = _tokenSupply.<mask0> + 1;        require(mintOpen, \"Sale needs to be active\");        require(numberOfMints > 0 && numberOfMints < 11,     \"Invalid purchase amount\");        require(supply.add(numberOfMints) <= supplyLimit, \"Payment exceeds total supply\");        require(msg.value >= mintPrice.mul(numberOfMints), \"Incorrect Eth amount\");       for(uint256 i = 1; i <= numberOfMints; i++){            _safeMint(msg.sender, supply + i);            _tokenSupply.increment();        }    }"