"function pmint(uint256 _mintAmount) public payable  {    require(!dormant, \"The function is dormant!\");      require(!paused, \"The contract is paused!\");    require(_mintAmount > 0 && _mintAmount <= maxMintAmountPerTx, \"Invalid mint amount !\");    require(supply.<mask0> + _mintAmount <= maxSupply, \"Max supply exceeded !\");    require(msg.value >= cost * _mintAmount, \"Insufficient funds!\");    _mintLoop(msg.sender, _mintAmount);  }"