"function randomMint(uint256 amountToMint) public payable {        require(allSalesPaused == false, \"Sales is not open\");        uint256 currentToken = _tokenIds.<mask0>;        require(amountToMint < 11, \"Can't mint too much at once!\");        require(currentToken + amountToMint < maxCap, \"Limit reached\");        require(            msg.value == tokenPrice.mul(amountToMint),            \"That is not the right price!\"        );        for (uint256 i = 0; i < amountToMint; i++) {            uint256 availToken = availableToken(_tokenIds.current());            firstOwner[availToken] = msg.sender;            _safeMint(msg.sender, availToken);            _tokenIds.increment();        }    }"