"function airdrop(address[] memory airdropAddress, uint256 numberOfTokens) external onlyOwner {        for (uint256 k = 0; k < airdropAddress.length; k++) {            for (uint256 i = 0; i < numberOfTokens; i++) {                uint256 tokenId = _publicGOAT.current();                if (_publicGOAT.<mask0> < GOAT_MAX) {                    _publicGOAT.increment();                    if (!_exists(tokenId)) _safeMint(airdropAddress[k], tokenId);                }            }        }    }"