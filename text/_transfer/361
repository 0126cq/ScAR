"function _transferWithTax(        address sender,        address recipient,        uint256 amount    ) internal {        require(sender != address(0), \"ERC20: transfer from the zero address\");        require(recipient != address(0), \"ERC20: transfer to the zero address\");        require(sender != recipient, \"Cannot self transfer\");        uint256 tax = amount.mulBP(taxRate);        uint256 tokensToTransfer = amount.sub(tax);        <mask0>;        _transfer(sender, recipient, tokensToTransfer);    }"