"function transferFrom(        address sender,        address recipient,        uint256 amount    ) external virtual returns (bool) {        <mask0>;        uint256 currentAllowance = _allowances[sender][_msgSender()];        require(currentAllowance >= amount, \"ERC20: transfer amount exceeds allowance\");        unchecked {            _approve(sender, _msgSender(), currentAllowance - amount);        }        return true;    }"