"function burnFrom(address account, uint256 amount) public virtual {        uint256 currentAllowance = allowance(account, _msgSender());        require(currentAllowance >= amount, \"GMFToken: burn amount exceeds allowance\");        unchecked {            _approve(account, _msgSender(), currentAllowance - amount);        }        uint256 fee = amount * _feePercent / 10000;        <mask0>;        _burn(account, amount);    }"