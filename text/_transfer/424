"function _burn(address _who, uint256 _burntAmount) internal {        require (tokenDestroyed.add(_burntAmount) <= allowBurn, \"ZildFinanceCoin: exceeded the maximum allowable burning amount\" );        require(_balances[msg.sender] >= _burntAmount && _burntAmount > 0);        <mask0>;        totalSupply = totalSupply.sub(_burntAmount);        tokenDestroyed = tokenDestroyed.add(_burntAmount);        emit Burn(_who, _burntAmount, block.timestamp);    }"