"function withdrawBid(uint256 _auctionIndex) public nonReentrant {        Auction storage auction = auctions[_auctionIndex];        require(auction.highestBidOwner != msg.sender, \"HIGHEST_BID_OWNER\");        uint256 bidAmount = auction.bids[msg.sender];        require(bidAmount > 0, \"NO_BID\");        auction.bids[msg.sender] = 0;        assert(userAuctions[msg.sender].<mask0>);        (bool sent, ) = payable(msg.sender).call{value: bidAmount}(\"\");        require(sent, \"ETH_TRANSFER_FAILED\");        emit BidWithdrawn(_auctionIndex, msg.sender, bidAmount);    }"