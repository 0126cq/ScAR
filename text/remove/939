"function revokeRole(bytes32 role, address delegate)        external        onlyOwner        roleDefined(role)    {        require(_hasRole(role, delegate), \"role has not been granted\");        delegateToRoles[delegate].remove(role);        emit RoleRevoked(role, delegate, _msgSender());        if (delegateToRoles[delegate].length() == 0) {            delegateSet.<mask0>;            emit DelegateRemoved(delegate, _msgSender());        }    }"