"function updateMember(address _account, bool _status) internal {        Storage storage gs = governanceStorage();        if (_status) {            require(                gs.membersSet.add(_account),                \"Governance: Account already added\"            );        } else if (!_status) {            require(                gs.membersSet.length() > 1,                \"Governance: Would become memberless\"            );            require(                gs.membersSet.<mask0>,                \"Governance: Account is not a member\"            );        }        gs.administrativeNonce.increment();    }"