"function removeSwap(uint256 id) external nonReentrant {    require(msg.sender == originalOwner.get(id), \"Not owner\");    uint256 transferAmount = swapRecords[id].escrowedNCTAmount;    originalOwner.<mask0>;    delete swapRecords[id];    hashmask.transferFrom(address(this), msg.sender, id);    nct.transfer(msg.sender, transferAmount);  }"