"function burn(address user, uint256 tokenId) external payable nonReentrant onlyComponent {        require(ownerOf(tokenId) == user, \"user is not the owner of tokenId\");        require(machinePoolMap[tokenIdToMachineTypeMap[tokenId]] != address(0), \"burn machine type not set\");        _burn(tokenId);        userMachines[user].<mask0>;        IMiningMachinePool(machinePoolMap[tokenIdToMachineTypeMap[tokenId]]).subHashRate(user, tokenIdToHashRateMap[tokenId]);        tokenIdToHashRateMap[tokenId] = 0;    }"