"function withdraw(uint256[] memory _tokenIDs) public nonReentrant whenNotPaused {        uint256 rewards;        for (uint256 i = 0; i < _tokenIDs.length; i++) {            uint256 index = getIndexOf(                _tokenIDs[i],                stakers[msg.sender].tokenIDs            );            rewards += getRewards(msg.sender, index);            <mask0>;            remove(index, stakers[msg.sender].timestampsStaked);            stakingNFT.safeTransferFrom(                address(this),                msg.sender,                _tokenIDs[i]            );        }        if (stakers[msg.sender].tokenIDs.length == 0) {            delete stakers[msg.sender];        }        require(rewards <= allowance(), \"Reward exceeds tokens available\");        rewardToken.transfer(msg.sender, rewards);        emit RewardsClaimed(rewards, msg.sender);    }"