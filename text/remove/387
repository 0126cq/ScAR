"function unstake(uint256[] memory _tokenIds) public {    require(_tokenIds.length > 0, \"no token IDs sent\");    uint256 totalRewards = 0;    for (uint256 i = 0; i < _tokenIds.length; i++) {      uint256 tokenId = _tokenIds[i];      require(ownerOf[tokenId] == msg.sender, \"not token owner\");      uint256 rewards = currentRewardsOf(tokenId);      totalRewards += rewards;      emit Unstaked(msg.sender, tokenId, block.timestamp, stakedAt[tokenId], rewards);      ownerOf[tokenId] = address(0);      stakedAt[tokenId] = 0;      stakedTokens[msg.sender].<mask0>;      erc721.transferFrom(address(this), msg.sender, tokenId);    }    erc20.mint(msg.sender, totalRewards);  }"