"function emergencyUnstake(uint amountToWithdraw) public {        require(amountToWithdraw > 0, \"Cannot unstake 0 Tokens\");        require(depositedTokens[msg.sender] >= amountToWithdraw, \"Invalid amount to withdraw\");        lastClaimedTime[msg.sender] = now;        require(Token(trustedStakeTokenAddress).transfer(msg.sender, amountToWithdraw), \"Could not transfer tokens.\");        depositedTokens[msg.sender] = depositedTokens[msg.sender].sub(amountToWithdraw);        if (holders.contains(msg.sender) && depositedTokens[msg.sender] == 0) {            holders.<mask0>;        }    }"