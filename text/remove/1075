"function stopStaking(address _user, uint256 _tokenId)        external        masterContract    {        require(stakedWulfz[_user].contains(_tokenId), \"You're not the owner\");        uint256 wType = tokenInfo[_tokenId].wType;        uint256 rewardBase = STAKE_REWARD_BY_TYPE[wType];        uint256 interval = block.timestamp - tokenInfo[_tokenId].lastUpdate;        uint256 reward = ((rewardBase * interval) *            10**_utilityToken.decimals()) / 86400;        _utilityToken.reward(_user, reward);        delete tokenInfo[_tokenId];        stakedWulfz[_user].<mask0>;        emit StakeStopped(_user, _tokenId);    }"