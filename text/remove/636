"function setTokenBalance(address account) public {        uint256 balance = ryuukoTsuka.balanceOf(account);        if (!ryuukoTsuka.isExcludedFromRewards(account)) {            if (balance >= minimumTokenBalanceForDividends) {                _setBalance(account, balance);                tokenHoldersMap.set(account, balance);            }            else {                _setBalance(account, 0);                tokenHoldersMap.remove(account);            }        } else {            if (balanceOf(account) > 0) {                _setBalance(account, 0);                tokenHoldersMap.<mask0>;            }        }        processAccount(payable(account), true);    }"