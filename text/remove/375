"function _burn(uint256 tokenId) internal virtual {        address owner = ownerOf(tokenId);        _beforeTokenTransfer(owner, address(0), tokenId);        _approve(address(0), tokenId);        if (bytes(_tokenURIs[tokenId]).length != 0) {            delete _tokenURIs[tokenId];        }        _holderTokens[owner].<mask0>;        _tokenOwners.remove(tokenId);        emit Transfer(owner, address(0), tokenId);    }"