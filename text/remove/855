"function transferLockOwnership (uint256 _lockID, address payable _newOwner) external {    require(msg.sender != _newOwner, 'OWNER');    TokenLock storage transferredLock = LOCKS[_lockID];    require(transferredLock.owner == msg.sender, 'NOT OWNER');    UserInfo storage user = USERS[_newOwner];    user.lockedTokens.add(transferredLock.lpToken);    EnumerableSet.UintSet storage user_locks = user.locksForToken[transferredLock.lpToken];    user_locks.add(transferredLock.lockID);    EnumerableSet.UintSet storage userLocks = USERS[msg.sender].locksForToken[transferredLock.lpToken];    userLocks.<mask0>;    if (userLocks.length() == 0) {      USERS[msg.sender].lockedTokens.remove(transferredLock.lpToken);    }    transferredLock.owner = _newOwner;    emit onTransferLockOwnership(_lockID, transferredLock.lpToken, msg.sender, _newOwner);  }"