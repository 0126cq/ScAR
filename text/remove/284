"function offerToPayCommission(        uint256 tokenId,         uint256 amount    )        public     {        require(_exists(tokenId), \"NFT: Nonexistent token\");        if (amount == 0) {            if (_commissions[tokenId].offerAddresses.contains(_msgSender())) {                _commissions[tokenId].offerAddresses.<mask0>;                delete _commissions[tokenId].offerPayAmount[_msgSender()];            }        } else {            _commissions[tokenId].offerPayAmount[_msgSender()] = amount;            _commissions[tokenId].offerAddresses.add(_msgSender());        }    }"