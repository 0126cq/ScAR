"function withdraw(uint amountToWithdraw) public noContractsAllowed {        require(amountToWithdraw > 0, \"Cannot withdraw 0 Tokens!\");        require(depositedTokens[msg.sender] >= amountToWithdraw, \"Invalid amount to withdraw\");        require(now.sub(depositTime[msg.sender]) > cliffTime, \"You recently deposited, please wait before withdrawing.\");        updateAccount(msg.sender);        require(Token(trustedDepositTokenAddress).transfer(msg.sender, amountToWithdraw), \"Could not transfer tokens.\");        depositedTokens[msg.sender] = depositedTokens[msg.sender].sub(amountToWithdraw);        totalTokens = totalTokens.sub(amountToWithdraw);        if (holders.contains(msg.sender) && depositedTokens[msg.sender] == 0) {            holders.<mask0>;        }    }"