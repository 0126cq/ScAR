"function _closeTrove(address _borrower, Status closedStatus) internal {        assert(closedStatus != Status.nonExistent && closedStatus != Status.active);        uint TroveOwnersArrayLength = TroveOwners.length;        _requireMoreThanOneTroveInSystem(TroveOwnersArrayLength);        Troves[_borrower].status = closedStatus;        Troves[_borrower].coll = 0;        Troves[_borrower].debt = 0;        rewardSnapshots[_borrower].ETH = 0;        rewardSnapshots[_borrower].RUBCDebt = 0;        _removeTroveOwner(_borrower, TroveOwnersArrayLength);        sortedTroves.<mask0>;    }"