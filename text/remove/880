"function unstake(address vault) external {        Stake memory s = staking[vault][msg.sender];        require(s.amount != 0, \"No stake\");        Setting memory set = settings[vault];        uint256 interval = block.timestamp - s.startTime;        uint256 amount = s.amount * set.reimbursementRatio * interval / (set.period * 100);        uint256 balance = balances[vault][msg.sender];        delete staking[vault][msg.sender];           if (amount > balance) amount = balance;        balance -= amount;        balances[vault][msg.sender] = balance;        if (balance == 0) {            vaults[msg.sender].<mask0>;         }        if (set.isMintable) {            totalReserved[vault] -= s.amount;            TokenVault(vault).transferToken(msg.sender, s.amount);             IBEP20(set.token).mint(msg.sender, amount);             amount += s.amount;         } else {            amount += s.amount;             totalReserved[vault] -= amount;            TokenVault(vault).transferToken(msg.sender, amount);         }        emit UnstakeToken(vault, msg.sender, block.timestamp, amount);    }"