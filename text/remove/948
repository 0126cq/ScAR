"function removeTokenReceiver(address _token, address _receiver)        external        whenNotPaused    {        address _sender = _msgSender();        tokenToReceivers[_sender][_token].<mask0>;        if (tokenToReceivers[_sender][_token].length() == 0) {            senderToTokens[_sender].remove(_token);        }        if (senderToTokens[_sender].length() == 0) {            senderSet.remove(_sender);        }        emit tokenReceiverRemoved(_sender, _token, _receiver);    }"