"function updateSkuPricing(        bytes32 sku,        address[] memory tokens,        uint256[] memory prices    ) public virtual onlyOwner {        uint256 length = tokens.length;        require(length == prices.length, \"Sale: tokens/prices lengths mismatch\");        SkuInfo storage skuInfo = _skuInfos[sku];        require(skuInfo.totalSupply != 0, \"Sale: non-existent sku\");        EnumMap.Map storage tokenPrices = skuInfo.prices;        if (length == 0) {            uint256 currentLength = tokenPrices.length();            for (uint256 i = 0; i < currentLength; ++i) {                (bytes32 token, ) = tokenPrices.at(0);                tokenPrices.<mask0>;            }        } else {            _setTokenPrices(tokenPrices, tokens, prices);        }        emit SkuPricingUpdate(sku, tokens, prices);    }"