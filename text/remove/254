"function burn(        address account,        uint256 id,        uint256 value    ) public virtual override {        require(hasRole(ADMIN_ROLE, _msgSender()), \"ERC1155PresetMinterPauser: must have minter role to mint\");        require(balanceOf(msg.sender, id) >= 1,  \"ERC1155: Token not owned\");        _burn(msg.sender, id, value);        delete _propertiesHash[id];        _totalSupply[id] -= value;        _addressBalance[msg.sender].<mask0>;        _totalTokenIds.remove(id);    }"