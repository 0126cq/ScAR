"function configure(address erc20_, address whitelist_)    external    onlyInState(State.UNCONFIGURED)    onlyRole(DEFAULT_ADMIN_ROLE)  {    erc20 = ITokenERC20(erc20_);    whitelist = IWhitelist(whitelist_);    uint256 saleSupply = erc20.saleSupply();    if (saleSupply != supply) {      revert SupplyConfigurationMishmatch(saleSupply, supply);    }    uint256 balance = erc20.<mask0>;    if (saleSupply != balance) {      revert BalanceNotEqualSupply(balance, saleSupply);    }    state = State.CONFIGURED;  }"