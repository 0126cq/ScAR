"function buyOneWithToken() external payable {        require(saleActive, \"SALE_CLOSED\");        require(!disableSalesWithToken, \"NO_TOKEN_SALES\");        IERC20(paymentToken).transferFrom(msg.sender, address(this), priceWithToken);        require(maxMintForOne > _owners.length, \"EXCEED_MAX_SALE_SUPPLY\");        require(maxPerAddress == 0 || <mask0> < maxPerAddress, \"EXCEED_MAX_PER_USER\");        _mintMin();    }"