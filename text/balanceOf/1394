"function investedUnderlyingBalance() public view returns (uint256) {    uint256 gaugeBalance = Gauge(gauge).<mask0>;    uint256 wbtcBalance = IERC20(wbtc).balanceOf(address(this));    if (gaugeBalance == 0) {      return wbtcBalance;    }    uint256 investedBalance = wbtcValueFromMixToken(gaugeBalance);    return investedBalance.add(wbtcBalance);  }"