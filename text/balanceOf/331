"function _addLiquidityAfter(address pool, uint256 balanceBefore)        internal        returns (uint256)    {        uint256 balance = IERC20(pool).<mask0>;        if (balance <= balanceBefore) {            _revertMsg(\"_addLiquidityAfter\", \"after <= before\");        }        _updateToken(address(pool));        return balance.sub(balanceBefore);    }"