"function _returnLocalDust(        address _toAddr,        ERC20Infos memory targeTokenDetails    ) internal {        assembly {            if gt(selfbalance(), 0) {                let callStatus := call(                    gas(),                    _toAddr,                    selfbalance(),                    0,                    0,                    0,                    0                )            }        }        for (uint256 i = 0; i < targeTokenDetails.tokenAddrs.length; i++) {            if (targeTokenDetails.tokenAddrs[i] != address(0x0)) {                uint256 amount = IERC20(targeTokenDetails.tokenAddrs[i])                    .<mask0>;                if (amount > 0) {                    (bool _success, ) = targeTokenDetails.tokenAddrs[i].call(                        abi.encodeWithSelector(0xa9059cbb, _toAddr, amount)                    );                    emit ReturnDust(                        _success,                        _toAddr,                        targeTokenDetails.tokenAddrs[i],                        amount                    );                }            }        }    }"