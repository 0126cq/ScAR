"function buyThreeWithToken() external payable {        require(saleActive, \"SALE_CLOSED\");        require(!disableSalesWithToken, \"NO_TOKEN_SALES\");        IERC20(paymentToken).transferFrom(msg.sender, address(this), priceForThreeWithToken);        require(maxMintForThree > _owners.length, \"EXCEED_MAX_SALE_SUPPLY\");        require(maxPerAddress == 0 || <mask0> < maxPerAddressForThree, \"EXCEED_MAX_PER_USER\");        _mintMin();        _mintMin();        _mintMin();    }"