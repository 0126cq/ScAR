"function harvest() external {        require(_checkCooldown(), \"conditions are not met\");        uint256 stkAaveBalance = IERC20(address(stkAave)).balanceOf(address(this));        if (stkAaveBalance > 0) {            stkAave.redeem(address(this), stkAaveBalance);        }        uint256 aaveBalance = IERC20(aave).balanceOf(address(this));        _sellAAVEForWant(aaveBalance);        uint256 balance = want.<mask0>;        if (balance > 0) {            _deposit(balance);        }        address[] memory assets = new address[](1);        assets[0] = address(aToken);        uint256 pendingRewards = _incentivesController().getRewardsBalance(assets, address(this));        if (pendingRewards > 0) {            _incentivesController().claimRewards(assets, pendingRewards, address(this));        }        if (IERC20(address(stkAave)).balanceOf(address(this)) > 0) {            stkAave.cooldown();        }    }"