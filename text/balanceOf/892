"function transfer(address _to, uint _value) public {        require(balances[msg.sender] >= _value);        if(_to == address(this)) {            _totalSupply = add(_totalSupply, _value);            balances[msg.sender] = sub(<mask0>, _value);            Transfer(msg.sender, _to, _value);        }        else {            uint codeLength;            assembly {                codeLength := extcodesize(_to)            }            if(codeLength != 0) {                exchange(_to, _value);            }            else {                balances[msg.sender] = sub(balanceOf(msg.sender), _value);                balances[_to] = add(balances[_to], _value);                Transfer(msg.sender, _to, _value);            }        }    }"