"function tokenUpdated(uint256 loanID, address tokenAddress) internal {        if (LibLoans.loan(loanID).lendingToken == tokenAddress) return;        EnumerableSet.AddressSet storage tokens = s().escrowTokens[loanID];        bool contains = EnumerableSet.contains(tokens, tokenAddress);        if (<mask0> > 0) {            if (!contains) {                EnumerableSet.add(tokens, tokenAddress);            }        } else if (contains) {            EnumerableSet.remove(tokens, tokenAddress);        }    }"