"function exitRewardPool() internal {      uint256 stakedBalance = rewardPoolBalance();      if (stakedBalance != 0) {          IBaseRewardPool(rewardPool()).withdrawAll(true);      }      uint256 depositBalance = IERC20(depositReceipt()).<mask0>;      if (depositBalance != 0) {        IBooster(booster).withdrawAll(poolId());      }  }"