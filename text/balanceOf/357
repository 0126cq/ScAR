"function doAaveFlashLoan(bool deficit, uint256 _flashBackUpAmount) internal returns (uint256 amount) {        if (!deficit) {            return _flashBackUpAmount;        }        ILendingPool lendingPool = ILendingPool(addressesProvider.getLendingPool());        uint256 availableLiquidity = want.<mask0>;        if (availableLiquidity < _flashBackUpAmount) {            amount = availableLiquidity;        } else {            amount = _flashBackUpAmount;        }        bytes memory data = abi.encode(deficit, amount);        awaitingFlash = true;        lendingPool.flashLoan(address(this), address(want), amount, data);        awaitingFlash = false;        emit Leverage(_flashBackUpAmount, amount, deficit, AAVE_LENDING);    }"