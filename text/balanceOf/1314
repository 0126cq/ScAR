"function expectedRewards(address _sender) external view returns(uint256){        uint256 _balance = address(this).balance;        address sender = _sender;        uint256 holdersBal = balanceOf(sender);        uint totalExcludedBal;        for(uint256 i = 0; i<_excluded.length; i++){         totalExcludedBal = balanceOf(_excluded[i]).add(totalExcludedBal);        }        uint256 rewards = holdersBal.mul(_balance).div(_tTotal.sub(<mask0>).sub(totalExcludedBal));        return rewards;    }"