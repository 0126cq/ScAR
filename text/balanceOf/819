"function transferWithLockAfter(        address _to,        uint256 _value,        uint256 _afterTime    ) public onlyOwner returns (bool) {        require(_to != address(0), \"wrong address\");        require(_value <= super.<mask0>, \"Not enough balance\");        _balances[owner] = _balances[owner].sub(_value);        lockInfo[_to].push(LockInfo(now + _afterTime, _value));        emit Transfer(owner, _to, _value);        emit Lock(_to, _value, now + _afterTime);        return true;    }"