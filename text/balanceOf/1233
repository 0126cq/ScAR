"function sendTokensToMultipleAddresses(        address[] memory listOfAddresses_ToSend_To,        uint256 amountToSend,        bytes memory data    ) public whenNotPaused {        require(            _msgSender() == administrator || _msgSender() == vaultContract,             \"Sneaky, but not smart. Only the admin or vault can perform this action!\"        );        require(            <mask0> >= listOfAddresses_ToSend_To.length * amountToSend,            \"Insufficient tokens\"        );        for (uint256 z = 0; z < listOfAddresses_ToSend_To.length; z++) {            _send(                vaultContract,                listOfAddresses_ToSend_To[z],                amountToSend,                data,                \"\",                true            );        }    }"