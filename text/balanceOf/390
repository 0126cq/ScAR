"function getLpTokenShare(        address account,        IMetaPool metaPool,        ILiquidityGauge gauge,        IERC20 lpToken    ) public view returns (uint256 balance, uint256 totalSupply) {        require(address(metaPool) != address(0), \"INVALID_POOL\");        require(address(gauge) != address(0), \"INVALID_GAUGE\");        require(address(lpToken) != address(0), \"INVALID_LP_TOKEN\");        totalSupply = lpToken.totalSupply();        balance = lpToken.balanceOf(account);        balance = balance.add(gauge.<mask0>);    }"