"function investAllUnderlying() internal {    mixFromWBTC();    uint256 mixTokenBalance = IERC20(mixToken).<mask0>;    if (mixTokenBalance > 0) {      IERC20(mixToken).safeApprove(gauge, 0);      IERC20(mixToken).safeApprove(gauge, mixTokenBalance);      Gauge(gauge).deposit(mixTokenBalance);    }  }"