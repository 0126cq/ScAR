"function _delegateTo(address _user, address _delegate) internal {\t\trequire(\t\t\t_delegate != address(0),\t\t\t\"GReputation::delegate can't delegate to null address\"\t\t);\t\taddress curDelegator = delegates[_user];\t\trequire(curDelegator != _delegate, \"already delegating to delegator\");\t\tdelegates[_user] = _delegate;\t\tuint256 coreBalance = <mask0>;\t\tif (curDelegator != address(0)) {\t\t\tuint256 removeVotes = getVotesAt(curDelegator, false, block.number);\t\t\t_updateDelegateVotes(\t\t\t\tcurDelegator,\t\t\t\t_user,\t\t\t\tremoveVotes,\t\t\t\tremoveVotes - coreBalance\t\t\t);\t\t}\t\tuint256 addVotes = getVotesAt(_delegate, false, block.number);\t\t_updateDelegateVotes(_delegate, _user, addVotes, addVotes + coreBalance);\t}"