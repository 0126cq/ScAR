"function stake(uint256 _stkId,uint256 amount) public virtual {        amount = amount.mul(10**18);    require(BAKED.<mask0>>= amount,\"Error: User Token Balance is insufficient\");    _totalSupply = _totalSupply + amount;    _balances[msg.sender] = _balances[msg.sender].add(amount);    BAKED.safeTransferFrom    (msg.sender, address(this), amount);    }"