"function setTotalDepositedAmount (uint256 newAmount) public onlyOwner {        require(newAmount > 0, \"Non-zero amount required\");        require(_records[currentPeriod].totalDeposited == 0, \"Deposit amount already set\");        require(_records[currentPeriod].dailyInterest == 0, \"Daily interest already set\");        uint256 currentBalance = underlyingTokenInterface.<mask0>;        require(currentBalance == 0, \"Deposits already available\");        _records[currentPeriod].totalDeposited = newAmount;    }"