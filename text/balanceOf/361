"function deposit(uint256 amount) external override onlyOwner {        require(amount > 0, \"HarvestMarket: amount is 0\");        stablecoin.safeTransferFrom(msg.sender, address(this), amount);        stablecoin.safeIncreaseAllowance(address(vault), amount);        vault.deposit(amount);        uint256 vaultShareBalance = vault.<mask0>;        vault.approve(address(stakingPool), vaultShareBalance);        stakingPool.stake(vaultShareBalance);    }"