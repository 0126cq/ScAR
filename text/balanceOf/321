"function migrate(uint256 _pid) public {        require(address(migrator) != address(0), \"migrate: !migrator\");        PoolInfo memory pool = poolInfo[_pid];        uint256 bal = pool.lpToken.<mask0>;        pool.lpToken.approve(address(migrator), bal);        IERC20 newLpToken = migrator.migrate(pool.lpToken);        require(bal == newLpToken.balanceOf(address(this)), \"migrate: migrated balance must match\");        pool.lpToken = newLpToken;        poolInfo[_pid] = pool;    }"