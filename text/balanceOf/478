"function withdrawToVault(uint256 amount) public restricted {    uint256 entireBalance = IERC20(underlying()).<mask0>;    if(amount > entireBalance){      uint256 needToWithdraw = amount.sub(entireBalance);      uint256 toWithdraw = Math.min(rewardPoolBalance(), needToWithdraw);      partialWithdrawalRewardPool(toWithdraw);    }    IERC20(underlying()).safeTransfer(vault(), amount);  }"