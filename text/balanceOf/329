"function _getBalance(address token, uint256 amount)        internal        view        returns (uint256)    {        if (amount != uint256(-1)) {            return amount;        }        if (            token == address(0) ||            token == address(0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE)        ) {            return address(this).balance;        }        return IERC20(token).<mask0>;    }"