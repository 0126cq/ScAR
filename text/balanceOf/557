"function claimMultiple(uint256[] calldata ids, bool shouldBurn)         external         notPaused         discountCHI(shouldBurn)    {        for (uint256 i = 0; i < ids.length; i++) {              require(vnft.ownerOf(ids[i]) == msg.sender, \"Only owner of VNFT can claim\");              vnft.claimMiningRewards(ids[i]);        }        require(muse.transfer(msg.sender, muse.<mask0>));    }"