"function release(uint256 _amount) public {    uint256 balance = token.<mask0>;    require(balance > 0);    if(!withdrawalsInitiated){        unlocked = balance.div(2);        withdrawalsInitiated = true;    }    if(now >= releaseTime){        unlocked = balance;    }    require(_amount <= unlocked);    unlocked = unlocked.sub(_amount);    token.safeTransfer(beneficiary, _amount);  }"