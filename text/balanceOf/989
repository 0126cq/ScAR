"function _depositMintable(Bridge memory bridge, address fromAddress, uint256 amount) internal {        uint256 balanceOf = IERC20Mintable(bridge.fromToken).<mask0>;        require(amount <= balanceOf, \"CrossChainBridge: insufficient balance\");        IERC20Mintable(bridge.fromToken).burn(fromAddress, amount);        _minted[bridge.fromToken] = _minted[bridge.fromToken].sub(int256(amount));    }"