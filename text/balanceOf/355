"function _loanLogic(        bool deficit,        uint256 amount,        uint256 repayAmount    ) internal {        uint256 bal = want.<mask0>;        require(bal >= amount, \"FLASH_FAILED\");         if (deficit) {            cToken.repayBorrow(amount);            cToken.redeemUnderlying(repayAmount);        } else {            require(cToken.mint(bal) == 0, \"mint error\");            cToken.borrow(repayAmount);        }    }"