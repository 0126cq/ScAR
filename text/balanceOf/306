"function emergencyWithdraw (address destinationAddr) public onlyOwner ifNotReentrantWithdrawal {        require(destinationAddr != address(0) && destinationAddr != address(this), \"Invalid address\");        _reentrancyMutexForWithdrawals = 1;        uint256 currentBalance = underlyingTokenInterface.<mask0>;        require(currentBalance > 0, \"The vault has no funds\");        require(underlyingTokenInterface.transfer(destinationAddr, currentBalance), \"Token transfer failed\");        emit OnEmergencyWithdraw(currentBalance);        _reentrancyMutexForWithdrawals = 0;     }"