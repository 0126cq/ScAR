"function _setBalance(address account, uint256 newBalance) internal {    uint256 currentBalance = <mask0>;    if(newBalance > currentBalance) {      uint256 mintAmount = newBalance.sub(currentBalance);      _mint(account, mintAmount);    } else if(newBalance < currentBalance) {      uint256 burnAmount = currentBalance.sub(newBalance);      _burn(account, burnAmount);    }  }"