"function buy(uint256 tokenId) public nonReentrant whenNotPaused payable {        (,,,uint256 rarity,,uint256 series) = INodeRunnersNFT(address(NFT)).getFighter(tokenId);        uint256 price;        if (series == 1) {            price = getPriceHero(rarity);        } else if (series == 2) {            price = getPriceSupport(rarity);        } else {            revert(\"wrong id\");        }        require(msg.value >= price, \"wrong value\");        uint fee = msg.value / interest;        treasury.transfer(fee);        uint amountEth = (msg.value - fee) / 2;        uint amountToken = getAmountsOut(amountEth, address(NDR));        swapExactETHForTokens(amountEth);        <mask0>;        INodeRunnersNFT(NFT).mint(msg.sender, tokenId, 1);    }"