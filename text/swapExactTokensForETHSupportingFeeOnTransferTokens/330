"function _claimOfWETH(address _addr) internal {        (, , uint256 ETHamount) = claimOf(_addr);        if (ETHamount > 0) {            IERC20Upgradeable(SROOTxTokenAddress).approve(                UNISWAP_V2_ROUTER,                ETHamount            );            address[] memory path;            path = new address[](2);            path[0] = SROOTxTokenAddress;            path[1] = WETH;            IUniswapV2Router(UNISWAP_V2_ROUTER)                .<mask0>;            for (uint256 i = 0; i < stakes[_addr].length; i++) {                stakes[_addr][i].claimedDate_WETH = block.timestamp;            }        }    }"