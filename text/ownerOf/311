"function claim(address _beneficiary, uint256 _comic) public {        require(tlgcmc.<mask0> == _beneficiary, \"LOSTAirdropV2: Beneficiary is not owner of Comic.\");        require(hasClaimed[_comic] == false, \"LOSTAirdropV2: Reward already claimed for Comic.\");        require(lost.balanceOf(address(this)) >= airdropPerComic, \"LOSTAirdropV2: Not enough rewards left. Try again later.\");        hasClaimed[_comic] = true;        lost.transfer(_beneficiary, airdropPerComic);    }"