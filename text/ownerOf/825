"function getOwnerCountByMrId(uint8 search_mrId, uint8 restrictedFlag) public view returns(uint256){        uint256 totalTokenCount = totalSupply();        uint256 tokenId;        uint8 mrId;        restrictedFlag = restrictedFlag % 2;        address[]  memory ownerArray = new address[](totalTokenCount);        uint256 count = 0;        address tokenOwner;        bool flag;        for(uint256 i = 0; i < totalTokenCount; i++){            if(restrictedFlag == 1 && getUserNftSellState(tokenId) != 1)                continue;            tokenId = tokenByIndex(i);            mrId = getMrId(tokenId);            if(mrId == search_mrId){                tokenOwner = <mask0>;                flag = false;                for(uint256 j = 0; j < count; j++){                    if(tokenOwner == ownerArray[j]){                        flag = true;                        break;                    }                }                if(!flag){                    ownerArray[count] = tokenOwner;                    count++;                }            }        }        return count;    }"