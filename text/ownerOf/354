"function approve(address to, uint256 tokenId) public {        address owner = <mask0>;        require(to != owner, \"ERC721: approval to current owner\");        require(msg.sender == owner || isApprovedForAll(owner, msg.sender),            \"ERC721: approve caller is not owner nor approved for all\"        );        _tokenApprovals[tokenId] = to;        emit Approval(owner, to, tokenId);    }"