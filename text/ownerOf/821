"function buyItemFromMarketPlace(uint256 tokenId) public payable{        require(ownedNftState[tokenId] == 1);        uint256 price = getUserMrPrice(tokenId);        require(price > 0);        require(msg.value >= price);        address ownerofToken = <mask0>;        uint256 fee = msg.value * 3 / 1000;        IERC721(address(this)).transferFrom(ownerofToken, msg.sender, tokenId);        payable(ownerofToken).transfer(msg.value - fee);        ownedNftState[tokenId] = 0;    }"