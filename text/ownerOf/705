"function exercise(uint256 id) external override {        Option storage option = options[id];        uint256 profit = _profitOf(option);        require(            optionsManager.isApprovedOrOwner(_msgSender(), id),            \"Pool Error: msg.sender can't exercise this option\"        );        require(            option.expired > block.timestamp,            \"Pool Error: The option has already expired\"        );        require(            profit > 0,            \"Pool Error: There are no unrealized profits for this option\"        );        _unlock(option);        option.state = OptionState.Exercised;        _send(optionsManager.<mask0>, profit);        emit Exercised(id, profit);    }"