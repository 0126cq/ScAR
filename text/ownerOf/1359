"function reveal(uint256[] calldata tokenIDs) external {    require(tx.origin == msg.sender, \"Trying to reveal from a contract\");    require(revealContractAddress != address(0), \"Reveal contract address unset\");    uint256[] memory newTokenIDs = new uint256[](tokenIDs.length);    for (uint256 i = 0; i < tokenIDs.length; ++i) {      require(msg.sender == <mask0>, \"Reveal from incorrect owner\");      _burn(tokenIDs[i]);      newTokenIDs[i] = _getRandomAvailableTokenId();      _numAvailableTokens -= 1;    }    RevealInterface revealContract = RevealInterface(revealContractAddress);    revealContract.mint(msg.sender, newTokenIDs);  }"