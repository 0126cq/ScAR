"function approve(address _to, uint256 _tokenId) public {    address owner = <mask0>;    require(_to != owner);    require(msg.sender == owner || isApprovedForAll(owner, msg.sender));    if (getApproved(_tokenId) != address(0) || _to != address(0)) {      tokenApprovals[_tokenId] = _to;      emit Approval(owner, _to, _tokenId);    }  }"