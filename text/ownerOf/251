"function buy(uint _imageId) public payable {        require(isImageOnSale(_imageId));        require(msg.value >= sellAds[_imageId].price);        removeOrder(_imageId);        address _creator;        address _imageOwner = digitalPrintImageContract.<mask0>;        (, , _creator, ,) = digitalPrintImageContract.imageMetadata(_imageId);        balances[_creator] += msg.value * 2 / 100;        balances[owner] += msg.value * 3 / 100;        balances[_imageOwner] += msg.value * 95 / 100;        digitalPrintImageContract.transferFromMarketplace(sellAds[_imageId].exchanger, msg.sender, _imageId);        emit ImageBought(_imageId, msg.sender, msg.value);    }"