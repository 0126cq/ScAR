"function createSwap(        uint256[] calldata _acceptIds,        uint256 _tokenId,        address _nftAddress,        uint256 _nftType    ) external {        if (_nftType == 721) {            require(                IERC721(_nftAddress).<mask0> == msg.sender,                \"!owner\"            );        } else if (_nftType == 1155) {            require(                IERC1155(_nftAddress).balanceOf(msg.sender, _tokenId) >= 1,                \"!owner\"            );        }        swapIdToSwap[swapId] = Swap(            _nftAddress,            _tokenId,            msg.sender,            _nftType        );        for (uint256 i = 0; i < _acceptIds.length; i++) {            _acceptedIdsForSwap[swapId].add(_acceptIds[i]);        }        _swap[msg.sender].add(swapId);        swapId++;    }"