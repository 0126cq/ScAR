"function createRace(        uint256 _tokenId,        uint256 _amount,        uint256 _raceSlot    )        external        slotAvailable(_raceSlot)        whenNotPaused()        returns (uint256)    {        require(_amount >= 5000000000000000000, 'PinkslipRacing: Min amount 5 tokens');         require(_raceSlot >= 0 && _raceSlot < 50, 'PinkslipRacing: Slots from 0 to 49');         require(PinkslipNFT(ERC721).<mask0> == address(msg.sender), 'PinkslipRacing: You must be the token owner');         uint256 lastSlotBySender = addressInSlot[msg.sender];        uint256 raceIdOnLastUsedSlotBySender = racesSlots[lastSlotBySender];        require(            raceIdOnLastUsedSlotBySender == 0 ||             races[raceIdOnLastUsedSlotBySender].owner != msg.sender  ||             _getTime() > races[raceIdOnLastUsedSlotBySender].endDate ,            'PinkslipRacing: You already on slot'        );        uint256 timeNow = _getTime();        uint256 newRaceId = totalRacesCount;        totalRacesCount += 1;        races[newRaceId] = Race({            tokenId: _tokenId,            owner: msg.sender,            amount: _amount,            acceptedBy: address(0x0),            tokenIdAccepted: 0,            winner: address(0x0),            endDate: timeNow + 24 hours        });        lastRaceByToken[_tokenId] = newRaceId;        racesSlots[_raceSlot] = newRaceId;        addressInSlot[msg.sender] = _raceSlot;        emit CreatedRace(newRaceId, _raceSlot, msg.sender, _tokenId, _amount, timeNow);        return newRaceId;    }"