"function buyToken(uint256 _tokenId) public payable {    require(msg.sender != address(0));    require(<mask0>);    address tokenOwner = ownerOf(_tokenId);    require(tokenOwner != address(0));    require(tokenOwner != msg.sender);    MysticBet memory mysticbet = allMysticBets[_tokenId];    require(msg.value >= mbjPrice);    require(mysticbet.forSale);    _transfer(tokenOwner, msg.sender, _tokenId);    address payable sendTo = mysticbet.currentOwner;    address payable feeSendTo = adminAddress;    sendTo.transfer(msg.value - msg.value * 5 / 100);    feeSendTo.transfer(msg.value * 5 / 100);    mysticbet.previousOwner = mysticbet.currentOwner;    mysticbet.currentOwner = msg.sender;    mysticbet.numberOfTransfers += 1;    allMysticBets[_tokenId] = mysticbet;  }"