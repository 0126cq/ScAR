"function _isApprovedOrOwner(address spender, uint256 tokenId) internal view returns (bool) {        require(<mask0>, \"ERC721: operator query for nonexistent token\");        address tokenOwner = ownerOf(tokenId);        return (spender == tokenOwner || getApproved(tokenId) == spender || isApprovedForAll(tokenOwner, spender));    }"