"function buyPixel(        uint32 x,        uint32 y,        uint8 colorIndex    ) public payable isAvailable nonReentrant {        if (!auctionStarted) {            auctionStarted = true;        }        uint256 pixelId = _getId(x, y);        if (!<mask0>) {            require(msg.value >= minInitialBid, \"Bid is below minimum bid.\");            _send(devAddress, msg.value);            _safeMint(msg.sender, pixelId);        } else {            address currentOwner = ownerOf(pixelId);            require(msg.sender != currentOwner, \"You already own this pixel.\");            uint256 minPrice = pixelPrice[pixelId] * 2;            require(msg.value >= minPrice, \"Bid is below minimum bid.\");            uint256 payout = (msg.value * 4) / 5;            _send(currentOwner, payout);            _send(devAddress, msg.value - payout);            _safeTransfer(currentOwner, msg.sender, pixelId, \"\");        }        pixelPrice[pixelId] = msg.value;        _setColor(pixelId, colorIndex);    }"