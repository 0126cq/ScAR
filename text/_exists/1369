"function ownershipOf(uint256 tokenId) internal view returns (TokenOwnership memory) {        require(<mask0>, \"ERC721A: owner query for nonexistent token\");        uint256 lowestTokenToCheck;        if (tokenId >= maxBatchSize) {            lowestTokenToCheck = tokenId - maxBatchSize + 1;        }        for (uint256 curr = tokenId; curr >= lowestTokenToCheck; curr--) {            TokenOwnership memory ownership = _ownerships[curr];            if (ownership.addr != address(0)) {                return ownership;            }        }        revert(\"ERC721A: unable to determine the owner of token\");    }"