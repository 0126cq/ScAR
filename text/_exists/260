"function toggleForSale(uint256 _tokenId) public {    require(msg.sender != address(0));    require(<mask0>);    address tokenOwner = ownerOf(_tokenId);    require(tokenOwner == msg.sender);    MysticBet memory mysticbet = allMysticBets[_tokenId];    if(mysticbet.forSale) {      mysticbet.forSale = false;    } else {      mysticbet.forSale = true;    }    allMysticBets[_tokenId] = mysticbet;  }"