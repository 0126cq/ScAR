"function setUserNftSellState(uint256 _tokenId, uint256 _price, uint8 state) public {        require(<mask0>, \"nonexistent token\");        require(msg.sender == ownerOf(_tokenId));        state = state % 2;        setUserMrPrice(_tokenId, _price);        if (state == 1)        {            approve(address(this), _tokenId);        }        else        {            approve(address(0), _tokenId);        }        ownedNftState[_tokenId] = state;    }"