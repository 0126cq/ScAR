"function _mintBatch(address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data) internal virtual {        require(to != address(0), \"ERC1155: mint to the zero address\");        require(ids.length == amounts.length, \"ERC1155: ids and amounts length mismatch\");        address operator = _msgSender();        <mask0>;        for (uint i = 0; i < ids.length; i++) {            _balances[ids[i]][to] += amounts[i];        }        emit TransferBatch(operator, address(0), to, ids, amounts);        _doSafeBatchTransferAcceptanceCheck(operator, address(0), to, ids, amounts, data);    }"