"function safeTransferFrom(        address from,        address to,        uint256 id,        uint256 amount,        bytes memory data    )        public        virtual        override    {        require(to != address(0), \"ERC1155: transfer to the zero address\");        require(            from == _msgSender() || isApprovedForAll(from, _msgSender()),            \"ERC1155: caller is not owner nor approved\"        );        address operator = _msgSender();        <mask0>;        _balances[id][from] = _balances[id][from].sub(amount, \"ERC1155: insufficient balance for transfer\");        _balances[id][to] = _balances[id][to].add(amount);        emit TransferSingle(operator, from, to, id, amount);        _doSafeTransferAcceptanceCheck(operator, from, to, id, amount, data);    }"