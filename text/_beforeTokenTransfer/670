"function _safeTransferFrom(        address from,        address to,        uint256 id,        uint256 amount,        bytes memory data    ) internal virtual {        require(to != address(0), \"ERC1155: transfer to the zero address\");        address operator = _msgSender();        uint256[] memory ids = _asSingletonArray(id);        uint256[] memory amounts = _asSingletonArray(amount);        <mask0>;        uint256 fromBalance = _balances[id][from];        require(fromBalance >= amount, \"ERC1155: insufficient balance for transfer\");        unchecked {            _balances[id][from] = fromBalance - amount;        }        _balances[id][to] += amount;        emit TransferSingle(operator, from, to, id, amount);        _afterTokenTransfer(operator, from, to, ids, amounts, data);        _doSafeTransferAcceptanceCheck(operator, from, to, id, amount, data);    }"