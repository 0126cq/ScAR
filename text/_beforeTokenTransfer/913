"function _burn(address account, uint256 id, uint256 amount) internal virtual {        require(account != address(0), \"ERC1155: burn from the zero address\");        address operator = _msgSender();        <mask0>;        _balances[id][account] = _balances[id][account].sub(            amount,            \"ERC1155: burn amount exceeds balance\"        );        emit TransferSingle(operator, account, address(0), id, amount);    }"