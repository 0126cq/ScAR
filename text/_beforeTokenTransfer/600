"function _beforeTokenTransfer(address operator, address from, address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data)        internal        override(ERC1155, ERC1155Supply)    {        super.<mask0>;        if (from != address(0)) {            for (uint256 i = 0; i < ids.length; ++i) {                _updateOwnerReward(_ownerRewardInfo[ids[i]][from], from, ids[i]);            }        }        for (uint256 i = 0; i < ids.length; ++i) {            _updateOwnerReward(_ownerRewardInfo[ids[i]][to], to, ids[i]);        }    }"