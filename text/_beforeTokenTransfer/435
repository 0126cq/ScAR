"function _mint(        address to,        uint256 id,        uint256 amount,        bytes memory data    ) internal virtual {        require(to != address(0), \"ERC1155: mint to the zero address\");        address operator = _msgSender();        <mask0>;        _balances[id][to] += amount;        emit TransferSingle(operator, address(0), to, id, amount);        _doSafeTransferAcceptanceCheck(operator, address(0), to, id, amount, data);    }"