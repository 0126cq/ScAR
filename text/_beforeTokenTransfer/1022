"function _beforeTokenTransfer(        address operator,        address from,        address to,        uint256[] memory ids,        uint256[] memory amounts,        bytes memory data    ) internal virtual override {        super.<mask0>;        if (from == address(0)) {            for (uint256 i = 0; i < ids.length; ++i) {                _totalSupply[ids[i]] += amounts[i];            }        }        if (to == address(0)) {            for (uint256 i = 0; i < ids.length; ++i) {                _totalSupply[ids[i]] -= amounts[i];            }        }    }"