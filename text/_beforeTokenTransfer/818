"function _beforeTokenTransfer(        address from,        address to,        uint256 amount    ) internal override {        super.<mask0>;        if (from != address(0))            IPendleForge(forge).updateDueInterests(underlyingAsset, expiry, from);        if (to != address(0)) IPendleForge(forge).updateDueInterests(underlyingAsset, expiry, to);    }"