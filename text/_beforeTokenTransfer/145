"function _mint(address account, uint256 id, uint256 amount, bytes memory data) internal virtual {        require(account != address(0), \"ERC1155: mint to the zero address\");        address operator = _msgSender();        <mask0>;        _balances[id][account] += amount;        emit TransferSingle(operator, address(0), account, id, amount);        _doSafeTransferAcceptanceCheck(operator, address(0), account, id, amount, data);    }"