"function _mintBatch(        address account,        uint256[] memory ids,        uint256[] memory amounts,        bytes memory data    ) internal virtual {        require(account != address(0), 'ERC1155: mint to the zero address');        require(            ids.length == amounts.length,            'ERC1155: ids and amounts length mismatch'        );        <mask0>;        mapping(uint256 => mapping(address => uint256))            storage balances = ERC1155BaseStorage.layout().balances;        for (uint256 i; i < ids.length; ) {            balances[ids[i]][account] += amounts[i];            unchecked {                i++;            }        }        emit TransferBatch(msg.sender, address(0), account, ids, amounts);    }"