"function _move(        address operator,        address from,        address to,        uint256 amount,        bytes memory userData,        bytes memory operatorData    )        private    {        <mask0>;        _balances[from] = _balances[from].sub(amount, \"ERC777: transfer amount exceeds balance\");        _balances[to] = _balances[to].add(amount);        emit Sent(operator, from, to, amount, userData, operatorData);        emit Transfer(from, to, amount);    }"