"function _burnBatch(address account, uint256[] memory ids, uint256[] memory amounts) internal virtual {        require(account != address(0), \"ERC1155: burn from the zero address\");        require(ids.length == amounts.length, \"ERC1155: ids and amounts length mismatch\");        address operator = _msgSender();        <mask0>;        for (uint i = 0; i < ids.length; i++) {            _balances[ids[i]][account] = _balances[ids[i]][account].sub(                amounts[i],                \"ERC1155: burn amount exceeds balance\"            );        }        emit TransferBatch(operator, account, address(0), ids, amounts);    }"