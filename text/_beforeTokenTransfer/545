"function _beforeTokenTransfer(address from, address to, uint256 amount) internal virtual override {      super.<mask0>;      if (from == address(0)) {        _updateAccountSnapshot(to);        _updateTotalSupplySnapshot();      } else if (to == address(0)) {        _updateAccountSnapshot(from);        _updateTotalSupplySnapshot();      } else {        _updateAccountSnapshot(from);        _updateAccountSnapshot(to);      }    }"