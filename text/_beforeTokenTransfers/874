"function _mint(address to, uint256 quantity) internal {        uint256 startTokenId = _currentIndex;        if (to == address(0)) revert MintToZeroAddress();        if (quantity == 0) revert MintZeroQuantity();        <mask0>;        unchecked {            _packedAddressData[to] +=                quantity *                ((1 << BITPOS_NUMBER_MINTED) | 1);            _packedOwnerships[startTokenId] =                _addressToUint256(to) |                (block.timestamp << BITPOS_START_TIMESTAMP) |                (_boolToUint256(quantity == 1) << BITPOS_NEXT_INITIALIZED);            uint256 updatedIndex = startTokenId;            uint256 end = updatedIndex + quantity;            do {                emit Transfer(address(0), to, updatedIndex++);            } while (updatedIndex < end);            _currentIndex = updatedIndex;        }        _afterTokenTransfers(address(0), to, startTokenId, quantity);    }"