"function tryRecover(bytes32 hash, bytes memory signature) internal pure returns (address, RecoverError) {        if (signature.length == 65) {            bytes32 r;            bytes32 s;            uint8 v;            assembly {                r := mload(add(signature, 0x20))                s := mload(add(signature, 0x40))                v := byte(0, mload(add(signature, 0x60)))            }            return <mask0>;        } else {            return (address(0), RecoverError.InvalidSignatureLength);        }    }"