"function verifyOpenSignature(uint256 _id, bool _hasTicket, bytes memory _signature) internal {        bytes32 hashed = keccak256(abi.encode(\"CHID:\", block.chainid, \"\\nGLD:\", _hasTicket, \"\\nTID:\", _id));        (address _signedBy,) = hashed.<mask0>;        require(_signedBy != address(0), \"ChocoNFT#INVALID-SIGN-1\");        require(_signedBy == signer, \"ChocoNFT#INVALID-SIGN-2\");    }"