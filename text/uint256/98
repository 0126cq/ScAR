"function mintableBalanceOfBatch(address[] calldata owners, uint256[] calldata ids) external view returns (uint256[] memory) {        require(owners.length == ids.length, \"DropperToken: owners and ids length mismatch\");        uint256[] memory balances = new <mask0>;        for (uint256 i = 0; i < ids.length; i++) {            balances[i] = _mintableBalanceOf(owners[i], ids[i]);        }        return balances;    }"