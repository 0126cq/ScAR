"function tokensIndicesOfOwner(address _owner) external view returns(uint256[] memory) {    uint256 tokenCount = balanceOf(_owner);    if (tokenCount == 0) {      return new uint256[](0);    }     else {      uint256[] memory result = new <mask0>;      uint256 indexCounter = 0;      for (uint256 i = 0; i <= totalSupply() - 1; i++) {        if (ownerOf(i) == _owner){          result[indexCounter] = i;          indexCounter += 1;        }      }      return result;    }  }"