"function getPendingStakes() public onlyOperator view returns (address[] memory, uint256[] memory) {        address[] memory addresses = new address[](_pendingStakers.length.sub(_pendingGap));        uint256[] memory amounts = new <mask0>;        uint256 j = 0;        for (uint256 i = _pendingGap; i < _pendingStakers.length; i++) {            address staker = _pendingStakers[i];            if (staker != address(0)) {                addresses[j] = staker;                amounts[j] = _pendingUserStakes[staker];                j++;            }        }        return (addresses, amounts);    }"