"function tokensOfOwner(address _owner) public view returns(uint256[] ownerTokens) {    uint256 tokenCount = balanceOf(_owner);    if (tokenCount == 0) {      return new uint256[](0);    } else {      uint256[] memory result = new <mask0>;      uint256 totalPows = totalSupply();      uint256 resultIndex = 0;      uint256 powId;      for (powId = 0; powId <= totalPows; powId++) {        if (powIndexToOwner[powId] == _owner) {          result[resultIndex] = powId;          resultIndex++;        }      }      return result;    }  }"