"function getSynthSuspensions(bytes32[] calldata synths)        external        view        returns (bool[] memory suspensions, uint256[] memory reasons)    {        suspensions = new bool[](synths.length);        reasons = new <mask0>;        for (uint i = 0; i < synths.length; i++) {            suspensions[i] = synthSuspension[synths[i]].suspended;            reasons[i] = synthSuspension[synths[i]].reason;        }    }"