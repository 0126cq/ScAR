"function _mintAmount(uint256 amount, address account) internal {        uint256[] memory ids = new uint256[](amount);        uint256[] memory amounts = new <mask0>;        uint256 newCurrentTokenId = _currentTokenId;        for (uint256 i = 0; i < amount; ) {            unchecked {                ids[i] = ++newCurrentTokenId;                amounts[i] = 1;                ++i;            }        }        _currentTokenId = newCurrentTokenId;        _mintBatch(account, ids, amounts, \"\");    }"