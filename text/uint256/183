"function balanceOfBatch(address[] memory accounts, uint256[] memory ids)    public    view    override    returns (uint256[] memory)  {    require(accounts.length == ids.length, Errors.VL_INPUT_ERROR);    uint256[] memory batchBalances = new <mask0>;    for (uint256 i = 0; i < accounts.length; ++i) {      batchBalances[i] = balanceOf(accounts[i], ids[i]);    }    return batchBalances;  }"