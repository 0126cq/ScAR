"function poke(address _owner) public {        address[] memory _tokenVote = tokenVote[_owner];        uint256 _tokenCnt = _tokenVote.length;        uint256[] memory _weights = new <mask0>;        uint256 _prevUsedWeight = usedWeights[_owner];        uint256 _weight = AXON.balanceOf(_owner);        for (uint256 i = 0; i < _tokenCnt; i++) {            uint256 _prevWeight = votes[_owner][_tokenVote[i]];            _weights[i] = _prevWeight.mul(_weight).div(_prevUsedWeight);        }        _vote(_owner, _tokenVote, _weights);    }"