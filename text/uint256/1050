"function estimateBatchAssetPriceInErc20(bytes memory data) public view returns(address[] memory erc20Addrs, uint256[] memory erc20Amounts) {        NftDetails[] memory nftDetails;        (nftDetails) = abi.decode(            data,            (NftDetails[])        );        erc20Addrs = new address[](nftDetails.length);         erc20Amounts = new <mask0>;        for (uint256 i = 0; i < nftDetails.length; i++) {            erc20Addrs[i] = Nft20Registry.nftToErc20(nftDetails[i].nftAddr);            require(erc20Addrs[i] != address(0), \"estimateBatchAssetPriceInErc20: unsupported nft\");            for (uint256 j = 0; j < nftDetails[i].tokenIds.length; j++) {                erc20Amounts[i] = erc20Amounts[i] + nftDetails[i].amounts[j]*NFT20_NFT_VALUE;            }        }    }"