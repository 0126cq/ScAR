"function getUserPermissions(address user)    external    view    override    returns (uint256[] memory, uint256)  {    uint256[] memory roles = new <mask0>;    uint256 rolesCount = 0;    uint256 userPermissions = _users[user].permissions;    for (uint256 i = 0; i < 256; i++) {      if ((userPermissions >> i) & 1 > 0) {        roles[rolesCount] = i;        rolesCount++;      }    }    return (roles, rolesCount);  }"