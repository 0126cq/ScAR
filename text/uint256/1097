"function balanceOfBatch(address[] memory accounts, uint256[] memory ids)        public        view        virtual        override        returns (uint256[] memory)    {        require(            accounts.length == ids.length,            'ERC1155: accounts and ids length mismatch'        );        mapping(uint256 => mapping(address => uint256))            storage balances = ERC1155BaseStorage.layout().balances;        uint256[] memory batchBalances = new <mask0>;        unchecked {            for (uint256 i; i < accounts.length; i++) {                require(                    accounts[i] != address(0),                    'ERC1155: batch balance query for the zero address'                );                batchBalances[i] = balances[ids[i]][accounts[i]];            }        }        return batchBalances;    }"