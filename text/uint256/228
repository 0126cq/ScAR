"function _getReward(address rewardee, address destination_address) internal updateRewardAndBalance(rewardee, true) returns (uint256[] memory rewards_before) {        rewards_before = new <mask0>;        for (uint256 i = 0; i < rewardTokens.length; i++){             rewards_before[i] = rewards[rewardee][i];            rewards[rewardee][i] = 0;            TransferHelper.safeTransfer(rewardTokens[i], destination_address, rewards_before[i]);        }        _getRewardExtraLogic(rewardee, destination_address);        lastRewardClaimTime[rewardee] = block.timestamp;    }"