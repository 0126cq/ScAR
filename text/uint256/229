"function getTokensByOwner(address _owner) public view returns(uint256[] memory ownerTokens) {        uint256 tokenCount = balanceOf(_owner);        if (tokenCount == 0) {            return new <mask0>;        } else {            uint256[] memory result = new uint256[](tokenCount);            for (uint256 index = 0; index < tokenCount; index++) {                result[index] = tokenOfOwnerByIndex(_owner, index);            }            return result;        }    }"