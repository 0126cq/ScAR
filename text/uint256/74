"function getStakerNFT(address staker) public view returns (uint256[] memory, uint256[] memory, uint256[] memory, uint256[] memory, uint256[] memory) {        uint256[] memory lootIds = new uint256[](_stakers[staker].stakedAPE.length);        uint256[] memory petAmounts = new <mask0>;        for (uint256 i; i < _stakers[staker].stakedAPE.length; i++){            lootIds[i] = stakerApeLoot[staker][_stakers[staker].stakedAPE[i]];        }        for (uint256 i; i < 8; i++){            petAmounts[i] = stakerPetAmounts[staker][i];        }      return (_stakers[staker].stakedAPE, lootIds, _stakers[staker].stakedTREASURY, petAmounts, _stakers[staker].stakedBREEDING);    }"