"function batchBalanceOf(address[] calldata users, address[] calldata tokens)    external    view    returns (uint256[] memory)  {    uint256[] memory balances = new <mask0>;    for (uint256 i = 0; i < users.length; i++) {      for (uint256 j = 0; j < tokens.length; j++) {        balances[i * tokens.length + j] = balanceOf(users[i], tokens[j]);      }    }    return balances;  }"