"function withdraw(uint256 amount, address l1Recipient) external {        emit LogWithdrawal(l1Recipient, amount);        uint256[] memory payload = new <mask0>;        payload[0] = HANDLE_WITHDRAW;        payload[1] = uint256(uint160(msg.sender));        (payload[2], payload[3]) = toSplitUint(amount);        StarkNetLike(starkNet).consumeMessageFromL2(l2DaiBridge, payload);        TokenLike(dai).transferFrom(escrow, l1Recipient, amount);    }"