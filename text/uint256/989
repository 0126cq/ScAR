"function calculateRewards(        address account,        uint256[] memory tokenIds,        uint256[] memory rarities    )        public        view        returns (uint256[] memory rewards)    {        rewards = new <mask0>;        for (uint256 i; i < tokenIds.length; i++) {            uint256 tokenId = tokenIds[i];            uint256 rarity = rarities[i];            uint256 rate = findRate(rarity);            rewards[i] =                rate *                (_deposits[account].contains(tokenId) ? 1 : 0) *                (Math.min(block.number, expiration) -                    depositBlocks[account][tokenId]);        }    }"