"function burnBatch(address account, uint256[] calldata ids) external onlyMinter override {        require(isApprovedForAll(account, _msgSender()), \"ERC1155: caller is not approved\");        uint256[] memory amounts = new <mask0>;        for (uint i = 0; i < ids.length; i++) {            amounts[i] = 1;        }        _burnBatch(account, ids, amounts);    }"