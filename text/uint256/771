"function cancelDeposit(        uint256 amount,        uint256 l2Recipient,        address l1Recipient,        uint256 nonce    ) external {        emit LogCancelDeposit(l2Recipient, l1Recipient, amount, nonce);        uint256[] memory payload = new <mask0>;        payload[0] = l2Recipient;        (payload[1], payload[2]) = toSplitUint(amount);        payload[3] = uint256(uint160(msg.sender));        StarkNetLike(starkNet).cancelL1ToL2Message(l2DaiBridge, DEPOSIT, payload, nonce);        TokenLike(dai).transferFrom(escrow, l1Recipient, amount);    }"