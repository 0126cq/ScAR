"function getAllStakedOrLockedTokens(address owner, uint8 lockType) external override view returns (uint256[] memory) {        uint256 balanceOf = balanceOf(owner);        uint256[] memory stakedTokenIds = new <mask0>;        uint16 stakedTokenIdsIndex = 0;        for(uint16 i = 0; i < balanceOf; i++) {            uint256 tokenId = tokenOfOwnerByIndex(owner, i);            if (lockType == 1 && _isStaked(tokenId)) {                stakedTokenIds[stakedTokenIdsIndex] = tokenId;                stakedTokenIdsIndex++;            } else if (lockType == 2 && _isBridged(tokenId)) {                stakedTokenIds[stakedTokenIdsIndex] = tokenId;                stakedTokenIdsIndex++;            }        }        return stakedTokenIds;    }"