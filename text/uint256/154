"function _updateMonth() internal {\t\tuint256 month = block.timestamp / 30 days;\t\tif (\t\t\tnameService.getAddress(\"FUND_MANAGER\") != address(0) &&\t\t\tmonth != currentMonth\t\t) {\t\t\tGoodFundManager gfm = GoodFundManager(\t\t\t\tnameService.getAddress(\"FUND_MANAGER\")\t\t\t);\t\t\tuint256 activeContractsCount = gfm.getActiveContractsCount();\t\t\taddress payable[] memory activeStakingList = new address payable[](\t\t\t\tactiveContractsCount\t\t\t);\t\t\tuint256[] memory contractLockedValue = new <mask0>;\t\t\tuint256 totalLockedValue;\t\t\tfor (uint256 i = 0; i < activeContractsCount; i++) {\t\t\t\tactiveStakingList[i] = payable(gfm.activeContracts(i));\t\t\t\t(, uint64 blockStart, uint64 blockEnd, ) = gfm\t\t\t\t\t.rewardsForStakingContract(activeStakingList[i]);\t\t\t\tif (blockStart <= block.number && blockEnd > block.number) {\t\t\t\t\t(, , , uint256 lockedValueInUSD, ) = SimpleStaking(\t\t\t\t\t\tactiveStakingList[i]\t\t\t\t\t).currentGains(true, false);\t\t\t\t\tcontractLockedValue[i] = lockedValueInUSD;\t\t\t\t\ttotalLockedValue += contractLockedValue[i];\t\t\t\t}\t\t\t}\t\t\tfor (uint256 i = 0; i < activeContractsCount; i++) {\t\t\t\tuint256 contractShare = totalLockedValue > 0\t\t\t\t\t? (monthlyReputationDistribution * contractLockedValue[i]) /\t\t\t\t\t\ttotalLockedValue\t\t\t\t\t: monthlyReputationDistribution / activeContractsCount;\t\t\t\tif (contractLockedValue[i] > 0) {\t\t\t\t\t_setMonthlyRewards(activeStakingList[i], contractShare);\t\t\t\t}\t\t\t}\t\t\tcurrentMonth = month;\t\t}\t}"