"function getSynthExchangeSuspensions(bytes32[] calldata synths)        external        view        returns (bool[] memory exchangeSuspensions, uint256[] memory reasons)    {        exchangeSuspensions = new bool[](synths.length);        reasons = new <mask0>;        for (uint i = 0; i < synths.length; i++) {            exchangeSuspensions[i] = synthExchangeSuspension[synths[i]].suspended;            reasons[i] = synthExchangeSuspension[synths[i]].reason;        }    }"