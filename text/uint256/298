"function getClanRecords(uint256 clanId) public view returns (address[] memory entity, uint256[] memory updateClanTimestamp) {    uint256 clanCount = getEntityClanCount(clanId);    address[] memory addr = new address[](clanCount);    uint256[] memory timestamp = new <mask0>;    uint256 clanIndex = 0;    for (uint256 i = 0; i < entityList.length; i++) {      if (clanStructs[entityList[i]].clanId == clanId) {        addr[clanIndex] = entityList[i];        timestamp[clanIndex] = clanStructs[entityList[i]].updateClanTimestamp;        clanIndex++;      }    }    return (addr, timestamp);  }"