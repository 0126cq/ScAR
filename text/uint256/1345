"function _exitPool(address _pool, uint256 _percent) internal returns (uint256 _amount0, uint256 _amount1)\t{\t\tif (_percent == 0) return (0, 0);\t\taddress[] memory _tokens = BPool(_pool).getFinalTokens();\t\t_amount0 = Transfers._getBalance(_tokens[0]);\t\t_amount1 = Transfers._getBalance(_tokens[1]);\t\tuint256 _poolAmount = Transfers._getBalance(_pool);\t\tuint256 _poolExitAmount = _poolAmount.mul(_percent).div(1e18);\t\tuint256[] memory _minAmountsOut = new <mask0>;\t\t_minAmountsOut[0] = 0;\t\t_minAmountsOut[1] = 0;\t\tBPool(_pool).exitPool(_poolExitAmount, _minAmountsOut);\t\t_amount0 = Transfers._getBalance(_tokens[0]).sub(_amount0);\t\t_amount1 = Transfers._getBalance(_tokens[1]).sub(_amount1);\t\treturn (_amount0, _amount1);\t}"