"function _getAssetPrices(bool useMax)        internal        returns (uint256[] memory assetPrices)    {        assetPrices = new <mask0>;        IMinMaxOracle oracle = IMinMaxOracle(priceProvider);        for (uint256 i = 0; i < allAssets.length; i++) {            string memory symbol = Helpers.getSymbol(allAssets[i]);            if (useMax) {                assetPrices[i] = oracle.priceMax(symbol).scaleBy(int8(18 - 8));            } else {                assetPrices[i] = oracle.priceMin(symbol).scaleBy(int8(18 - 8));            }        }    }"