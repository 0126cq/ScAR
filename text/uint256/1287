"function mint(address to, uint32 count) internal {        if (count > 1) {            uint256[] memory ids = new <mask0>;            uint256[] memory amounts = new uint256[](uint256(count));            for (uint32 i = 0; i < count; i++) {                ids[i] = totalSupply + i;                amounts[i] = 1;            }            _mintBatch(to, ids, amounts, \"\");        } else {            _mint(to, totalSupply, 1, \"\");        }        totalSupply += count;    }"