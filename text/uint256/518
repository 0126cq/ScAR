"function getValidSlotsForReroll(Attributes memory attributes)    internal    pure    returns (uint256[] memory)  {    bool[] memory validSlots = new bool[](8);    uint256 numValidSlots = 0;    if (attributes.accessories != 99) {      validSlots[uint256(Slot.accessories)] = true;      numValidSlots++;    }    if (attributes.beard != 99) {      validSlots[uint256(Slot.beard)] = true;      numValidSlots++;    }    if (attributes.eyes != 99) {      validSlots[uint256(Slot.eyes)] = true;      numValidSlots++;    }    if (attributes.hat != 99) {      validSlots[uint256(Slot.hat)] = true;      numValidSlots++;    }    if (attributes.lips != 99) {      validSlots[uint256(Slot.lips)] = true;      numValidSlots++;    }    if (attributes.neck != 99) {      validSlots[uint256(Slot.neck)] = true;      numValidSlots++;    }    if (attributes.smoke != 99) {      validSlots[uint256(Slot.smoke)] = true;      numValidSlots++;    }    uint256[] memory valid = new <mask0>;    uint256 counter = 0;    for (uint256 i = 0; i < validSlots.length; i++) {      if (validSlots[i]) {        valid[counter] = i;        counter++;      }    }    return valid;  }"