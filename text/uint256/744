"function updateSubContractState(address _token, uint256 _cardId, uint256 _tokenId, bytes calldata state) internal {    if (state.length == 0) return;    (,,,,,,,address subContract) = IRainiCard(_token).cards(_cardId);    uint256[] memory ids = new <mask0>;    bytes[] memory states = new bytes[](1);    ids[0] = _tokenId;    states[0] = state;    if (subContract != address(0)) {      IRainiCustomNFT(subContract).setTokenStates(ids, states);    }  }"