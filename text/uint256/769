"function forceWithdrawal(uint256 amount, uint256 l2Sender) external whenOpen {        emit LogForceWithdrawal(msg.sender, amount, l2Sender);        uint256[] memory payload = new <mask0>;        payload[0] = l2Sender;        payload[1] = uint256(uint160(msg.sender));        (payload[2], payload[3]) = toSplitUint(amount);        StarkNetLike(starkNet).sendMessageToL2(l2DaiBridge, FORCE_WITHDRAW, payload);    }"