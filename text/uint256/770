"function startDepositCancellation(        uint256 amount,        uint256 l2Recipient,        uint256 nonce    ) external {        emit LogStartDepositCancellation(l2Recipient, amount, nonce);        uint256[] memory payload = new <mask0>;        payload[0] = l2Recipient;        (payload[1], payload[2]) = toSplitUint(amount);        payload[3] = uint256(uint160(msg.sender));        StarkNetLike(starkNet).startL1ToL2MessageCancellation(l2DaiBridge, DEPOSIT, payload, nonce);    }"