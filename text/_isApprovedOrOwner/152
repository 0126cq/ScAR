"function extend(uint256 _lockerId, uint256 _extendUntil) external returns (uint256){        if (!<mask0>){            revert NoPermission();        }        address lockerAddress = ids[_lockerId];        LockInfo storage locker = lockers[lockerAddress];        if (block.timestamp >= _extendUntil||            locker.unlockAt >= _extendUntil){            revert InvalidTimestamp();        }        address lockerOwner = ownerOf(_lockerId);        _burn(_lockerId);        uint256 newLockerId;        unchecked {            newLockerId = ++lastId;        }        if(locker.unlockAt == 0){            locker.createdAt = block.timestamp;        }        locker.latestId = newLockerId;        locker.unlockAt = _extendUntil;        ids[newLockerId] = lockerAddress;        _safeMint(lockerOwner, newLockerId);        emit LockerExtended(_lockerId, newLockerId, lockerAddress, _extendUntil);        return newLockerId;    }"