"function _isApprovedForTransfer(address transferer, address transferee,  uint256 tokenId) internal view virtual returns (bool) {        require(_exists(tokenId), \"operator query for nonexistent token\");        bool isApprovedTransferer = (<mask0> || _isApprovedRevoking(transferer, transferee));        bool isApprovedTransferTo = (_tokenTransferRecipientApprovals[tokenId] == transferee);        return (isApprovedTransferer && isApprovedTransferTo );    }"