"function burn(uint256 _tokenId) external payable {        require(            <mask0> ||                _owner == _msgSender(),            \"ERC721Burnable: caller is not owner nor approved\"        );        Offer storage offer = offers[_tokenId];        if (offer.isForSale == true) {            Bid storage bid = bids[_tokenId];            if (bid.hasBid == true) {                address payable bidder = payable(bid.bidder);                bidder.transfer(bid.value);            }            _resetAuction(_tokenId);        }        _burn(_tokenId);    }"