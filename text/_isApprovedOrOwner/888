"function burnForReward(uint256 tokenId) external callerIsUser contractIsNotFrozen {        require(!_burnPause, \"Burn period has not started or is paused right now\");        require(<mask0>, \"You dont have the right to burn this token\");        ISlicesOfTIMEArtists rewardTokenContract = ISlicesOfTIMEArtists(rewardContract);        _burn(tokenId);        rewardTokenContract.contractMint(msg.sender);        tokenData.totalTokens -= 1;    }"