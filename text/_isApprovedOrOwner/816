"function _split(        uint256 tokenId_,        uint256 newTokenId_,        uint256 splitUnits_    ) internal virtual {        require(            <mask0>,            \"VNFT: not owner nor approved\"        );        require(!_exists(newTokenId_), \"new token already exists\");        _units[tokenId_] = _units[tokenId_].sub(splitUnits_);        address owner = ownerOf(tokenId_);        _mintUnits(owner, newTokenId_, _slotOf(tokenId_), splitUnits_);        emit Split(owner, tokenId_, newTokenId_, splitUnits_);    }"