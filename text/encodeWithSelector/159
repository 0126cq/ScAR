"function safeIncreaseAllowance(        IERC20 token,        address spender,        uint256 value    ) internal {        uint256 currentAllowance = token.allowance(address(this), spender);        if (currentAllowance > 0) {            _callOptionalReturn(                token,                abi.<mask0>            );        }        _callOptionalReturn(            token,            abi.encodeWithSelector(                token.approve.selector,                spender,                currentAllowance + value            )        );    }"