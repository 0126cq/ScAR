"function safeTransferFrom(address erc20TokenAddress, address from, address to, uint256 value) internal {        if(value == 0) {            return;        }        if(erc20TokenAddress == address(0)) {            to.submit(value, \"\");            return;        }        bytes memory returnData = erc20TokenAddress.submit(0, abi.<mask0>);        require(returnData.length == 0 || abi.decode(returnData, (bool)), 'TRANSFERFROM_FAILED');    }"