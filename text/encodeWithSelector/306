"function safeApprove(        IERC20 token,        address spender,        uint value    ) internal {        require(            (value == 0) || (token.allowance(address(this), spender) == 0),            \"SafeERC20: approve from non-zero to non-zero allowance\"        );        _callOptionalReturn(            token,            abi.<mask0>        );    }"