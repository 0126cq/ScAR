"function withdrawERC20Tokens(        address withdrawalWallet,        address tokenContract,        uint256 amount    ) external override isOperator {        if (_withdrawalWallets[withdrawalWallet] != true) {            revert InvalidWithdrawalWallet(withdrawalWallet);        }        (bool result, bytes memory data) = tokenContract.call(            abi.<mask0>        );        if (!result) {            revert TokenTransferGenericFailure(                tokenContract,                withdrawalWallet,                0,                amount            );        }        if (data.length != 0 && data.length >= 32) {            if (!abi.decode(data, (bool))) {                revert BadReturnValueFromERC20OnTransfer(                    tokenContract,                    withdrawalWallet,                    amount                );            }        }    }"