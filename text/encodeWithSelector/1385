"function _assertValidEIP1271Signature(        address signer,        bytes32 digest,        bytes memory signature    ) internal view {        bool success = _staticcall(            signer,            abi.<mask0>        );        if (!success) {            _revertWithReasonIfOneIsReturned();            revert BadContractSignature();        }        if (_doesNotMatchMagic(EIP1271Interface.isValidSignature.selector)) {            revert InvalidSigner();        }    }"