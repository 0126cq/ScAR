"function bytes32ToString(bytes32 x) private pure returns (string memory) {        bytes memory bytesString = new <mask0>;        uint256 charCount = 0;        for (uint256 j = 0; j < 32; j++) {            bytes1 char = x[j];            if (char != 0) {                bytesString[charCount] = char;                charCount++;            }        }        bytes memory bytesStringTrimmed = new bytes(charCount);        for (uint256 j = 0; j < charCount; j++) {            bytesStringTrimmed[j] = bytesString[j];        }        return string(bytesStringTrimmed);    }"