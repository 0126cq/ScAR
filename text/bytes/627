"function toString(bytes memory data) internal pure returns(string memory) {        bytes memory str = new <mask0>;        str[0] = \"0\";        str[1] = \"x\";        for (uint256 i = 0; i < data.length; i++) {            str[2+i*2] = ALPHABET[uint256(uint8(data[i] >> 4))];            str[3+i*2] = ALPHABET[uint256(uint8(data[i] & 0x0f))];        }        return string(str);    }"