"function uint2str(uint i) internal pure returns (string memory) {        if (i == 0) {            return \"0\";        }        uint j = i;        uint _i = i;        uint len;        while (j != 0) {            len++;            j /= 10;        }        bytes memory bstr = new <mask0>;        uint k = len.sub(1);        while (_i != 0) {            bstr[k--] = byte(uint8(48 + _i % 10));            _i /= 10;        }        return string(bstr);    }"