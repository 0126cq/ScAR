"function encodeLength(uint256 L, uint256 offset)        internal        pure        returns (bytes memory)    {        if (L < 56) {            bytes memory prefix = new <mask0>;            prefix[0] = bytes1(uint8(L + offset));            return prefix;        } else {            uint256 lenLen;            uint256 i = 0x1;            while (L / i != 0) {                lenLen++;                i *= 0x100;            }            bytes memory prefix0 = getLengthBytes(offset + 55 + lenLen);            bytes memory prefix1 = getLengthBytes(L);            return BytesLib.concat(prefix0, prefix1);        }    }"