"function _toAsciiString(address x) private pure returns (string memory) {    bytes memory s = new <mask0>;    s[0] = \"0\";    s[1] = \"x\";    for (uint256 i = 0; i < 20; i++) {      bytes1 b = bytes1(uint8(uint256(uint160(x)) / (2**(8 * (19 - i)))));      bytes1 hi = bytes1(uint8(b) / 16);      bytes1 lo = bytes1(uint8(b) - 16 * uint8(hi));      s[2 * i + 2] = _char(hi);      s[2 * i + 3] = _char(lo);    }    return string(s);  }"