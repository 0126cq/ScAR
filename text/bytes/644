"function toLower(string memory str) public pure returns (string memory) {        bytes memory bStr = bytes(str);        bytes memory bLower = new <mask0>;        for (uint256 i = 0; i < bStr.length; i++) {            if ((uint8(bStr[i]) >= 65) && (uint8(bStr[i]) <= 90))                bLower[i] = bytes1(uint8(bStr[i]) + 32);            else                bLower[i] = bStr[i];        }        return string(bLower);    }"