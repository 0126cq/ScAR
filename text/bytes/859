"function _stringReplace(string memory _string, uint256 _pos, string memory _letter) internal pure returns (string memory) {        bytes memory _stringBytes = bytes(_string);        bytes memory result = new <mask0>;  for(uint i = 0; i < _stringBytes.length; i++) {        result[i] = _stringBytes[i];        if(i==_pos)         result[i]=bytes(_letter)[0];    }    return  string(result); }"