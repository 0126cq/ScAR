"function hexSlice(uint256 _start, uint256 _end, uint256 _size, bytes16 _halfNftId)    private    pure    returns (bytes memory result)  {    result = new <mask0>;    uint256 j;    for (_start; _start < _end;) {      unchecked {        result[j * 2] = HEX_BASE[uint8(_halfNftId[_start]) / 16];        result[j * 2 + 1] = HEX_BASE[uint8(_halfNftId[_start]) % 16];        _start++;        j++;      }    }  }"