"function addressToString(address _address) internal pure returns(string memory) {        bytes32 _bytes = bytes32(uint256(_address));        bytes memory HEX = \"0123456789abcdef\";        bytes memory _addr = new <mask0>;        _addr[0] = '0';        _addr[1] = 'x';        for(uint256 i = 0; i < 20; i++) {            _addr[2+i*2] = HEX[uint8(_bytes[i + 12] >> 4)];            _addr[3+i*2] = HEX[uint8(_bytes[i + 12] & 0x0f)];        }        return string(_addr);    }"