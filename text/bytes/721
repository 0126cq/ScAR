"function decode(string memory _str) internal pure returns (string memory) {        require( (bytes(_str).length % 4) == 0, \"Length not multiple of 4\");        bytes memory _bs = bytes(_str);        uint i = 0;        uint j = 0;        uint dec_length = (_bs.length/4) * 3;        bytes memory dec = new bytes(dec_length);        for (; i< _bs.length; i+=4 ) {            (dec[j], dec[j+1], dec[j+2]) = dencode4(                bytes1(_bs[i]),                bytes1(_bs[i+1]),                bytes1(_bs[i+2]),                bytes1(_bs[i+3])            );            j += 3;        }        while (dec[--j]==0)        {}        bytes memory res = new <mask0>;        for (i=0; i<=j;i++)            res[i] = dec[i];        return string(res);    }"