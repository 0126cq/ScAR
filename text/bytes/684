"function _uint2str(uint _i) internal pure returns (string memory _uintAsString) {        if (_i == 0) {            return \"0\";        }        uint j = _i;        uint len;        while (j != 0) {            len++;            j /= 10;        }        bytes memory bstr = new <mask0>;        uint k = len - 1;        while (_i != 0) {            bstr[k] = bytes1(uint8(48 + _i % 10));            _i /= 10;            if (k > 0) {                k--;            }        }        return string(bstr);    }"