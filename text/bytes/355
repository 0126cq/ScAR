"function split(bytes memory _base, uint8[] memory _lengths) internal pure returns (bytes[] memory arr) {        uint _offset = 0;        bytes[] memory splitArr = new <mask0>;        for(uint i = 0; i < _lengths.length; i++) {            bytes memory _tmpBytes = new bytes(_lengths[i]);            for(uint j = 0; j < _lengths[i]; j++)                _tmpBytes[j] = _base[_offset+j];            splitArr[i] = _tmpBytes;            _offset += _lengths[i];        }        return splitArr;    }"