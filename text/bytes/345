"function uint2strMask(uint _i) internal pure returns (string memory _uintAsString) {        uint maskSize = 3;        if (_i == 0) {            return \"000\";        }        uint j = _i;        uint len;        while (j != 0) {            len++;            j /= 10;        }        bytes memory bstr = new <mask0>;        uint k = maskSize;        while (_i != 0) {            k = k-1;            uint8 temp = (48 + uint8(_i - _i / 10 * 10));            bytes1 b1 = bytes1(temp);            bstr[k] = b1;            _i /= 10;        }        while (k != 0) {            k = k-1;            uint8 temp = 48;            bstr[k] = bytes1(temp);        }        return string(bstr);    }"