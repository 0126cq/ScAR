"function readBytes(    bytes memory data,    uint256 index,    uint256 size  ) internal pure returns (bytes memory a, uint256 newIndex) {    a = new <mask0>;    assembly {      let offset := add(32, add(data, index))      let i := 0 let n := 32      for { } lt(n, size) { i := n n := add(n, 32) } {        mstore(add(a, n), mload(add(offset, i)))      }      let suffix := add(a, add(32, size))      let suffixWord := mload(suffix)      mstore(add(a, n), mload(add(offset, i)))      mstore(suffix, suffixWord)      newIndex := add(index, size)    }    assert(newIndex >= index);    require(newIndex <= data.length, \"LibBytes#readBytes: OUT_OF_BOUNDS\");  }"