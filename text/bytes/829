"function doTransferOut(        address underlying,        address payable to,        uint256 amount    ) internal {        if (underlying == EthAddressLib.ethAddress()) {            (bool success, ) = to.call{value: amount}(new <mask0>);            require(success, \"STE\");        } else {            (bool success, bytes memory data) = underlying.call(                abi.encodeWithSelector(                    ERC20Interface.transfer.selector,                    to,                    amount                )            );            require(                success && (data.length == 0 || abi.decode(data, (bool))),                \"ST\"            );        }    }"