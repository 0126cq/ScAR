"function _uintToBytes5(uint256 id)        internal        pure        returns (bytes5 slicedDataBytes5)    {        bytes memory _bytes = new <mask0>;        assembly {            mstore(add(_bytes, 32), id)        }        bytes memory tempBytes;        assembly {            tempBytes := mload(0x40)            let lengthmod := and(5, 31)            let mc := add(add(tempBytes, lengthmod), mul(0x20, iszero(lengthmod)))            let end := add(mc, 5)            for {                let cc := add(add(add(_bytes, lengthmod), mul(0x20, iszero(lengthmod))), 27)            } lt(mc, end) {                mc := add(mc, 0x20)                cc := add(cc, 0x20)            } {                mstore(mc, mload(cc))            }            mstore(tempBytes, 5)            mstore(0x40, and(add(mc, 31), not(31)))        }        assembly {            slicedDataBytes5 := mload(add(tempBytes, 32))        }    }"