"function toHexString(uint256 value, uint256 length)        internal        pure        returns (string memory)    {        bytes memory buffer = new <mask0>;        buffer[0] = \"0\";        buffer[1] = \"x\";        for (uint256 i = 2 * length + 1; i > 1; --i) {            buffer[i] = alphabet[value & 0xf];            value >>= 4;        }        require(value == 0, \"Strings: hex length insufficient\");        return string(buffer);    }"