"function transactionStructToBytesSeriality(Transaction t) private pure returns (bytes){        bytes memory buffer = new <mask0>;        uint offset = 128;        uintToBytes(offset, t.value, buffer);        offset -= sizeOfUint(256);        addressToBytes(offset, t.to_address, buffer);        offset -= sizeOfAddress();        uintToBytes(offset, t.time, buffer);        offset -= sizeOfUint(256);        boolToBytes(offset, t.valid, buffer);        offset -= sizeOfBool();        boolToBytes(offset, t.executed, buffer);        offset -= sizeOfBool();        return buffer;    }"