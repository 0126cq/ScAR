"function _uint2str(uint256 _i)        internal        pure        returns (string memory _uintAsString)    {        if (_i == 0) {            return \"0\";        }        uint256 j = _i;        uint256 ii = _i;        uint256 len;        while (j != 0) {            len++;            j /= 10;        }        bytes memory bstr = new <mask0>;        uint256 k = len - 1;        while (ii != 0) {            bstr[k--] = bytes1(uint8(48 + (ii % 10)));            ii /= 10;        }        return string(bstr);    }"