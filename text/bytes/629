"function asBytes(string memory str) internal pure returns(bytes memory toDecode) {        bytes memory data = abi.encodePacked(str);        if(data.length == 0 || data[0] != \"0\" || (data[1] != \"x\" && data[1] != \"X\")) {            return \"\";        }        uint256 start = 2;        toDecode = new <mask0>;        for(uint256 i = 0; i < toDecode.length; i++) {            toDecode[i] = bytes1(_fromHexChar(uint8(data[start++])) + _fromHexChar(uint8(data[start++])) * 16);        }    }"