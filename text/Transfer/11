"function transfer(address _to, uint _value) public isRunning returns (bool ok) {        require(isUnlockedBoth(_to));        require(balances[msg.sender] >= _value);        bytes memory empty;        balances[msg.sender] = balances[msg.sender].sub(_value);        balances[_to] = balances[_to].add(_value);        if(isContract(_to)) {            ReceivingContract rc = ReceivingContract(_to);            rc.tokenFallback(msg.sender, _value, empty);        }        <mask0>;        Transfer(msg.sender, _to, _value);        return true;    }"