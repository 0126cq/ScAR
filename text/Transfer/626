"function transferFrom(address src, address dst, uint wad)        public        stoppable        returns (bool)    {        if (src != msg.sender && !_trusted[src][msg.sender]) {            _approvals[src][msg.sender] = sub(_approvals[src][msg.sender], wad);        }        _balances[src] = sub(_balances[src], wad);        _balances[dst] = add(_balances[dst], wad);        <mask0>;        return true;    }"