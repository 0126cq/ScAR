"function transfer(address _to, uint256 _value) public validAddress(_to) transfersAllowed returns (bool success){        require(balanceOf[msg.sender] >= _value && balanceOf[_to] + _value > balanceOf[_to]);         balanceOf[msg.sender] = safeSub(balanceOf[msg.sender], _value);        balanceOf[_to] = safeAdd(balanceOf[_to], _value);        <mask0>;        return true;    }"