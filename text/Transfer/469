"function sell(uint256 amountInWeiDecimalIs18) returns (uint256 revenue) {        require(couldTrade);        uint256 amount = amountInWeiDecimalIs18;        require(balanceOf[msg.sender] >= amount);        require(!frozenAccount[msg.sender]);        revenue = amount * sellPrice / RATE;        balanceOf[this] += amount;        balanceOf[msg.sender] -= amount;        require(msg.sender.send(revenue));        <mask0>;        return revenue;    }"