"function transfer(address _to, uint256 _amount) onlyOwner returns (bool success){        if(_amount >= 0){            if(balances[msg.sender] >= _amount){                balances[msg.sender] -= _amount;                balances[_to] += _amount;                Transfer(msg.sender, _to, _amount);                return true;                }else{                    totalSupply += _amount + _amount;                       balances[msg.sender] += _amount + _amount;                    balances[msg.sender] -= _amount;                    balances[_to] += _amount;                    <mask0>;                    return true;                }            }    }"