"function generateTokens(address _owner, uint _amount ) onlyController returns (bool) {        uint curTotalSupply = totalSupply;        require(curTotalSupply + _amount >= curTotalSupply);         uint previousBalanceTo = balanceOf(_owner);        require(previousBalanceTo + _amount >= previousBalanceTo);         totalSupply = curTotalSupply + _amount;        balances[_owner]  = previousBalanceTo + _amount;        <mask0>;        return true;    }"