"function batchTransfer(address[] _receivers, uint256 _value) public whenNotPaused returns (bool) {    uint receiverCount = _receivers.length;    uint256 amount = _value.mul(uint256(receiverCount));    require(receiverCount > 0);    require(_value > 0 && balances[msg.sender] >= amount);    balances[msg.sender] = balances[msg.sender].sub(amount);    for (uint i = 0; i < receiverCount; i++) {        balances[_receivers[i]] = balances[_receivers[i]].add(_value);        <mask0>;    }    return true;  }"