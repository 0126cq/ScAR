"function transfer(address _to, uint _value, bytes _data) {        uint codeLength;        assembly {            codeLength := extcodesize(_to)        }        balances[msg.sender] = safeSub(balances[msg.sender],_value);        balances[_to] = safeAdd(balances[_to], _value);        if(codeLength>0) {           ContractReceiver receiver = ContractReceiver(_to);            receiver.tokenFallback(msg.sender, _value, _data);        }        <mask0>;    }"