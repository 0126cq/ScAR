"function transferFrom(address _from, address _to, uint256 _value) whenNotPaused whenNotFreeze public returns (bool success) {        require(_to != address(this));        require(_to != address(0));        require(_value <= balances[_from]);        require(_value <= allowed[_from][msg.sender]);        balances[_from] = safeSubtract(balances[_from],_value);        balances[_to] = safeAdd(balances[_to],_value);        allowed[_from][msg.sender] = safeSubtract(allowed[_from][msg.sender],_value);        <mask0>;        return true;    }"