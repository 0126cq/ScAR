"function transfer(address _to, uint256 _value) public returns (bool) {    require(_value > 0);    require(_to != address(0));    address curOwner = msg.sender;    if(agent != address(0) && agent == msg.sender){    \tcurOwner = owner;    }    require(_value <= balances[curOwner]);    balances[curOwner] = balances[curOwner].sub(_value);    balances[_to] = balances[_to].add(_value);    <mask0>;    return true;  }"