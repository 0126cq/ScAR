"function transferFrom(address _from, address _to, uint256 _value) returns (bool success) {    if (balances[_from] >= _value && allowed[_from][msg.sender] >= _value && balances[_to] + _value > balances[_to]) {      balances[_to] += _value;      balances[_from] -= _value;      allowed[_from][msg.sender] -= _value;      <mask0>;      return true;    } else { return false; }  }"