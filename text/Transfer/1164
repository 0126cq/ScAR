"function transferFrom(address _from, address _to, uint256 _value)        public enabledState onlyPayloadSize(3 * 32)     {        require(balances[_from] >= _value);        require(allowed[_from][msg.sender] >= _value);        balances[_from] = sub( balances[_from], _value );        balances[_to] = add( balances[_to], _value );        allowed[_from][msg.sender] = sub( allowed[_from][msg.sender], _value );        <mask0>;    }"