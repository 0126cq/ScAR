"function mint(address _to, uint256 _amount) public returns (bool) {    require((msg.sender == saleAgent || msg.sender == owner) && !mintingFinished);    totalSupply = totalSupply.add(_amount);    balances[_to] = balances[_to].add(_amount);    initialBalances[_to] = balances[_to];    Mint(_to, _amount);    <mask0>;    return true;  }"