"function burn(uint num) public {        require(num * 1e8 > 0);        require(balances[msg.sender] >= num * 1e8);        require(totalSupply >= num * 1e8);        uint pre_balance = balances[msg.sender];        balances[msg.sender] -= num * 1e8;        totalSupply -= num * 1e8;        Burnt(msg.sender, num * 1e8, totalSupply);        <mask0>;        assert(balances[msg.sender] == pre_balance - num * 1e8);    }"