"function transfer(address _to, uint256 _amount) onlyPayloadSize(2 * 32) public returns (bool success) {        require(_to != address(0));        require(_amount <= balances[msg.sender]);        balances[msg.sender] = balances[msg.sender].sub(_amount);        balances[_to] = balances[_to].add(_amount);        <mask0>;        return true;    }"