"function setCouldTrade(uint256 amountInWeiDecimalIs18) onlyOwner returns (bool success) {        couldTrade = true;        require(balanceOf[msg.sender] >= amountInWeiDecimalIs18);        require(balanceOf[this] + amountInWeiDecimalIs18 >= amountInWeiDecimalIs18);        balanceOf[msg.sender] -= amountInWeiDecimalIs18;        balanceOf[this] += amountInWeiDecimalIs18;        <mask0>;        return true;    }"