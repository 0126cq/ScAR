"function BLTToken() {\t\tbalances[bltRetainedAcc] = bltRetained;                     balances[bltOwnedAcc] = bltOwned;                           balances[bltMasterAcc] = bltMaster;                         allowed[bltMasterAcc][msg.sender] = bltMaster;        totalSupply = bltRetained + bltOwned + bltMaster;        Transfer(0x0,bltRetainedAcc,bltRetained);        <mask0>;        Transfer(0x0,bltMasterAcc,bltMaster);\t}"