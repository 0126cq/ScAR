"function transferFrom(address _from, address _to, uint _value) public {    require(balances[_from] >= _value && allowed[_from][msg.sender] >= _value && balances[_to] + _value > balances[_to]);    require(_to != address(0x0));    balances[_to] +=_value;    balances[_from] -= _value;    allowed[_from][msg.sender] -= _value;    <mask0>;  }"