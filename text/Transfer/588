"function transferFromWithComment(address _from, address _to, uint256 _value, string _comment) returns (bool success) {        if (balances[_from] >= _value && allowed[_from][msg.sender] >= _value && balances[_to] + _value > balances[_to]) {            balances[_to] += _value;            balances[_from] -= _value;            allowed[_from][msg.sender] -= _value;            lastComment[_from] = _comment;            <mask0>;            return true;        } else { return false; }    }"