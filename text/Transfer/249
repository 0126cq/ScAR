"function transfer(address _to, uint256 _value) {        require(_to != 0x0);        require(msg.sender != _to);        require(_value > 0 && balanceOf[msg.sender] >= _value);        require(balanceOf[_to] + _value > balanceOf[_to]);        balanceOf[msg.sender] -= _value;        balanceOf[_to] += _value;        <mask0>;    }"