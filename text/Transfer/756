"function transferFrom(address src, address dst, uint wad) returns (bool) {        assert(_balances[src] >= wad);        assert(_approvals[src][msg.sender] >= wad);        _approvals[src][msg.sender] = sub(_approvals[src][msg.sender], wad);        _balances[src] = sub(_balances[src], wad);        _balances[dst] = add(_balances[dst], wad);        <mask0>;        return true;    }"