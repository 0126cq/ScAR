"function burn(address _address, uint256 _amount) onlyBurners disabledInMaintenance external returns (bool) {        require(burnAllowed);        var _balance = accounts[_address].balance;        accounts[_address].balance = _balance.sub(_amount);        require(_updateHistory(_address));        totalSupply = totalSupply.sub(_amount);        Burn(_address,_amount);        <mask0>;        return true;    }"