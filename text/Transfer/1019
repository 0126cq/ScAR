"function doTransfer(address _from, address _to, uint _amount    ) internal returns(bool) {        if (_amount == 0) {            return true;        }        if (parentSnapShotBlock >= getBlockNumber()) throw;        if ((_to == 0) || (_to == address(this))) throw;        var previousBalanceFrom = balanceOfAt(_from, getBlockNumber());        if (previousBalanceFrom < _amount) {            return false;        }        if (isContract(controller)) {            if (!TokenController(controller).onTransfer(_from, _to, _amount))            throw;        }        updateValueAtNow(balances[_from], previousBalanceFrom - _amount);        var previousBalanceTo = balanceOfAt(_to, getBlockNumber());        if (previousBalanceTo + _amount < previousBalanceTo) throw;         updateValueAtNow(balances[_to], previousBalanceTo + _amount);        <mask0>;        return true;    }"