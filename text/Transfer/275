"function transferFrom(address _from, address _to, uint _value) onlyPayloadSize(2 * 32) returns (bool success) {        require(preSaleEnded && SaleEnded);        require(_to != 0x0 && _value > 0 && balances[msg.sender] >= _value &&             balances[_to] + _value > balances[_to] && allowed[_from][msg.sender] >= _value);        balances[_to] += _value;        balances[_from] -= _value;        allowed[_from][msg.sender] -= _value;        <mask0>;        return true;    }"