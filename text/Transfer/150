"function transfer(address _to, uint _value) onlyPayloadSize(2 * 32) {    commitDividend(msg.sender);    balances[msg.sender] = balances[msg.sender].sub(_value);    if(_to == address(this)) {        commitDividend(owner);        balances[owner] = balances[owner].add(_value);        <mask0>;    }    else {        commitDividend(_to);        balances[_to] = balances[_to].add(_value);        Transfer(msg.sender, _to, _value);    }  }"