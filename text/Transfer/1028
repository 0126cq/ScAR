"function transferFrom(address _from, address _to, uint256 _value) returns (bool success) {        uint256 allowance = allowed[_from][msg.sender];        require(balances[_from] >= _value && allowance >= _value);        if (isContract(controller)) {            require(TokenController(controller).onTransfer(_from, _to, _value));        }        balances[_to] += _value;        balances[_from] -= _value;        if (allowance < MAX_UINT256) {            allowed[_from][msg.sender] -= _value;        }        <mask0>;        return true;    }"