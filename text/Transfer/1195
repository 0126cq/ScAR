"function transferFrom(address _from, address _to, uint256 _value) whenTransferAllowed public returns (bool) {        require(_to != address(0));        require(_value <= balances[_from]);        require(_value <= allowed[_from][msg.sender]);        balances[_from] = balances[_from] - _value;        balances[_to] = balances[_to] + _value;        allowed[_from][msg.sender] = allowed[_from][msg.sender] - _value;        <mask0>;        return true;    }"