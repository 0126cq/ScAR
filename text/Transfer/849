"function transfer(address to, uint value) {        uint codeLength;        bytes memory empty;        assembly {            codeLength := extcodesize(to)        }        balances[msg.sender] = balances[msg.sender].sub(value);        balances[to] = balances[to].add(value);        if(codeLength>0) {            ERC223ReceivingContract receiver = ERC223ReceivingContract(to);            receiver.tokenFallback(msg.sender, value, empty);        }        <mask0>;    }"