"function doTransfer(address _from, address _to, uint _amount    ) internal {           if (_amount == 0) {               <mask0>;                   return;           }           require(parentSnapShotBlock < block.number);           require((_to != 0) && (_to != address(this)));           var previousBalanceFrom = balanceOfAt(_from, block.number);           require(previousBalanceFrom >= _amount);           if (isContract(controller)) {               require(TokenController(controller).onTransfer(_from, _to, _amount));           }           updateValueAtNow(balances[_from], previousBalanceFrom - _amount);           var previousBalanceTo = balanceOfAt(_to, block.number);           require(previousBalanceTo + _amount >= previousBalanceTo);            updateValueAtNow(balances[_to], previousBalanceTo + _amount);           Transfer(_from, _to, _amount);    }"