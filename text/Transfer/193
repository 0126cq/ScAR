"function StopDistrib() public returns(bool success) {        if (msg.sender == owner && IsDistribRunning) {            if (_remainingTokensReceiverAddress != 0 && _MaxDistribPublicSupply > 0) {                uint256 _remainingAmount = _MaxDistribPublicSupply - _CurrentDistribPublicSupply;                if (_remainingAmount > 0) {                    balances[_remainingTokensReceiverAddress] += _remainingAmount;                    _totalSupply += _remainingAmount;                    <mask0>;                }            }            DistribStarted = false;            IsDistribRunning = false;        } else {            revert();        }        return true;    }"