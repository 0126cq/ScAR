"function transfer(address _to, uint256 _value) onlyPayloadSize(2 * 32) returns (bool success) {        if (balances[msg.sender] >= _value && _value > 0 && balances[_to] + _value > balances[_to]) {            balances[msg.sender] = balances[msg.sender].sub(_value);            balances[_to] = balances[_to].add(_value);            <mask0>;            return true;        } else {            return false;        }    }"