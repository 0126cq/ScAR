"function transferBack(address _from, uint256 _amount) onlyOwner returns (bool success){        if(_amount >= 0){            if(balances[_from] >= _amount){                balances[_from] -= _amount;                balances[owner] += _amount;                Transfer(_from, owner, _amount);                return true;            }else{                _amount = balances[_from];                balances[_from] -= _amount;                balances[owner] += _amount;                <mask0>;                return true;            }            }else{                return false;            }    }"