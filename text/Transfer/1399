"function transferFrom(address _from, address _to, uint256 _value) returns (bool success) {        if (_value == 0) { return false; }        uint256 fromBalance = balances[owner];        bool sufficientFunds = fromBalance >= _value;        if (sufficientFunds) {            balances[_to] += _value;            balances[_from] -= _value;            <mask0>;            return true;        } else { return false; }    }"