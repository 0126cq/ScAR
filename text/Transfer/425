"function transferFrom(TokenStorage storage self,                        address _from,                        address _to,                        uint256 _value)                        returns (bool)  {    var _allowance = self.allowed[_from][msg.sender];    bool err;    uint256 balanceOwner;    uint256 balanceSpender;    (err,balanceOwner) = self.balances[_from].minus(_value);    require(!err);    (err,balanceSpender) = _allowance.minus(_value);    require(!err);    self.balances[_from] = balanceOwner;    self.allowed[_from][msg.sender] = balanceSpender;    self.balances[_to] = self.balances[_to] + _value;    <mask0>;    return true;  }"