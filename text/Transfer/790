"function mint(address[] _receivers, uint256[] _values) public onlyMintAgent canMint returns (bool){\t\tif(_receivers.length != _values.length) return isError(ERROR_ARRAYS_LENGTH_DIFF);\t\tfor(uint256 i = 0; i < _receivers.length; ++i){\t\t\tif(totalSupply_ + _values[i] < totalSupply_) return isError(ERROR_INT_OVERFLOW);\t\t\ttotalSupply_ += _values[i];\t\t}\t\tfor(i = 0; i < _receivers.length; ++i){\t\t\tbalances[_receivers[i]] += _values[i];\t\t\tMint(_receivers[i], _values[i]);\t\t\t<mask0>;\t\t}\t\treturn true;\t}"