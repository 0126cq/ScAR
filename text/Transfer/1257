"function transferFrom(address from, address to, uint value) returns (bool) {        assert(_balances[from] >= value);        assert(_approvals[from][msg.sender] >= value);        _approvals[from][msg.sender] = sub(_approvals[from][msg.sender], value);        _balances[from] = sub(_balances[from], value);        _balances[to] = add(_balances[to], value);        <mask0>;        return true;    }"