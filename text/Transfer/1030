"function destroyTokens(address _owner, uint _amount    ) onlyController returns (bool) {        uint curTotalSupply = totalSupply;        require(curTotalSupply >= _amount);        uint previousBalanceFrom = balanceOf(_owner);        require(previousBalanceFrom >= _amount);        totalSupply = curTotalSupply - _amount;        balances[_owner] = previousBalanceFrom - _amount;        <mask0>;        return true;    }"