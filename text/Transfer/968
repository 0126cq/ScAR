"function transfer(address _to, uint256 _value) returns (bool success) {        if(msg.data.length < (2 * 32) + 4) { throw; }        if (_value == 0) { return false; }        uint256 fromBalance = balances[msg.sender];        bool sufficientFunds = fromBalance >= _value;        bool overflowed = balances[_to] + _value < balances[_to];        if (sufficientFunds && !overflowed) {            balances[msg.sender] -= _value;            balances[_to] += _value;            <mask0>;            return true;        } else { return false; }    }"