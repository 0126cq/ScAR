"function generateTokens(address _owner, uint _amount    ) onlyController returns (bool) {        uint curTotalSupply = getValueAt(totalSupplyHistory, getBlockNumber());        if (curTotalSupply + _amount < curTotalSupply) throw;         updateValueAtNow(totalSupplyHistory, curTotalSupply + _amount);        var previousBalanceTo = balanceOf(_owner);        if (previousBalanceTo + _amount < previousBalanceTo) throw;         updateValueAtNow(balances[_owner], previousBalanceTo + _amount);        <mask0>;        return true;    }"