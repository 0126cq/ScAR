"function buyTokens(address contributor) payable {        uint256 amount = calcAmount();        require(contributor != 0x0) ;        require(minimal_token_sell < amount);        require((token.totalSupply() + amount) <= maximumCap);        require(validPurchase());        token.mint(contributor, amount);        TokenPurchase(0x0, contributor, msg.value, amount);        <mask0>;        wallet.transfer(msg.value);    }"