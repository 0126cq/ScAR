"function _burn(address account, uint256 amount) internal virtual {        require(account != address(0), \"EXP: burn from the zero address\");        _beforeTokenTransfer(account, address(0), amount);        uint256 accountBalance = _balances[account];        require(accountBalance >= amount, \"EXP: burn amount exceeds balance\");        unchecked {            _balances[account] = accountBalance - amount;        }        _totalSupply -= amount;        _totalBurned += amount;        emit Transfer(account, address(0), amount);        <mask0>;    }"