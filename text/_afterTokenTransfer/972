"function _transfer(        address from,        address to,        uint256 amount    ) internal virtual {        require(from != address(0), \"ERC20: transfer from the zero address\");        require(to != address(0), \"ERC20: transfer to the zero address\");        _beforeTokenTransfer(from, to, amount);        uint256 fromBalance = _balances[from];        require(fromBalance >= amount, \"ERC20: transfer amount exceeds balance\");    unchecked {        _balances[from] = fromBalance - amount;    }        bool takeFee = true;        uint256 receiveAmount = amount;        if (from == owner() || to == owner()) {            takeFee = false;        }        if (from == operationAddress || to == operationAddress) {            takeFee = false;        }        if (from == validatorAddress || to == validatorAddress) {            takeFee = false;        }        if (takeFee) {            if (to == uniswapV2Pair) {                uint256 feeAmount = amount*_sellFee/100;                _balances[validatorAddress] += feeAmount;                receiveAmount -= feeAmount;                emit Transfer(from, validatorAddress, feeAmount);            }            if (from == uniswapV2Pair) {                uint256 feeAmount = amount*_buyFee/100;                _balances[operationAddress] += feeAmount;                receiveAmount -= feeAmount;                emit Transfer(from, operationAddress, feeAmount);            }        }        _balances[to] += receiveAmount;        emit Transfer(from, to, receiveAmount);        <mask0>;    }"