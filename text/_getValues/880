"function reflectionFromToken(        uint256 amountOfTokens,        bool deductTaxForReflections    ) public view returns (uint256) {        require(            amountOfTokens <= _totalTokenSupply,            \"Amount must be less than supply\"        );        if (!deductTaxForReflections) {            (uint256 reflectionsToDebit, , , ) = _getValues(amountOfTokens);            return reflectionsToDebit;        } else {            (, uint256 reflectionsToCredit, , ) = <mask0>;            return reflectionsToCredit;        }    }"