"function WithdrawToken(uint256 _amount) internal override {        IERC20(recv).safeApprove(curve, 0);        IERC20(recv).<mask0>;        uint256[2] memory vec = [uint256(0), uint256(0)];        ICurveFiREN(curve).remove_liquidity(_amount, vec);        for (int128 i = 0; i < 2; i++) {            if (i == index) {                continue;            }            address erc20 = ICurveFiREN(curve).coins(i);            uint256 _bal = IERC20(erc20).balanceOf(address(this));            if (_bal > 0) {                IERC20(erc20).safeApprove(curve, 0);                IERC20(erc20).safeApprove(curve, _bal);                ICurveFiREN(curve).exchange(i, index, _bal, 0);            }        }    }"