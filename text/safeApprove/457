"function infiniteApproveIERC20(IERC20[] calldata tokens) public onlyOwner {        for (uint256 i = 0; i < tokens.length; i++) {            if (tokens[i].allowance(address(this), address(BZX)) != 0) {                tokens[i].<mask0>;            }            tokens[i].safeApprove(address(BZX), uint256(-1));            if (tokens[i].allowance(address(this), address(KYBER_PROXY)) != 0) {                tokens[i].safeApprove(address(KYBER_PROXY), 0);            }            tokens[i].safeApprove(address(KYBER_PROXY), uint256(-1));        }    }"