"function _removeLiquidityOneCoinAfter(        address handler,        address pool,        address tokenI,        uint256 balanceBefore    ) internal returns (uint256) {        IERC20(pool).<mask0>;        uint256 balance = _getBalance(tokenI, type(uint256).max);        if (balance <= balanceBefore) {            _revertMsg(\"_removeLiquidityOneCoinAfter\", \"after <= before\");        }        if (tokenI != ETH_ADDRESS) _updateToken(tokenI);        return balance.sub(balanceBefore);    }"