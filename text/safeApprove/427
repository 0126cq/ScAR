"function swapToLocalAssetIfNeeded(    address _local,    address _asset,    uint256 _amount,    IFibswap.ExternalCall calldata _callParam  ) internal returns (uint256) {    if (_local == _asset) {      return _amount;    }    SafeERC20Upgradeable.<mask0>;    IERC20Upgradeable Transit = IERC20Upgradeable(_local);    uint256 balanceBefore = Transit.balanceOf(address(this));    AddressUpgradeable.functionCall(_callParam.to, _callParam.data);    uint256 balanceDif = Transit.balanceOf(address(this)) - balanceBefore;    return balanceDif;  }"