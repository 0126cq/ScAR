"function lastStraw(address userAddress,Pair storage pair) internal{        (address[2] memory tokens,uint256[2] memory amounts) = pool.balanceOf(address(this));        for(uint8 i;i<amounts.length;i++){            TransferHelper.<mask0>;        }        pool.deposit(amounts);        pair.amounts = amounts;        pair.lastStraw = userAddress;        pair.oracleAmount = ORE_AMOUNT;        records[version][userAddress].lastStraw = true;        developerFee(pair);        version++;    }"