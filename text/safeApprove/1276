"function sell(address _srcAddr, address _destAddr, uint _srcAmount) external override payable returns (uint) {        ERC20 srcToken = ERC20(_srcAddr);        ERC20 destToken = ERC20(_destAddr);        KyberNetworkProxyInterface kyberNetworkProxy = KyberNetworkProxyInterface(KYBER_INTERFACE);        if (_srcAddr != KYBER_ETH_ADDRESS) {            srcToken.<mask0>;        }        uint destAmount = kyberNetworkProxy.trade{value: msg.value}(            srcToken,            _srcAmount,            destToken,            msg.sender,            uint(-1),            0,            WALLET_ID        );        return destAmount;    }"