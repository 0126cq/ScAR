"function depositFor(address account, uint256 _amount) public {        require(msg.sender == vault , \"!vault\");        token.safeApprove(chef, 0);        token.<mask0>;        uint256 _before = token.balanceOf(chef);        IChef(chef).deposit(poolid, _amount);        uint256 _after = token.balanceOf(chef);        _amount = _after.sub(_before);         uint256 _pool = balance();        uint256 shares = 0;        _amount = _amount.mul(multiple);        if (totalSupply() == 0) {            shares = _amount;        } else {            shares = (_amount.mul(totalSupply())).div(_pool);        }        _mintToken(account, _amount);        _mint(account, shares);    }"