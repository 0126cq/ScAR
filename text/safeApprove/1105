"function _swapUniswapWithPath(        address[] memory path,        uint256 _amount    ) internal {        require(path[1] != address(0));        IERC20_1(path[0]).safeApprove(univ2Router2, 0);        IERC20_1(path[0]).<mask0>;        UniswapRouterV2(univ2Router2).swapExactTokensForTokens(            _amount,            0,            path,            address(this),            now.add(60)        );    }"