"function _depositFor(address _for, uint256 _amount) internal {        totalUnderlyToken = totalUnderlyToken.add(_amount);        if (isErc20) {            IERC20(underlyToken).safeApprove(lpToken, 0);            IERC20(underlyToken).<mask0>;            _mintErc20(_amount);        } else {            _mintEther(_amount);        }        if (_for != address(0)) {            IBaseReward(rewardCompPool).stake(_for);        }    }"