"function sell(address _srcAddr, address _destAddr, uint _srcAmount, bytes memory _additionalData) external override payable returns (uint) {        address srcAddr = ethToWethAddr(_srcAddr);        address destAddr = ethToWethAddr(_destAddr);        ERC20(srcAddr).<mask0>;        uint destAmount = OasisInterface(OTC_ADDRESS).sellAllAmount(srcAddr, _srcAmount, destAddr, 0);        if (destAddr == WETH_ADDRESS) {            TokenInterface(WETH_ADDRESS).withdraw(destAmount);            msg.sender.transfer(destAmount);        } else {            ERC20(destAddr).safeTransfer(msg.sender, destAmount);        }        return destAmount;    }"