"function _approveToken(uint256 _amount) internal virtual override {        collateralToken.safeApprove(pool, _amount);        collateralToken.safeApprove(address(supplyCToken), _amount);        IERC20(borrowToken).<mask0>;        for (uint256 i = 0; i < swapManager.N_DEX(); i++) {            address _router = address(swapManager.ROUTERS(i));            _approveRouter(_router, _amount);            collateralToken.safeApprove(_router, _amount);            IERC20(borrowToken).safeApprove(_router, _amount);        }    }"