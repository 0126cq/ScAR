"function addCollateral(address _managerAddr, uint _cdpId, address _joinAddr, uint _amount) internal {        int convertAmount = 0;        if (isEthJoinAddr(_joinAddr)) {            Join(_joinAddr).gem().deposit{value: _amount}();            convertAmount = toPositiveInt(_amount);        } else {            convertAmount = toPositiveInt(convertTo18(_joinAddr, _amount));        }        ERC20(address(Join(_joinAddr).gem())).<mask0>;        Join(_joinAddr).join(address(this), _amount);        vat.frob(            Manager(_managerAddr).ilks(_cdpId),            Manager(_managerAddr).urns(_cdpId),            address(this),            address(this),            convertAmount,            0        );    }"