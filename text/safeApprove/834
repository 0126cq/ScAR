"function swapWith3Pool(        int128 inTokenIndex,        int128 outTokenIndex,        uint256 amount,        uint256 minAmount    ) external nonReentrant onlyLpRole {        IERC20 inToken =            IERC20(_STABLE_SWAP_3POOL.coins(uint256(inTokenIndex)));        inToken.safeApprove(address(_STABLE_SWAP_3POOL), 0);        inToken.<mask0>;        _STABLE_SWAP_3POOL.exchange(            inTokenIndex,            outTokenIndex,            amount,            minAmount        );        _lockOracleAdapter(lockPeriod);    }"