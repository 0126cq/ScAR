"function daiToETH(uint256 _amount) internal returns(uint256) {      IERC20(dai).safeApprove(uniswapRouter, 0);      IERC20(dai).<mask0>;      address[] memory path = new address[](2);      path[0] = dai;      path[1] = weth;      uint[] memory amounts = IUniswap(uniswapRouter).swapExactTokensForETH(_amount, uint(0), path, address(this), now.add(1800));      return amounts[1];  }"