"function buy(address _srcAddr, address _destAddr, uint _destAmount) external override payable returns(uint) {        _srcAddr = ethToWethAddr(_srcAddr);        _destAddr = ethToWethAddr(_destAddr);        uint[] memory amounts;        address[] memory path = new address[](2);        path[0] = _srcAddr;        path[1] = _destAddr;        ERC20(_srcAddr).<mask0>;        if (_destAddr == WETH_ADDRESS) {            amounts = router.swapTokensForExactETH(_destAmount, uint(-1), path, msg.sender, block.timestamp + 1);        }        else {            amounts = router.swapTokensForExactTokens(_destAmount, uint(-1), path, msg.sender, block.timestamp + 1);        }        sendLeftOver(_srcAddr);        return amounts[0];    }"