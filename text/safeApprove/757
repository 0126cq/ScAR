"function _repayToPool(        address _poolTokenAddress,        ERC20 _underlyingToken,        uint256 _amount    ) internal {        _amount = Math.min(            _amount,            ICToken(_poolTokenAddress).borrowBalanceCurrent(address(this))         );        if (_amount > 0) {            if (_poolTokenAddress == cEth) {                IWETH(wEth).withdraw(_amount);                 ICEther(_poolTokenAddress).repayBorrow{value: _amount}();            } else {                _underlyingToken.<mask0>;                if (ICToken(_poolTokenAddress).repayBorrow(_amount) != 0)                    revert RepayOnCompoundFailed();            }        }    }"