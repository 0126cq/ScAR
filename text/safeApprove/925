"function safeApproveMax(        ERC20 token,        address spender,        uint256 amount,        string memory location    ) internal {        uint256 allowance = ERC20(token).allowance(address(this), spender);        if (allowance < amount) {            if (allowance > 0) {                safeApprove(token, spender, 0, location);            }            <mask0>;        }    }"