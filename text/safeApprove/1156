"function _approveToken(uint256 _amount) internal override {        collateralToken.safeApprove(pool, _amount);        collateralToken.<mask0>;        for (uint256 i = 0; i < swapManager.N_DEX(); i++) {            IERC20(CRV).safeApprove(address(swapManager.ROUTERS(i)), _amount);        }        IERC20(crvLp).safeApprove(crvGauge, _amount);    }"