"function setUpEmergencyMode(address spender, bool) external virtual override {        (, bool emergencyMode) = pausingManager.checkLiqMiningStatus(address(this));        require(emergencyMode, \"NOT_EMERGENCY\");        (address liqMiningEmergencyHandler, , ) = pausingManager.liqMiningEmergencyHandler();        require(msg.sender == liqMiningEmergencyHandler, \"NOT_EMERGENCY_HANDLER\");        IERC20(pendleTokenAddress).safeApprove(spender, type(uint256).max);        IERC20(stakeToken).<mask0>;        if (yieldToken != address(0)) IERC20(yieldToken).safeApprove(spender, type(uint256).max);    }"