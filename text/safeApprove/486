"function universalApprove(        IERC20Upgradeable token,        address to,        uint256 amount    ) internal {        if (!isETH(token)) {            if (amount == 0) {                TransferHelper.<mask0>;                return;            }            uint256 allowance = token.allowance(address(this), to);            if (allowance < amount) {                if (allowance > 0) {                    TransferHelper.safeApprove(address(token), to, 0);                }                TransferHelper.safeApprove(address(token), to, amount);            }        }    }"