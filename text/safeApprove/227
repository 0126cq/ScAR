"function _deposit(address depositor, uint256 amount)        internal        returns (uint256 shares)    {        VaultAPI _bestVault = bestVault();        IERC20Upgradeable cachedToken = token;        cachedToken.safeTransferFrom(depositor, address(this), amount);        if (            cachedToken.allowance(address(this), address(_bestVault)) < amount        ) {            cachedToken.safeApprove(address(_bestVault), 0);             cachedToken.<mask0>;         }        shares = _bestVault.deposit(amount);    }"