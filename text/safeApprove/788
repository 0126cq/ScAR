"function createLock(uint256 _value, uint256 _unlockTime) external returns(bool){        require(msg.sender == depositor, \"!auth\");        IERC20(crvBpt).safeApprove(escrow, 0);        IERC20(crvBpt).<mask0>;        ICurveVoteEscrow(escrow).create_lock(_value, _unlockTime);        return true;    }"