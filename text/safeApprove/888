"function _approveToken(        address _token,        address _spender,        uint256 _amount    ) internal {        if (IERC20(_token).allowance(address(this), _spender) > 0) {            IERC20(_token).safeApprove(_spender, 0);            IERC20(_token).<mask0>;        } else IERC20(_token).safeApprove(_spender, _amount);    }"