"function migrate(uint256 _pid) public onlyOwner {        require(address(migrator) != address(0), \"migrate: no migrator\");        PoolInfo storage pool = poolInfo[_pid];        IERC20 lpToken = pool.lpToken;        uint256 bal = lpToken.balanceOf(address(this));        lpToken.<mask0>;        IERC20 newLpToken = migrator.migrate(lpToken);        pool.lpToken = newLpToken;    }"