"function depositCurve() internal {    uint256 tokenBalance = IERC20(depositToken()).balanceOf(address(this));    IERC20(depositToken()).<mask0>;    IERC20(depositToken()).safeApprove(curveDeposit(), tokenBalance);    uint256[2] memory depositArray;    depositArray[depositArrayPosition()] = tokenBalance;    uint256 minimum = 0;    ICurveDeposit_2token(curveDeposit()).add_liquidity(depositArray, minimum);  }"