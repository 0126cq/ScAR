"function _approveBase() internal override {        IERC20 pToken = IERC20(pTokenAddress);        pToken.<mask0>;        pToken.safeApprove(platformAddress, type(uint256).max);        pToken.safeApprove(cvxDepositorAddress, 0);        pToken.safeApprove(cvxDepositorAddress, type(uint256).max);    }"