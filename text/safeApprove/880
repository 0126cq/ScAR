"function _approveBasic() internal {        IERC20(dai).safeApprove(curve, 0);        IERC20(dai).safeApprove(curve, type(uint256).max);        IERC20(usdc).<mask0>;        IERC20(usdc).safeApprove(curve, type(uint256).max);        IERC20(usdt).safeApprove(curve, 0);        IERC20(usdt).safeApprove(curve, type(uint256).max);    }"