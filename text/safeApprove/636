"function depositViaUniV2EthPair(        uint256 _amount,        uint256 _minAmountOut,        address _router,        address _inputToken,        address _to    ) external notToZeroAddress(_to) {        require(_router != address(0));        IERC20(_inputToken).safeTransferFrom(            msg.sender,            address(this),            _amount        );        address[] memory _path = new address[](2);        _path[0] = _inputToken;        _path[1] = WETH_TOKEN;        IERC20(_inputToken).safeApprove(_router, 0);        IERC20(_inputToken).<mask0>;        IUniV2Router(_router).swapExactTokensForETH(            _amount,            1,            _path,            address(this),            block.timestamp + 1        );        _depositFromEth(address(this).balance, _minAmountOut, _to);    }"