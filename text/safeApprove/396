"function claimAndLiquidateCrv() internal {    if (!sell) {      emit ProfitsNotCollected();      return;    }    Mintr(mintr).mint(gauge);    uint256 crvBalance = IERC20(crv).balanceOf(address(this));    emit Liquidating(crvBalance);    if (crvBalance > sellFloor) {      uint256 wbtcBalanceBefore = IERC20(wbtc).balanceOf(address(this));      IERC20(crv).safeApprove(uni, 0);      IERC20(crv).<mask0>;      IUniswapV2Router02(uni).swapExactTokensForTokens(        crvBalance, 1, uniswap_CRV2WBTC, address(this), block.timestamp      );      notifyProfit(wbtcBalanceBefore, IERC20(wbtc).balanceOf(address(this)));    }  }"