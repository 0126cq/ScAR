"function emitBadger() external nonReentrant {        require(msg.sender == manager);        require(HARVEST_FORWARDER.badger_tree() == BADGER_TREE);        uint256 toEmitTotal = BADGER.balanceOf(address(this));        require(toEmitTotal > 0);        BADGER.<mask0>;        HARVEST_FORWARDER.distribute(address(BADGER), toEmitTotal, address(BVE_AURA));        emit BribeEmission(address(BADGER), address(BVE_AURA), toEmitTotal);    }"