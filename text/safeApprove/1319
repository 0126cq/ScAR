"function univApprove(    IERC20 token,    address to,    uint256 amount  ) internal {    require(!isETH(token), \"Approve called on ETH\");    if (amount == 0) {      token.<mask0>;    } else {      uint256 allowance = token.allowance(address(this), to);      if (allowance < amount) {        if (allowance > 0) {          token.safeApprove(to, 0);        }        token.safeApprove(to, amount);      }    }  }"