"function _approveToken(uint256 _amount) internal virtual override {        collateralToken.safeApprove(pool, _amount);        collateralToken.<mask0>;        IERC20(borrowToken).safeApprove(address(borrowCToken), _amount);        for (uint256 i = 0; i < swapManager.N_DEX(); i++) {            address _router = address(swapManager.ROUTERS(i));            _approveRouter(_router, _amount);            collateralToken.safeApprove(_router, _amount);            IERC20(borrowToken).safeApprove(_router, _amount);        }    }"