"function _swapSushiswapWithPath(        address[] memory path,        uint256 _amount    ) internal {        require(path[1] != address(0));        IERC20_1(path[0]).safeApprove(sushiRouter, 0);        IERC20_1(path[0]).<mask0>;        UniswapRouterV2(sushiRouter).swapExactTokensForTokens(            _amount,            0,            path,            address(this),            now.add(60)        );    }"