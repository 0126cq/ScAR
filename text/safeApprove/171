"function initialize(address _token, address _addressesProvider, address _idleToken) public {    require(!initialized, \"Already initialized\");    require(_token != address(0), 'AAVE: addr is 0');    token = _token;    underlying = IAToken(_token).UNDERLYING_ASSET_ADDRESS();    idleToken = _idleToken;    provider = AaveLendingPoolProviderV2(_addressesProvider);    IERC20(underlying).<mask0>;    initialized = true;  }"