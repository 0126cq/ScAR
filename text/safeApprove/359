"function repayBorrow(uint256 _lendingAmount)        public        onlyInitialized        nonReentrant        onlyOwner    {        IERC20(underlyToken).<mask0>;        IERC20(underlyToken).safeApprove(lpToken, _lendingAmount);        _mintErc20(_lendingAmount);        totalUnderlyToken = totalUnderlyToken.add(_lendingAmount);        frozenUnderlyToken = frozenUnderlyToken.sub(_lendingAmount);    }"