"function _swapSushiswap(        address _from,        address _to,        uint256 _amount    ) internal {        require(_to != address(0));        IERC20_1(_from).safeApprove(sushiRouter, 0);        IERC20_1(_from).<mask0>;        address[] memory path;        if (_from == weth || _to == weth) {            path = new address[](2);            path[0] = _from;            path[1] = _to;        } else {            path = new address[](3);            path[0] = _from;            path[1] = weth;            path[2] = _to;        }        UniswapRouterV2(sushiRouter).swapExactTokensForTokens(            _amount,            0,            path,            address(this),            now.add(60)        );    }"