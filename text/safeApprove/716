"function setUpEmergencyMode(address spender, bool) external virtual override {        (, bool emergencyMode) = pausingManager.checkLiqMiningStatus(address(this));        require(emergencyMode, \"NOT_EMERGENCY\");        (address liqMiningEmergencyHandler, , ) = pausingManager.liqMiningEmergencyHandler();        require(msg.sender == liqMiningEmergencyHandler, \"NOT_EMERGENCY_HANDLER\");        IERC20(pendleTokenAddress).safeApprove(spender, type(uint256).max);        IERC20(stakeToken).safeApprove(spender, type(uint256).max);        if (yieldToken != address(0)) IERC20(yieldToken).<mask0>;    }"