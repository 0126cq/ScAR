"function _setApprovals(        address _want,        address _crv,        address _cvx,        address _mim,        address _crv3,        address _convexVault,        address _stableSwap2Pool,        address[] memory _routerArray    ) internal {        IERC20(_want).safeApprove(address(_convexVault), type(uint256).max);        for(uint i=0; i<_routerArray.length; i++) {            address _router = _routerArray[i];            IERC20(_crv).<mask0>;            IERC20(_crv).safeApprove(address(_router), type(uint256).max);            IERC20(_cvx).safeApprove(address(_router), 0);            IERC20(_cvx).safeApprove(address(_router), type(uint256).max);        }        IERC20(_mim).safeApprove(address(_stableSwap2Pool), type(uint256).max);        IERC20(_crv3).safeApprove(address(_stableSwap2Pool), type(uint256).max);        IERC20(_want).safeApprove(address(_stableSwap2Pool), type(uint256).max);    }"