"function sell(address _srcAddr, address _destAddr, uint _srcAmount) external payable override returns (uint) {        _srcAddr = ethToWethAddr(_srcAddr);        _destAddr = ethToWethAddr(_destAddr);        uint[] memory amounts;        address[] memory path = new address[](2);        path[0] = _srcAddr;        path[1] = _destAddr;        ERC20(_srcAddr).<mask0>;        if (_destAddr == WETH_ADDRESS) {            amounts = router.swapExactTokensForETH(_srcAmount, 1, path, msg.sender, block.timestamp + 1);        }        else {            amounts = router.swapExactTokensForTokens(_srcAmount, 1, path, msg.sender, block.timestamp + 1);        }        return amounts[amounts.length - 1];    }"