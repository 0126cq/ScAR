"function registerNameCore(uint256 _pID, bytes32 _name)        private    {        if (pIDxName_[_name] != 0)            require(plyrNames_[_pID][_name] == true, \"sorry that names already taken\");        plyr_[_pID].name = _name;        pIDxName_[_name] = _pID;        if (plyrNames_[_pID][_name] == false)        {            plyrNames_[_pID][_name] = true;        }        CompanyShare.deposit.<mask0>;    }"