"function flush(uint256 amount) external {        require(address(strategy) != address(0), \"TrueFiPool: Pool has no strategy set up\");        require(amount <= currencyBalance(), \"TrueFiPool: Insufficient currency balance\");        uint256 expectedMinStrategyValue = strategy.<mask0>.add(withToleratedStrategyLoss(amount));        token.approve(address(strategy), amount);        strategy.deposit(amount);        require(strategy.value() >= expectedMinStrategyValue, \"TrueFiPool: Strategy value expected to be higher\");        emit Flushed(amount);    }"