"function oraclize_query(uint timestamp, string datasource, bytes[] argN, uint gaslimit) oraclizeAPI internal returns (bytes32 id){        uint price = oraclize.getPrice(datasource, gaslimit);        if (price > 1 ether + tx.gasprice*gaslimit) return 0;         bytes memory args = ba2cbor(argN);        return oraclize.queryN_withGasLimit.<mask0>;    }"