"function mintWithEther(        address receiver,        uint256 maxPriceAllowed)        external        payable        nonReentrant        returns (uint256)    {        require (msg.value > 0, \"msg.value == 0\");        uint256 netCollateralAmount;        uint256 interestDepositRemaining;        if (totalSupply() > 0) {            (netCollateralAmount, interestDepositRemaining,) = IBZx(bZxContract).getTotalEscrow(                loanOrderHash,                address(this),                false             );        }        uint256 currentPrice = _tokenPrice(netCollateralAmount, interestDepositRemaining);        if (maxPriceAllowed != 0) {            require(                currentPrice <= maxPriceAllowed,                \"price too high\"            );        }        WETHInterface(wethContract).deposit.<mask0>;        return _mintWithToken(            receiver,            wethContract,            msg.value,            currentPrice        );    }"