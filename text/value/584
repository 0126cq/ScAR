"function transferFrom(address _from, address _to, uint256 _amount, bytes _data, string _custom_fallback)        public        returns (bool success)    {        if (isContract(controller)) {            if (!TokenController(controller).onTransfer(_from, _to, _amount))               throw;        }        require(super.transferFrom(_from, _to, _amount));        if (isContract(_to)) {            ERC223ReceivingContract receiver = ERC223ReceivingContract(_to);            receiver.call.<mask0>;        }        ERC223Transfer(_from, _to, _amount, _data);        return true;    }"