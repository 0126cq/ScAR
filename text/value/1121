"function buyRecipient(address recipient) duringCrowdSale payable {        require(!halted);        uint tokens = safeMul(msg.value, price(block.timestamp));        require(safeAdd(saleTokenSupply,tokens)<=coinAllocation );        balances[recipient] = safeAdd(balances[recipient], tokens);        totalSupply = safeAdd(totalSupply, tokens);        saleTokenSupply = safeAdd(saleTokenSupply, tokens);        amountRaised = safeAdd(amountRaised, msg.value);        if (!founder.call.<mask0>) revert();         Buy(recipient, msg.value, tokens);    }"