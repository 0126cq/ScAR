"function BuyLevel1Ownership(address _seller) payable public { \tif (!isLevel1SetForOwnershipSale[_seller]) revert('Address Not Set For Ownership Sale');\tif (level1OwnewshipSalePrice[_seller] == 0) revert('Price cannot be 0');\tif \t(msg.value == getLevel1OwnershipSalePriceWithFee(_seller))\t{\t\t\tmneAddress.Payment.<mask0>();\t\tif (!payable(_seller).send(getLevel1OwnershipSalePrice(_seller))) revert('Error While Executing Payment.');\t\t\text1.TransferGenesis(_seller, msg.sender);\t\t\t\tisLevel1SetForOwnershipSale[_seller] = false;\t\tlevel1OwnewshipSalePrice[_seller] = 0;\t\t\t\t\temit Level1OwnershipTransfer(_seller, msg.sender);\t\tcountSales++;\t}\telse\t{\t\trevert('Value not correct');\t}}"