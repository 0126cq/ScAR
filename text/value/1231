"function BuyTickets(address _sender, uint256[] memory _max) public payable returns (uint256)    {\t\trequire(msg.sender == address(mne));\t\trequire(_sender == tx.origin);\t\tif (_max[0] == 0) revert('value is 0');\t\tif (playerBlock[_sender] == 0)\t\t{\t\t\t\tuint valueStakeHolder = msg.value * stakeHoldersfee / 100;\t\t\t\t\t\t\t\tticketsSold += _max[0];\t\t\t\t\t\tuint totalEthfee = ethfee * _max[0];\t\t\tuint totalMneFee = mnefee * _max[0];\t\t\tplayerBlock[_sender] = block.number;\t\t\tplayerTickets[_sender] = _max[0];\t\t\t\t\t\tif (msg.value < totalEthfee) revert('Not enough ETH.');\t\t\tmne.Payment.<mask0>();\t\t\ttotalSentToStakeHolders += valueStakeHolder;\t\t\treturn totalMneFee;\t\t}\t\telse \t\t{\t\t\trevert('You must play the tickets first');\t\t}    }"