"function transfer(address _to, uint _value, bytes _data, string _custom_fallback) public returns (bool success) {        require(_to != address(0));        if (isContract(_to)) {            if (balanceOf(msg.sender) < _value) revert();            balances[msg.sender] = balanceOf(msg.sender).sub(_value);            balances[_to] = balanceOf(_to).add(_value);            assert(_to.call.<mask0>);            emit Transfer(msg.sender, _to, _value, _data);            emit Transfer(msg.sender, _to, _value);            return true;        } else {            return transferToAddress(_to, _value, _data);        }    }"