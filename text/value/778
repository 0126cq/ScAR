"function repayBehalfExplicit(address borrower, CEther cEther_) public payable {        uint received = msg.value;        uint borrows = cEther_.borrowBalanceCurrent(borrower);        if (received > borrows) {            cEther_.repayBorrowBehalf.<mask0>;            msg.sender.transfer(received - borrows);        } else {            cEther_.repayBorrowBehalf.value(received)(borrower);        }    }"