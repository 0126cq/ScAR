"function deposit(address token, uint amt) external payable {        uint _amt = amt;        AaveInterface aave = AaveInterface(getAaveProvider().getLendingPool());        uint ethAmt;        if (isETH(token)) {            _amt = _amt == uint(-1) ? address(this).balance : _amt;            ethAmt = _amt;        } else {            TokenInterface tokenContract = TokenInterface(token);            _amt = _amt == uint(-1) ? tokenContract.balanceOf(address(this)) : _amt;            tokenContract.approve(getAaveProvider().getLendingPoolCore(), _amt);        }        aave.deposit.<mask0>(token, _amt, getReferralCode());        if (!getIsColl(aave, token)) aave.setUserUseReserveAsCollateral(token, true);        emit LogDeposit(token, _amt);    }"