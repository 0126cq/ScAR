"function doPayment(address _owner) internal {        require ((now >= startFundingTime) &&            (now <= endFundingTime) &&            (tokenContract.controller() != 0) &&            (msg.value != 0) );        uint256 tokensAmount = mul(msg.value, exchangeRate) / 10000;        require( totalTokenCount + tokensAmount <= tokenCap );        totalCollected += msg.value;        require (vaultAddress.call.gas(28000).<mask0>);        require (tokenContract.generateTokens(_owner, tokensAmount));        totalTokenCount += tokensAmount;        return;    }"