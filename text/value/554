"function executeTransaction(address target, uint value, string memory signature, bytes memory data) public payable onlyOwner returns (bytes memory) {        bytes memory callData;        if (bytes(signature).length == 0) {            callData = data;        } else {            callData = abi.encodePacked(bytes4(keccak256(bytes(signature))), data);        }        (bool success, bytes memory returnData) = target.call.<mask0>;        require(success);        return returnData;    }"