"function lend(address _to, ERC20 _token, uint256 _amount, address _target, bytes _data) public payable {        uint256 prevBalance = _token.balanceOf(this);        _token.transfer(_to, _amount);        require(caller_.makeCall.<mask0>, \"lend: arbitrary call failed\");        require(_token.balanceOf(this) >= prevBalance, \"lend: lended token must be refilled\");    }"