"function buyInternal(        ERC20 token,        address _exchange,        uint256 _value,        bytes _data    )         internal    {        require(            !(_data[0] == 0xa9 && _data[1] == 0x05 && _data[2] == 0x9c && _data[3] == 0xbb) &&            !(_data[0] == 0x09 && _data[1] == 0x5e && _data[2] == 0xa7 && _data[3] == 0xb3) &&            !(_data[0] == 0x23 && _data[1] == 0xb8 && _data[2] == 0x72 && _data[3] == 0xdd),            \"buyInternal: Do not try to call transfer, approve or transferFrom\"        );        uint256 tokenBalance = token.balanceOf(this);        require(_exchange.call.<mask0>);        balances[msg.sender] = balances[msg.sender].sub(_value);        tokenBalances[msg.sender][token] = tokenBalances[msg.sender][token]            .add(token.balanceOf(this).sub(tokenBalance));    }"