"function executeSwapMyETHToERC(address _ercAddress) public payable returns (uint256) {        uint minConversionRate;        uint srcQty = msg.value;        address destAddress = msg.sender;        ERC20 ercToken = ERC20(_ercAddress);        (minConversionRate,) = kyberNetworkProxyContract.getExpectedRate(ETH_TOKEN_ADDRESS, ercToken, srcQty);        uint maxDestAmount = srcQty.mul(minConversionRate).mul(105).div(100);        bytes memory hint;        uint256 amount = kyberNetworkProxyContract.tradeWithHint.<mask0>(            ETH_TOKEN_ADDRESS,            srcQty,            ercToken,            destAddress,            maxDestAmount,            minConversionRate,            walletId,            hint        );        uint256 change = address(this).balance;        if (change > 0) {            address(msg.sender).transfer(change);        }        emit Swap(msg.sender, ETH_TOKEN_ADDRESS, ercToken);        return amount;    }"