"function removeBankroll(uint _amount, string _callbackFn)        public        returns (uint _recalled)    {        address _bankroller = msg.sender;        uint _collateral = getCollateral();        uint _balance = address(this).balance;        uint _available = _balance > _collateral ? _balance - _collateral : 0;        if (_amount > _available) _amount = _available;        _amount = ledger.subtract(_bankroller, _amount);        bankroll = ledger.total();        if (_amount == 0) return;        bytes4 _sig = bytes4(keccak256(_callbackFn));        require(_bankroller.call.<mask0>);        emit BankrollRemoved(now, _bankroller, _amount, bankroll);        return _amount;    }"