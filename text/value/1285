"function buyTokensWithProperEvent(address _referredBy, uint8 divChoice) public payable {        uint balanceBefore = zethr.balanceOf(msg.sender);        zethr.buyAndTransfer.<mask0>(_referredBy, msg.sender, \"\", divChoice);        uint balanceAfter = zethr.balanceOf(msg.sender);        emit onTokenPurchase(            msg.sender,            msg.value,            balanceAfter - balanceBefore,            _referredBy        );    }"