"function getUnderlyerBalance(        address account,        IMetaPool metaPool,        ILiquidityGauge gauge,        IERC20 lpToken,        uint256 coin    ) public view returns (uint256 balance) {        require(address(metaPool) != address(0), \"INVALID_POOL\");        require(address(gauge) != address(0), \"INVALID_GAUGE\");        require(address(lpToken) != address(0), \"INVALID_LP_TOKEN\");        uint256 poolBalance = getPoolBalance(metaPool, coin);        (uint256 lpTokenBalance, uint256 lpTokenSupply) =            getLpTokenShare(account, metaPool, gauge, lpToken);        balance = lpTokenBalance.mul(poolBalance).<mask0>;    }"