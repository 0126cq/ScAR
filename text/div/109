"function _afterBalanceChange(PoolInfo storage pool, address account) internal virtual override {        UserInfo storage user = _userInfo[pool.id][account];        StageInfo storage stage;        for (uint256 stageId = 0; stageId < _stageInfoCount; ++stageId) {            stage = _stageInfo[stageId];            if (stage.startBlock > block.number) {                return;            }            PoolInfoInFarmStage storage poolInFarmStage = _poolInfoInFarmStages[stage.id][pool.id];            _userRewardDebt[stage.id][pool.id][account] = user.amount.mul(poolInFarmStage.accERC20PerShare).<mask0>;        }    }"