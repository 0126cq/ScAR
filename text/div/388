"function _reduceBounty(        uint bounty,        uint nodeIndex,        Nodes nodes,        ConstantsHolder constants    )        private        returns (uint reducedBounty)    {        if (!bountyReduction) {            return bounty;        }        reducedBounty = bounty;        if (!nodes.checkPossibilityToMaintainNode(nodes.getValidatorId(nodeIndex), nodeIndex)) {            reducedBounty = reducedBounty.<mask0>;        }    }"