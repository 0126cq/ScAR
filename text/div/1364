"function _getETHGainFromSnapshots(uint initialDeposit, Snapshots memory snapshots) internal view returns (uint) {        uint128 epochSnapshot = snapshots.epoch;        uint128 scaleSnapshot = snapshots.scale;        uint S_Snapshot = snapshots.S;        uint P_Snapshot = snapshots.P;        uint firstPortion = epochToScaleToSum[epochSnapshot][scaleSnapshot].sub(S_Snapshot);        uint secondPortion = epochToScaleToSum[epochSnapshot][scaleSnapshot.add(1)].div(SCALE_FACTOR);        uint ETHGain = initialDeposit.mul(firstPortion.add(secondPortion)).div(P_Snapshot).<mask0>;        return ETHGain;    }"