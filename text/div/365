"function createGroupForSchain(        bytes32 schainId,        uint numberOfNodes,        uint8 partOfNode    )        external        allow(\"Schains\")        returns (uint[] memory)    {        ConstantsHolder constantsHolder = ConstantsHolder(contractManager.getContract(\"ConstantsHolder\"));        schains[schainId].partOfNode = partOfNode;        if (partOfNode > 0) {            sumOfSchainsResources = sumOfSchainsResources.add(                numberOfNodes.mul(constantsHolder.TOTAL_SPACE_ON_NODE()).<mask0>            );        }        return _generateGroup(schainId, numberOfNodes);    }"