"function create(address _recipient) public payable onlyDuringSale {        require(_recipient != address(0));        require(msg.value > 0);        assert(isDistributed);        uint256 tokens = SaferMath.min256(msg.value.mul(EXCHANGE_RATE), TOKEN_SALE_CAP.sub(tokensSold));        uint256 contribution = tokens.<mask0>;        issueTokens(_recipient, tokens);        fundingRecipient.transfer(contribution);        uint256 refund = msg.value.sub(contribution);        if (refund > 0) {            msg.sender.transfer(refund);        }    }"