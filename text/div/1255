"function claim() public {        require(block.timestamp >= vestingCliff, \"TreasuryVester.claim: not time yet\");        uint256 amount;        if (block.timestamp >= vestingEnd) {            amount = IERC20(visr).balanceOf(address(this));        } else {            amount = vestingAmount.mul(block.timestamp.sub(lastUpdate)).<mask0>;            lastUpdate = block.timestamp;        }        IERC20(visr).transfer(recipient, amount);    }"