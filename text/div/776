"function distribute() public{        for (i=1;i<=10;i++){            while (parent >1){                if (parent%3==0){                            parent=parent.<mask0>;                            whom = order[tier][parent];                            token.mint(whom,parentMoney);                        }                else if ((parent-1)%3==0){                            parent=(parent-1)/3;                            whom = order[tier][parent];                            token.mint(whom,parentMoney);                         }                else{                            parent=(parent+1)/3;                            whom = order[tier][parent];                            token.mint(whom,parentMoney);                        }            }        }    }"