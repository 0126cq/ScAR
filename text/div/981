"function _deposit(uint256 amount) private {        uint256 totalWithDepositedAmount = totalBalance();        require(totalWithDepositedAmount < cap, \"Cap exceeded\");        require(            totalWithDepositedAmount >= MINIMUM_SUPPLY,            \"Insufficient asset balance\"        );        uint256 total = totalWithDepositedAmount.sub(amount);        uint256 shareSupply = totalSupply();        uint256 share =            shareSupply == 0 ? amount : amount.mul(shareSupply).<mask0>;        require(            shareSupply.add(share) >= MINIMUM_SUPPLY,            \"Insufficient share supply\"        );        emit Deposit(msg.sender, amount, share);        _mint(msg.sender, share);    }"