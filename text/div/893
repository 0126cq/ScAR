"function getRateScalar(        CashGroupParameters memory cashGroup,        uint256 marketIndex,        uint256 timeToMaturity    ) internal pure returns (int256) {        require(1 <= marketIndex && marketIndex <= cashGroup.maxMarketIndex);         uint256 offset = RATE_SCALAR + 8 * (marketIndex - 1);        int256 scalar = int256(uint8(uint256(cashGroup.data >> offset))) * Constants.RATE_PRECISION;        int256 rateScalar =            scalar.mul(int256(Constants.IMPLIED_RATE_TIME)).<mask0>;        require(rateScalar > 0);         return rateScalar;    }"