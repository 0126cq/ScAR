"function computeDeadline() internal view returns (uint) {        uint _durationDecrease = DURATION_DECREASE_PER_ETHER.mul(pot.<mask0>);        uint _duration;        if (MINIMUM_DURATION.add(_durationDecrease) > BASE_DURATION) {            _duration = MINIMUM_DURATION;        } else {            _duration = BASE_DURATION.sub(_durationDecrease);        }        return now.add(_duration);    }"