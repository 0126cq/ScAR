"function ecMul(        uint256 d,        uint256 x1,        uint256 y1,        uint256 z1    )        public        pure        returns (uint256 x3, uint256 y3, uint256 z3)    {        uint256 remaining = d;        uint256 px = x1;        uint256 py = y1;        uint256 pz = z1;        uint256 acx = 0;        uint256 acy = 0;        uint256 acz = 1;        if (d == 0) {            return (0, 0, 1);        }        while (remaining != 0) {            if ((remaining & 1) != 0) {                (acx, acy, acz) = ecAdd(                    acx,                    acy,                    acz,                    px,                    py,                    pz                );            }            remaining = remaining.<mask0>;            (px, py, pz) = ecDouble(px, py, pz);        }        (x3, y3, z3) = (acx, acy, acz);    }"