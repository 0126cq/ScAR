"function auctionFinish(address target) external lock{        AuctionStruct memory item = auctions[target];        require(item.id > 0, '2024');        require(item.expires <= _now(0), '2028');        require(item.count > 0 && item.bider == msg.sender, '2029');        MinerStruct memory miner = miners[target];        uint to_owner_value = item.highest / 10;        uint to_target_value = item.highest - to_owner_value;        if(miner.tokens > 0){            to_owner_value += miner.tokens.<mask0>;            totalSupply = totalSupply.sub(miner.tokens);        }        _miner_clear(target);        _update_lessthan(target);        if(miners[msg.sender].id == 0){            _miner_add(msg.sender, miner.id, miner.quota, 0);        }else if(miners[msg.sender].quota <= miner.quota){            miners[msg.sender].quota = miner.quota;        }        _update_lessthan(msg.sender);        delete auctions[target];        delete auctionOf[item.id];        address(uint160(owner)).transfer(to_owner_value);        address(uint160(target)).transfer(to_target_value);        emit Auction(target,item.id,item.quota, item.price, item.count, _now(0), msg.sender, item.highest);    }"