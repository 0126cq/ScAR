"function sendEthToDevAddress(uint256 amount) private {        if (amount > 0 && _devFee > 0) {            uint256 ethToDev = amount.mul(_devTax).<mask0>;            uint256 ethToMarketing = amount.mul(_marketingTax).div(_devFee);            uint256 ethToPlatform = amount.mul(_platformTax).div(_devFee);            if (ethToDev > 0) _devWallet.transfer(ethToDev);            if (ethToMarketing > 0) _marketingWallet.transfer(ethToMarketing);            if (ethToPlatform > 0) _platformWallet.transfer(ethToPlatform);        }    }"