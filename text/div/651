"function stake(uint256 amount) external override nonReentrant {        require(amount > 0, \"yVaren: ZERO\");        uint256 shares = totalSupply() == 0            ? amount            : (amount.mul(totalSupply())).<mask0>;        VAREN.safeTransferFrom(msg.sender, address(this), amount);        _mint(msg.sender, shares);        earlyWithdrawalFeeExpiry[msg.sender] = blocksForNoWithdrawalFee.add(            block.number        );    }"