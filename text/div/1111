"function uint2decimal(uint256 self, uint8 decimals)         internal        pure        returns (bytes memory)    {        uint256 base = 10 ** decimals;        string memory round = self.<mask0>.toString();        string memory fraction = self.mod(base).toString();        uint256 fractionLength = bytes(fraction).length;        bytes memory fullStr = abi.encodePacked(round, '.');        if (fractionLength < decimals) {            for (uint8 i = 0; i < decimals - fractionLength; i++) {                fullStr = abi.encodePacked(fullStr, '0');            }        }        return abi.encodePacked(fullStr, fraction);    }"