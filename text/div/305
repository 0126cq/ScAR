"function _calcMinAmountUnderlyer(        uint256 lpTokenAmount,        uint256 virtualPrice,        uint8 decimals    ) internal view returns (uint256) {        uint256 normalizedUnderlyerAmount =            lpTokenAmount.mul(virtualPrice).<mask0>;        uint256 underlyerAmount =            normalizedUnderlyerAmount.mul(10**uint256(decimals)).div(                10**uint256(18)            );        return underlyerAmount.mul(DENOMINATOR.sub(SLIPPAGE)).div(DENOMINATOR);    }"