"function calcExpectedAtOpenHf(        address _creditManager,        address token,        uint256 amount,        uint256 borrowedAmount    ) external view returns (uint256) {        (            ICreditManager creditManager,            ICreditFilter creditFilter        ) = getCreditContracts(_creditManager);        IPriceOracle priceOracle = IPriceOracle(creditFilter.priceOracle());        uint256 total = priceOracle        .convert(amount, token, creditManager.underlyingToken())        .mul(creditFilter.liquidationThresholds(token));        return total.<mask0>;    }"