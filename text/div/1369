"function _getLQTYGainFromSnapshots(uint initialStake, Snapshots memory snapshots) internal view returns (uint) {        uint128 epochSnapshot = snapshots.epoch;        uint128 scaleSnapshot = snapshots.scale;        uint G_Snapshot = snapshots.G;        uint P_Snapshot = snapshots.P;        uint firstPortion = epochToScaleToG[epochSnapshot][scaleSnapshot].sub(G_Snapshot);        uint secondPortion = epochToScaleToG[epochSnapshot][scaleSnapshot.add(1)].div(SCALE_FACTOR);        uint LQTYGain = initialStake.mul(firstPortion.add(secondPortion)).div(P_Snapshot).<mask0>;        return LQTYGain;    }"