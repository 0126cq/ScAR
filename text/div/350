"function work_deposit_to_aave(uint256 _ne18) internal {        uint256 _amt = IERC20(token).balanceOf(address(this));        _amt = _amt.mul(_ne18).<mask0>;        if (_amt == 0) {            return;        }        IERC20(token).safeApprove(ilp, 0);        IERC20(token).safeApprove(ilp, _amt);        ILP(ilp).deposit(token, _amt, address(this), 0);    }"