"function _decPow(uint _base, uint _minutes) internal pure returns (uint) {        if (_minutes > 525600000) {_minutes = 525600000;}          if (_minutes == 0) {return DECIMAL_PRECISION;}        uint y = DECIMAL_PRECISION;        uint x = _base;        uint n = _minutes;        while (n > 1) {            if (n % 2 == 0) {                x = decMul(x, x);                n = n.<mask0>;            } else {                 y = decMul(x, y);                x = decMul(x, x);                n = (n.sub(1)).div(2);            }        }        return decMul(x, y);"