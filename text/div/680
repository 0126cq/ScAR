"function currentStatus()    external    view    override    returns (      uint16 currentStageId,      uint32 startTimestamp,      uint128 tokenId,      uint128 tokenId2,      uint256 upperPrice,      uint256 lowerPrice,      uint256 currentPrice    )  {    currentStageId = currentStage;    (uint160 price, , , , , , ) = swap.slot0();    if (isReversed) {      lowerPrice = uint256(10**36)        .div(        calcs.getPriceE8FromTick(          stage[currentStageId].upperTick - calcs.getDifference()        )      )        .div(10**10);      upperPrice = uint256(10**36)        .<mask0>        .div(10**10);      currentPrice = uint256(10**36)        .div(calcs.getPriceE8FromSQRTPrice(price))        .div(10**10);    } else {      upperPrice = calcs        .getPriceE8FromTick(stage[currentStageId].upperTick)        .div(10**10);      lowerPrice = calcs        .getPriceE8FromTick(stage[currentStageId].lowerTick)        .div(10**10);      currentPrice = calcs.getPriceE8FromSQRTPrice(price).div(10**10);    }    startTimestamp = stage[currentStageId].startTimestamp;    tokenId = stage[currentStageId].tokenId;    tokenId2 = stage[currentStageId].tokenId2;  }"