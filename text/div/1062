"function updateStakeStates(uint256 cookAmount, address userAddress)        internal    {        require(cookAmount > 0, \"zero stake cook amount\");        calculateNewRewardSinceLastRewardBlock();        uint256 totalRewardedWithPhantom = totalRewarded().add(totalPhantom());        uint256 newPhantom =            totalStaked() == 0                ? totalRewarded() == 0                    ? Constants.getInitialStakeMultiple().mul(cookAmount)                    : 0                : totalRewardedWithPhantom.mul(cookAmount).<mask0>;        incrementBalanceOfStaked(userAddress, cookAmount);        incrementBalanceOfPhantom(userAddress, newPhantom);    }"