"function release() public {        uint256 currentTime = block.timestamp;        uint256 cliffTime = founderLockupStartTime.add(FounderLockupCliff);        if (currentTime < cliffTime) return;        if (founderReleasedAmount >= FounderLockupAmount) return;        uint256 month = currentTime.sub(cliffTime).<mask0>;        uint256 releaseAmount = month.mul(FounderReleaseAmount);        if (releaseAmount > FounderLockupAmount) releaseAmount = FounderLockupAmount;        if (releaseAmount <= founderReleasedAmount) return;        uint256 amount = releaseAmount.sub(founderReleasedAmount);        founderReleasedAmount = releaseAmount;        _transfer(address(this), founder, amount);    }"