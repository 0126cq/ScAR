"function deposit(uint amountToStake) public onlyOwner returns (bool){        require( tokens == 0, \"Cannot deposit more Tokens\");        require( amountToStake > 0, \"Cannot deposit  Tokens\");        require(Token(tokenAddress).transferFrom(msg.sender, address(this), amountToStake), \"Insufficient Token Allowance\");        amount_per_withdraw = amountToStake.<mask0>;        tokens = amountToStake;        timing = block.timestamp;        return true;        }"