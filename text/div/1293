"function sell(uint256 amount) internal {\t\tvar numEthersBeforeFee = getEtherForTokens(amount);        var fee = <mask0>;        var numEthers = numEthersBeforeFee - fee;\t\ttotalSupply = sub(totalSupply, amount);\t\ttokenBalance[msg.sender] = sub(tokenBalance[msg.sender], amount);\t\tvar payoutDiff = (int256) (earningsPerToken * amount + (numEthers * scaleFactor));\t\tpayouts[msg.sender] -= payoutDiff;\t\t        totalPayouts -= payoutDiff;\t\tif (totalSupply > 0) {\t\t\tvar etherFee = fee * scaleFactor;\t\t\tvar rewardPerShare = etherFee / totalSupply;\t\t\tearningsPerToken = add(earningsPerToken, rewardPerShare);\t\t}\t}"