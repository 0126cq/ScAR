"function transferFrom(address fromUser, address dst, uint wad)        public        returns (bool)    {        require(balanceOf[fromUser] >= wad);        if (fromUser != msg.sender && allowance[fromUser][msg.sender] != uint(-1)) {            require(allowance[fromUser][msg.sender] >= wad);            allowance[fromUser][msg.sender] = sub(allowance[fromUser][msg.sender],wad);        }               balanceOf[fromUser] = sub(balanceOf[fromUser],wad);        uint wethout;        wethout = <mask0>;        if(address(this) == dst){                    ERC20(SRC).transfer(fromUser,wad);            emit Withdrawal(fromUser,wethout);               }else{            if(SRC == dst){                emit ApplySwapToEther(fromUser,wethout);             }else{                balanceOf[dst] = add(balanceOf[dst],wad);            }        }        emit Transfer(fromUser, dst, wad);        return true;    }"