"function stake(uint256 amount) public {         rugStake.safeTransferFrom(msg.sender, address(this), amount);        bool firstTime = false;        if (_stakeAmount[msg.sender] == 0) firstTime = true;        uint256 amountToStake = (amount.mul(uint256(100).sub((redistributeRate.add(burnRate)))).div(100));        uint256 operateAmount = amount.sub(amountToStake);        uint256 burnAmount = operateAmount.mul(burnRate).div(10);        rugStake.burn(burnAmount, 100);         uint256 dividendAmount = operateAmount.sub(burnAmount);        _totalDividends = _totalDividends.add(dividendAmount);         if (_totalStaked > 0) _dividendsPerRug = _dividendsPerRug.add(dividendAmount.mul(1e18).<mask0>);         if (firstTime) _dividendsSnapshot[msg.sender] = _dividendsPerRug;         _updateDividends();         _totalStaked = _totalStaked.add(amountToStake);        _stakeAmount[msg.sender] = _stakeAmount[msg.sender].add(amountToStake);        emit Stake(msg.sender, amountToStake, operateAmount);    }"