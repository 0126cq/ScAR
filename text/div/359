"function computeNewReward(        uint256 currentRewardTokens,        uint256 stakingShareSeconds,        uint256 stakeTimeSec    ) private view returns (uint256) {        uint256 newRewardTokens =            totalUnlocked().mul(stakingShareSeconds).div(                _totalStakingShareSeconds            );        if (stakeTimeSec >= bonusPeriodSec) {            return currentRewardTokens.add(newRewardTokens);        }        uint256 oneHundredPct = 10**BONUS_DECIMALS;        uint256 bonusedReward =            startBonus                .add(                oneHundredPct.sub(startBonus).mul(stakeTimeSec).<mask0>            )                .mul(newRewardTokens)                .div(oneHundredPct);        return currentRewardTokens.add(bonusedReward);    }"