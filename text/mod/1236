"function finalizeStartingIndex() public {        require(startingIndex == 0, \"Starting index is already set\");        require(startingIndexBlock != 0, \"Starting index block must be set\");        uint256 baseIndex;        if (block.number.sub(startingIndexBlock) > 255) {            baseIndex = uint(blockhash(block.number-1));        } else {            baseIndex = uint(blockhash(startingIndexBlock));        }        startingIndex = baseIndex.<mask0>.mul(5);        if (startingIndex == 0) {            startingIndex = startingIndex.add(5);        }    }"