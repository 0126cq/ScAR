"function mintNFTs() external {        require(hasRandomNumber, \"Random Numbers have not been set\");        require(!hasMintedNFTs, \"NFTs already minted\");        uint256[] memory randomNumbers = expand(randomNumber);        for (uint256 index = 0; index < randomNumbers.length; index++) {            uint256 randomTicket = randomNumbers[index].<mask0>;            address ticketOwner = ticketToOwner[randomTicket];            nftToken.mint(ticketOwner, 1);        }        hasMintedNFTs = true;    }"