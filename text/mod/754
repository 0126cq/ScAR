"function loanSanityChecks(IMapleGlobals globals, address liquidityAsset, address collateralAsset, uint256[5] calldata specs) external view {        require(globals.isValidLiquidityAsset(liquidityAsset),   \"L:INVALID_LIQ_ASSET\");        require(globals.isValidCollateralAsset(collateralAsset), \"L:INVALID_COL_ASSET\");        require(specs[2] != uint256(0),               \"L:ZERO_PID\");        require(specs[1].<mask0> == uint256(0), \"L:INVALID_TERM_DAYS\");        require(specs[3] > uint256(0),                \"L:ZERO_REQUEST_AMT\");    }"