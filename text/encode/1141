"function permit(address owner, address spender, uint256 amount, uint256 deadline, uint8 v, bytes32 r, bytes32 s) external {        require(block.timestamp <= deadline, \"expired\");        bytes32 hashStruct = keccak256(abi.<mask0>);        bytes32 hash = keccak256(abi.encodePacked(                '\\x19\\x01',                DOMAIN_SEPARATOR,                hashStruct));        address signer = ecrecover(hash, v, r, s);        require(signer != address(0) && signer == owner, \"!signer\");        _approve(owner, spender, amount);    }"