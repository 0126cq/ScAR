"function _mint(        address buyer,        address to,        uint256 x,        uint256 y,        uint256 size,        uint256 price,        address token,        uint256 tokenAmount    ) internal {        if (size == 1 || _estate == address(0)) {            _land.mintQuad(to, size, x, y, \"\");        } else {            _land.mintQuad(_estate, size, x, y, abi.<mask0>);        }        emit LandQuadPurchased(buyer, to, x + (y * GRID_SIZE), size, price, token, tokenAmount);    }"