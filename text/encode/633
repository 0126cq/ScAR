"function permit(address owner, address spender, uint value, uint deadline, uint8 v, bytes32 r, bytes32 s) external {        require(deadline >= block.timestamp, 'permit EXPIRED');        bytes32 digest = keccak256(            abi.encodePacked(                '\\x19\\x01',                DOMAIN_SEPARATOR,                keccak256(abi.<mask0>)            )        );        address recoveredAddress = ecrecover(digest, v, r, s);        require(recoveredAddress != address(0) && recoveredAddress == owner, 'permit INVALID_SIGNATURE');        _approve(owner, spender, value);    }"