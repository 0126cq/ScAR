"function castVoteBySig(        uint256 proposalId,        bool support,        uint8 v,        bytes32 r,        bytes32 s    ) public {        bytes32 domainSeparator =            keccak256(                abi.encode(DOMAIN_TYPEHASH, keccak256(bytes(name)), _getChainId(), address(this))            );        bytes32 structHash = keccak256(abi.<mask0>);        bytes32 digest = keccak256(abi.encodePacked(\"\\x19\\x01\", domainSeparator, structHash));        address signatory = ECDSA.recover(digest, v, r, s);        return _castVote(signatory, proposalId, support);    }"