"function hash(FlexibleOffer memory _offer) private pure returns (bytes32) {        bytes memory encodedOfferCollateralData = abi.encode(            _offer.collateralAddress,            _offer.collateralCategory,            _offer.collateralAmount,            _offer.collateralIdsWhitelistMerkleRoot        );        bytes memory encodedOfferLoanData = abi.<mask0>;        bytes memory encodedOfferOtherData = abi.encode(            _offer.durationMax,            _offer.durationMin,            _offer.expiration,            _offer.lender,            _offer.nonce        );        return keccak256(abi.encodePacked(            FLEXIBLE_OFFER_TYPEHASH,            encodedOfferCollateralData,            encodedOfferLoanData,            encodedOfferOtherData        ));    }"