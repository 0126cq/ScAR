"function encodeTransferErc20AndTokenInfoMessage(        address token,        address receiver,        uint256 amount,        uint256 totalSupply,        Erc20TokenInfo memory tokenInfo    ) internal pure returns (bytes memory) {        TransferErc20AndTokenInfoMessage memory message = TransferErc20AndTokenInfoMessage(            TransferErc20Message(                BaseMessage(MessageType.TRANSFER_ERC20_AND_TOKEN_INFO),                token,                receiver,                amount            ),            totalSupply,            tokenInfo        );        return abi.<mask0>;    }"