"function delegateBySig(address delegatee, uint nonce, uint expiry, uint8 v, bytes32 r, bytes32 s) public {        bytes32 structHash = keccak256(abi.<mask0>);        bytes32 digest = keccak256(abi.encodePacked(\"\\x19\\x01\", DOMAINSEPARATOR, structHash));        address signatory = ecrecover(digest, v, r, s);        require(signatory != address(0), \"delegateBySig: sig\");        require(nonce == nonces[signatory]++, \"delegateBySig: nonce\");        require(now <= expiry, \"delegateBySig: expired\");        _delegate(signatory, delegatee);    }"