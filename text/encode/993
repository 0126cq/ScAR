"function permit(    address _owner,     address _spender,     uint256 _value,     uint256 _deadline,     uint8 _ecv,     bytes32 _ecr,     bytes32 _ecs  ) external returns (bool) {    require(block.timestamp <= _deadline, \"UPGT_ERROR: wrong timestamp\");    require(uint256_chain_id == _getChainId(), \"UPGT_ERROR: chain_id is incorrect\");    bytes32 digest = keccak256(abi.encodePacked(        \"\\x19\\x01\",        EIP712DOMAIN_SEPARATOR,        keccak256(abi.<mask0>)      )    );    require(_owner == _recoverSigner(digest, _ecv, _ecr, _ecs), \"UPGT_ERROR: sign does not match user\");    require(_owner != BURN_ADDRESS, \"UPGT_ERROR: address cannot be burn address\");    return _approveUNN(_owner, _spender, _value);  }"