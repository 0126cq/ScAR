"function _buildJSON(    bytes memory name,    bytes memory attributes,    bytes memory image  ) internal pure returns (bytes memory) {    bytes memory output = abi.encodePacked(      '{\"name\":\"',      name,      '\",\"description\":\"Build your guild, battle your foes with the first on-chain PvP. Hunt for fortune and glory shall be yours!\",\"attributes\":[',      attributes,      '],\"image\":\"',      image,      '\"}'    );    return abi.encodePacked(\"data:application/json;base64,\", Base64.<mask0>);  }"