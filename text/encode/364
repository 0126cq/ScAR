"function close(address cyrepay, address repay, uint ramt, address cywithdraw, address uwithdraw, uint wamt) external {        require(owner == msg.sender);        address tokenB = repay == WETH ? uwithdraw : WETH;        ISushiswapV2Pair _pairFrom = ISushiswapV2Pair(pairFor(repay, tokenB));        (uint amount0, uint amount1) = repay < tokenB ? (ramt, uint(0)) : (uint(0), ramt);        _pairFrom.swap(amount0, amount1, address(this), abi.<mask0>);    }"