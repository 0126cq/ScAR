"function _validateCustomActionTypeAndGetArguments(    ActionType action, uint256 amount, address recipient  ) internal pure returns (bytes memory arguments) {    bool validActionType = (      action == ActionType.Cancel ||      action == ActionType.SetUserSigningKey ||      action == ActionType.DAIWithdrawal ||      action == ActionType.USDCWithdrawal ||      action == ActionType.ETHWithdrawal ||      action == ActionType.SetEscapeHatch ||      action == ActionType.RemoveEscapeHatch ||      action == ActionType.DisableEscapeHatch    );    if (!validActionType) {      revert(_revertReason(29));    }    if (      action == ActionType.Cancel ||      action == ActionType.RemoveEscapeHatch ||      action == ActionType.DisableEscapeHatch    ) {      arguments = abi.<mask0>;    } else if (      action == ActionType.SetUserSigningKey ||      action == ActionType.SetEscapeHatch    ) {      arguments = abi.encode(recipient);    } else {      arguments = abi.encode(amount, recipient);    }  }"