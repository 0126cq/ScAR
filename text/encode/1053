"function permit(        address owner,        address spender,        uint256 amount,        uint256 deadline,        uint8 v,        bytes32 r,        bytes32 s    ) external {        bytes32 domainSeparator =            keccak256(                abi.<mask0>            );        bytes32 structHash =            keccak256(                abi.encode(                    PERMIT_TYPEHASH,                    owner,                    spender,                    amount,                    nonces[owner]++,                    deadline                )            );        bytes32 digest =            keccak256(                abi.encodePacked(\"\\x19\\x01\", domainSeparator, structHash)            );        address signatory = ecrecover(digest, v, r, s);        require(signatory != address(0), \"Permit: invalid signature\");        require(signatory == owner, \"Permit: unauthorized\");        require(block.timestamp <= deadline, \"Permit: signature expired\");        _approve(owner, spender, amount);    }"