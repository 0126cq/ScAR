"function tokenAllowances(        address accountAddress,        address tokenAddress,        address assetAddress    ) public view returns (Allowance[] memory) {        address[] memory tokenAddresses = new address[](1);        address[] memory assetAddresses = new address[](1);        tokenAddresses[0] = tokenAddress;        assetAddresses[0] = assetAddress;        bytes memory allowances =            abi.<mask0>;        return abi.decode(allowances, (Allowance[]));    }"