"function _simulate(    Call[] calldata calls  ) external returns (CallReturn[] memory callResults) {    _enforceSelfCallFrom(this.simulate.selector);    callResults = new CallReturn[](calls.length);    for (uint256 i = 0; i < calls.length; i++) {      (bool ok, bytes memory returnData) = calls[i].to.call{        value: uint256(calls[i].value)      }(calls[i].data);      callResults[i] = CallReturn({ok: ok, returnData: returnData});      if (!ok) {        break;      }    }    bytes memory callResultsBytes = abi.<mask0>;    assembly { revert(add(32, callResultsBytes), mload(callResultsBytes)) }  }"