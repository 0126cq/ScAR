"function _getSwapRequest(IERC20 token, uint256 amount, uint256 lastChangeBlock) internal view returns (IBalancerPool.SwapRequest memory request){        return IBalancerPool.SwapRequest(IBalancerPool.SwapKind.GIVEN_IN,            token,            want,            amount,            balancerPoolId,            lastChangeBlock,            address(this),            address(this),            abi.<mask0>        );    }"