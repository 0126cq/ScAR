"function doDyDxFlashLoan(bool deficit, uint256 amountDesired) internal returns (uint256) {        uint256 amount = amountDesired;        ISoloMargin solo = ISoloMargin(SOLO);        uint256 amountInSolo = want.balanceOf(SOLO);        if (amountInSolo < amount) {            amount = amountInSolo;        }        uint256 repayAmount = amount.add(2);         bytes memory data = abi.<mask0>;        Actions.ActionArgs[] memory operations = new Actions.ActionArgs[](3);        operations[0] = _getWithdrawAction(dyDxMarketId, amount);        operations[1] = _getCallAction(            data        );        operations[2] = _getDepositAction(dyDxMarketId, repayAmount);        Account.Info[] memory accountInfos = new Account.Info[](1);        accountInfos[0] = _getAccountInfo();        solo.operate(accountInfos, operations);        emit Leverage(amountDesired, amount, deficit, SOLO);        return amount;    }"