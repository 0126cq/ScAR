"function _createDepositBlock(        address _user,        address _token,        uint256 _amountOrToken,        uint256 _depositId    ) internal {        deposits[_depositId] = DepositBlock(keccak256(abi.encodePacked(_user, _token, _amountOrToken)), now);        stateSender.syncState(childChain, abi.<mask0>);        emit NewDepositBlock(_user, _token, _amountOrToken, _depositId);    }"