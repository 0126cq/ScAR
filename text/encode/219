"function burnBatch(address account, uint256[] calldata itemIds, uint256[] calldata amounts, bytes memory) authorizedOnly override(ItemProjection, Item) public {        require(account != address(0), \"required account\");        uint256[] memory interoperableInterfaceAmounts = new uint256[](amounts.length);        for(uint256 i = 0 ; i < interoperableInterfaceAmounts.length; i++) {            interoperableInterfaceAmounts[i] = toInteroperableInterfaceAmount(amounts[i], itemIds[i], account);        }        IItemMainInterface(mainInterface).mintTransferOrBurn(true, abi.encode(true, abi.<mask0>));        emit TransferBatch(msg.sender, account, address(0), itemIds, amounts);    }"