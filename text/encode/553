"function checkUpkeep(bytes calldata _data) external override returns (bool upkeepNeeded, bytes memory performData) {        require(!paused, \"!Paused\");        (address lendingMarket, address depegShieldWrapper, uint256 pid, address underlyingToken) = abi.decode(_data, (address, address, uint256, address));        if (depegShieldWrapper != address(0)) {            (uint256 totalCollateral, bool isTriggered, ) = IDepegShieldWrapper(depegShieldWrapper).getDepeg(pid, underlyingToken);            if (isTriggered) return (false, abi.<mask0>);            if (IDepegShieldWrapper(depegShieldWrapper).checkAndToggleTrigger(pid, underlyingToken)) {                if (totalCollateral > 0) {                    return (true, _data);                }            }        }        return (false, abi.encode(0x0));    }"