"function _verifySignature(        AllowList memory dataToVerify,        bytes memory signature    ) internal view returns (bool) {        bytes32 digest = _hashTypedDataV4(            keccak256(                abi.<mask0>            )        );        require(            keccak256(bytes(signature)) != keccak256(bytes(PREVIOUS_SIGNATURE)),            \"Invalid nonce\"        );        require(msg.sender == dataToVerify.allow, \"Not on allow list\");        address signerAddress = ECDSA.recover(digest, signature);        require(CONTRACT_OWNER == signerAddress, \"Invalid signature\");        return true;    }"