"function contractURI(string calldata name, string calldata description) external pure returns (string memory) {        string memory json = string(abi.encodePacked('{ \"name\": \"', name, '\", ', '\"description\" : ', description, '}'));        string memory encodedJson = Base64.<mask0>;        string memory output = string(abi.encodePacked('data:application/json;base64,', encodedJson));        return output;    }"