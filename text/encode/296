"function getMetadata(uint256 _tokenId) private view returns (bytes memory) {        string memory imageURI = Base64.<mask0>;        string[3] memory eachAttribute = [            pieces[_tokenId].colorOf,             pieces[_tokenId].styleOf,            pieces[_tokenId].typeOf        ];        bytes memory attributes = abi.encodePacked(            '[{\"trait_type\": \"COLOR\", \"value\": \"', eachAttribute[0],'\"}, ',            '{\"trait_type\": \"STYLE_OF_ART\", \"value\": \"', eachAttribute[1],'\"}, ',            '{\"trait_type\": \"TYPE_OF_ART\", \"value\": \"', eachAttribute[2],'\"}], '        );          return            abi.encodePacked(                    '{\"name\": \"', pieces[_tokenId].title,'\",',                 '\"description\": \"NFT image of the original -Pixel Art Sculpture- series of works by sculptor Ichitaro Suzuki. ',                eachDescription[_tokenId],                '\", ',                '\"attributes\": ',                attributes,                '\"image\": \"data:image/svg+xml;base64,',imageURI,'\"}'            );    }"