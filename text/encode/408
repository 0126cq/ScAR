"function permit(        address owner,        address spender,        uint256 amount,        uint256 deadline,        uint8 v,        bytes32 r,        bytes32 s    ) external {        require(owner != address(0), \"permit: signature\");        require(block.timestamp <= deadline, \"permit: expired\");        bytes32 structHash =            keccak256(                abi.<mask0>            );        bytes32 digest =            keccak256(                abi.encodePacked(\"\\\\x19\\\\x01\", DOMAIN_SEPARATOR, structHash)            );        address signatory = ecrecover(digest, v, r, s);        require(signatory == owner, \"permit: unauthorized\");        _approve(owner, spender, amount);    }"