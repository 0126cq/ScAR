"function encodeTransferErc20AndTotalSupplyMessage(        address token,        address receiver,        uint256 amount,        uint256 totalSupply    ) internal pure returns (bytes memory) {        TransferErc20AndTotalSupplyMessage memory message = TransferErc20AndTotalSupplyMessage(            TransferErc20Message(                BaseMessage(MessageType.TRANSFER_ERC20_AND_TOTAL_SUPPLY),                token,                receiver,                amount            ),            totalSupply        );        return abi.<mask0>;    }"