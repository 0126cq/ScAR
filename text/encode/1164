"function downPayWithETH(TradeDetails[] memory tradeDetails, uint loanAmt) public payable nonReentrant onlyBeacon {        address _solo = _dYdX_SoloMargin_;        address _token = _WETH_;        uint256 marketId = _getMarketIdFromTokenAddress(_solo, _token);        uint _amount = IERC20(_token).balanceOf(_solo);        uint256 repayAmount = _amount.add(2);           IERC20(_token).approve(_solo, repayAmount);        Actions.ActionArgs[] memory operations = new Actions.ActionArgs[](3);        operations[0] = _getWithdrawAction(marketId, _amount);        operations[1] = _getCallAction(            abi.<mask0>        );        operations[2] = _getDepositAction(marketId, repayAmount);        Account.Info[] memory accountInfos = new Account.Info[](1);        accountInfos[0] = _getAccountInfo();        ISoloMargin(_solo).operate(accountInfos, operations);        if(address(this).balance > 0)            _msgSender().transfer(address(this).balance);    }"