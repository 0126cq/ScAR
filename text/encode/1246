"function swapTONtoWTON(uint256 amount, bool toWTON) external override lock {        checkTokamak();        if (toWTON) {            require(                swapProxy != address(0),                \"TokamakStaker: swapProxy is zero\"            );            require(                IERC20BASE2(ton).balanceOf(address(this)) >= amount,                \"TokamakStaker: swapTONtoWTON ton balance is insufficient\"            );            bytes memory data = abi.<mask0>;            require(                ITON(ton).approveAndCall(wton, amount, data),                \"TokamakStaker:swapTONtoWTON approveAndCall fail\"            );        } else {            require(                IERC20BASE2(wton).balanceOf(address(this)) >= amount,                \"TokamakStaker: swapTONtoWTON wton balance is insufficient\"            );            require(                IIWTON2(wton).swapToTON(amount),                \"TokamakStaker:swapToTON fail\"            );        }    }"