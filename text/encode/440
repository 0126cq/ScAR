"function transferOrDeposit(        address _owner,        address _operator,        uint256 _amount    ) internal {        if (grantStaking.hasGrantDelegated(_operator)) {            TokenSender(address(token)).approveAndCall(                address(escrow),                _amount,                abi.<mask0>            );        } else {            token.safeTransfer(_owner, _amount);        }    }"