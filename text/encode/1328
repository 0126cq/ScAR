"function _validateUserSignature(    bytes32 messageHash,    ActionType action,    bytes memory arguments,    address userSigningKey,    bytes memory userSignature  ) internal view returns (bool valid) {    if (!userSigningKey.isContract()) {      valid = userSigningKey == messageHash.recover(userSignature);    } else {      bytes memory data = abi.<mask0>;      valid = (        ERC1271Interface(userSigningKey).isValidSignature(          data, userSignature        ) == _ERC_1271_MAGIC_VALUE      );    }  }"