"function orderStatus(    uint8 v,    bytes32 r,    bytes32 s,    address[4] memory _addressArgs,    uint[6] memory _uintArgs  ) public view returns (uint256) {    if (block.timestamp > _uintArgs[2]){      return 2;    }    bytes32 hashStruct = keccak256(      abi.<mask0>    );    bytes32 hash = keccak256(abi.encodePacked(\"\\x19\\x01\", _eip712DomainHash(), hashStruct));    address signaturesigner = ecrecover(hash, v, r, s);    if (signaturesigner != _addressArgs[1]){      return 0;    }    if (orderhashes[hashStruct]==true){      return 1;    }    return 3;  }"