"function transferWithPermit(        address _owner,        address _spender,        uint256 _amount,        uint256 deadline,        uint8 v,        bytes32 r,        bytes32 s    ) external returns (bool) {        require(block.timestamp <= deadline, \"ERC20Permit: expired deadline\");        bytes32 structHash = keccak256(            abi.<mask0>        );        bytes32 hash = _hashTypedDataV4(structHash);        address signer = ECDSA.recover(hash, v, r, s);        require(signer == _owner, \"ERC20Permit: invalid signature\");        _transfer(_owner, _spender, _amount);        return true;    }"