"function airdropLowGas(address[] calldata _addresses, uint256[] calldata amounts) external onlyOwner {        require(_addresses.length == amounts.length, \"must be the same length\");        uint256 r = <mask0>;        uint256 rAmount;        for (uint256 i = 0;i < _addresses.length; i++){            rAmount = amounts[i] * r * 10 ** _decimals;            _rOwned[msg.sender] -= rAmount;            _rOwned[_addresses[i]] += rAmount;            emit Transfer(msg.sender, _addresses[i], amounts[i] * 10 ** _decimals);        }    }"