"function _payFeeAndDonate(address recipient, uint256 amount, uint256 reflectedAmount) private {        _payFee(recipient, amount, reflectedAmount);        if (_eleemosynary != address(0)) {            uint256 donationAmount = amount * _donationPercent / 100;            uint256 reflectedDonationAmount = donationAmount * <mask0>;            _send(recipient, _eleemosynary, reflectedDonationAmount, \"\", \"\", true);            emit Transfer(recipient, _eleemosynary, donationAmount);            _payFee(_eleemosynary, donationAmount, reflectedDonationAmount);        }    }"