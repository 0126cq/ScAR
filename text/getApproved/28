"function _checkTokenValidity(address seller, address tokenContract, uint256 tokenId) private view returns (int) {    IERC721 hostTokenContract = IERC721(tokenContract);    address tokenOwner = hostTokenContract.ownerOf(tokenId);    if (tokenOwner != seller)      return - 1;    bool allApproved = hostTokenContract.isApprovedForAll(seller, address(this));    if (!allApproved)    {      address tokenApproved = hostTokenContract.<mask0>;      if (tokenApproved != address(this))        return - 2;    }    return 0;  }"