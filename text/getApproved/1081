"function transferFrom(        address from,        address to,        uint256 tokenId    ) public virtual {        if (!_exists(tokenId)) revert OwnerQueryForNonexistentToken();        if (ownerOf(tokenId) != from) revert TransferFromIncorrectOwner();        if (to == address(0)) revert TransferToZeroAddress();        bool isApprovedOrOwner = (msg.sender == from ||            msg.sender == <mask0> ||            isApprovedForAll(from, msg.sender));        if (!isApprovedOrOwner) revert TransferCallerNotOwnerNorApproved();        delete _tokenApprovals[tokenId];        _owners[tokenId] = to;        if (tokenId > 0 && _owners[tokenId - 1] == address(0)) {            _owners[tokenId - 1] = from;        }        emit Transfer(from, to, tokenId);    }"