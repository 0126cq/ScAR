"function depositNFTToken(address _tokenAddress, uint256 _tokenID)    external    whenNotPaused    whenVaultStarted    returns (bool)  {    require(_tokenAddress == address(goldFinchPoolToken), \"Not Goldfinch Pool Token\");    require(isValidPool(_tokenAddress, _tokenID) == true, \"Not a valid pool\");    require(IERC721(_tokenAddress).ownerOf(_tokenID) == msg.sender, \"User does not own this token\");    require(      IERC721(_tokenAddress).<mask0> == address(this),      \"User has not approved the vault for this token\"    );    uint256 purchasePrice = getJuniorTokenValue(_tokenAddress, _tokenID);    require(purchasePrice > 0, \"The amount of stable coin to get is not larger than 0\");    require(      usdcCoin.balanceOf(address(this)) >= purchasePrice,      \"The vault does not have sufficient stable coin\"    );    IERC721(_tokenAddress).safeTransferFrom(msg.sender, address(this), _tokenID);    usdcCoin.safeTransfer(msg.sender, purchasePrice);    emit DepositNFT(_tokenAddress, msg.sender, _tokenID);    return true;  }"