"function claim () public nonReentrant {        uint balance;        uint currentEpoch = getCurrentEpoch();        if (currentEpoch > NUMBER_OF_EPOCHS + 1) {            lastClaimedEpoch = NUMBER_OF_EPOCHS;            _bond.transfer(owner(), _bond.balanceOf(address (this)));            return;        }        if (currentEpoch > lastClaimedEpoch) {            balance = (currentEpoch - 1  - lastClaimedEpoch) * totalDistributedBalance / NUMBER_OF_EPOCHS;        }        lastClaimedEpoch = currentEpoch - 1;        if (balance > 0) {            _bond.transfer(<mask0>, balance);        }    }"