"function mint(uint256 _mintAmount) public payable {    require(!paused,\"Contract is paused\");    uint256 supply = totalSupply();    require(_mintAmount > 0, \"Mint amount should be greater than zero\");    require(_mintAmount <= maxMintAmount,\"Mint amount exceeded\");    require(supply + _mintAmount <= maxSupply, \"Mint amount exceeded\");     require(_mintAmount+waveMintCounter<=waveMaxAmount, \"Maximum number of NFTs minted in this phase\");    if (msg.sender != <mask0>) {        if(presaleMode){            require(addressesWithSpecificMintAmount[msg.sender]>0, \"User is not whitelisted\");            require(addressesMintCount[msg.sender] + _mintAmount <= addressesWithSpecificMintAmount[msg.sender], \"Max NFT per address minted\");        }        require(msg.value >= (cost * _mintAmount),\"You do not have enough funds\");    }    for (uint256 i = 1; i <= _mintAmount; i++) {        addressesMintCount[msg.sender]++;      _safeMint(msg.sender, supply + i);    }    waveMintCounter+=_mintAmount;  }"