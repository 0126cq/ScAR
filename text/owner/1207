"function executeProposal(bytes32 _proposalId, int256 _param) external onlyVotingMachine(_proposalId) returns(bool) {        Avatar avatar = proposalsInfo[msg.sender][_proposalId].avatar;        SchemeProposal memory proposal = organizationsProposals[address(avatar)][_proposalId];        require(proposal.scheme != address(0));        delete organizationsProposals[address(avatar)][_proposalId];        emit ProposalDeleted(address(avatar), _proposalId);        if (_param == 1) {            ControllerInterface controller = ControllerInterface(avatar.<mask0>);            if (proposal.addScheme) {                require(controller.registerScheme(                        proposal.scheme,                        proposal.parametersHash,                        proposal.permissions,                        address(avatar))                );            }            if (!proposal.addScheme) {                require(controller.unregisterScheme(proposal.scheme, address(avatar)));            }        }        emit ProposalExecuted(address(avatar), _proposalId, _param);        return true;    }"