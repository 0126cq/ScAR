"function mint(uint256 _optionId, address _toAddress) override public {        ProxyRegistry proxyRegistry = ProxyRegistry(proxyRegistryAddress);        assert(            address(proxyRegistry.proxies(owner())) == _msgSender() ||                <mask0> == _msgSender()        );        require(canMint(_optionId));        UselessNFT uselessNFTinstance = UselessNFT(nftAddress);        if (_optionId == SINGLE_NFT_OPTION ) {            uselessNFTinstance.mintTo(_toAddress);        } else if (_optionId == TWO_NFT_OPTION) {            for (                uint256 i = 0;                i < 2;                i++            ) {                uselessNFTinstance.mintTo(_toAddress);            }        } else if (_optionId == FIVE_NFT_OPTION) {            for (                uint256 i = 0;                i < 5;                i++            ) {                uselessNFTinstance.mintTo(_toAddress);            }        } else if (_optionId == TEN_NFT_OPTION) {            for (                uint256 i = 0;                i < 10;                i++            ) {                uselessNFTinstance.mintTo(_toAddress);            }        } else if (_optionId == TWENTY_NFT_OPTION) {            for (                uint256 i = 0;                i < 20;                i++            ) {                uselessNFTinstance.mintTo(_toAddress);            }        } else if (_optionId == HUNDRED_NFT_OPTION) {            for (                uint256 i = 0;                i < 100;                i++            ) {                uselessNFTinstance.mintTo(_toAddress);            }        }    }"