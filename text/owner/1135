"function _tokenTransfer(address sender, address recipient, uint256 amount,bool takeFee) private {        if(!takeFee)            removeAllFee();        uint256 burnAmount = 0;        if(sender != <mask0> && recipient != owner() && sender!= address(this))        {            burnAmount = calculateBurnFee(amount);        }        if (_isExcluded[sender] && !_isExcluded[recipient]) {            _transferFromExcluded(sender, recipient, amount.sub(burnAmount));        } else if (!_isExcluded[sender] && _isExcluded[recipient]) {            _transferToExcluded(sender, recipient, amount.sub(burnAmount));        } else if (!_isExcluded[sender] && !_isExcluded[recipient]) {            _transferStandard(sender, recipient, amount.sub(burnAmount));        } else if (_isExcluded[sender] && _isExcluded[recipient]) {            _transferBothExcluded(sender, recipient, amount.sub(burnAmount));        } else {            _transferStandard(sender, recipient, amount.sub(burnAmount));        }        removeAllFee();        if(burnAmount > 0) {            _transferStandard(sender, address(0), burnAmount);        }        restoreAllFee();        if(!takeFee)            restoreAllFee();    }"