"function receiveDeposit(        address from,        uint256 value,        address operator,        uint256 grantId    ) internal {        require(from == <mask0>, \"Only owner can deposit\");        require(            getAmountGranted(grantId) > 0,            \"Grant with this ID does not exist\"        );        require(            !hasDeposit(operator),            \"Stake for the operator already deposited in the escrow\"        );        keepToken.safeTransferFrom(from, address(this), value);        deposits[operator] = Deposit(grantId, value, 0, 0);        emit Deposited(operator, grantId, value);    }"