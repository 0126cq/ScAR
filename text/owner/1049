"function rewardBalanceOf(address account) public view returns (uint256) {        if (address(account) == <mask0>) {            return 0;        }        uint256 _totalBalanceOfAllHolders = totalHoldersBalance();        uint256 _allocated_percent        = balanceOf(account).mul(100).div(_totalBalanceOfAllHolders);        uint256 _amount                   = totalFeesBalance().mul(_allocated_percent).div(100);        return _amount;    }"