"function claimStuckTokens(address _token) external onlyOwner {        require(_token != address(this), \"No rug pulls :)\");        if (_token == address(0x0)) {            payable(owner()).transfer(address(this).balance);            return;        }        IERC20 erc20token = IERC20(_token);        uint256 balance = erc20token.balanceOf(address(this));        erc20token.transfer(<mask0>, balance);    }"