"function mint(uint256 _mintAmount) public payable {    require(!paused);    uint256 supply = totalSupply();    require(_mintAmount > 0);    require(_mintAmount <= maxMintAmount);    require(supply + _mintAmount <= maxSupply);    if (msg.sender != <mask0>) {        if(onlyWhitelisted == true) {            require(isWhitelisted(msg.sender));            uint256 ownerMintedCount = addressMintedBalance[msg.sender];            require(ownerMintedCount + _mintAmount <= nftPerAddressLimit);        }        require(msg.value >= cost * _mintAmount);    }    for (uint256 i = 1; i <= _mintAmount; i++) {      addressMintedBalance[msg.sender]++;      _safeMint(msg.sender, supply + i);    }  }"