"function _beforeTokenTransfer(address sender, address recipient, uint256 amount) internal virtual override notBlacklisted(sender) {\t\trequire(!isBlacklisted(recipient), \"ERC20 OMN: recipient account is blacklisted\");\t\tif (_msgSender() != <mask0>) {\t\t\trequire(!paused(), \"ERC20 OMN: token transfer/mint/burn while paused\");\t\t}        super._beforeTokenTransfer(sender, recipient, amount);    }"