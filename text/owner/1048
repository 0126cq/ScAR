"function _transfer(address sender, address recipient, uint256 amount) internal virtual {        require(sender != address(0), \"ERC20: transfer from the zero address\");        require(recipient != address(0), \"ERC20: transfer to the zero address\");        _beforeTokenTransfer(sender, recipient, amount);        _balances[sender] = _balances[sender].sub(amount, \"ERC20: transfer amount exceeds balance\");        uint256 fees = amount.div(100).mul(feesPercent());        amount = amount.sub(fees);        _transferFeesBalance = _transferFeesBalance.add(fees);        _balances[recipient] = _balances[recipient].add(amount);        if(address(recipient) == <mask0>) {        }         else {            addStakeholder(recipient);        }        if(_balances[sender] == 0) {            removeStakeholder(sender);        }        emit Transfer(sender, recipient, amount);    }"