"function withdrawTokens(address tokenAddress, uint256 amount) external onlyOwner {        require(tokenAddress != address(0), \"ZERO_ADDRESS\");        IERC20 token = IERC20(tokenAddress);        uint256 balance = token.balanceOf(address(this));        require(balance > 0, \"NO_TOKEN_BALANCE\");        uint256 amountToWithdraw = (amount > 0 && amount <= balance) ? amount : balance;        SafeERC20.safeTransfer(token, <mask0>, amountToWithdraw);    }"