"function revoke(address beneficiary) public onlyOwner {    TokenAward storage award = getTokenAwardStorage(beneficiary);    require(award.revocable, \"Cannot be revoked\");    require(!award.revoked, \"Already revoked\");    uint256 unreleased = getReleasableAmount(beneficiary);    award.released += unreleased;    uint256 refund = award.amount - award.released;    award.revoked = true;    award.amount = award.released;    targetToken.transfer(beneficiary, unreleased);    targetToken.transfer(<mask0>, refund);    emit Released(beneficiary, unreleased);    emit Revoked(beneficiary, refund);  }"