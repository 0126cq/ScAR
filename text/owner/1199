"function upgradeController(address _newController, Avatar _avatar)    external    onlyUpgradingScheme    isAvatarValid(address(_avatar))    returns(bool)    {        require(newController == address(0));           require(_newController != address(0));        newController = _newController;        avatar.transferOwnership(_newController);        require(avatar.owner() == _newController);        if (nativeToken.owner() == address(this)) {            nativeToken.transferOwnership(_newController);            require(nativeToken.<mask0> == _newController);        }        if (nativeReputation.owner() == address(this)) {            nativeReputation.transferOwnership(_newController);            require(nativeReputation.owner() == _newController);        }        emit UpgradeController(address(this), newController);        return true;    }"