"function recoverERC20(address tokenAddress, uint256 amount) public virtual onlyOwner {        IERC20 token = IERC20(tokenAddress);        if (amount == 0) {            uint256 balance = token.balanceOf(address(this));            token.transfer(<mask0>, balance);        } else if (amount != 0) {            token.transfer(owner(), amount);        }    }"