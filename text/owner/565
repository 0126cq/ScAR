"function excludeFromMaxTx(address excludeAddress_, bool isExcluded_) public onlyOwner {        if (!isExcluded_) {            require(excludeAddress_ != <mask0> && excludeAddress_ != address(this) && excludeAddress_ != address(0xdead) && excludeAddress_ != address(dexRouter) && excludeAddress_ != dexPair, \"Token: excluded from max TX address invalid\");        }        _isExcludedFromMaxTx[excludeAddress_] = isExcluded_;    }"