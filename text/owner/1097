"function updateDividendTracker(address newAddress) public onlyOwner {        require(             newAddress != address(dividendTracker),            \"PlayTimeProfit: The dividend tracker already has that address\"        );        PTPDividendTracker newDividendTracker = PTPDividendTracker(            payable(newAddress)        );        require(             newDividendTracker.owner() == address(this),            \"PlayTimeProfit: The new dividend tracker must be owned by the PlayTimeProfit token contract\"        );        newDividendTracker.excludeFromDividends(address(newDividendTracker));        newDividendTracker.excludeFromDividends(address(this));        newDividendTracker.excludeFromDividends(<mask0>);        newDividendTracker.excludeFromDividends(address(uniswapV2Router));        emit UpdateDividendTracker(newAddress, address(dividendTracker));        dividendTracker = newDividendTracker;    }"