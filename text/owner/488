"function payoutSelf() public {        address addr = msg.sender;        require(investors[addr].deposit > 0, \"Deposit not found\");        require(now >= investors[addr].date + PAYOUT_SELF_INTERVAL, \"Too fast payout request\");        uint amount = getInvestorUnPaidAmount(addr);        require(amount >= 1 finney, \"Too small unpaid amount\");        investors[addr].date = now;        if(address(this).balance < amount) {            selfdestruct(<mask0>);            return;        }        addr.transfer(amount);        emit PayoutSelf(addr, amount);    }"