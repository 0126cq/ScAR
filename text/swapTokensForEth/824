"function _transferBothExcluded(address sender, address recipient, uint256 tAmount, bool useRFI) private {        (uint256 rAmount, uint256 rTransferAmount, uint256 rFee, uint256 burnFee, uint256 charityFee, uint256 tTransferAmount, uint256 tFee) = _getValues(tAmount);        _tOwned[sender] = _tOwned[sender].sub(tAmount);        _rOwned[sender] = _rOwned[sender].sub(rAmount);        if(useRFI == false) {            _rOwned[recipient] = _rOwned[recipient].add(rAmount);            _tOwned[recipient] = _tOwned[recipient].add(tAmount);            emit Transfer(sender,recipient,tAmount);            return;        }        _tOwned[recipient] = _tOwned[recipient].add(tTransferAmount);        _rOwned[recipient] = _rOwned[recipient].add(rTransferAmount);               uint256 currentRate = _getRate();        uint256 rBurnFee = burnFee.mul(currentRate);        _takeLiquidity(charityFee);        <mask0>;        _reflectFee(rFee, tFee);        _tTotal = _tTotal.sub(burnFee);         _rTotal = _rTotal.sub(rBurnFee);         emit Transfer(sender, recipient, tTransferAmount);        emit Transfer(_msgSender(), address(0), burnFee);    }"