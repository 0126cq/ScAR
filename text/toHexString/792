"function mint(address player_, uint cardId_) public returns (uint256) {        require(cardId_ != 0 && cardInfoes[cardId_].cardId != 0, \"K: wrong cardId\");        if (superMinter != _msgSender()) {            require(minters[_msgSender()][cardId_] > 0, \"K: not minter\");            minters[_msgSender()][cardId_] -= 1;        }        require(cardInfoes[cardId_].currentAmount < cardInfoes[cardId_].maxAmount, \"k: amount out of limit\");        cardInfoes[cardId_].currentAmount += 1;        uint tokenId = random(gasleft());        cardIdMap[tokenId] = cardId_;        _mint(player_, tokenId);        _setTokenURI(tokenId, tokenId.<mask0>);        return tokenId;    }"