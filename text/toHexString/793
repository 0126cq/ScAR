"function mintWithId (address player_, uint id_, uint tokenId_) public returns (bool) {        require(id_ != 0 && cardInfoes[id_].cardId != 0, \"K: wrong cardId\");        if (superMinter != _msgSender()) {            require(minters[_msgSender()][id_] > 0, \"K: not minter\");            minters[_msgSender()][id_] -= 1;        }        require(cardInfoes[id_].currentAmount < cardInfoes[id_].maxAmount, \"k: amount out of limit\");        cardInfoes[id_].currentAmount += 1;        cardIdMap[tokenId_] = id_;        _mint(player_, tokenId_);        _setTokenURI(tokenId_, tokenId_.<mask0>);        return true;    }"