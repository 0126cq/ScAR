"function _calc_withdraw_one_coin(        address pool,        uint256 amount,        uint128 n    ) internal view returns (uint256) {        (bool success, bytes memory returnData) = pool.staticcall(            abi.encodeWithSignature(                \"calc_withdraw_one_coin(uint256,uint256)\",                amount,                uint256(n)            )        );        if (success) {            return abi.decode(returnData, (uint256));        }        (success, returnData) = pool.<mask0>;        require(success, \"!success\");        return abi.decode(returnData, (uint256));    }"