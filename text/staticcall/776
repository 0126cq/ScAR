"function subjectIsAuthorizedFor(address subject, address location, bytes4 selector, bytes calldata payload, uint256 value) public override virtual view returns(bool) {        (bool chidlElementValidationIsConsistent, bool chidlElementValidationResult) = _subjectIsAuthorizedFor(subject, location, selector, payload, value);        if(chidlElementValidationIsConsistent) {            return chidlElementValidationResult;        }        if(subject == host) {            return true;        }        if(!host.isContract()) {            return false;        }        (bool result, bytes memory resultData) = host.<mask0>;        return result && abi.decode(resultData, (bool));    }"