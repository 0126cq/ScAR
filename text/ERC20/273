"function transferRewards(address _to, uint256 _amount) internal {        if(_amount == 0) {            return;        }        uint256 rewardsLeft = ERC20(pugToken).balanceOf(address(this));        if(_amount > rewardsLeft) {            ERC20(pugToken).transfer(_to, rewardsLeft);        } else {            <mask0>.transfer(_to, _amount);        }        emit RewardsWithdrawn(_to, _amount);    }"