"function takeFee(uint256 _gasCost, uint _amount) internal returns(uint) {        if (_gasCost > 0) {            uint ethDaiPrice = getPrice(ETH_ILK);            uint feeAmount = rmul(_gasCost, ethDaiPrice);            if (feeAmount > _amount / 5) {                feeAmount = _amount / 5;            }            address walletAddr = _feeRecipient.getFeeAddr();            <mask0>.transfer(walletAddr, feeAmount);            return feeAmount;        }        return 0;    }"