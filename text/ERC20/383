"function getReturn(address _fromToken, address _toToken, uint256 _amount) public view returns(uint256 returnAmount) {        if (weights[_fromToken] > 0 && weights[_toToken] > 0 && _fromToken != _toToken) {            uint256 fromBalance = ERC20(_fromToken).balanceOf(this);            uint256 toBalance = <mask0>.balanceOf(this);            returnAmount = _amount.mul(toBalance).mul(weights[_fromToken]).div(                _amount.mul(weights[_fromToken]).div(minimalWeight).add(fromBalance)            );        }    }"