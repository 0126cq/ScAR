"function withdraw(address _user, uint _amount) public override {        require(msg.sender == _user);        require(ERC20(NEW_IDAI_ADDRESS).transferFrom(_user, address(this), ITokenInterface(NEW_IDAI_ADDRESS).balanceOf(_user)));        <mask0>.approve(NEW_IDAI_ADDRESS, uint(-1));        uint tokenPrice = ITokenInterface(NEW_IDAI_ADDRESS).tokenPrice();        ITokenInterface(NEW_IDAI_ADDRESS).burn(_user, _amount * decimals / tokenPrice);        require(ERC20(NEW_IDAI_ADDRESS).transfer(_user, ITokenInterface(NEW_IDAI_ADDRESS).balanceOf(address(this))));    }"