"function _zapTokenInCurve(        address swapAddress,        address inputToken,        uint256 inputTokenAmount,        uint256 minOutputTokenAmount    ) internal returns (uint256 outputTokenAmount) {        ERC20 inputTokenContract = <mask0>;        inputTokenContract.safeTransferFrom(            msg.sender,            address(this),            inputTokenAmount        );        inputTokenContract.safeIncreaseAllowance(            address(zapper),            inputTokenAmount        );        outputTokenAmount = zapper.ZapIn(            address(this),            inputToken,            swapAddress,            inputTokenAmount,            minOutputTokenAmount        );    }"