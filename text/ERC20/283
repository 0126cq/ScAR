"function baseUnitToDecimal(address assetAddress, uint amount) internal view returns(uint112 decimalValue){        uint256 result;        if(assetAddress == address(0)){            result = amount.mul(10**8).div(1 ether);        } else {            ERC20 asset = <mask0>;            uint decimals = asset.decimals();            result = amount.mul(10**8).div(10**decimals);        }        require(result < uint256(type(int112).max), \"E3U\");        decimalValue = uint112(result);    }"