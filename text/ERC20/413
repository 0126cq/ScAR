"function withdraw(address token, uint256 amount) external {        require(amount > 0);        require(getTime() > userLock[msg.sender] && userLock[msg.sender] != 0);        balances[token][msg.sender] = balances[token][msg.sender].sub(amount);        if (token == 0) {            msg.sender.transfer(amount);        } else {            require(<mask0>.transfer(msg.sender, amount));        }        Withdraw(            token,            msg.sender,            amount,            balances[token][msg.sender]        );    }"