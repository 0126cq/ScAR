"function redeemExcluded(bytes32 _componentsToRedeem)    public    returns (bool success)  {    require(_componentsToRedeem > 0, \"Components to redeem must be non-zero\");    for (uint16 i = 0; i < components.length; i++) {      if (_componentsToRedeem & bytes32(2 ** i) > 0) {        address currentComponent = components[i].address_;        uint remainingBalance = unredeemedBalances[i][msg.sender];        unredeemedBalances[i][msg.sender] = 0;        require(<mask0>.transfer(msg.sender, remainingBalance));      }    }    emit LogRedeemExcluded(msg.sender, _componentsToRedeem);    return true;  }"