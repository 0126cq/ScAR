"function payback(address _market, address _tokenAddr, uint256 _amount, uint256 _rateMode) public burnGas(3) payable {        address lendingPool = ILendingPoolAddressesProviderV2(_market).getLendingPool();        _tokenAddr = changeToWeth(_tokenAddr);        if (_tokenAddr == WETH_ADDRESS) {            TokenInterface(WETH_ADDRESS).deposit{value: msg.value}();        } else {            uint amountToPull = min(_amount, <mask0>.balanceOf(msg.sender));            ERC20(_tokenAddr).safeTransferFrom(msg.sender, address(this), amountToPull);        }        approveToken(_tokenAddr, lendingPool);        ILendingPoolV2(lendingPool).repay(_tokenAddr, _amount, _rateMode, payable(address(this)));        if (_tokenAddr == WETH_ADDRESS) {            TokenInterface(WETH_ADDRESS).withdraw(ERC20(WETH_ADDRESS).balanceOf(address(this)));            _tokenAddr = ETH_ADDR;        }        withdrawTokens(_tokenAddr);    }"