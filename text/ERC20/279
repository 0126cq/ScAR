"function receiveApproval(address _from, uint256 _value, address _token, bytes _extraData) onlyOwner public {        require(_token != address(0));        require(_from != address(0));        ERC20 tk = <mask0>;        require(tk.transferFrom(_from, this, _value));        bytes32 timestamp = bytesToBytes32(_extraData);        deposRecs.push(TransferRecord(uint256(timestamp), _from, _value));        emit ReceiveDeposit(_from, _value, _token, _extraData);    }"