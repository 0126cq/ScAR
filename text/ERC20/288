"function _swapEthToToken(        address[] memory swapPath,        uint256 exchangeAmount,        uint256 deadline,        uint256 slippage,        uint256 tokensPerEth    ) internal returns (uint256) {        uint256[] memory amounts =            sushiswapRouter.getAmountsOut(exchangeAmount, swapPath);        uint256 sushiAmount = amounts[amounts.length - 1];        uint256 portion = _calculatePortion(sushiAmount, slippage);        uint256 calculatedPrice =            (exchangeAmount.mul(tokensPerEth)).div(10**18);        uint256 decimals = <mask0>.decimals();        if (decimals < 18) {            calculatedPrice = calculatedPrice.mul(10**(18 - decimals));        }        if (sushiAmount > calculatedPrice) {            require(                sushiAmount.sub(calculatedPrice) <= portion,                \"PRICE_ERROR_1\"            );        } else {            require(                calculatedPrice.sub(sushiAmount) <= portion,                \"PRICE_ERROR_2\"            );        }        uint256[] memory swapResult =            sushiswapRouter.swapExactETHForTokens{value: exchangeAmount}(                _getMinAmount(sushiAmount, slippage),                swapPath,                address(this),                deadline            );        return swapResult[swapResult.length - 1];    }"