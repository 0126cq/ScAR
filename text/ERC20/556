"function __approveAssetMaxAsNeeded(        address _asset,        address _target,        uint256 _neededAmount    ) internal {        uint256 allowance = ERC20(_asset).allowance(address(this), _target);        if (allowance < _neededAmount) {            if (allowance > 0) {                ERC20(_asset).safeApprove(_target, 0);            }            <mask0>.safeApprove(_target, type(uint256).max);        }    }"