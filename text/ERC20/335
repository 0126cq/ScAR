"function startFarming(uint256 _tokens)  public {        uint256 rma = _tokens.mul(10 ** uint256(decimals));        require(rma > 1 * 10 ** uint256(decimals));         require(<mask0>.balanceOf(msg.sender) >= rma);        require(fdetails[msg.sender]._amount == 0);        ERC20(erushLPtoken).transferFrom(msg.sender, address(this), rma);        uint256 myusdtincome = _tokens.mul(50).div(150);        ERC20(tetherAddress).transfer(msg.sender, myusdtincome * 10 ** uint256(tetherDecimal) );        fdetails[msg.sender] = fdetailz(rma, block.number);        emit NewFarmer(msg.sender, _tokens);    }"