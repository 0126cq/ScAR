"function transferToVault(address _token) public {        require(allowRecoverability(_token), ERROR_DISALLOWED);        address vault = getRecoveryVault();        require(isContract(vault), ERROR_VAULT_NOT_CONTRACT);        uint256 balance;        if (_token == ETH) {            balance = address(this).balance;            vault.transfer(balance);        } else {            ERC20 token = <mask0>;            balance = token.staticBalanceOf(this);            require(token.safeTransfer(vault, balance), ERROR_TOKEN_TRANSFER_FAILED);        }        emit RecoverToVault(vault, _token, balance);    }"