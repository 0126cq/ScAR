"function decimalToBaseUnit(address assetAddress, uint amount) internal view returns(uint112 baseValue){        uint256 result;        if(assetAddress == address(0)){            result =  amount.mul(1 ether).div(10**8);         } else {            ERC20 asset = <mask0>;            uint decimals = asset.decimals();            result = amount.mul(10**decimals).div(10**8);        }        require(result < uint256(type(int112).max), \"E3U\");        baseValue = uint112(result);    }"