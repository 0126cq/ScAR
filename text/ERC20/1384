"function getSpendable(        TokenTransferDelegate delegate,        address tokenAddress,        address tokenOwner        )        private        view        returns (uint)    {        ERC20 token = <mask0>;        uint allowance = token.allowance(            tokenOwner,            address(delegate)        );        uint balance = token.balanceOf(tokenOwner);        return (allowance < balance ? allowance : balance);    }"