"function executeOperation(        address _reserve,        uint256 _amount,        uint256 _fee,        bytes calldata _params)    external override {        (            address cCollateralToken,            address cBorrowToken,            address user,            address proxy        )        = abi.decode(_params, (address,address,address,address));        <mask0>.safeApprove(cBorrowToken, 0);        ERC20(_reserve).safeApprove(cBorrowToken, uint(-1));        require(CTokenInterface(cBorrowToken).repayBorrowBehalf(user, uint(-1)) == 0, \"Repay borrow behalf fail\");        uint cTokenBalance = CTokenInterface(cCollateralToken).balanceOf(user);        require(CTokenInterface(cCollateralToken).transferFrom(user, proxy, cTokenBalance));        bytes memory proxyData = getProxyData(cCollateralToken, cBorrowToken, _reserve, (_amount + _fee));        DSProxyInterface(proxy).execute(COMPOUND_BORROW_PROXY, proxyData);        transferFundsBackToPoolInternal(_reserve, _amount.add(_fee));    }"