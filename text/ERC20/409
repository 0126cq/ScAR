"function addAsset( address _token, address _oracle, uint256 _price)         external onlyOwner {        require ( whitelist[_token] == false, \"Formation.Fi: Token exists\");        if (_oracle == address(0)){           require(_price != 0, \"zero price\");        }        else {        require(_price == 0, \"not zero price\");        }        uint8 _decimals = 0;        if (_token!=address(0)){        _decimals = <mask0>.decimals();        }        Asset memory _asset = Asset(_token, _oracle, _price, _decimals);        indexAsset[_token] = index;        assets.push(_asset);        index = index +1;        whitelist[_token] = true;    }"