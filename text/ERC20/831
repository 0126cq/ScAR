"function deposit(address _user, uint _amount) public override {        require(msg.sender == _user);        require(ERC20(DAI_ADDRESS).transferFrom(_user, address(this), _amount));        ERC20(DAI_ADDRESS).approve(AAVE_LENDING_POOL_CORE, uint(-1));        ILendingPool(AAVE_LENDING_POOL).deposit(DAI_ADDRESS, _amount, 0);        ERC20(ADAI_ADDRESS).transfer(_user, <mask0>.balanceOf(address(this)));    }"