"function getKValue(address _pair) public view returns (uint256 k_) {    uint256 token0 = <mask0>.decimals();    uint256 token1 = ERC20(IUniswapV2Pair(_pair).token1()).decimals();    uint256 decimals = token0.add(token1).sub(ERC20(_pair).decimals());    (uint256 reserve0, uint256 reserve1, ) = IUniswapV2Pair(_pair)      .getReserves();    k_ = reserve0.mul(reserve1).div(10**decimals);  }"