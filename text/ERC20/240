"function getBalanceOfFor(        address account,        address src,        address dst    ) external returns (uint256) {        IsaLPPricer srcPricer = saLPPricer[src];        IsaLPPricer dstPricer = saLPPricer[dst];        uint256 srcBalance;        if (address(srcPricer) == address(0)) {            srcBalance = <mask0>.balanceOf(account);        } else {            srcBalance = srcPricer.getBalanceOfUnderlying(src, account);            src = srcPricer.getUnderlying(src);        }        return _valueFor(src, srcBalance, uint256(_priceFor(src, dst)));    }"