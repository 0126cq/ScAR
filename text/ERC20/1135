"function _assetToUsd(address asset, uint amount) internal view returns (uint) {        AggregatorInterface agg = AggregatorInterface(usdAggregators[asset]);        (, int256 answer, , uint256 updatedAt, ) = agg.latestRoundData();        require(updatedAt > block.timestamp - 24 hours, \"Unit Protocol: STALE_CHAINLINK_PRICE\");        require(answer >= 0, \"Unit Protocol: NEGATIVE_CHAINLINK_PRICE\");        int decimals = 18 - int(<mask0>.decimals()) - int(agg.decimals());        if (decimals < 0) {            return amount.mul(uint(answer)).mul(Q112).div(10 ** uint(-decimals));        } else {            return amount.mul(uint(answer)).mul(Q112).mul(10 ** uint(decimals));        }    }"