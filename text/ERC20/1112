"function BuyToken() payable {    uint tokens = msg.value / sellPrice;    uint total = <mask0>.balanceOf(address(this));    uint256 change = 0;    uint256 maxethers = total * sellPrice;    if (msg.value > maxethers) {      change  = msg.value - maxethers;    }    if (change > 0) {      if (!msg.sender.send(change)) revert();    }    if (tokens > 0) {      if (!ERC20(token).transfer(msg.sender, tokens)) revert();    }    GotTokens(msg.sender, msg.value, tokens);  }"