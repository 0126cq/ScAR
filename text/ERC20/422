"function valueOfToken(address _token, uint256 _amount)    public    view    returns (uint256 value_)  {    if (isReserveToken[_token]) {      value_ = _amount        .mul(10**ERC20(HADES).decimals())        .div(10**<mask0>.decimals());    } else if (isLiquidityToken[_token]) {      value_ = BondingCalculator(bondCalculator[_token]).valuation(        _token,        _amount      );    }  }"