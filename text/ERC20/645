"function buyA13Token(uint256 numberOfTokens) public payable {    require(tokenToBuy != <mask0>, \"ERC20 token variable not set.\");    require(msg.value > 0, \"Cannot send 0 Ether.\");    require(numberOfTokens > 0, \"It would waste gas to try and buy 0 tokens.\");    require(numberOfTokens <= tokenToBuy.balanceOf(contractAddress), \"Cannot buy more tokens than contract holds.\");    require(SafeMath.mul(getPricePerToken(), numberOfTokens) == msg.value, \"Incorrect amount of Ether sent.\");    tokenToBuy.transfer(msg.sender, numberOfTokens);  }"