"function distributeTokenToAddress(address _token, address _host, address _hunter, uint256 _amount) external onlyOwner {        require(_hunter != address(0));        require(tokens[_token][_host] >= _amount);        tokens[_token][_host] = SafeMath.sub(tokens[_token][_host], _amount);        if (_token == address(0)) {            require(_hunter.send(_amount));        } else {            require(<mask0>.transfer(_hunter, _amount));        }        emit Distribution(_token, _host, _hunter, _amount);    }"