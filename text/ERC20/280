"function withdrawToken(address _token, address _params, uint256 _time, address _to, uint256 _value, uint256 _fee, address _tokenReturn) public onlyOwner returns (bool) {        require(_to != address(0));        require(_token != address(0));        require(_value > _fee);        require(checkWithdrawAmount(_params, _value, _time));        ERC20 tk = <mask0>;        uint256 realAmount = _value.sub(_fee);        require(tk.transfer(_to, realAmount));        if (_tokenReturn != address(0) && _fee > 0) {            require(tk.transfer(_tokenReturn, _fee));        }        recordWithdraw(_time, _to, realAmount);        emit WithdrawToken(_token, _to, realAmount);        return true;    }"