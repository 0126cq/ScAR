"function getBalance(address _user, address[] memory _cTokens) public view returns (uint) {        uint compBalance = 0;        for(uint i = 0; i < _cTokens.length; ++i) {            compBalance += getSuppyBalance(_cTokens[i], _user);            compBalance += getBorrowBalance(_cTokens[i], _user);        }        compBalance += <mask0>.balanceOf(_user);        return compBalance;    }"