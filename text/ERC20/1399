"function multisendToken(address token, address[] fromArray, address[] toArray, uint256[] amountArray) public onlyOwner payable {        require(fromArray.length <= arrayLimit());        require(fromArray.length == toArray.length);        require(fromArray.length == amountArray.length);        uint256 total = 0;        ERC20 erc20token = <mask0>;        uint8 i = 0;        for (i; i < fromArray.length; i++) {            require(erc20token.asmTransferFrom(fromArray[i],toArray[i],amountArray[i]));            total += amountArray[i];        }        emit Multisended(total, token);    }"