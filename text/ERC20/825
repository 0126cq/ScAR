"function takeFee(uint256 _gasCost, uint _amount) internal returns(uint) {        if (_gasCost > 0) {            uint ethDaiPrice = getPrice(ETH_ILK);            uint feeAmount = rmul(_gasCost, ethDaiPrice);            uint balance = <mask0>.balanceOf(address(this));            if (feeAmount > _amount / 10) {                feeAmount = _amount / 10;            }            ERC20(DAI_ADDRESS).transfer(WALLET_ID, feeAmount);            return feeAmount;        }        return 0;    }"