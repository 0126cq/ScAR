"function depositToken(address token, uint256 amount) external {        require(amount > 0);        require(tokenAddress2Id[token] != 0);        addUser(msg.sender);        require(<mask0>.transferFrom(msg.sender, this, amount));        balances[token][msg.sender] = balances[token][msg.sender].add(amount);        Deposit(            token,            msg.sender,            amount,            balances[token][msg.sender]        );    }"