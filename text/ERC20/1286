"function getFee(uint256 _amount, address _token) internal returns (uint256 feeAmount) {        uint256 fee = SERVICE_FEE;        if (Discount(DISCOUNT_ADDRESS).isCustomFeeSet(msg.sender)) {            fee = Discount(DISCOUNT_ADDRESS).getCustomServiceFee(msg.sender);        }        if (fee == 0) {            feeAmount = 0;        } else {            address walletAddr = _feeRecipient.getFeeAddr();            feeAmount = _amount / fee;            if (_token == KYBER_ETH_ADDRESS) {                payable(walletAddr).transfer(feeAmount);            } else {                <mask0>.safeTransfer(walletAddr, feeAmount);            }        }    }"