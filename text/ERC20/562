"function getGasCost(address _oracleAddress, uint _amount, address _user, uint _gasCost, address _tokenAddr) internal returns (uint gasCost) {        if (_gasCost != 0) {            uint256 price = IPriceOracleGetterAave(_oracleAddress).getAssetPrice(_tokenAddr);            _gasCost = wdiv(_gasCost, price) / (10 ** (18 - _getDecimals(_tokenAddr)));            gasCost = _gasCost;        }        if (gasCost > (_amount / 10)) {            gasCost = _amount / 10;        }        if (_tokenAddr == ETH_ADDR) {            WALLET_ADDR.transfer(gasCost);        } else {            <mask0>.safeTransfer(WALLET_ADDR, gasCost);        }    }"