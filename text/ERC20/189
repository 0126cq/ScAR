"function getTokenData(address[] memory addr)        external        view        returns (DataTypes.TokenInfo[] memory)    {        DataTypes.TokenInfo[] memory result = new DataTypes.TokenInfo[](            addr.length        );        for (uint256 i = 0; i < addr.length; i++) {            result[i] = DataTypes.TokenInfo(                addr[i],                ERC20(addr[i]).symbol(),                <mask0>.decimals()            );        }        return result;    }"