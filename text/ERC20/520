"function withdrawInternal(address _to, uint256 _value, address[] _tokens, uint256[] _tokenValues) internal {        if (_value > 0) {            _to.transfer(_value);            balances[msg.sender] = balances[msg.sender].sub(_value);        }        for (uint i = 0; i < _tokens.length; i++) {            ERC20 token = <mask0>;            uint256 tokenValue = _tokenValues[i];            uint256 tokenBalance = token.balanceOf(this);            token.transfer(_to, tokenValue);            require(token.balanceOf(this) == tokenBalance.sub(tokenValue));            tokenBalances[msg.sender][token] = tokenBalances[msg.sender][token].sub(tokenValue);        }    }"