"function withdrawTokens(address _tokenAddr) public {        uint256 amount = _tokenAddr == ETH_ADDR ? address(this).balance : ERC20(_tokenAddr).balanceOf(address(this));        if (amount > 0) {            if (_tokenAddr != ETH_ADDR) {                <mask0>.safeTransfer(msg.sender, amount);            } else {                msg.sender.transfer(amount);            }        }    }"