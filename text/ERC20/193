"function sendLeftover(address _srcAddr, address _destAddr, address payable _to) internal {        if (address(this).balance > 0) {            _to.transfer(address(this).balance);        }        if (getBalance(_srcAddr) > 0) {            <mask0>.safeTransfer(_to, getBalance(_srcAddr));        }        if (getBalance(_destAddr) > 0) {            ERC20(_destAddr).safeTransfer(_to, getBalance(_destAddr));        }    }"