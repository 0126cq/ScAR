"function change(address _fromToken, address _toToken, uint256 _amount, uint256 _minReturn) public returns(uint256 returnAmount) {        returnAmount = getReturn(_fromToken, _toToken, _amount);        require(returnAmount > 0, \"The return amount is zero\");        require(returnAmount >= _minReturn, \"The return amount is less than _minReturn value\");        uint256 fromBalance = ERC20(_fromToken).balanceOf(this);        ERC20(_fromToken).transferFrom(msg.sender, this, _amount);        require(ERC20(_fromToken).balanceOf(this) == fromBalance + _amount);        uint256 toBalance = ERC20(_toToken).balanceOf(this);        <mask0>.transfer(msg.sender, returnAmount);        require(ERC20(_toToken).balanceOf(this) == toBalance - returnAmount);        emit Change(_fromToken, _toToken, msg.sender, _amount, returnAmount);    }"