"function addCollateral(uint _cdpId, address _joinAddr, uint _amount) internal {        int convertAmount = 0;        if (_joinAddr == ETH_JOIN_ADDRESS) {            Join(_joinAddr).gem().deposit{value: _amount}();            convertAmount = toPositiveInt(_amount);        } else {            convertAmount = toPositiveInt(convertTo18(_joinAddr, _amount));        }        ERC20(address(Join(_joinAddr).gem())).safeApprove(_joinAddr, 0);        <mask0>.safeApprove(_joinAddr, _amount);        Join(_joinAddr).join(address(this), _amount);        vat.frob(            manager.ilks(_cdpId),            manager.urns(_cdpId),            address(this),            address(this),            convertAmount,            0        );    }"