"function withdraw(address _tokenAddr, address _cTokenAddr, uint _amount, bool _isCAmount) public burnGas(5) {        if (_isCAmount) {            require(CTokenInterface(_cTokenAddr).redeem(_amount) == 0);        } else {            require(CTokenInterface(_cTokenAddr).redeemUnderlying(_amount) == 0);        }        if (_tokenAddr != ETH_ADDR) {            ERC20(_tokenAddr).safeTransfer(msg.sender, <mask0>.balanceOf(address(this)));        } else {            msg.sender.transfer(address(this).balance);        }    }"