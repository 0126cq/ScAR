"function withdrawTokens(address _tokenAddr) public {        uint256 amount = _tokenAddr == ETH_ADDR ? address(this).balance : <mask0>.balanceOf(address(this));        if (amount > 0) {            if (_tokenAddr != ETH_ADDR) {                ERC20(_tokenAddr).safeTransfer(msg.sender, amount);            } else {                msg.sender.transfer(amount);            }        }    }"