"function TokenVesting(    address _beneficiary,    uint256 _start,    uint256 _cliff,    uint256 _duration,    bool    _revocable,    address _token  ) {    require(_beneficiary != 0x0);    require(_cliff <= _duration);    beneficiary = _beneficiary;    start       = _start;    cliff       = _start.add(_cliff);    duration    = _duration;    revocable   = _revocable;    token       = <mask0>;  }"