"function borrow(address _cCollToken, address _cBorrowToken, address _borrowToken, uint _amount) public {        address[] memory markets = new address[](2);        markets[0] = _cCollToken;        markets[1] = _cBorrowToken;        ComptrollerInterface(COMPTROLLER_ADDR).enterMarkets(markets);        require(CTokenInterface(_cBorrowToken).borrow(_amount) == 0);        if (_borrowToken != ETH_ADDR) {            <mask0>.safeTransfer(msg.sender, ERC20(_borrowToken).balanceOf(address(this)));        } else {            msg.sender.transfer(address(this).balance);        }    }"