"function getBalance(address _user, address[] memory _cTokens) public view returns (uint256) {        uint256 compBalance = 0;        for (uint256 i = 0; i < _cTokens.length; ++i) {            compBalance += getSuppyBalance(_cTokens[i], _user);            compBalance += getBorrowBalance(_cTokens[i], _user);        }        compBalance = add_(comp.compAccrued(_user), compBalance);        compBalance += <mask0>.balanceOf(_user);        return compBalance;    }"