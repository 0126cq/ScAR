"function deposit(uint256 _value, address _token) external payable {        uint256 receivedValue;        if (_token == ETHEREUM) {            require(                _value == msg.value,                \"DarknodePayment: mismatched deposit value\"            );            receivedValue = msg.value;            address(store).transfer(msg.value);        } else {            require(                msg.value == 0,                \"DarknodePayment: unexpected ether transfer\"            );            require(                registeredTokenIndex[_token] != 0,                \"DarknodePayment: token not registered\"            );            receivedValue = <mask0>.safeTransferFromWithFees(                msg.sender,                address(store),                _value            );        }        emit LogPaymentReceived(msg.sender, _token, receivedValue);    }"