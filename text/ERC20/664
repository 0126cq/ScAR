"function borrowAaveV2(        address _lendingPoolV2,        address _tokenAddr,        uint256 _amount,        uint256 _type    ) internal {        _tokenAddr = _tokenAddr == ETH_ADDR ? WETH_ADDRESS : _tokenAddr;        ILendingPoolV2(_lendingPoolV2).borrow(            _tokenAddr,            _amount,            _type,            AAVE_REFERRAL_CODE,            address(this)        );        <mask0>.safeTransfer(msg.sender, _amount);    }"