"function readUint8Uint8(    bytes memory data,    uint256 index  ) internal pure returns (    uint8 a,    uint8 b,    uint256 newIndex  ) {    assembly {      let word := mload(add(index, add(32, data)))      a := shr(248, word)      b := and(shr(240, word), 0xff)      newIndex := add(index, 2)    }    <mask0>;    require(newIndex <= data.length, \"LibBytes#readUint8Uint8: OUT_OF_BOUNDS\");  }"