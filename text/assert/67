"function _addToLockedInPendingDelegations(address holder, uint amount) private returns (uint) {        uint currentMonth = _getCurrentMonth();        if (_lockedInPendingDelegations[holder].month < currentMonth) {            _lockedInPendingDelegations[holder].amount = amount;            _lockedInPendingDelegations[holder].month = currentMonth;        } else {            <mask0>;            _lockedInPendingDelegations[holder].amount = _lockedInPendingDelegations[holder].amount.add(amount);        }    }"