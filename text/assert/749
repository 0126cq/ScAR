"function AssetPool(address _bpTokenAddress, uint _baseLockPercent, uint _startLockTime, uint _stopLockTime, uint _linearRelease) {        <mask0>;        baseLockPercent = _baseLockPercent;        startLockTime = _startLockTime;        stopLockTime = _stopLockTime;        linearRelease = _linearRelease;        bpTokenAddress = _bpTokenAddress;        bp = BPToken(bpTokenAddress);        owner = msg.sender;    }"