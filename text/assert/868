"function transferFrom(address _from, address _to, uint _value) whenNotPaused public returns (bool) {        <mask0>;        assert(approvals[_from][msg.sender] >= _value);        approvals[_from][msg.sender] = approvals[_from][msg.sender].sub(_value);        balanceOf[_from] = balanceOf[_from].sub(_value);        balanceOf[_to] = balanceOf[_to].add(_value);        emit Transfer(_from, _to, _value);        return true;    }"