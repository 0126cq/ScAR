"function capToMax(Exp memory anchorPrice, Exp memory price) view internal returns (Error, bool, Exp memory) {        Exp memory one = Exp({mantissa : mantissaOne});        Exp memory onePlusMaxSwing;        Exp memory oneMinusMaxSwing;        Exp memory max;        Exp memory min;        Error err;        (err, onePlusMaxSwing) = addExp(one, maxSwing);        if (err != Error.NO_ERROR) {            return (err, false, Exp({mantissa : 0}));        }        (err, max) = mulExp(anchorPrice, onePlusMaxSwing);        if (err != Error.NO_ERROR) {            return (err, false, Exp({mantissa : 0}));        }        if (greaterThanExp(price, max)) {            return (Error.NO_ERROR, true, max);        }        (err, oneMinusMaxSwing) = subExp(one, maxSwing);        if (err != Error.NO_ERROR) {            return (err, false, Exp({mantissa : 0}));        }        (err, min) = mulExp(anchorPrice, oneMinusMaxSwing);        <mask0>;        if (lessThanExp(price, min)) {            return (Error.NO_ERROR, true, min);        }        return (Error.NO_ERROR, false, price);    }"