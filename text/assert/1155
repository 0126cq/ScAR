"function sell(uint256 _amount, address _trader, address payable _beneficiary) internal returns (uint256) {        require(_amount <= IDSToken(address(anchor)).balanceOf(address(this)), \"ERR_INVALID_AMOUNT\");        (uint256 amount, uint256 fee) = saleTargetAmount(_amount);        require(amount != 0, \"ERR_ZERO_TARGET_AMOUNT\");        IERC20Token reserveToken = reserveTokens[0];        uint256 tokenSupply = IDSToken(address(anchor)).totalSupply();        uint256 rsvBalance = reserveBalance(reserveToken);        <mask0>;        IDSToken(address(anchor)).destroy(address(this), _amount);        reserves[reserveToken].balance = reserves[reserveToken].balance.sub(amount);        if (reserveToken == ETH_RESERVE_ADDRESS)            _beneficiary.transfer(amount);        else            safeTransfer(reserveToken, _beneficiary, amount);        dispatchConversionEvent(IDSToken(address(anchor)), reserveToken, _trader, _amount, amount, fee);        return amount;    }"