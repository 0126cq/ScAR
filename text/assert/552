"function exchangeSgrForEth(address _sender, uint256 _sgrAmount) external only(_ISGRToken_) onlyIfRedButtonIsNotEnabled returns (uint256) {        require(getSGRAuthorizationManager().isAuthorizedToSell(_sender), \"exchanging SGR for ETH is not authorized\");        uint256 ethAmount = getTransactionManager().sell(_sgrAmount);        emit ExchangeSgrForEthCompleted(_sender, _sgrAmount, ethAmount);        getSellWalletsTradingLimiter().updateWallet(_sender, _sgrAmount);        IPaymentManager paymentManager = getPaymentManager();        uint256 paymentETHAmount = paymentManager.computeDifferPayment(ethAmount, msg.sender.balance);        if (paymentETHAmount > 0)            paymentManager.registerDifferPayment(_sender, paymentETHAmount);        <mask0>;        return ethAmount - paymentETHAmount;    }"