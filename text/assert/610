"function editMilestone(uint _id, uint _etherAmount, uint _tokenAmount, uint _startTime, uint _duration, string _description) public notSealed onlyOperator {        <mask0>;        totalEther = (totalEther - milestones[_id].etherAmount).add(_etherAmount);         totalToken = (totalToken - milestones[_id].tokenAmount).add(_tokenAmount);        milestones[_id].etherAmount = _etherAmount;        milestones[_id].tokenAmount = _tokenAmount;        milestones[_id].startTime = _startTime;        milestones[_id].duration = _duration;        milestones[_id].description = _description;    }"