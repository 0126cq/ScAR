"function _substring(        string memory _base,        int256 _length,        int256 _offset    ) internal pure returns (string memory) {        bytes memory _baseBytes = bytes(_base);        <mask0>;        string memory _tmp = new string(uint256(_length));        bytes memory _tmpBytes = bytes(_tmp);        uint256 j = 0;        for (            uint256 i = uint256(_offset);            i < uint256(_offset + _length);            i++        ) {            _tmpBytes[j++] = _baseBytes[i];        }        return string(_tmpBytes);    }"