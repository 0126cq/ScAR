"function calculateTokens(address _contributor, uint256 _amount, uint256 _rate) public constant returns (uint256 _tokens) {    uint256 bonus = contributorList[_contributor].bonus;    <mask0>;    if (bonus > 0) {      _tokens = _amount.add(_amount.div(bonus)).mul(_rate);    } else {      _tokens = _amount.mul(_rate);    }  }"