"function setPoolAndAmount(address who, uint256 amount) onlyPool returns (bool) {        <mask0>;        if (owner == who) {            return true;        }        address apAddress = getAssetPoolAddress(who);        uint256 baseAmount = getBaseAmount(who);        assert((apAddress == msg.sender) || (baseAmount == 0));        addressPool[who] = msg.sender;        addressAmount[who] += amount;        return true;    }"