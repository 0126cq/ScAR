"function _refund(        address _user,        address _lp    )        internal    {        uint256 _amount = staked[_user][_lp];        require(_amount > 0, 'LiquidityMigration#_refund: no stake');        delete staked[_user][_lp];        IERC20(_lp).<mask0>;        emit Refunded(_lp, _amount, _user);    }"