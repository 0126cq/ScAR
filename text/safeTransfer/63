"function withdraw(uint256 pid, uint256 amount) public {        PoolInfo memory pool = updatePool(pid);        UserInfo memory user = userInfo[pid][msg.sender];        user.rewardDebt = user.rewardDebt - int256((amount * pool.accGroPerShare) / ACC_GRO_PRECISION);        user.amount = user.amount - amount;        userInfo[pid][msg.sender] = user;        pool.lpToken.<mask0>;        emit LogWithdraw(msg.sender, pid, amount);    }"