"function _transferDmgOut(        address recipient,        uint amount,        address dmgToken,        uint contractDmgBalance    ) internal returns (uint) {        if (contractDmgBalance < amount) {            IERC20(dmgToken).<mask0>;            return contractDmgBalance;        } else {            IERC20(dmgToken).safeTransfer(recipient, amount);            return amount;        }    }"