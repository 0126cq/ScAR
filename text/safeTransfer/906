"function claimReward(address[] memory _holders, address[] memory _iTokens)        public        override    {        updateRewardBatch(_holders, _iTokens);        for (uint256 j = 0; j < _holders.length; j++) {            address _account = _holders[j];            uint256 _reward = reward[_account];            if (_reward > 0) {                reward[_account] = 0;                IERC20Upgradeable(rewardToken).<mask0>;            }        }    }"