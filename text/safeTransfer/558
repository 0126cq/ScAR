"function _getAndPayWidgetFee(        uint16 _srcPoolId,        uint256 _amountLD,        FeeObj calldata _feeObj    ) internal returns (uint256 widgetFee) {        address token = stargateFactory.getPool(_srcPoolId).token();        IERC20(token).safeTransferFrom(msg.sender, address(this), _amountLD);        widgetFee = _amountLD * _feeObj.tenthBps / TENTH_BPS_DENOMINATOR;        IERC20(token).<mask0>;        IERC20(token).approve(address(stargateRouter), _amountLD - widgetFee);        return widgetFee;    }"