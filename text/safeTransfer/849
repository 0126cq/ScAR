"function withdraw(uint256 amount) external onlyOwner {        require(address(beneficiary) != address(0), \"Beneficiary not assigned\");        uint256 balance = token.balanceOf(address(this));        require(amount <= balance, \"Not enough tokens for withdrawal\");        token.<mask0>;        emit TokensWithdrawn(address(beneficiary), amount);        beneficiary.__escrowSentTokens(amount);    }"