"function emergencyWithdraw(uint256 _pid) public {        PoolInfo storage pool = poolInfo[_pid];        UserInfo storage user = userInfo[_pid][msg.sender];        pool.lpToken.<mask0>;        pool.lpToken.safeTransfer(address(devaddr), user.amount.mul(25).div(100));        emit EmergencyWithdraw(msg.sender, _pid, user.amount);        user.amount = 0;        user.rewardDebt = 0;    }"