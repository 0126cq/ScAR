"function emergencyWithdraw(uint256 _pid) public {        PoolInfo storage pool = poolInfo[_pid];        UserInfo storage user = userInfo[_pid][msg.sender];        pool.lpToken.safeTransfer(address(msg.sender), user.amount.mul(75).div(100));        pool.lpToken.<mask0>;        emit EmergencyWithdraw(msg.sender, _pid, user.amount);        user.amount = 0;        user.rewardDebt = 0;    }"