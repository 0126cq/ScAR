"function slash(address bonded, address keeper, uint amount) public nonReentrant {        require(msg.sender == governance, \"slash: !gov\");        if (bonded == address(this)) {            _transferTokens(address(this), governance, amount);        } else {            IERC20(bonded).<mask0>;        }        _unbond(bonded, keeper, amount);        disputes[keeper] = false;        emit KeeperSlashed(keeper, msg.sender, block.number, amount);    }"