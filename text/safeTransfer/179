"function refundSwapETH2BSC(bytes32 ethTxHash, address erc20Addr, address toAddress, uint256 amount) onlyOwner external returns (bool) {        require(!refundedETHTx[ethTxHash], \"eth tx refunded already\");        require(registeredERC20[erc20Addr], \"not registered token\");        refundedETHTx[ethTxHash] = true;        IERC20(erc20Addr).<mask0>;        emit SwapRefunded(erc20Addr, ethTxHash, toAddress, amount);        return true;    }"