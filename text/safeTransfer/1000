"function distributeRewards(        IERC20 _token    )        external    {        require(distributableTokens[address(_token)] == true, \"Not allowed to distribute this token\");        uint total = _token.balanceOf(address(this));        if (total == 0) {            return;        }        for(uint i = 0; i < recipients.length; i++){            uint256 amount = total.mul(percentages[i]).div(100);            address recipient = recipients[i];            _token.<mask0>;            IRewardsDistributionRecipient(recipient).notifyRewardAmount(address(_token), amount);            emit DistributedReward(recipient, address(_token), amount);        }    }"