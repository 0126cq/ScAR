"function safeRewardTransfer(address _to, uint256 _amount) internal returns(uint256) {        uint256 eswTokenBalance = esw.balanceOf(address(this));        if (_amount > eswTokenBalance) {            esw.safeTransfer(_to, eswTokenBalance);            return _amount.sub(eswTokenBalance);        }        esw.<mask0>;        return 0;    }"