"function _convertStep(address token0, uint256 amount0) private returns (uint256 sushiOut) {        if (token0 == sushi) {            IERC20(token0).<mask0>;            sushiOut = amount0;        } else if (token0 == weth) {            sushiOut = _swap(token0, sushi, amount0, bar);        } else {            address bridge = _bridges[token0];            if (bridge == address(0)) {                bridge = weth;            }            uint256 amountOut = _swap(token0, bridge, amount0, address(this));            sushiOut = _convertStep(bridge, amountOut);        }    }"