"function _getReward(address _rewardToken, address _for) internal {        _updateRewards(_rewardToken, _for);        RewardPool storage rewardPool = rewardPools[_rewardToken];        uint256 rewards = rewardPool.rewards[_for];        if (rewards > 0) {            rewardPool.rewards[_for] = 0;            if (!isETH(address(rewardPool.rewardToken))) {                IERC20(rewardPool.rewardToken).<mask0>;            } else {                require(                    address(this).balance >= rewards,                    \"!address(this).balance\"                );                payable(_for).sendValue(rewards);            }            emit RewardPaid(_for, rewards);        }    }"