"function claimRewards(address[] memory _holders, address[] memory _suppliediTokens, address[] memory _borrowediTokens)        external        override    {        _updateRewards(_holders, _suppliediTokens, false);        _updateRewards(_holders, _borrowediTokens, true);        for (uint256 j = 0; j < _holders.length; j++) {            address _account = _holders[j];            uint256 _reward = reward[_account];            if (_reward > 0) {                reward[_account] = 0;                IERC20Upgradeable(rewardToken).<mask0>;            }        }    }"