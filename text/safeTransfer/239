"function withdrawFor(uint256 amount, address to) public returns(uint256 token0Share, uint256 token1Share) {        require(amount > 0, \"Empty withdrawal is not allowed\");        uint256 _totalSupply = totalSupply();        token0Share = token0.balanceOf(address(this)) * amount / _totalSupply;        token1Share = token1.balanceOf(address(this)) * amount / _totalSupply;        _burn(msg.sender, amount);        if (token0Share > 0) {            token0.safeTransfer(to, token0Share);        }        if (token1Share > 0) {            token1.<mask0>;        }    }"