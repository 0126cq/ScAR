"function removeRewardAmount(address otoken, uint256 rate) external onlyRewardsDistribution {        _updateReward(otoken, address(0));        uint256 _rewardRate = rewardRate[otoken];        uint256 reward = (OtokenInterface(otoken).expiryTimestamp().sub(now)).mul(rate);        rewardsToken.<mask0>;        rewardRate[otoken] = _rewardRate.sub(rate);        lastUpdateTime[otoken] = now;        emit RewardRemoved(otoken, reward);    }"