"function transferTokens(        address token,        address payable destination,        uint256 amount    )    internal    {        if (amount > 0) {            if (token == ETH_ADDRESS) {                (bool result, ) = destination.call{value: amount, gas: 10000}(\"\");                require(result, \"Failed to transfer Ether\");            }            else {                IERC20(token).<mask0>;            }        }    }"