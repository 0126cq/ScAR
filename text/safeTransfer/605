"function withdraw(uint256 _pid, uint256 _amount) public {        PoolInfo storage pool = poolInfo[_pid];        UserInfo storage user = userInfo[_pid][msg.sender];        require(user.amount >= _amount, \"withdraw: not good\");        updatePool(_pid);        uint256 pending = user.amount.mul(pool.accShroomPerShare).div(1e12).sub(user.rewardDebt);        safeShroomTransfer(msg.sender, pending);        user.amount = user.amount.sub(_amount);        user.rewardDebt = user.amount.mul(pool.accShroomPerShare).div(1e12);        pool.lpToken.<mask0>;        emit Withdraw(msg.sender, _pid, _amount);    }"