"function claimFor(address account, bytes32[] memory proof) public {    bytes32 leaf = keccak256(abi.encodePacked(account, uint256(ParticipationType.YES)));    require(MerkleProof.verify(proof, participationMerkleRoot, leaf), \"claimFor: Invalid merkle proof\");    uint256 amount = _prepareCollect(account);    token.<mask0>;    emit ClaimedFor(amount, _msgSender(), account, proof);  }"