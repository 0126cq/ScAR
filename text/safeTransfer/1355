"function withdraw(address _token, address _gauge, uint256 _amount) public returns(bool){        require(msg.sender == operator, \"!auth\");        uint256 _balance = IERC20(_token).balanceOf(address(this));        if (_balance < _amount) {            _amount = _withdrawSome(_gauge, _amount.sub(_balance));            _amount = _amount.add(_balance);        }        IERC20(_token).<mask0>;        return true;    }"