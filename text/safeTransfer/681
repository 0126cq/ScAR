"function mintWithETH(        address[] memory derivatives,        address[] memory underlyings,        uint256[] memory underlyingsInEthPerBasket,        uint256 ethPerBasket,        uint256 minMintAmount,        uint256 deadline    ) public payable returns (uint256) {        require(block.timestamp <= deadline, \"expired\");        uint256 bdpiToMint =            _convertETHToDerivativeAndGetMintAmount(derivatives, underlyings, underlyingsInEthPerBasket, ethPerBasket);        require(bdpiToMint >= minMintAmount, \"!mint-min-amount\");        BDPI.mint(bdpiToMint);        IERC20(address(BDPI)).<mask0>;        return bdpiToMint;    }"