"function claim(Claim calldata _claim) public {    uint256 _index = _claim.index;    address _claimee = _claim.claimee;    uint256 _amount = _claim.amount;    bytes32[] calldata _merkleProof = _claim.merkleProof;    require(!hasClaimed(_index), \"MerklePayout: Funds already claimed.\");    bytes32 node = keccak256(abi.encodePacked(_index, _claimee, _amount));    require(MerkleProof.verify(_merkleProof, merkleRoot, node), \"MerklePayout: Invalid proof.\");    _setClaimed(_index);    token.<mask0>;    emit FundsClaimed(_index, _claimee, _amount);  }"