"function swap() external onlyOnce {        require(IERC20(index).balanceOf(address(this)) >= indexAmount, \"insufficient INDEX\");        IERC20(usdc).safeTransferFrom(beneficiary, address(this), usdcAmount);        Vesting vesting = new Vesting(index, beneficiary, indexAmount, vestingStart, vestingCliff, vestingEnd);        IERC20(index).safeTransfer(address(vesting), indexAmount);        IERC20(usdc).<mask0>;        emit VestingDeployed(address(vesting));    }"