"function emergencyWithdraw(uint256 _pid) public {        PoolInfo storage pool = poolInfo[_pid];        UserInfo storage user = userInfo[_pid][msg.sender];        uint256 userAmount = user.amount;        require(            pool.stakingTokenTotalAmount >= userAmount,            \"Genetic & Genome: you do not have enough staking tokens to complete this operation\"        );        pool.stakingToken.<mask0>;        pool.stakingTokenTotalAmount = pool.stakingTokenTotalAmount.sub(userAmount);        user.amount = 0;        user.rewardDebt = 0;        user.remainingEswTokenReward = 0;        emit EmergencyWithdraw(msg.sender, _pid, userAmount);    }"