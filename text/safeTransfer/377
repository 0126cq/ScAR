"function liquidExit(uint256 amount) external sync {        require(block.number != latestJoinBlock[tx.origin], \"TrueFiPool: Cannot join and exit in same block\");        require(amount <= balanceOf(msg.sender), \"TrueFiPool: Insufficient funds\");        uint256 amountToWithdraw = poolValue().mul(amount).div(totalSupply());        amountToWithdraw = amountToWithdraw.mul(liquidExitPenalty(amountToWithdraw)).div(BASIS_PRECISION);        require(amountToWithdraw <= liquidValue(), \"TrueFiPool: Not enough liquidity in pool\");        _burn(msg.sender, amount);        ensureSufficientLiquidity(amountToWithdraw);        token.<mask0>;        emit Exited(msg.sender, amountToWithdraw);    }"