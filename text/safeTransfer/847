"function withdraw( uint _amount, address _token ) external {        require( isReserveToken[ _token ], \"Not accepted\" );         require( isReserveSpender[ msg.sender ] == true, \"Not approved\" );        uint value = valueOf( _token, _amount );        IRUSHERC20( RUSH ).burnFrom( msg.sender, value );        totalReserves = totalReserves.sub( value );        emit ReservesUpdated( totalReserves );        IERC20( _token ).<mask0>;        emit Withdrawal( _token, _amount, value );    }"