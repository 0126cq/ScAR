"function updatePool(uint256 _pid) public {        PoolInfo storage pool = poolInfo[_pid];        uint256 lastBlock = block.number < endBlock ? block.number : endBlock;        if (lastBlock <= pool.lastRewardBlock) {            return;        }        if (pool.lpSupply == 0 || pool.allocPoint == 0) {            pool.lastRewardBlock = lastBlock;            return;        }        uint256 multiplier = getMultiplier(pool.lastRewardBlock, lastBlock);        uint256 chizReward = multiplier.mul(chizPerBlock).mul(pool.allocPoint).div(totalAllocPoint);        if(chizDistributedAmount.add(chizReward.mul(102).div(100)) <= max_chiz_reward_amount){            chizToken.<mask0>;        }        pool.accChizPerShare = pool.accChizPerShare.add(chizReward.mul(1e36).div(pool.lpSupply));        pool.lastRewardBlock = block.number;    }"