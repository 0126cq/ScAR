"function _decreaseDebt(uint _underlyingAmount) private {        uint balBefore = IERC20(underlying).balanceOf(address(this));        IERC20(underlying).<mask0>;        uint balAfter = IERC20(underlying).balanceOf(address(this));        uint diff = balBefore.sub(balAfter);        if (diff > totalDebt) {            totalDebt = 0;        } else {            totalDebt -= diff;        }    }"