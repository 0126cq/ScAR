"function getReward() public updateReward(msg.sender) checkhalve {        uint256 reward = earned(msg.sender);        if (reward > 0) {            rewards[msg.sender] = 0;            uint256 nowTotalSupply = REWARD_TOKEN.totalSupply();            reward = reward.mul(nowTotalSupply).div(origTotalSupply);            REWARD_TOKEN.<mask0>;            emit RewardPaid(msg.sender, reward);        }    }"