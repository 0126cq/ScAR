"function withdraw(uint256 _pid, uint256 _amount) public {        PoolInfo storage pool = poolInfo[_pid];        UserInfo storage user = userInfo[_pid][msg.sender];        require(            user.amount >= _amount,            \"Genetic & Genome: you do not have enough tokens to complete this operation\"        );        updatePool(_pid);        uint256 pending =            user.amount.mul(pool.accEswPerShare).div(1e12).sub(user.rewardDebt).add(user.remainingEswTokenReward);        pool.stakingToken.<mask0>;        user.remainingEswTokenReward = safeRewardTransfer(msg.sender, pending);        user.amount = user.amount.sub(_amount);        require(            pool.stakingTokenTotalAmount >= _amount,            \"Genetic & Genome: you do not have enough staking tokens to complete this operation\"        );        pool.stakingTokenTotalAmount = pool.stakingTokenTotalAmount.sub(_amount);        user.rewardDebt = user.amount.mul(pool.accEswPerShare).div(1e12);        emit Withdraw(msg.sender, _pid, _amount);    }"