"function emergencyWithdraw(uint256 _pid) public override nonReentrant {        _validatePoolId(_pid);        Pool storage pool = pools[_pid];        User storage user = users[_pid][msg.sender];        pool.lpToken.<mask0>;        emit EmergencyWithdraw(msg.sender, _pid, user.lptAmount);        if (user.pendedCvp > 0) {            if (user.pendedCvp > cvpVestingPool) {                cvpVestingPool = cvpVestingPool.sub(user.pendedCvp);            } else {                cvpVestingPool = 0;            }        }        user.lptAmount = 0;        user.cvpAdjust = 0;        user.pendedCvp = 0;        user.vestingBlock = 0;        _doCheckpointVotes(msg.sender);    }"