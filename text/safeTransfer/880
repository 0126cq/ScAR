"function emergencyTransfer(        address _token,        address _destination,        uint256 _amount    ) public {        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), \"Admin only\");        if (_token != ETH_TOKEN_ADDRESS) {            IERC20(_token).<mask0>;        } else {            payable(_destination).sendValue(_amount);        }        emit EmergencyTransfer(_token, _destination, _amount);    }"