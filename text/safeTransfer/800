"function claimReward() public override {        BalanceData memory userData = balance[msg.sender];        userData.rewardAmount = _calcNextReward(userData, currentTerm);        userData.term = uint64(currentTerm);        require(userData.rewardAmount > 0, \"No Reward\");        uint256 rewardAmount = userData.rewardAmount;        userData.rewardAmount = 0;        balance[msg.sender] = userData;        REWARD_TOKEN.<mask0>;    }"