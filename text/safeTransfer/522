"function getReward() override public nonReentrant updateReward(msg.sender) {        uint lastEpoch = lastAccountEpoch[msg.sender];        uint reward = rewards[lastEpoch][msg.sender];        if (reward > 0) {            rewards[lastEpoch][msg.sender] = 0;            rewardsToken.<mask0>;            emit RewardPaid(msg.sender, reward);        }    }"