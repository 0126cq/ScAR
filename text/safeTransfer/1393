"function _unlockReward(address owner, address to) internal returns (uint256) {        uint256 toBeunlockedAmt = ICoverData(data).getBuyCoverInsurTokenEarned(owner);        if (toBeunlockedAmt > 0) {            ICoverData(data).decreaseTotalInsurTokenRewardAmount(toBeunlockedAmt);            ICoverData(data).decreaseBuyCoverInsurTokenEarned(owner, toBeunlockedAmt);            IERC20Upgradeable(insur).<mask0>;            emit UnlockCoverRewardEvent(owner, toBeunlockedAmt);        }        return toBeunlockedAmt;    }"