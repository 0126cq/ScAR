"function _getReward(address otoken, address account) internal {        _updateReward(otoken, account);        uint256 reward = rewards[otoken][account];        if (reward > 0) {            rewards[otoken][account] = 0;            ERC20Interface _rewardsToken = rewardsToken;            uint256 rewardsBalance = _rewardsToken.balanceOf(address(this));            if (rewardsBalance > 0) {                _rewardsToken.<mask0>;                emit RewardPaid(otoken, account, reward);            }        }    }"