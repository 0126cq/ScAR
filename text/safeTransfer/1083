"function redeem() external payable nonReentrant\t{\t\taddress payable _from = msg.sender;\t\tuint256 _value = msg.value;\t\trequire(!released, \"token already redeemed\");\t\tuint256 _fractionsCount = balanceOf(_from);\t\tuint256 _redeemAmount = redeemAmountOf(_from);\t\treleased = true;\t\tif (_fractionsCount > 0) _burn(_from, _fractionsCount);\t\t_safeTransferFrom(paymentToken, _from, _value, payable(address(this)), _redeemAmount);\t\tIERC721(target).<mask0>;\t\temit Redeem(_from, _fractionsCount, _redeemAmount);\t\t_cleanup();\t}"