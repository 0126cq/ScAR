"function stake(uint256 _amount, address _recipient) external returns (bool) {    rebase();    SafeERC20.safeTransferFrom(      IERC20(HADES),      msg.sender,      address(this),      _amount    );    Claim memory info = warmupInfo[_recipient];    require(!info.lock, \"Deposits for account are locked\");    warmupInfo[_recipient] = Claim({      deposit: info.deposit.add(_amount),      gons: info.gons.add(sHades(sHADES).gonsForBalance(_amount)),      expiry: epoch.number.add(warmupPeriod),      lock: false    });    SafeERC20.<mask0>;    return true;  }"