"function burnMultiERC20Capsule(address _capsule, uint256 _id)        external        nonReentrant        onlyValidCapsuleCollections(_capsule)    {        address[] memory tokens = _multiERC20Capsule[_capsule][_id].tokenAddresses;        uint256[] memory amounts = _multiERC20Capsule[_capsule][_id].tokenAmounts;        require(tokens.length > 0, Errors.NOT_ERC20_CAPSULE_ID);        delete _multiERC20Capsule[_capsule][_id];        string memory uri = ICapsule(_capsule).tokenURI(_id);        ICapsule(_capsule).burn(_msgSender(), _id);        for (uint256 i; i < tokens.length; i++) {            IERC20(tokens[i]).<mask0>;        }        emit MultiERC20CapsuleBurnt(_msgSender(), _capsule, tokens, amounts, uri);    }"