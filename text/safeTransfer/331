"function getReward() public updateReward(msg.sender) {        uint256 reward = earned(msg.sender);        if (reward > 0) {            rewards[msg.sender] = 0;            if (tx.origin == msg.sender || !IController(controller()).greyList(msg.sender)) {                rewardToken.<mask0>;                emit RewardPaid(msg.sender, reward);            } else {                emit RewardDenied(msg.sender, reward);            }        }    }"