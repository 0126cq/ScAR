"function claimNFT(uint256 collectionId_, uint256 index_) external {        Collection storage collection = allCollections[collectionId_];        require(collection.soldCount == collection.size, \"Not finished\");        address winner = getWinner(collectionId_, index_);        require(winner == _msgSender(), \"Only winner can claim\");        uint256 nftId = nftsByCollectionId[collectionId_][index_];        require(allNFTs[nftId].collectionId == collectionId_, \"Already claimed\");        if (allNFTs[nftId].paid == 0) {            if (collection.willAcceptBLES) {                allNFTs[nftId].paid = allNFTs[nftId].price.mul(                    FEE_BASE.sub(collection.commissionRate)).div(FEE_BASE);                IERC20(blesToken).safeTransfer(allNFTs[nftId].owner, allNFTs[nftId].paid);            } else {                allNFTs[nftId].paid = allNFTs[nftId].price.mul(                    FEE_BASE.sub(feeRate).sub(collection.commissionRate)).div(FEE_BASE);                IERC20(baseToken).<mask0>;            }        }        _withdrawNFT(_msgSender(), nftId, true);    }"