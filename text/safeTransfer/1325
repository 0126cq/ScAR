"function getReward() public nonReentrant updateReward(msg.sender) {        uint256 reward = rewards[msg.sender];        if (reward > 0) {            uint256 balance = rewardsToken.balanceOf(address(this));            if (rewards[msg.sender] > balance) {                reward = balance;            }            rewards[msg.sender] = 0;            rewardsToken.<mask0>;            emit RewardPaid(msg.sender, reward);        }    }"