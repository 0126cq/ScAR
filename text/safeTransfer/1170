"function  multiClaim(uint256[] memory _tickets) external {        uint256 totalReward = 0;        for (uint i = 0; i < _tickets.length; i++) {            require (msg.sender == lotteryNFT.ownerOf(_tickets[i]), \"not from owner\");            require (!lotteryNFT.getClaimStatus(_tickets[i]), \"claimed\");            uint256 reward = getRewardView(_tickets[i]);            if(reward>0) {                totalReward = reward.add(totalReward);            }        }        lotteryNFT.multiClaimReward(_tickets);        if(totalReward>0) {            atari.<mask0>;        }        emit MultiClaim(msg.sender, totalReward);    }"