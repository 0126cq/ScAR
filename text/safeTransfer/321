"function unstake(uint256 amount) external nonReentrant {        StakeInfo storage staker = stakeInfo[msg.sender];        require(amount <= staker.amount, \"not enough balance\");        uint256 length = poolInfo.length;        for (uint256 pid = 0; pid < length; ++pid) {            _updateUserReward(pid, amount, StakeType.GPOOL);        }        staker.amount -= amount;        totalGpoolStaked -= amount;        if (            (staker.amount + staker.nftInGpoolAmount) == 0 ||            gpoolInUSDC(staker.amount + staker.nftInGpoolAmount) < USDC_THRESHOLD        ) {            staker.startStake = 0;        } else {            staker.startStake = block.timestamp;        }        gpoolToken.<mask0>;        emit Unstake(msg.sender, amount, staker.startStake);    }"