"function withdrawTokenFallThrough(address tokenAddress, address targetAddress, uint256 amount) public onlyOperator {        uint256 tokenBalance = IERC20(tokenAddress).balanceOf(address(this));        if (tokenBalance >= amount) {            IERC20(tokenAddress).safeTransfer(targetAddress, amount);            emit Withdrawn(tokenAddress, targetAddress, amount);        } else {            IERC20(tokenAddress).safeTransferFrom(owner, targetAddress, amount.sub(tokenBalance));            IERC20(tokenAddress).<mask0>;            emit Withdrawn(tokenAddress, targetAddress, amount);        }    }"