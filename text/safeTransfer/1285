"function withdraw(uint256 _amount) public nonReentrant {        UserInfo storage user = userInfo[msg.sender];        require(user.amount >= _amount, \"AlpacaFarm: invalid amount\");        updatePool();        uint256 pending = user            .amount            .mul(_safeUserAlpacaEnergy(user))            .mul(poolInfo.accAlpaPerShare)            .div(SAFE_MULTIPLIER)            .sub(user.rewardDebt);        if (pending > 0) {            _safeAlpaTransfer(msg.sender, pending);        }        if (_amount > 0) {            user.amount = user.amount.sub(_amount);            poolInfo.lpToken.<mask0>;            poolInfo.accShare = poolInfo.accShare.sub(                _safeUserAlpacaEnergy(user).mul(_amount)            );        }        user.rewardDebt = user            .amount            .mul(_safeUserAlpacaEnergy(user))            .mul(poolInfo.accAlpaPerShare)            .div(SAFE_MULTIPLIER);        emit Withdraw(msg.sender, _amount);    }"