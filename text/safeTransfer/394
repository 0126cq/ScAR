"function emergencyWithdraw(uint256 _pid) public {        PoolInfo storage pool = poolInfo[_pid];        UserInfo storage user = userInfo[_pid][msg.sender];        uint256 amount = user.amount;        user.amount = 0;        user.rewardDebt = 0;        pool.lpToken.<mask0>;        emit EmergencyWithdraw(msg.sender, _pid, amount);    }"