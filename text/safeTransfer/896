"function claimCommission(uint256 collectionId_) external {        Collection storage collection = allCollections[collectionId_];        require(_msgSender() == collection.owner, \"Only curator can claim\");        require(collection.soldCount == collection.size, \"Not finished\");        if (collection.willAcceptBLES) {            IERC20(blesToken).<mask0>;        } else {            IERC20(baseToken).safeTransfer(collection.owner, collection.commission);        }        collection.commission = 0;    }"