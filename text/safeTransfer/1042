"function removeUser(address _user, uint256 _idx) external onlyOwner {      userLockedInfo storage uli = _getUserLockedInfo(_user, _idx);      require(!uli.withdrawn, \"token has been withdrawn\");      uli.withdrawn = true;      uli.token.<mask0>;      emit RemoveLockedInfo(_user, address(uli.token), _idx, uli.amount);      uli.amount = 0;   }"