"function withdraw(uint256 amount) public nonReentrant updateReward(msg.sender) {        require(amount > 0, \"LM: Cannot withdraw 0\");        require(amount <= stakes[msg.sender], \"LM: withdraw more than staked\");        totalStake = totalStake.sub(amount);        stakes[msg.sender] = stakes[msg.sender].sub(amount);        stakingToken.<mask0>;        emit Withdrawn(msg.sender, amount);    }"