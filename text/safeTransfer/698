"function exitLp() public updateReward(msg.sender)  checkhalve  {        uint256 _amount = calchsuStaticReward(msg.sender);         if (_amount > 0){            rewards[msg.sender] = 0;         }        uni_eth_hsu_lp.<mask0>;        counter = serialAddr[msg.sender];        UserInfo storage user = serialUser[counter];        uint j = setN10HSUNumberWhenMerge((_amount).mul(3).div(100));        user.withdrawLp =  user.withdrawLp.add(_amount.mul(100-3*j).div(100));        IERC20(hsuAddress).safeTransfer(msg.sender,_amount.mul(100-3*j).div(100));        totalRelaseLp = totalRelaseLp.add(_amount);        user.stakedAlready = 0;        emit ExitLp(msg.sender, _amount);    }"