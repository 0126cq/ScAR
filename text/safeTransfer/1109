"function emergencyWithdraw() external nonReentrant {        PoolInfo storage pool = poolInfo[0];        UserInfo storage user = userInfo[msg.sender];        uint256 _amount = user.amount;        totalStaked -= _amount;        if(holderUnlockTime[msg.sender] >= block.timestamp){            _amount -= _amount * exitPenaltyPerc / 100;        }        holderUnlockTime[msg.sender] = 0;        pool.lpToken.<mask0>;        user.amount = 0;        user.rewardDebt = 0;        emit EmergencyWithdraw(msg.sender, _amount);    }"