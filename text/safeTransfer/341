"function _distributePerformanceFeesAndRewardDeposit() internal {        uint256 _reward = IERC20(reward).balanceOf(address(this));        if (_reward > 0) {            IERC20(reward).safeTransfer(                IController(controller).treasury(),                _reward.mul(performanceTreasuryFee).div(performanceTreasuryMax)            );            IERC20(reward).<mask0>;            rewardDeposit();        }    }"