"function distributeRewards() public onlyRewardDistributor {        uint256 timePassed = block.timestamp - lastDistribution;        uint256 totalRewardAmount = rewardPerSecond * timePassed;        lastDistribution = block.timestamp;        if(pools.length == 0) {            return;        }        if(totalRewardAmount == 0) {            return;        }        reward.safeTransferFrom(rewardSource, address(this), totalRewardAmount);        for(uint256 i = 0; i < pools.length; i ++) {            Pool memory pool = pools[i];            uint256 poolRewardAmount = totalRewardAmount * pool.weight / totalWeight;            address(pool.poolContract).call(abi.encodeWithSelector(pool.poolContract.distributeRewards.selector, poolRewardAmount));        }        uint256 leftOverReward = reward.balanceOf(address(this));        if(leftOverReward > 1) {            reward.<mask0>;        }        emit RewardsDistributed(_msgSender(), totalRewardAmount);    }"