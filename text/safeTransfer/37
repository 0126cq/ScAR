"function withdraw(uint256 poolId, uint256 amount) public {        require(poolId < _poolInfoCount, \"withdraw: Pool is not exists\");        PoolInfo storage pool = _poolInfo[poolId];        require(amount > 0, \"withdraw: can't withdraw zero amount\");        UserInfo storage user = _userInfo[poolId][msg.sender];        require(user.amount >= amount, \"withdraw: can't withdraw more than deposit\");        _beforeBalanceChange(pool, msg.sender);        user.amount = user.amount.sub(amount);        pool.lpToken.<mask0>;        emit Withdraw(msg.sender, poolId, amount);        _afterBalanceChange(pool, msg.sender);    }"