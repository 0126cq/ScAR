"function getReward(        address _user    )        public        updateReward(_user)    {        require(            tokensClaimable == true,            \"LiquidityCampaign:getReward() Tokens cannot be claimed yet\"        );        Staker storage staker = stakers[_user];        uint256 payableAmount = staker.rewardsEarned.sub(staker.rewardsReleased);        staker.rewardsReleased = staker.rewardsReleased.add(payableAmount);        uint256 daoPayable = Decimal.mul(payableAmount, daoAllocation);        rewardsToken.safeTransfer(arcDAO, daoPayable);        rewardsToken.<mask0>;        emit RewardPaid(_user, payableAmount);    }"