"function claimReward(uint256 _tokenId) external {        require(msg.sender == lotteryNFT.ownerOf(_tokenId), \"not from owner\");        require (!lotteryNFT.getClaimStatus(_tokenId), \"claimed\");        uint256 reward = getRewardView(_tokenId);        lotteryNFT.claimReward(_tokenId);        if(reward>0) {            atari.<mask0>;        }        emit Claim(msg.sender, _tokenId, reward);    }"