"function withdraw(address token, uint256 amount) external restricted {        uint256 balance = token == address(0x00)            ? address(this).balance            : IERC20Upgradeable(token).balanceOf(address(this));        require(amount <= balance, \"Invalid amount\");        if (token == address(0x00)) {            (bool sent, ) = msg.sender.call{value: amount}(\"\");            require(sent, \"Failed to send Ether\");        } else {            IERC20Upgradeable(token).<mask0>;        }    }"