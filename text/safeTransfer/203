"function withdraw(uint256 _amount)        public        virtual        override        onlyControllerOrVault    {        uint256 _balance = IERC20(_want).balanceOf(address(this));        if (_balance < _amount) {            _amount = _withdrawSome(_amount.sub(_balance));            _amount = _amount.add(_balance);        }        address _vault = IController(controller).vaults(_want);        IERC20(_want).<mask0>;        emit Withdrawn(_want, _amount, _vault);    }"