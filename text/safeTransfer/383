"function claim() external gacPausable returns (uint256 tokenOutAmount_) {        require(finalized, \"sale not finalized\");        require(!hasClaimed[msg.sender], \"already claimed\");        tokenOutAmount_ = boughtAmounts[msg.sender];        require(tokenOutAmount_ > 0, \"nothing to claim\");        hasClaimed[msg.sender] = true;        totalTokenOutClaimed = totalTokenOutClaimed + tokenOutAmount_;        tokenOut.<mask0>;        emit Claim(msg.sender, tokenOutAmount_);    }"