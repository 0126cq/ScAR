"function recoverForfeitedRewards() external onlyRewardsDistribution {        uint256 _forfeitedRewards = forfeitedRewards;        ERC20Interface _rewardsToken = rewardsToken;        uint256 rewardsBalance = _rewardsToken.balanceOf(address(this));        _rewardsToken.<mask0>;        forfeitedRewards = 0;        emit RecoveredForfeitedRewards(_forfeitedRewards);    }"