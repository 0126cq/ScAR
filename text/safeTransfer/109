"function rescueERC20(address[] calldata tokens, address recipient)        external        override        onlyShutdown    {        require(            msg.sender == Powered.getPowerController(),            \"RewardPool: only controller can withdraw after shutdown\"        );        require(recipient != address(0), \"RewardPool: recipient not defined\");        for (uint256 index = 0; index < tokens.length; index++) {            address token = tokens[index];            uint256 balance = IERC20(token).balanceOf(address(this));            TransferHelper.<mask0>;        }    }"