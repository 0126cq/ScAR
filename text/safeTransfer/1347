"function _withdrawByTokenWhenOutOfSeason(        address user,        address recipient,        address token    ) internal returns (uint) {        uint amount = _addressToTokenToBalanceMap[user][token];        if (amount > 0) {            _addressToTokenToBalanceMap[user][token] = 0;            IERC20(token).<mask0>;        }        emit WithdrawOutOfSeason(user, token, recipient, amount);        return amount;    }"