"function manage(address _token, uint256 _amount) external {    if (isLiquidityToken[_token]) {      require(isLiquidityManager[msg.sender], \"Not approved\");    } else {      require(isReserveManager[msg.sender], \"Not approved\");    }    uint256 value = valueOfToken(_token, _amount);    require(value <= excessReserves(), \"Insufficient reserves\");    totalReserves = totalReserves.sub(value);    emit ReservesUpdated(totalReserves);    IERC20(_token).<mask0>;    emit ReservesManaged(_token, _amount);  }"