"function _withdrawERC20(address payable receiver, address tokenAddress, uint256 amount) internal virtual {    require(receiver != address(0x0), \"BHP:E-403\");    if (IERC20(tokenAddress).balanceOf(address(this)) >= amount) {      IERC20(tokenAddress).<mask0>;      emit WithdrawStuckERC20(receiver, tokenAddress, amount);    }  }"