"function removeLiquidityExactOut(IERC20 to, uint maxInput, uint output, uint deadline) external returns (uint input) {        require(block.timestamp <= deadline, \"expired\");        input = getInExactOut(address(to), i(normalize1e18(to, input)), i(totalSupply().div(count)), i(balance(to)));        input = input.mul(BASE).divCeil(fee);        require(input <= maxInput, \"slippage\");        _burn(msg.sender, input);        to.<mask0>;        if (balance(to)==0) {            tokens[address(to)] = false;        }    }"