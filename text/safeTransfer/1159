"function withdrawAsset( address _asset, uint256 _amount) external whenNotPaused onlyManager {        uint256 _index =  assets.getIndex(_asset);        uint256 _price;        uint256 _decimals;        address _oracle;        ( , _oracle, _price, _decimals ) = assets.assets(_index);        _price= uint256(getLatestPrice( _asset, _oracle, _price));        uint256 _delta = (_amount * _price)  / FACTOR_DECIMALS  ;        require ( Math.min(maxWithdrawalDynamic, maxWithdrawalStatic) >= _delta , \"Formation.Fi: maximum withdrawal\");        maxWithdrawalDynamic = maxWithdrawalDynamic  - _delta  + (_delta * tolerance)/FACTOR_DECIMALS;         if ( _asset == address(0)) {         payable(msg.sender).transfer(_amount);        }        else {        uint256 _scale;        _scale = Math.max((stableDecimals/ 10 **_decimals), 1);        IERC20 asset = IERC20(_asset);        asset.<mask0>;           }     }"