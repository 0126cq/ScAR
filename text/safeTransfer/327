"function restake(uint256 _validatorId) public override whenNotPaused {\t\taddress validatorShare = stakeManager.getValidatorContract(\t\t\t_validatorId\t\t);\t\tuint256 balanceBeforeRewards = IERC20Upgradeable(token).balanceOf(\t\t\taddress(this)\t\t);\t\twithdrawRewards(validatorShare);\t\tuint256 rewards = IERC20Upgradeable(token).balanceOf(address(this)) -\t\t\tbalanceBeforeRewards;\t\tuint256 rewardsToDistribute = (rewards * feePercent) / 100;\t\tuint256 treasuryRewards = (rewardsToDistribute * entityFees.treasury) /\t\t\t100;\t\tuint256 insuranceRewards = (rewardsToDistribute *\t\t\tentityFees.insurance) / 100;\t\tIERC20Upgradeable(token).safeTransfer(treasury, treasuryRewards);\t\tIERC20Upgradeable(token).<mask0>;\t\tuint256 amountRestaked = IERC20Upgradeable(token).balanceOf(\t\t\taddress(this)\t\t) - balanceBeforeRewards;\t\tbuyVoucher(validatorShare, amountRestaked, 0);\t\temit Restake(msg.sender, _validatorId, amountRestaked);\t\temit DistributeRewards(msg.sender, treasuryRewards, insuranceRewards);\t}"