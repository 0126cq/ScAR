"function withdraw(uint256 _pid, uint256 _amount) public {        PoolInfo storage pool = poolInfo[_pid];        UserInfo storage user = userInfo[_pid][msg.sender];        require(user.amount >= _amount, \"withdraw: not good\");        updatePool(_pid);        uint256 pending = user.amount.mul(pool.accRewardsPerShare).div(1e12).sub(user.rewardDebt);        if(pending > 0) {            totalRewardDebt = totalRewardDebt.sub(pending);            safeRewardsTransfer(msg.sender, pending);        }        if(_amount > 0) {            user.amount = user.amount.sub(_amount);            pool.lpToken.<mask0>;        }        user.rewardDebt = user.amount.mul(pool.accRewardsPerShare).div(1e12);        emit Withdraw(msg.sender, _pid, _amount);    }"