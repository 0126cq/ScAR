"function pay(        address token,        address payer,        address recipient,        uint256 value    ) internal {        if (token == weth && address(this).balance >= value) {            IWETH9(weth).deposit{value: value}();             IWETH9(weth).transfer(recipient, value);        } else if (payer == address(this)) {            TransferHelper.<mask0>;        } else {            TransferHelper.safeTransferFrom(token, payer, recipient, value);        }    }"