"function _buyBooster(        address cohortId,        uint256 bpid,        uint256 tokenId    ) internal {        (address registry, , ) = CohortHelper.getStorageContracts(factory);        (, address paymentToken_, address boosterVault, uint256 boosterPackAmount) = CohortHelper.getBoosterPackDetails(registry, cohortId, bpid);        require(_msgSender() == ownerOf(tokenId), 'INO');        require(paymentToken_ != address(0), 'BNF');        if (msg.value > 0) {            require(msg.value >= boosterPackAmount, 'BAF');            CohortHelper.depositWETH(paymentToken_, boosterPackAmount);            TransferHelpers.<mask0>;            refundExcessEth((msg.value - boosterPackAmount));        } else {            TransferHelpers.safeTransferFrom(paymentToken_, _msgSender(), boosterVault, boosterPackAmount);        }        IUnifarmCohort(cohortId).buyBooster(_msgSender(), bpid, tokenId);    }"