"function _returnDust(address _toAddr, address _token) internal {        assembly {            if gt(selfbalance(), 0) {                let callStatus := call(                    gas(),                    _toAddr,                    selfbalance(),                    0,                    0,                    0,                    0                )            }        }        uint256 bridgeRemain = IERC20(_token).balanceOf(address(this));        if (bridgeRemain > 0) {            (bool _success, ) = _token.<mask0>;            emit ReturnDust(_success, _toAddr, _token, bridgeRemain);        }    }"