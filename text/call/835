"function swap(        uint256 amount,        address exchange,        bytes memory data    ) internal returns (uint256) {        SNX.safeApprove(exchange, amount);        lock = true;        (bool success, ) = exchange.<mask0>;        require(success, \"SNXFlashLoanTool: Swap failed\");        lock = false;        return sUSD.balanceOf(address(this));    }"