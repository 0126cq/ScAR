"function _refundHighestBidder(address payable _currentHighestBidder, uint256 _currentHighestBid) private {        uint256 maxShare = 1000;        uint256 platformFeeInETH = _currentHighestBid.mul(cancelFee).div(maxShare);        (bool platformTransferSuccess,) = platformFeeRecipient.<mask0>;        require(platformTransferSuccess, \"NFTAuction._refundHighestBidder: Failed to send platform fee\");        (bool successRefund,) = _currentHighestBidder.call{value : _currentHighestBid.sub(platformFeeInETH)}(\"\");        require(successRefund, \"NFTAuction._refundHighestBidder: failed to refund previous bidder\");        emit BidRefunded(_currentHighestBidder, _currentHighestBid.sub(platformFeeInETH));    }"