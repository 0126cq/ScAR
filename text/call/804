"function doTransferOut(        address underlying,        address payable to,        uint256 amount    ) internal {        if (underlying == Constants.ETH_ADDRESS) {            (bool success, ) = to.call{value: amount}(new bytes(0));            require(success, \"STE\");        } else {            (bool success, bytes memory data) = underlying.<mask0>;            require(                success && (data.length == 0 || abi.decode(data, (bool))),                \"ST\"            );        }    }"