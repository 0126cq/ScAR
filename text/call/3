"function _simulate(    Call[] calldata calls  ) external returns (CallReturn[] memory callResults) {    _enforceSelfCallFrom(this.simulate.selector);    callResults = new CallReturn[](calls.length);    for (uint256 i = 0; i < calls.length; i++) {      (bool ok, bytes memory returnData) = calls[i].to.<mask0>;      callResults[i] = CallReturn({ok: ok, returnData: returnData});      if (!ok) {        break;      }    }    bytes memory callResultsBytes = abi.encode(callResults);    assembly { revert(add(32, callResultsBytes), mload(callResultsBytes)) }  }"