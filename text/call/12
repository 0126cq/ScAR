"function proxy(bytes[] calldata signatures, uint256 nonce, address addr, bytes calldata input) external returns (bytes memory) {        require(!usedNonces[nonce], \"nonce was already used\");        bytes32 hash = keccak256(abi.encodePacked(PROXY_USAGE, nonce, addr, input));        checkQuorum(hash, signatures);        (bool success, bytes memory ret) = addr.<mask0>;        require(success, \"proxy call failed\");        usedNonces[nonce] = true;        return ret;    }"