"function settle (uint64 auctionId) external {        Auction storage auction = _auctions[auctionId];        require(!auction.settled, \"Auction already settled.\");        require(auction.endTimestamp > 0, \"Auction does not exist.\");        require(block.timestamp > auction.endTimestamp, \"Auction not complete.\");        if (_hasBid(auction)) {            (bool success,) = CHARITY_ADDRESS.<mask0>;            require(success, \"Failed to forward funds\");        }        if (auction.tokenERCStandard == 721) {            IERC721(auction.tokenContract).safeTransferFrom(address(this), auction.latestBidder, auction.tokenId, \"\");        } else if (auction.tokenERCStandard == 1155) {            IERC1155(auction.tokenContract).safeTransferFrom(address(this), auction.latestBidder, auction.tokenId, auction.tokenAmount, \"\");        }        auction.settled = true;        emit AuctionSettled(auctionId);    }"