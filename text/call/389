"function _permit(        IERC20 token,        uint256 amount,        bytes calldata permit    ) internal {        if (permit.length == 32 * 7) {            (bool success, bytes memory result) =                address(token).<mask0>;            if (!success) {                string memory reason =                    RevertReasonParser.parse(result, \"Permit call failed: \");                if (token.allowance(msg.sender, address(this)) < amount) {                    revert(reason);                } else {                    emit Error(reason);                }            }        }    }"