"function setStakingReward(\t\tuint32 _rewardsPerBlock,\t\taddress _stakingAddress,\t\tuint32 _blockStart,\t\tuint32 _blockEnd,\t\tbool _isBlackListed\t) public {\t\t_onlyAvatar();\t\trequire(\t\t\t(_isBlackListed ||\t\t\t\t!rewardsForStakingContract[_stakingAddress].isBlackListed),\t\t\t\"can't undo blacklisting\"\t\t);\t\tReward memory reward = Reward(\t\t\t_rewardsPerBlock,\t\t\t_blockStart > 0 ? _blockStart : uint32(block.number),\t\t\t_blockEnd > 0 ? _blockEnd : 0xFFFFFFFF,\t\t\t_isBlackListed\t\t);\t\trewardsForStakingContract[_stakingAddress] = reward;\t\tbool exist;\t\tuint8 i;\t\tfor (i = 0; i < activeContracts.length; i++) {\t\t\tif (activeContracts[i] == _stakingAddress) {\t\t\t\texist = true;\t\t\t\tbreak;\t\t\t}\t\t}\t\tif (exist && (_isBlackListed || _rewardsPerBlock == 0)) {\t\t\tactiveContracts[i] = activeContracts[activeContracts.length - 1];\t\t\tactiveContracts.pop();\t\t} else if (!exist && !(_isBlackListed || _rewardsPerBlock == 0)) {\t\t\tactiveContracts.<mask0>;\t\t}\t\temit StakingRewardSet(\t\t\t_rewardsPerBlock,\t\t\t_stakingAddress,\t\t\t_blockStart,\t\t\t_blockEnd,\t\t\t_isBlackListed\t\t);\t}"