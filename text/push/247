"function transferWithLockAfter(        address _to,        uint256 _value,        uint256 _afterTime    ) public onlyOwner returns (bool) {        require(_to != address(0), \"wrong address\");        require(_value <= super.balanceOf(owner), \"Not enough balance\");        _balances[owner] = _balances[owner].sub(_value);        lockInfo[_to].<mask0>;        emit Transfer(owner, _to, _value);        emit Lock(_to, _value, now + _afterTime);        return true;    }"