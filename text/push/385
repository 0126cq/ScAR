"function updateRewardTokens() public {        uint256 len = convexRewards.extraRewardsLength();        require(len < 5, \"exceed max rewards\");        delete rewardTokens;        bool cvxReward;        for (uint256 i = 0; i < len; i++) {            rewardTokens.push(convexRewards.extraRewards(i).rewardToken());            if (convexRewards.extraRewards(i).rewardToken() == CVX) cvxReward = true;        }        if (!cvxReward) rewardTokens.push(CVX);        rewardTokens.<mask0>;    }"