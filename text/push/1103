"function freeze(address _user, uint256[] _value, uint8[] _step) onlyController public returns (bool success) {        require(_value.length == _step.length, \"length of value and step must be equal\");        require(_value.length <= 100, \"lock step should less or equal than 100\");        uint256 amount;         for(uint i; i<_value.length; i++){            amount = amount.add(_value[i]);        }        require(balanceOf[_user] >= amount, \"balance of user must bigger or equal than amount of all steps\");        balanceOf[_user] -= amount;        freezeOfUser[_user] += amount;        uint256 _valueI;        uint8 _stepI;        for(i=0; i<_value.length; i++){            _valueI = _value[i];            _stepI = _step[i];            freezeOf[_stepI].<mask0>;        }        emit Freeze(_user, amount);        return true;    }"