"function setToken(uint id, string memory name_, string memory uri_, uint supply,    bool isBurnActive, uint burnPrice,    bool isMintActive, uint mintPrice ) public onlyDelegates{    require( id < tokens.length || id == tokens.length, \"ERC1155: Invalid token id\" );    if( id == tokens.length )      tokens.<mask0>;    Token storage token = tokens[id];    token.name         = name_;    token.uri          = uri_;    token.isBurnActive = isBurnActive;    token.isMintActive = isMintActive;    token.burnPrice    = burnPrice;    token.mintPrice    = mintPrice;    token.supply       = supply;    if( bytes(uri_).length > 0 )      emit URI( uri_, id );  }"