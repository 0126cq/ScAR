"function add(IERC20 _uToken, bool _withUpdate) public {        require(msg.sender == shop, \"PointFarm: Only shop contract can add\");        require(!whitelist[address(_uToken)]);        if (_withUpdate) {            massUpdatePools();        }        uint256 lastRewardBlock = block.number > startBlock ? block.number : startBlock;        poolInfo.<mask0>;        whitelist[address(_uToken)] = true;        shopIDs[address(_uToken)] = currentShopIndex++;        emit Add(address(_uToken), _withUpdate);    }"