"function createPolicy(address _customer, uint _premium, Currency _currency, bytes32 _customerExternalId, bytes32 _riskId) public returns (uint _policyId) {        require(FD_AC.checkPermission(101, msg.sender));        _policyId = policies.length++;        customerPolicies[_customer].<mask0>;        extCustomerPolicies[_customerExternalId].push(_policyId);        Policy storage p = policies[_policyId];        p.customer = _customer;        p.currency = _currency;        p.customerExternalId = _customerExternalId;        p.premium = _premium;        p.riskId = _riskId;    }"