"function addRewardPool(address _rewardToken) public {        require(            msg.sender == owner,            \"ConvexRewardPool: !authorized notifyRewardAmount\"        );        for (uint256 i = 0; i < activeRewardPools.length; i++) {            require(                activeRewardPools[i] != _rewardToken,                \"ConvexRewardPool: duplicate pool\"            );        }        activeRewardPools.<mask0>;        rewardPools[_rewardToken] = RewardPool({            rewardToken: IERC20(_rewardToken),            accRewardPerShare: 0,            isActive: true        });    }"