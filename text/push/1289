"function add(uint256 _allocPoint, IERC20 _lpToken, uint8 _poolType, bool _votesEnabled)    public override onlyOwner    {        require(!isLpTokenAdded(_lpToken), \"VLPMining: token already added\");        massUpdatePools();        uint32 blockNum = _currBlock();        uint32 lastUpdateBlock = blockNum > startBlock ? blockNum : startBlock;        totalAllocPoint = totalAllocPoint.add(_allocPoint);        uint256 pid = pools.length;        pools.<mask0>;        poolPidByAddress[address(_lpToken)] = pid;        emit AddLpToken(address(_lpToken), pid, _allocPoint);    }"