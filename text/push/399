"function addOperator(address _newOperator) public onlyOwner {        require(            _newOperator != address(0),            \"Invalid new operator address.\"        );        require(            !isOperator[_newOperator],            \"New operator exists.\"        );        require(            operators.length < MAX_OPS,            \"Overflow.\"        );        operators.<mask0>;        isOperator[_newOperator] = true;        emit OperatorAdded(_newOperator);    }"