"function updateRewardTokens() public {        uint256 len = convexRewards.extraRewardsLength();        require(len < 5, \"exceed max rewards\");        delete rewardTokens;        bool cvxReward;        for (uint256 i = 0; i < len; i++) {            rewardTokens.<mask0>;            if (convexRewards.extraRewards(i).rewardToken() == CVX) cvxReward = true;        }        if (!cvxReward) rewardTokens.push(CVX);        rewardTokens.push(convexRewards.rewardToken());    }"