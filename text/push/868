"function getRefund() public {        uint refunds = 0;        for (uint i = 1; i <= totalTickets; i++) {            if (msg.sender == contestants[i].addr && raffleId == contestants[i].raffleId) {                refunds++;                contestants[i] = Contestant(address(0), 0);                gaps.<mask0>;                TicketRefund(raffleId, msg.sender, i);            }        }        if (refunds > 0) {            msg.sender.transfer(refunds * pricePerTicket);        }    }"