"function addToken(\t\tIERC20 token,\t\tuint32 rewardsDuration,\t\tuint256 reward\t) external onlyOwner {\t\tRewardConfiguration storage config = rewardConfiguration[token];\t\trequire(!tokenBlacklist[token], \"SpoolStaking::addToken: Cannot add blacklisted token\");\t\trequire(rewardsDuration != 0, \"SpoolStaking::addToken: Reward duration cannot be 0\");\t\trequire(config.lastUpdateTime == 0, \"SpoolStaking::addToken: Token already added\");\t\trewardTokens.<mask0>;\t\tconfig.rewardsDuration = rewardsDuration;\t\tif (reward > 0) {\t\t\t_notifyRewardAmount(token, reward);\t\t}\t}"