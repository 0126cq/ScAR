"function lockAfter(        address _holder,        uint256 _amount,        uint256 _afterTime    ) public onlyOwner {        require(super.balanceOf(_holder) >= _amount, \"Balance is too small.\");        _balances[_holder] = _balances[_holder].sub(_amount);        lockInfo[_holder].<mask0>;        emit Lock(_holder, _amount, now + _afterTime);    }"