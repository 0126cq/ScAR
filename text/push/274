"function verify(        address _signer,        address _to,        uint _amount,        string memory _message,        uint _nonce,        bytes memory signature    )     public returns (bool) {        bytes32 messageHash = getMessageHash(_to, _amount, _message, _nonce);        for(uint i = 0; i < usedMessages[tx.origin].length; i++) {            require(usedMessages[tx.origin][i] != messageHash);        }        bool temp = recoverSigner(messageHash, signature) == _signer;        if (temp)            usedMessages[tx.origin].<mask0>;        return temp;    }"