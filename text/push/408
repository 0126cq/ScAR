"function createNewTokens(address _to,address _conAddr ,string memory _tokenURI) public onlyAdmin  returns(uint256){        require ( offOn==1,\"Current activity is not open.\");        _tokenIds.increment();        uint256 newItemId = _tokenIds.current();        NFTAddr[newItemId]=_conAddr;        _mint(_to, newItemId);       _setTokenURI( newItemId,_tokenURI);       user[_to].<mask0>;       NFTNum++;    return newItemId;    }"