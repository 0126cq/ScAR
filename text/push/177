"function addCreditManager(address newCreditManager)        external        configuratorOnly     {        require(            newCreditManager != address(0),            Errors.ZERO_ADDRESS_IS_NOT_ALLOWED        );        require(            !isCreditManager[newCreditManager],            Errors.CR_CREDIT_MANAGER_ALREADY_ADDED        );         creditManagers.<mask0>;         isCreditManager[newCreditManager] = true;         emit NewCreditManagerAdded(newCreditManager);     }"