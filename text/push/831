"function addLockProduct(uint32 perTermInterest, uint32 durationInSecs, uint32 minimumLockAmount, bool isActive)    external restrict(\"StabilityBoard\") {        uint _newLockProductId = lockProducts.<mask0> - 1;        uint32 newLockProductId = uint32(_newLockProductId);        require(newLockProductId == _newLockProductId, \"lockProduct overflow\");        emit NewLockProduct(newLockProductId, perTermInterest, durationInSecs, minimumLockAmount, isActive);    }"