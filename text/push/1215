"function setJoinedCrowdsales(address addr) private onlyOwner {    assert(addr != address(0));    assert(joinedCrowdsalesLen <= joinedCrowdsalesLenMax);    assert(!isTierJoined(addr));    joinedCrowdsales.<mask0>;    joinedCrowdsaleState[addr] = JoinedCrowdsaleStatus({      isJoined: true,      position: joinedCrowdsalesLen    });    joinedCrowdsalesLen++;  }"