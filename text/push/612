"function stakeNft(uint256 _nftId) external {        _xoids.transferFrom(msg.sender, address(this), _nftId);        _ctzn.transferFrom(msg.sender, address(this), erc20StakingAmount);        _allNftStaked[_stakingId] = nftStaking(            _nftId,            block.timestamp,            msg.sender        );        _ownerStakingId[msg.sender].<mask0>;        emit NftStaked(msg.sender, _stakingId, _nftId, block.timestamp);        _stakingId++;    }"