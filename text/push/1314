"function _updateUserBalance(LibBarnStorage.Stake[] storage checkpoints, uint256 amount) internal {        if (checkpoints.length == 0) {            checkpoints.push(LibBarnStorage.Stake(block.timestamp, amount, block.timestamp, address(0)));        } else {            LibBarnStorage.Stake storage old = checkpoints[checkpoints.length - 1];            if (old.timestamp == block.timestamp) {                old.amount = amount;            } else {                checkpoints.<mask0>;            }        }    }"