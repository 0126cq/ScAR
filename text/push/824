"function addSigners(address[] signers) public {        require(msg.sender == address(this), \"only callable via MultiSig\");        for (uint i= 0; i < signers.length; i++) {            if (!isSigner[signers[i]]) {                require(signers[i] != address(0), \"new signer must not be 0x0\");                activeSignersCount++;                allSigners.<mask0>;                isSigner[signers[i]] = true;                emit SignerAdded(signers[i]);            }        }    }"