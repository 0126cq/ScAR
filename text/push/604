"function addAddressToIndex(address scammer, address theAddressToIndex) private returns(bool success){    bool addressFound = false;    for(uint i = 0; i < flaggedIndex[scammer].length; i++){        if(flaggedIndex[scammer][i] == theAddressToIndex){            addressFound = true;            break;        }    }    if(!addressFound){        flaggedIndex[scammer].<mask0>;    }    return true;}"