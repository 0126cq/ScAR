"function _writeCheckpoint(        Checkpoint[] storage ckpts,        function(uint256, uint256) view returns (uint256) op,        uint256 delta    ) private returns (uint256 oldWeight, uint256 newWeight) {        uint256 pos = ckpts.length;        oldWeight = pos == 0 ? 0 : ckpts[pos - 1].votes;        newWeight = op(oldWeight, delta);        if (pos > 0 && ckpts[pos - 1].fromBlock == block.number) {            ckpts[pos - 1].votes = SafeCast.toUint224(newWeight);        } else {            ckpts.<mask0>;        }    }"