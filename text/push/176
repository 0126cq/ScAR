"function addPool(address newPoolAddress)        external        configuratorOnly     {        require(            newPoolAddress != address(0),            Errors.ZERO_ADDRESS_IS_NOT_ALLOWED        );        require(!isPool[newPoolAddress], Errors.CR_POOL_ALREADY_ADDED);         pools.<mask0>;         isPool[newPoolAddress] = true;         emit NewPoolAdded(newPoolAddress);     }"