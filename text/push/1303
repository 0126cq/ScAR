"function approveAddOwnerRequest () public onlyOwners {    require (!addOwner.isExecute && !addOwner.isCanceled);    require (addOwner.creationTimestamp + lifeTime >= uint32(now));    require (!owners[addOwner.newOwner]);    for (uint i = 0; i < addOwner.confirmators.length; i++){      require(addOwner.confirmators[i] != msg.sender);    }    addOwner.confirms++;    addOwner.confirmators.<mask0>;    if(addOwner.confirms >= needApprovesToConfirm){      addOwner.isExecute = true;      owners[addOwner.newOwner] = true;      ownersCount++;    }    emit AddOwnerRequestUpdate(msg.sender, addOwner.confirms, addOwner.isExecute);  }"