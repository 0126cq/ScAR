"function add(uint256 allocPoint, IERC20 _lpToken) external {        require(msg.sender == manager, 'add: !manager');        totalAllocPoint += allocPoint;        require(!activeLpTokens[address(_lpToken)], 'add: lpToken already added');        poolInfo.<mask0>;        activeLpTokens[address(_lpToken)] = true;        emit LogLpTokenAdded(address(_lpToken));        emit LogAddPool(poolInfo.length - 1, allocPoint, _lpToken);    }"