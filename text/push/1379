"function _addConvexPool(        uint256 _originConvexPid,        address _lpToken,        address _originCrvRewards,        address _originStash,        address _curveSwapAddress    ) internal {        address virtualBalance = IVirtualBalanceWrapperFactory(virtualBalanceWrapperFactory).createWrapper(address(this));        address rewardCrvPool = IConvexRewardFactoryV2(convexRewardFactory).createReward(rewardCrvToken, virtualBalance, address(this), address(this));        address rewardCvxPool = IConvexRewardFactoryV2(convexRewardFactory).createReward(rewardCvxToken, virtualBalance, address(this), address(this));        uint256 extraRewardsLength = IOriginConvexRewardPool(_originCrvRewards).extraRewardsLength();        for (uint256 i = 0; i < extraRewardsLength; i++) {            address extraReward = IOriginConvexRewardPool(_originCrvRewards).extraRewards(i);            address extraRewardPool = IConvexRewardFactoryV2(convexRewardFactory).createReward(                IOriginConvexRewardPool(extraReward).rewardToken(),                virtualBalance,                rewardCrvPool,                address(this)            );            IConvexRewardPool(rewardCrvPool).addExtraReward(extraRewardPool);        }        poolInfo.<mask0>;        emit AddConvexPool(_originConvexPid, _lpToken, _curveSwapAddress);    }"