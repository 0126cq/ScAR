"function addPool(address _poolContract, uint256 _weight) external onlyGov {        distributeRewards();        require(_poolContract != address(0), \"LiquidityMiningManager.addPool: pool contract must be set\");        require(!poolAdded[_poolContract], \"LiquidityMiningManager.addPool: Pool already added\");        require(pools.length < MAX_POOL_COUNT, \"LiquidityMiningManager.addPool: Max amount of pools reached\");        pools.<mask0>;        poolAdded[_poolContract] = true;        totalWeight += _weight;        reward.safeApprove(_poolContract, type(uint256).max);        emit PoolAdded(_poolContract, _weight);    }"