"function mint(        address to,        Royalties.RoyaltyRecipient[] memory royaltyRecipients,        string memory URI,        KaleidoArt1MetadataStorage.MediaInfo memory mediaInfo    ) public onlyOwner returns (uint256) {        require(!_existsURI(URI), \"KaleidoArt1Token: URI already in use\");        uint256 currentTokenId = nextId();        _safeMint(to, currentTokenId);        for (uint256 i = 0; i < royaltyRecipients.length; i++) {            _royalties[currentTokenId].<mask0>;        }        setMediaInfo(currentTokenId, URI, mediaInfo);        return currentTokenId;    }"