"function addReserve(IERC20Token _token, uint32 _weight)        public        virtual        override        ownerOnly        inactive        validAddress(address(_token))        notThis(address(_token))        validReserveWeight(_weight)    {        require(address(_token) != address(anchor) && !reserves[_token].isSet, \"ERR_INVALID_RESERVE\");        require(_weight <= PPM_RESOLUTION - reserveRatio, \"ERR_INVALID_RESERVE_WEIGHT\");        require(reserveTokenCount() < uint16(-1), \"ERR_INVALID_RESERVE_COUNT\");        Reserve storage newReserve = reserves[_token];        newReserve.balance = 0;        newReserve.weight = _weight;        newReserve.isSet = true;        reserveTokens.<mask0>;        reserveRatio += _weight;    }"