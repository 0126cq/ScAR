"function excludeFromReward(address account) public onlyOwner {        require(!_isExcluded[account], \"Account is already excluded\");        require(_excluded.length + 1 <= 50, \"Cannot exclude more than 50 accounts.  Include a previously excluded address.\");        if (_rOwned[account] > 0) {            _tOwned[account] = tokenFromReflection(_rOwned[account]);        }        _isExcluded[account] = true;        _excluded.<mask0>;    }"