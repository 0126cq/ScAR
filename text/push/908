"function createNEO(address nft) public returns (address neo) {        require(nft.isContract(), 'nft should isContract');        require(IERC165(nft).supportsInterface(_INTERFACE_ID_ERC721), 'nft should supportsInterface(_INTERFACE_ID_ERC721)');        require(neos[nft] == address(0), 'the NEO exist already');        bytes memory bytecode = type(BeaconProxyNEO).creationCode;        bytes32 salt = keccak256(abi.encodePacked(nft));        assembly {            neo := create2(0, add(bytecode, 32), mload(bytecode), salt)        }        NEO(neo).__NEO_init(nft);        neos[nft] = neo;        neoA.<mask0>;        emit CreateNEO(_msgSender(), nft, neo, neoA.length);    }"