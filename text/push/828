"function addLoanProduct(uint32 term, uint32 discountRate, uint32 collateralRatio, uint minDisbursedAmount,                                uint32 defaultingFeePt, bool isActive)    external restrict(\"StabilityBoard\") {        uint _newProductId = products.<mask0> - 1;        uint32 newProductId = uint32(_newProductId);        require(newProductId == _newProductId, \"productId overflow\");        emit LoanProductAdded(newProductId);    }"