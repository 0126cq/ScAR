"function addPool(        string memory _name,        IERC20 _token,        uint256 _weight    ) public override onlyOwner {        totalWeight = totalWeight.add(_weight);        uint256 index = pools.length;        indexByToken[address(_token)].<mask0>;        pools.push(            Pool({name: _name, token: _token, weight: _weight, totalSupply: 0})        );        emit PoolAdded(_msgSender(), index, _name, address(_token), _weight);    }"