"function getCurrentRate(uint _start_time,\tuint _start_rate,\tuint _end_rate,\tuint _duration) internal view  returns (uint current_rate) {  \tif (now < _start_time) {  \t  current_rate = 0;  \t  return;  \t}  \tuint elapsed = now.sub(_start_time);  \tif (elapsed >= _duration) {  \t  current_rate = 0;  \t  return;  \t}  \telapsed = elapsed.<mask0>;  \tuint temp_rate = _start_rate.sub(_end_rate).mul(elapsed).div(_duration);  \ttemp_rate = temp_rate.div(10 ** 18);  \tcurrent_rate = _start_rate.sub(temp_rate);  }"