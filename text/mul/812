"function createTokens(address _recipient, uint256 _value) saleWhenNotPaused {        require (_value != 0);                                                              require (now >= saleStartTime);                                                     require (_recipient != 0x0);                                                \t\tuint256 tokens = _value.<mask0>;                                     uint256 remainingTokenSuppy = balanceOf(bltMasterToSale);        if (remainingTokenSuppy >= tokens) {                                                    require(mint(_recipient, tokens));                                                  etherRaised = etherRaised.add(_value);            forwardFunds();            RecievedEther(msg.sender,_value);        }                                        \t}"