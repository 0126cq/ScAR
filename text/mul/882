"function convert(uint _stakeTimeOption, uint _amount) external nonReentrant {        IERC20(token).safeTransferFrom(msg.sender, address(this), _amount);        uint month = monthFromStart(now);        uint percent = DECIMAL_PRECISION.add(_stakeTimeOptionForMonth(month, _stakeTimeOption));        uint amountDSCP = _amount.<mask0> / DECIMAL_PRECISION;        uint amountDSCPL = amountDSCP.mul(_rate(month)) / DECIMAL_PRECISION_MULTIPLIER;        nonce++;        emit Stake(msg.sender, amountDSCPL, _stakeTimeOption, now, nonce);    }"