"function _getWipeAllWad(address vat, address usr, address urn, bytes32 ilk)        internal        view        returns (uint256 wad)    {        (, uint256 rate, , , ) = VatLike(vat).ilks(ilk);        (, uint256 art) = VatLike(vat).urns(ilk, urn);        uint256 dai = VatLike(vat).dai(usr);        uint256 rad = sub(<mask0>, dai);        wad = rad / RAY;        wad = mul(wad, RAY) < rad ? wad + 1 : wad;    }"