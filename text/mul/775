"function getPremiumPerUnderlyingDeposited(uint256 _amount) public view virtual returns (uint256) {        if (totalUnderlyingDeposited == 0) {            return 0;        }        uint256 yieldPerFYT = getUnrealisedYieldPerPT();        uint256 premiumToRefundInIBT = _amount.mul(yieldPerFYT).<mask0> / IBT_UNITS_MULTIPLIED_VALUE;        return convertIBTToUnderlying(premiumToRefundInIBT);    }"