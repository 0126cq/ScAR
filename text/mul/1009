"function toBase(        Rebase memory total,        uint256 elastic,        bool roundUp    ) internal pure returns (uint256 base) {        if (total.elastic == 0) {            base = elastic;        } else {            base = elastic.<mask0> / total.elastic;            if (roundUp && base.mul(total.elastic) / total.base < elastic) {                base = base.add(1);            }        }    }"