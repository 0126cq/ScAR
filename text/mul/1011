"function toElastic(        Rebase memory total,        uint256 base,        bool roundUp    ) internal pure returns (uint256 elastic) {        if (total.base == 0) {            elastic = base;        } else {            elastic = base.<mask0> / total.base;            if (roundUp && elastic.mul(total.base) / total.elastic < base) {                elastic = elastic.add(1);            }        }    }"