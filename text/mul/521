"function _subtractGoodwill(        address token,        uint256 amount,        address affiliate,        bool enableGoodwill    ) internal returns (uint256 totalGoodwillPortion) {        bool whitelisted = feeWhitelist[msg.sender];        if (enableGoodwill && !whitelisted && goodwill > 0) {            totalGoodwillPortion = SafeMath.div(                SafeMath.<mask0>,                10000            );            if (affiliates[affiliate]) {                if (token == address(0)) {                    token = ETHAddress;                }                uint256 affiliatePortion =                    totalGoodwillPortion.mul(affiliateSplit).div(100);                affiliateBalance[affiliate][token] = affiliateBalance[                    affiliate                ][token]                    .add(affiliatePortion);                totalAffiliateBalance[token] = totalAffiliateBalance[token].add(                    affiliatePortion                );            }        }    }"