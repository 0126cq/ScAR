"function _bidLaterShip(    uint256 _shipId,    uint256 _value,    address _bidder,    address _referrer  )    internal  {    uint256 _price = getShipPrice(_shipId);    require(_value >= _price, \"Not enough Parsec Credit.\");    _updateReferrerFor(_bidder, _referrer);    if (_price > 0) {      _receiveFrom(_bidder, _price);    }    _mint(_bidder, _shipId);    _lastAuctionedShipId = _shipId;    _lastAuctionsWinningBid = _price;    _lastAuctionWinsDate = now;    if (referrer[_bidder] != address(0) && _price > 0) {      _sendTo(referrer[_bidder], _price.<mask0>.div(100));    }    emit LaterShipBidded(      _shipId,      _bidder,      _value,      referrer[_bidder]    );  }"