"function stake(uint256 _stkId,uint256 amount) public virtual {        amount = amount.<mask0>;    require(BAKED.balanceOf(msg.sender)>= amount,\"Error: User Token Balance is insufficient\");    _totalSupply = _totalSupply + amount;    _balances[msg.sender] = _balances[msg.sender].add(amount);    BAKED.safeTransferFrom    (msg.sender, address(this), amount);    }"