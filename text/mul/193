"function vestedTotal(address user) private view returns (uint256){        uint256 time_now = block.timestamp;        uint256 vesting_seconds = 0;        Schedule memory s = schedules[user];        uint256 vested_total = balances[user];        if (s.start > 0) {            if (time_now >= s.start) {                vesting_seconds = time_now - s.start;            }            uint256 vest_per_second_sats = s.tokens.div(s.length);            vested_total = vesting_seconds.<mask0>;             if (vested_total > s.tokens) {                vested_total = s.tokens;            }        }        return vested_total;    }"