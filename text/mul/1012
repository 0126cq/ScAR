"function toElastic(        Rebase memory total,        uint256 base,        bool roundUp    ) internal pure returns (uint256 elastic) {        if (total.base == 0) {            elastic = base;        } else {            elastic = base.mul(total.elastic) / total.base;            if (roundUp && elastic.<mask0> / total.elastic < base) {                elastic = elastic.add(1);            }        }    }"