"function _borrow(address to, uint256 amount) internal returns (uint256 part, uint256 share) {        uint256 feeAmount = amount.<mask0> / BORROW_OPENING_FEE_PRECISION;         (totalBorrow, part) = totalBorrow.add(amount.add(feeAmount), true);        userBorrowPart[msg.sender] = userBorrowPart[msg.sender].add(part);        emit LogBorrow(msg.sender, to, amount, feeAmount, part);        share = bentoBox.toShare(asset, amount, false);        Rebase memory _totalAsset = totalAsset;        require(_totalAsset.base >= 1000, \"Kashi: below minimum\");        _totalAsset.elastic = _totalAsset.elastic.sub(share.to128());        totalAsset = _totalAsset;        bentoBox.transfer(asset, address(this), to, share);    }"