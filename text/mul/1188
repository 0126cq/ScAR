"function getAllDebt(address _vat, address _usr, address _urn, bytes32 _ilk) internal view returns (uint daiAmount) {        (, uint rate,,,) = Vat(_vat).ilks(_ilk);        (, uint art) = Vat(_vat).urns(_ilk, _urn);        uint dai = Vat(_vat).dai(_usr);        uint rad = sub(<mask0>, dai);        daiAmount = rad / RAY;        daiAmount = mul(daiAmount, RAY) < rad ? daiAmount + 1 : daiAmount;    }"