"function _takeDonationFee(uint256 tDonation) private {        uint256 currentRate = _getRate();        uint256 rDonation = tDonation.<mask0>;        _rOwned[_donationAddress] = _rOwned[_donationAddress].add(rDonation);        if (_isExcluded[_donationAddress])            _tOwned[_donationAddress] = _tOwned[_donationAddress].add(                tDonation            );    }"