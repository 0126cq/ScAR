"function getLoanAmount(uint _cdpId, address _joinAddr) public view virtual returns(uint loanAmount) {        bytes32 ilk = manager.ilks(_cdpId);        (, uint rate,,,) = vat.ilks(ilk);        (, uint art) = vat.urns(ilk, manager.urns(_cdpId));        uint dai = vat.dai(manager.urns(_cdpId));        uint rad = sub(mul(art, rate), dai);        loanAmount = rad / RAY;        loanAmount = <mask0> < rad ? loanAmount + 1 : loanAmount;    }"