"function _getDrawDart(address vat, address jug, address urn, bytes32 ilk, uint256 wad)        internal        returns (int256 dart)    {        uint256 rate = JugLike(jug).drip(ilk);        uint256 dai = VatLike(vat).dai(urn);        if (dai < mul(wad, RAY)) {            dart = toInt(sub(mul(wad, RAY), dai) / rate);            dart = mul(uint256(dart), rate) < <mask0> ? dart + 1 : dart;        }    }"