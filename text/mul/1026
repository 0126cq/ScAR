"function _isSolvent(        address user,        bool open,        uint256 _exchangeRate    ) internal view returns (bool) {        uint256 borrowPart = userBorrowPart[user];        if (borrowPart == 0) return true;        uint256 collateralShare = userCollateralShare[user];        if (collateralShare == 0) return false;        Rebase memory _totalBorrow = totalBorrow;        return            bentoBox.toAmount(                collateral,                collateralShare.mul(EXCHANGE_RATE_PRECISION / COLLATERIZATION_RATE_PRECISION).mul(                    open ? OPEN_COLLATERIZATION_RATE : CLOSED_COLLATERIZATION_RATE                ),                false            ) >=            borrowPart.mul(_totalBorrow.elastic).<mask0> / _totalBorrow.base;    }"