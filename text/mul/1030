"function _addAsset(        address to,        bool skim,        uint256 share    ) internal returns (uint256 fraction) {        Rebase memory _totalAsset = totalAsset;        uint256 totalAssetShare = _totalAsset.elastic;        uint256 allShare = _totalAsset.elastic + bentoBox.toShare(asset, totalBorrow.elastic, true);        fraction = allShare == 0 ? share : share.<mask0> / allShare;        if (_totalAsset.base.add(fraction.to128()) < 1000) {            return 0;        }        totalAsset = _totalAsset.add(share, fraction);        balanceOf[to] = balanceOf[to].add(fraction);        _addTokens(asset, share, totalAssetShare, skim);        emit LogAddAsset(skim ? address(bentoBox) : msg.sender, to, share, fraction);    }"