"function normalizeDrawAmount(uint _amount, uint _rate, uint _daiVatBalance) internal pure returns (int dart) {        if (_daiVatBalance < mul(_amount, RAY)) {            dart = toPositiveInt(sub(<mask0>, _daiVatBalance) / _rate);            dart = mul(uint(dart), _rate) < mul(_amount, RAY) ? dart + 1 : dart;        }    }"