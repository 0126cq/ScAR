"function calculateTokensToMint(        nTokenPortfolio memory nToken,        int256 amountToDepositInternal,        uint256 blockTime    ) internal view returns (int256) {        require(amountToDepositInternal >= 0);         if (amountToDepositInternal == 0) return 0;        if (nToken.lastInitializedTime != 0) {            uint256 nextSettleTime = nToken.getNextSettleTime();            require(nextSettleTime > blockTime, \"Requires settlement\");        }        int256 assetCashPV = nToken.getNTokenAssetPV(blockTime);        require(assetCashPV >= 0);        if (nToken.totalSupply == 0) {            return amountToDepositInternal;        } else {            return amountToDepositInternal.<mask0>.div(assetCashPV);        }    }"