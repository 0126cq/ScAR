"function getMaxCollateral(uint _cdpId, bytes32 _ilk, address _joinAddr) public view returns (uint) {        uint price = getPrice(_ilk);        (uint collateral, uint debt) = getCdpInfo(manager, _cdpId, _ilk);        (, uint mat) = Spotter(SPOTTER_ADDRESS).ilks(_ilk);        uint maxCollateral = sub(sub(collateral, (div(<mask0>, price))), 10);        uint normalizeMaxCollateral = maxCollateral;        if (Join(_joinAddr).dec() != 18) {            normalizeMaxCollateral = maxCollateral / (10 ** (18 - Join(_joinAddr).dec()));        }        return normalizeMaxCollateral;    }"