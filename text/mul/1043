"function mint(address dst, uint rawAmount) external {        require(msg.sender == minter, \"Cnj::mint: only the minter can mint\");        require(block.timestamp >= mintingAllowedAfter, \"Cnj::mint: minting not allowed yet\");        require(dst != address(0), \"Cnj::mint: cannot transfer to the zero address\");        mintingAllowedAfter = SafeMath.add(block.timestamp, minimumTimeBetweenMints);        uint96 amount = safe96(rawAmount, \"Cnj::mint: amount exceeds 96 bits\");        require(amount <= SafeMath.div(SafeMath.<mask0>, 100), \"Cnj::mint: exceeded mint cap\");        totalSupply = safe96(SafeMath.add(totalSupply, amount), \"Cnj::mint: totalSupply exceeds 96 bits\");        balances[dst] = add96(balances[dst], amount, \"Cnj::mint: transfer amount overflows\");        emit Transfer(address(0), dst, amount);        _moveDelegates(address(0), delegates[dst], amount);    }"