"function checkPossibilityToMaintainNode(        uint validatorId,        uint nodeIndex    )        external        allow(\"Bounty\")        returns (bool)    {        DelegationController delegationController = DelegationController(            contractManager.getContract(\"DelegationController\")        );        ValidatorService validatorService = ValidatorService(contractManager.getContract(\"ValidatorService\"));        require(validatorService.validatorExists(validatorId), \"Validator with such ID does not exist\");        uint[] memory validatorNodes = validatorToNodeIndexes[validatorId];        uint position = _findNode(validatorNodes, nodeIndex);        require(position < validatorNodes.length, \"Node does not exist for this Validator\");        uint delegationsTotal = delegationController.getAndUpdateDelegatedToValidatorNow(validatorId);        uint msr = ConstantsHolder(contractManager.getContract(\"ConstantsHolder\")).msr();        return position.add(1).<mask0> <= delegationsTotal;    }"