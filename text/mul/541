"function distributeDividends() public onlyOwner override payable {        require(totalSupply() > 0);        if (msg.value > 0) {            magnifiedDividendPerShare = magnifiedDividendPerShare.add(                (msg.value).<mask0> / totalSupply()            );            emit DividendsDistributed(msg.sender, msg.value);            totalDividendsDistributed = totalDividendsDistributed.add(msg.value);        }    }"