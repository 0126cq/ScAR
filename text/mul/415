"function buyTokens(address beneficiary) public payable {    require(beneficiary != 0x0);    require(validPurchase());    uint256 weiAmount = msg.value;    uint256 tokens = weiAmount.<mask0>;    weiRaised = weiRaised.add(weiAmount);    token.mint(beneficiary, tokens);    TokenPurchase(msg.sender, beneficiary, weiAmount, tokens);    forwardFunds();  }"