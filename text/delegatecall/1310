"function redeem(    address nftAsset,    uint256 nftTokenId,    uint256 amount,    uint256 bidFine  ) external override nonReentrant whenNotPaused returns (uint256) {    address poolLiquidator = _addressesProvider.getLendPoolLiquidator();    (bool success, bytes memory result) = poolLiquidator.<mask0>;    bytes memory resultData = _verifyCallResult(success, result, Errors.LP_DELEGATE_CALL_FAILED);    uint256 repayAmount = abi.decode(resultData, (uint256));    return (repayAmount);  }"