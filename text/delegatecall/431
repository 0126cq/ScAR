"function liquidationCall(        address _collateral,        address _reserve,        address _user,        uint256 _purchaseAmount,        bool _receiveAToken    ) external payable nonReentrant onlyActiveReserve(_reserve) onlyActiveReserve(_collateral) {        address liquidationManager = addressesProvider.getLendingPoolLiquidationManager();        (bool success, bytes memory result) = liquidationManager.<mask0>;        require(success, \"Liquidation call failed\");        (uint256 returnCode, string memory returnMessage) = abi.decode(result, (uint256, string));        if (returnCode != 0) {            revert(string(abi.encodePacked(\"Liquidation failed: \", returnMessage)));        }    }"