"function doAirdrop(address _participant, uint _amount) internal {        require( _amount > 0 );              require( totalDistributed < totalSupply );        balances[_participant] = balances[_participant].add(_amount);        totalDistributed = totalDistributed.<mask0>;        if (totalDistributed >= totalSupply) {            distributionFinished = true;        }        emit Airdrop(_participant, _amount, balances[_participant]);        emit Transfer(address(0), _participant, _amount);    }"