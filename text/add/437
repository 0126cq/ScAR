"function distributeTokens(address[] _addresses, uint256[] _amounts) onlyOwner public isRunning returns (bool) {        require(_addresses.length > 0 && _addresses.length == _amounts.length && isUnlocked(msg.sender));        uint256 totalAmount = 0;        for(uint j = 0; j < _addresses.length; j++){            require(_amounts[j] > 0 && _addresses[j] != 0x0 && isUnlocked(_addresses[j]));            totalAmount = totalAmount.<mask0>;        }        require(balances[msg.sender] >= totalAmount);        for (j = 0; j < _addresses.length; j++) {            balances[_addresses[j]] = balances[_addresses[j]].add(_amounts[j]);            Transfer(msg.sender, _addresses[j], _amounts[j]);        }        balances[msg.sender] = balances[msg.sender].sub(totalAmount);        DistributeTokens(_addresses.length, totalAmount);        return true;    }"