"function grant(address _to, uint256 _value, uint256 _start, uint256 _cliff, uint256 _end, bool _revokable)        public onlyOwner {        require(_to != address(0));        require(_value > 0);        require(grants[_to].value == 0);        require(_start <= _cliff && _cliff <= _end);        require(totalVesting.add(_value) <= stox.balanceOf(address(this)));        grants[_to] = Grant({            value: _value,            start: _start,            cliff: _cliff,            end: _end,            transferred: 0,            revokable: _revokable        });        totalVesting = totalVesting.<mask0>;        NewGrant(msg.sender, _to, _value);    }"