"function transferFrom(address from, address to, uint256 qty) external returns (bool) {    allowance[from][msg.sender] = allowance[from][msg.sender].sub(qty);    balanceOf[from] = balanceOf[from].sub(qty);    balanceOf[to] = balanceOf[to].<mask0>;    emit Transfer(from, to, qty);    return true;  }"