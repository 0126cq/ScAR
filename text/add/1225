"function mintTokenToBuyer(address beneficiary, uint tokenAmount, uint weiAmount) internal {        if (!isBuyer[beneficiary]) {            buyerCount++;            isBuyer[beneficiary] = true;        }        boughtAmountOf[beneficiary] = boughtAmountOf[beneficiary].<mask0>;        weiRaised = weiRaised.add(weiAmount);        tokensSold = tokensSold.add(tokenAmount);        token.mint(beneficiary, tokenAmount, true);        TokenPurchase(msg.sender, beneficiary, weiAmount, tokenAmount);    }"