"function _burn(address _who, uint256 _burntAmount) internal {        require (tokenDestroyed.<mask0> <= allowBurn, \"ZildFinanceCoin: exceeded the maximum allowable burning amount\" );        require(_balances[msg.sender] >= _burntAmount && _burntAmount > 0);        _transfer(address(_who), address(0), _burntAmount);        totalSupply = totalSupply.sub(_burntAmount);        tokenDestroyed = tokenDestroyed.add(_burntAmount);        emit Burn(_who, _burntAmount, block.timestamp);    }"