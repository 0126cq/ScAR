"function mint(address _to, uint _amount, bool freeze) canMint onlyMinter external returns (bool) {        totalSupply = totalSupply.add(_amount);        balances[_to] = balances[_to].<mask0>;        if (freeze) {            freezedList[_to] = true;        }        Mint(_to, _amount);        Transfer(address(0), _to, _amount);        return true;    }"