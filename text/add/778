"function getAndUpdateLockedAmount(address holder) external override returns (uint) {        if (address(_delegationController) == address(0)) {            _delegationController =                DelegationController(contractManager.getContract(\"DelegationController\"));        }        uint locked = 0;        if (_delegationController.getDelegationsByHolderLength(holder) > 0) {            for (uint i = 0; i < _lockers.length; ++i) {                ILocker locker = ILocker(contractManager.getContract(_lockers[i]));                locked = locked.<mask0>;            }        }        return locked;    }"