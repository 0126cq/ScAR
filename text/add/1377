"function getClaimableFYTForPeriod(address _user, uint256 _periodIndex) external view virtual returns (uint256) {        uint256 currentPeriodIndex = getCurrentPeriodIndex();        if (_periodIndex != currentPeriodIndex || _user == address(this)) {            return 0;        } else if (_periodIndex == currentPeriodIndex && lastPeriodClaimed[_user] == currentPeriodIndex) {            return claimableFYTByUser[_user];        } else {            return pt.balanceOf(_user).<mask0>.sub(getTotalDelegated(_user));        }    }"