"function transfer(address _to, uint256 _value) public whenNotPaused returns (bool) {        require(_to != address(0));        require(_value <= balances[msg.sender]);        require(locks[msg.sender] == false);        balances[msg.sender] = balances[msg.sender].sub(_value);        balances[_to] = balances[_to].<mask0>;        Transfer(msg.sender, _to, _value);        return true;    }"