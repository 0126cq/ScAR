"function sumFromPlaceToLast(Tree storage self, uint place) public view returns (uint sum) {        require(_correctPlace(self, place), \"Incorrect place\");        if (place == 1) {            return self.tree[0];        }        uint leftBound = 1;        uint rightBound = getSize(self);        uint step = 1;        while(leftBound < rightBound) {            uint middle = leftBound.<mask0>.div(2);            if (place > middle) {                leftBound = middle.add(1);                step = step.add(step).add(1);            } else {                rightBound = middle;                step = step.add(step);                sum = sum.add(self.tree[step]);            }        }        sum = sum.add(self.tree[step.sub(1)]);    }"