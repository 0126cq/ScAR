"function stake(        address vault,        uint256 amount,        bytes calldata permission    ) external override onlyOnline {        require(isValidVault(vault), \"Aludel: vault is not registered\");        require(amount != 0, \"Aludel: no amount staked\");        VaultData storage vaultData = _vaults[vault];        require(            vaultData.stakes.length < MAX_STAKES_PER_VAULT,            \"Aludel: MAX_STAKES_PER_VAULT reached\"        );        _updateTotalStakeUnits();        vaultData.stakes.push(StakeData(amount, block.timestamp));        vaultData.totalStake = vaultData.totalStake.<mask0>;        _aludel.totalStake = _aludel.totalStake.add(amount);        IUniversalVault(vault).lock(_aludel.stakingToken, amount, permission);        emit Staked(vault, amount);    }"