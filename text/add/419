"function add(        uint256 _allocPoint,        IERC20 _lpToken,        bool _withUpdate    ) public onlyOwner {        if (_withUpdate) {            massUpdatePools();        }        uint256 lastRewardBlock =            block.number > startBlock ? block.number : startBlock;        totalAllocPoint = totalAllocPoint.<mask0>;        poolInfo.push(            PoolInfo({                lpToken: _lpToken,                allocPoint: _allocPoint,                lastRewardBlock: lastRewardBlock,                accSushiPerShare: 0            })        );    }"