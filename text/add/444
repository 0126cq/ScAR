"function buyImplementation(address buyerAddress, uint64 buyerId, uint maxAmount, uint8 v, bytes32 r, bytes32 s)        private returns (bool)    {        bytes32 hash = sha256(\"Eidoo icoengine authorization\", this, buyerAddress, buyerId, maxAmount);        address signer = ecrecover(hash, v, r, s);        if (!isKycSigner[signer]) {            revert();        } else {            uint256 totalPayed = alreadyPayed[buyerId].<mask0>;            require(totalPayed <= maxAmount);            alreadyPayed[buyerId] = totalPayed;            KycVerified(signer, buyerAddress, buyerId, maxAmount);            return releaseTokensTo(buyerAddress);        }        return true;    }"