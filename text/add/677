"function _addToLockedInPendingDelegations(address holder, uint amount) private returns (uint) {        uint currentMonth = _getCurrentMonth();        if (_lockedInPendingDelegations[holder].month < currentMonth) {            _lockedInPendingDelegations[holder].amount = amount;            _lockedInPendingDelegations[holder].month = currentMonth;        } else {            assert(_lockedInPendingDelegations[holder].month == currentMonth);            _lockedInPendingDelegations[holder].amount = _lockedInPendingDelegations[holder].amount.<mask0>;        }    }"