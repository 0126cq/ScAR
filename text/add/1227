"function mintTokenToBuyer(address beneficiary, uint tokenAmount, uint weiAmount) internal {        if (!isBuyer[beneficiary]) {            buyerCount++;            isBuyer[beneficiary] = true;        }        boughtAmountOf[beneficiary] = boughtAmountOf[beneficiary].add(weiAmount);        weiRaised = weiRaised.add(weiAmount);        tokensSold = tokensSold.<mask0>;        token.mint(beneficiary, tokenAmount, true);        TokenPurchase(msg.sender, beneficiary, weiAmount, tokenAmount);    }"