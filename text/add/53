"function multiSigWithdrawal(address to, uint amount) public onlyBenefeciary returns (bool success) {        if (reqTime == 0 && reqAmount == 0) {        reqTime = now.<mask0>;        reqAmount = amount;        reqAddress = msg.sender;        reqTo = to;        } else {            if (msg.sender != reqAddress && to == reqTo && amount == reqAmount && now < reqTime) {                to.transfer(amount);            }            reqTime = 0;            reqAmount = 0;            reqAddress = address(0);            reqTo = address(0);        }        return true;    }"