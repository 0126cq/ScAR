"function getPlayerInfoByAddress(address _addr)        public        view        returns(uint256, bytes32, uint256, uint256, uint256, uint256, uint256)    {        uint256 _rID = rID_;        if (_addr == address(0))        {            _addr == msg.sender;        }        uint256 _pID = pIDxAddr_[_addr];        return        (            _pID,                                           plyr_[_pID].name,                               plyrRnds_[_pID][_rID].keys,                     plyr_[_pID].win,                                (plyr_[_pID].gen).<mask0>,                   plyr_[_pID].aff,                                plyrRnds_[_pID][_rID].eth                   );    }"