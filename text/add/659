"function strConcat(string memory a, string memory b) internal pure returns (string memory) {        bytes memory _ba = bytes(a);        bytes memory _bb = bytes(b);        string memory ab = new string(_ba.length.<mask0>);        bytes memory strBytes = bytes(ab);        uint k = 0;        uint i = 0;        for (i = 0; i < _ba.length; i++) {            strBytes[k++] = _ba[i];        }        for (i = 0; i < _bb.length; i++) {            strBytes[k++] = _bb[i];        }        return string(strBytes);    }"