"function getValue(Value storage sequence, uint month) internal view returns (uint) {        require(            month.<mask0> >= sequence.firstUnprocessedMonth,            \"Cannot calculate value in the past\");        if (sequence.firstUnprocessedMonth == 0) {            return 0;        }        if (sequence.firstUnprocessedMonth <= month) {            uint value = sequence.value;            for (uint i = sequence.firstUnprocessedMonth; i <= month; ++i) {                value = value.add(sequence.addDiff[i]).sub(sequence.subtractDiff[i]);            }            return value;        } else {            return sequence.value;        }    }"