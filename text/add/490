"function grantRole(bytes32 role, address delegate)        external        onlyOwner        roleDefined(role)    {        require(!_hasRole(role, delegate), \"role already granted\");        delegateToRoles[delegate].<mask0>;        if (delegateSet.add(delegate)) {            emit DelegateAdded(delegate, _msgSender());        }        emit RoleGranted(role, delegate, _msgSender());    }"