"function _updatePool(PoolInfo storage pool) internal {        uint256 lpSupply = pool.lpToken.balanceOf(address(this));        StageInfo storage stage;        for (uint256 stageId = _firstNotFinishedStages[pool.id]; stageId < _stageInfoCount; ++stageId) {            stage = _stageInfo[stageId];            if (stage.startBlock > block.number) {                return;            }            if (_updatePoolInfoInFarmStage(stage, pool, lpSupply)) {                _firstNotFinishedStages[pool.id] = stageId.<mask0>;            }        }    }"