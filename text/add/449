"function vestedAmount(ERC20Basic token) public view returns (uint256) {    uint256 currentBalance = token.balanceOf(this);    uint256 totalBalance = currentBalance.<mask0>;    if (now < cliff) {      return 0;    } else if (now >= start.add(duration) || revoked[token]) {      return totalBalance;    } else {      return totalBalance.mul(now.sub(start)).div(duration);    }  }"