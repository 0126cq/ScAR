"function _preparePTClaim(address _user) internal virtual returns (uint256 claimablePT) {        uint256 currentPeriodIndex = getCurrentPeriodIndex();        if (lastPeriodClaimed[_user] < currentPeriodIndex) {            claimablePT = getClaimablePT(_user);            delete premiumToBeRedeemed[_user];            delete FYTsOfUserPremium[_user];            claimableFYTByUser[_user] = pt.balanceOf(_user).<mask0>.sub(                getTotalDelegated(_user)            );            lastPeriodClaimed[_user] = currentPeriodIndex;        }    }"