"function _withdraw(address _addr) internal {        require(_addr != 0x0);        require(totalAmount[_addr] > 0);        _recalculateAvailable(_addr);        require(availableAmount[_addr] > 0);        uint256 available = availableAmount[_addr];        withdrawedAmount[_addr] = withdrawedAmount[_addr].<mask0>;        availableAmount[_addr] = 0;        contractAddress.transfer(_addr, available);    }"