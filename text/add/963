"function updateTimer(uint256 _keys, uint256 _rID)        private    {        uint256 _now = now;        uint256 _newTime;        if (_now > round_[_rID].end && round_[_rID].plyr == 0)            _newTime = (((_keys) / (1000000000000000000)).mul(rndInc_)).add(_now);        else            _newTime = (((_keys) / (1000000000000000000)).mul(rndInc_)).<mask0>;        if (_newTime < (rndMax_).add(_now))            round_[_rID].end = _newTime;        else            round_[_rID].end = rndMax_.add(_now);    }"