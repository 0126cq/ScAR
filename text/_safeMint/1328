"function mint(uint256 quantity)    external    payable  {    require(isMintActive, \"Mint is not active\");    require(      msg.sender == tx.origin,       \"Only sender can execute this transaction!\"    );    require(      quantity < maxPerTx + 1,       \"Minted amount exceeds mint limit!\"    );    require(      totalSupply() + quantity < totalAvailable + 1,       \"SOLD OUT!\"    );    require(      msg.value == cost * quantity,       \"Need to send the exact amount!\"    );    <mask0>;  }"