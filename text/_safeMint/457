"function mint(uint256 numberOfMints) public payable {        uint256 supply = totalSupply();        require(saleActive,                                 \"Sale must be active to mint\");        require(numberOfMints > 0 && numberOfMints < 6,    \"Invalid purchase amount\");        require(supply.add(numberOfMints) <= maxSupply,     \"Purchase would exceed max supply of tokens\");        require(price.mul(numberOfMints) == msg.value,      \"Ether value sent is not correct\");        for(uint256 i; i < numberOfMints; i++) {            <mask0>;        }    }"