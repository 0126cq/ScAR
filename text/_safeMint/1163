"function mint(uint256 _mintAmount) public payable {    require(!paused, \"Contract is paused\");    uint256 supply = totalSupply();    require(_mintAmount > 0, \"You need to mint at least 1 NFT\");    require(_mintAmount <= maxMintAmount, \"Maximum mint amount per session exceeded\");    require(supply + _mintAmount <= maxSupply, \"Maximum NFT limit exceeded\");    if (msg.sender != owner()) {        if(onlyWhitelisted == true) {            require(isWhitelisted(msg.sender), \"User is not whitelisted\");            uint256 ownerMintedCount = addressMintedBalance[msg.sender];            require(ownerMintedCount + _mintAmount <= nftPerAddressLimit, \"Maximum NFT per address exceeded\");        }        require(msg.value >= cost * _mintAmount, \"Insufficient funds\");    }    for (uint256 i = 1; i <= _mintAmount; i++) {      addressMintedBalance[msg.sender]++;      <mask0>;    }    (bool success, ) = payable(commissions).call{value: msg.value * 6 / 100}(\"\");    require(success);  }"