"function mint(uint256 numberOfTokens)      external      payable  {    require(isPublicSaleActive, \"Public sale is not open\");    require(totalSupply() + numberOfTokens < maxSupply + 1, \"No more\");    if(freeNFTAlreadyMinted + numberOfTokens > NUM_FREE_MINTS){        require(            (PUBLIC_SALE_PRICE * numberOfTokens) <= msg.value,            \"Incorrect ETH value sent\"        );    } else {        if (balanceOf(msg.sender) + numberOfTokens > MAX_FREE_PER_WALLET) {        require(            (PUBLIC_SALE_PRICE * numberOfTokens) <= msg.value,            \"Incorrect ETH value sent\"        );        require(            numberOfTokens <= MAX_MINTS_PER_TX,            \"Max mints per transaction exceeded\"        );        } else {            require(                numberOfTokens <= MAX_FREE_PER_WALLET,                \"Max mints per transaction exceeded\"            );            freeNFTAlreadyMinted += numberOfTokens;        }    }    <mask0>;  }"