"function mintInWhitelist() external payable onlyWhenWhitelistSaleIsActive {        require(totalSupply() + 1 <= maxNFTCirculation, \"TCM: Cap exceeded\");        require(mintCost <= msg.value, \"TCM: Incorrect Ether Value\");        require(whitelist[msg.sender] == true, \"User not in whitelist\");        require(hasBought[msg.sender] == false, \"TCM: User may only buy one pass\");        hasBought[msg.sender] = true;          whitelist[msg.sender] = false;         <mask0>;    }"