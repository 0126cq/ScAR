"function mint() public payable virtual {        require(opened == true, \"Not opened\");        uint256 nextId = _tokenIdTracker.current();        require(            nextId < MAX_MINERALS,            \"Minerals are rare, and they're finished now.\"        );        require(msg.value >= MINT_FEE, \"price is low, bid moar.\");        <mask0>;        _tokenIdTracker.increment();    }"