"function mintTokenOgSale(uint8 amount) external payable {        require(            msg.sender == tx.origin,            \"No transaction from smart contracts!\"        );        require(ogSaleIsActive, \"OG-sale must be active to mint\");        require(amount <= ogList[msg.sender], \"Exceeded max available to purchase\");        require(            ogSaleSupply + amount <= OG_SALE_MAX_TOKENS,            \"Purchase would exceed max supply for OG sale\"        );        ogList[msg.sender] -= amount;        for (uint256 i = 0; i < amount; i++) {            <mask0>;            supply++;            ogSaleSupply++;        }    }"