"function mint(uint8 numberOfTokens) public payable nonReentrant {        require(saleIsActive, \"not allowed\");        require(numberOfTokens <= MAX_PURCHASE, \"exceeds purchase limit\");        require(totalSupply().add(numberOfTokens) <= currentMaxTokens, \"exceeds supply\");        require(TOKEN_PRICE.mul(numberOfTokens) <= msg.value, \"send more ETH\");        uint256 tokenId;        for(uint8 i; i < numberOfTokens; i++) {            tokenId = totalSupply().add(1);            <mask0>;        }    }"