"function mint(uint256 _mintAmount, bytes memory _signature) public payable {        require(!paused, \"the contract is paused\");        uint256 supply = totalSupply();        require(_mintAmount > 0, \"need to mint at least 1 NFT\");        require(supply + _mintAmount <= maxSupply, \"max NFT limit exceeded\");        uint256 maxMint = maxMintAmount;        uint256 nftPerAdress = nftPerAddressLimit;        if (msg.sender != owner()) {            if (onlyWhitelisted == true) {                if (!isOGListed(_signature)) {                    require(                        isWhitelisted(_signature),                        \"user is not whitelisted\"                    );                } else {                    maxMint = 2;                    nftPerAdress = 2;                }                uint256 ownerMintedCount = addressMintedBalance[msg.sender];                require(                    ownerMintedCount + _mintAmount <= nftPerAdress,                    \"max NFT per address exceeded\"                );            }            require(                _mintAmount <= maxMint,                \"max mint amount per session exceeded\"            );            require(msg.value >= cost * _mintAmount, \"insufficient funds!\");        }        for (uint256 i = 1; i <= _mintAmount; i++) {            addressMintedBalance[msg.sender]++;            <mask0>;        }    }"