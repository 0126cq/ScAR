"function magicGift(address[] calldata receivers) external onlyOwner {        require(            _tokenIds.current() + receivers.length <= MAX_TOKENS,            \"Exceeds maximum token supply\"        );        require(            numberOfGifts + receivers.length <= MAX_GIFTS,            \"Exceeds maximum allowed gifts\"        );        for (uint256 i = 0; i < receivers.length; i++) {            numberOfGifts++;            <mask0>;            _tokenIds.increment();        }    }"