"function mintToken(uint8 amount) external payable {        require(            msg.sender == tx.origin,            \"No transaction from smart contracts!\"        );        require(amount > 0 && amount <= 5, \"Max 5 NFTs per transaction\");        require(saleIsActive, \"Sale must be active to mint\");        require(msg.value >= PRICE * amount, \"Not enough ETH for transaction\");        for (uint256 i = 0; i < amount; i++) {            <mask0>;            supply++;        }    }"