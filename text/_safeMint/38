"function publicSaleMintAnimals(uint8 amount) external payable botDefender {        Config memory config = _config;        if (!config.isPublicSaleActive) revert ErrorPublicSaleNotStarted();        if (amount > PUBLIC_SALE_MAX_MINT_PER_TX) revert ErrorExceedPublicSaleTransactionLimit();        ensureSupply(config, amount);        ensureSufficientValue(config.publicSalePrice, amount);        if (incrementPublicMinted(amount) > PUBLIC_SALE_MAX_MINT_PER_WALLET) revert ErrorExceedPublicSaleWalletLimit();        <mask0>;    }"