"function whitelistMint(uint8 _mintAmount) external payable {        uint8 nft = NFTPerWLAddress[msg.sender];       require(isWhitelisted[msg.sender],  \"You are not whitelisted\");       require (nft + _mintAmount <= maxMintAmountPerTx, \"Exceeds max  limit  per address\");    require(!WLpaused, \"Whitelist minting is over!\");         if(nft >= maxFreeMintAmountPerWallet)    {    require(msg.value >= whiteListCost * _mintAmount, \"Insufficient funds!\");    }    else {         uint8 costAmount = _mintAmount + nft;        if(costAmount > maxFreeMintAmountPerWallet)       {        costAmount = costAmount - maxFreeMintAmountPerWallet;        require(msg.value >= whiteListCost * costAmount, \"Insufficient funds!\");       }    }     <mask0>;      NFTPerWLAddress[msg.sender] =nft + _mintAmount;      delete _mintAmount;       delete nft;    }"