"function mint(uint8 _mintAmount) external payable  {    uint totalSupply = totalSupply();    require(totalSupply + _mintAmount <= maxSupply, \"Excedes max supply.\");    uint8 _nftperaddress = NFTPerAddress[msg.sender];    require(_mintAmount + _nftperaddress <= maxMintAmountPerWallet, \"Exceeds max per transaction.\");    require(!paused, \"The contract is paused!\");    require(msg.value == cost * _mintAmount, \"Insufficient funds!\");    <mask0>;     NFTPerAddress[msg.sender] = _mintAmount + _nftperaddress;     delete totalSupply;     delete _mintAmount;  }"