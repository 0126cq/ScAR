"function mintTo(address _to) public payable {        require(onlyERC20MintingMode == false, \"Only minting with ERC-20 tokens is enabled.\");        require(getNextTokenId() <= collectionSize, \"Cannot mint over supply cap of 1000\");        require(mintingOpen == true, \"Minting is not open right now!\");        require(canMintAmount(_to, 1), \"Wallet address is over the maximum allowed mints\");        require(msg.value == getPrice(1), \"Value needs to be exactly the mint fee!\");        <mask0>;    }"