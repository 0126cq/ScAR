"function mint(uint8 numTokens) external payable {        require(msg.sender == tx.origin, \"User wallet required\");        require(publicSaleStartTime != 0 && publicSaleStartTime <= block.timestamp, \"sales is not started\");        require(numTokens <= 2, \"Max 2 NFTs per transaction\");        require(minted[msg.sender] + numTokens <= maxMintPerWallet, \"limit per wallet reached\");        require(nextTokenId + numTokens <= maxMint, \"Mint exceeds supply\");        _safeMint(msg.sender, ++nextTokenId);        if (numTokens == 2) {            <mask0>;        }        minted[msg.sender] += numTokens;    }"