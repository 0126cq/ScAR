"function buyFrogs(uint tokensNumber) public payable {        require(tokensNumber > 0, \"Wrong amount\");        require(!finalized, \"Finalized!\");        require(mainSaleActive, \"Sale must be active to mint tokens\");        require(tokensNumber <= maxTokensPerTransaction, \"Max tokens per transaction number exceeded\");        require(_tokenIdCounter.current() + tokensNumber <= nftsNumber, \"Tokens number to mint exceeds number of public tokens\");        require(tokenPrice*tokensNumber <= msg.value, \"Ether value sent is too low\");        for (uint i = 0; i < tokensNumber; i++) {            <mask0>;            setXDone(_tokenIdCounter.current());            _tokenIdCounter.increment();        }    }"