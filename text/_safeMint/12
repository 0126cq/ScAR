"function sekiraPublicMint(uint8 amount) external payable botDefender {        Config memory config = _config;        require(config.isPublicSaleActive, \"SeKira: public sale is not started\");        ensureSupply(config, amount);        ensureSufficientValue(config.publicSalePrice, amount);        require(incrementPublicMinted(amount) <= PUBLIC_SALE_MAX_MINT, \"SeKira: exceed public sale mint limit\");        <mask0>;    }"