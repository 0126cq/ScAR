"function mintTokenWlSale(uint8 amount) external payable {        require(            msg.sender == tx.origin,            \"No transaction from smart contracts!\"        );        require(wlSaleIsActive, \"OG-sale must be active to mint\");        require(amount <= whiteList[msg.sender], \"Exceeded max available to purchase\");        require(msg.value >= PRICE * amount, \"Not enough ETH for transaction\");        require(            wlSaleSupply + amount <= WL_SALE_MAX_TOKENS,            \"Purchase would exceed max supply for OG sale\"        );        whiteList[msg.sender] -= amount;        for (uint256 i = 0; i < amount; i++) {            <mask0>;            supply++;            wlSaleSupply++;        }    }"