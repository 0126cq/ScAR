"function mintAndAirdrop(    address[] calldata walletAddresses,    uint256[] calldata amounts  ) external onlyOwner {    require(totalSupply() + amounts.length <= 2454, \"airdrop goes till 2454\");    require(      amounts.length == walletAddresses.length,      \"arrays not equal in length\"    );    require(      walletAddresses.length <= 100,      \"you can only airdrop 100 tokens at once\"    );    for (uint256 x = 0; x < walletAddresses.length; x++) {      <mask0>;    }    emit MintAndAirdrop(walletAddresses.length);  }"