"function airdrop(address[] memory recipients, uint256[] memory amounts) external onlyOwner {        require(recipients.length == amounts.length, \"Arrays lengths are different\");        uint256 numToMint = 0;        for (uint256 i=0; i<amounts.length; i++) {            numToMint += amounts[i];        }        require((numToMint + totalSupply()) <= maxSupply, \"Not enough token left\");        for (uint256 i=0; i<recipients.length; i++) {            <mask0>;        }    }"