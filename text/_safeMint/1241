"function mintPaperclip(uint numberOfTokens) public payable {        require(saleIsActive, \"Sale is not active!\");        require(numberOfTokens > 0 && numberOfTokens <= maxPaperclipPurchase, \"You can only mint two per transaction!\");        require(totalSupply().add(numberOfTokens) <= MAX_PAPERCLIPS, \"Maximum supply reached!\");        if(totalSupply() < freePaperclips) {            require(totalSupply().add(numberOfTokens) <= freePaperclips, \"Those are not free anymore!\");            for(uint i = 0; i < numberOfTokens; i++) {                uint mintIndex = totalSupply();                if(totalSupply() < MAX_PAPERCLIPS) {                    _safeMint(msg.sender, mintIndex);                }            }        } else {            require(msg.value >= paperclipPrice.mul(numberOfTokens), \"Ether value sent is not correct\");            for(uint i = 0; i < numberOfTokens; i++) {                uint mintIndex = totalSupply();                if (totalSupply() < MAX_PAPERCLIPS) {                    <mask0>;                }            }        }    }"