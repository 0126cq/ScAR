"function buyWhite(uint tokensNumber) public payable {        require(tokensNumber > 0, \"Wrong amount\");        require(!finalized, \"Finalized!\");        require(_tokenIdCounter.current() + tokensNumber <= nftsNumber, \"Tokens number to mint exceeds number of public tokens\");        require(tokenPriceWhite * tokensNumber <= msg.value, \"Ether value sent is too low\");        require(whitelistSaleActive, \"Maybe later\");        require(tokensNumber <= whitelist[msg.sender], \"You can't claim more than your allotment\");        for (uint i = 0; i < tokensNumber; i++) {            require(whitelist[msg.sender] >= 1, \"You don't have any more to claim\");            require(tokensNumber - i <= whitelist[msg.sender], \"You can't claim more than your allotment\");            require(_tokenIdCounter.current()<= nftsNumber + 1, \"Sry I dont have enough left ;(\");            claimed[msg.sender] += 1;            whitelist[msg.sender] =whitelist[msg.sender] - 1;            <mask0>;            setXDone(_tokenIdCounter.current());            _tokenIdCounter.increment();        }    }"