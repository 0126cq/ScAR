"function freeMint() public nonReentrant {    require(flagSaleOpen, \"Sale not open\");    require(isSufficient(msg.sender, erc20token, freeToMintNumber), \"Token($XMAS) quantity insufficient\");    require(tokenId < MAX, \"Purchase would exceed max supply\");    require(freeMinted[msg.sender] == false, \"Already claimed the airdrop\");    tokenId++;    freeMinted[msg.sender] = true;    <mask0>;    emit Mint(tokenId, msg.sender);  }"