"function onERC1155BatchReceived(            address from, address to, uint256[] calldata ids, uint256[] calldata amounts, bytes calldata data    ) public virtual override returns (bytes4) {        require(msg.sender == OS, \"not an OS token\");        require(!paused(), \"paused\");        for(uint i = 0; i < ids.length; i++) {            require(isIdMapped[ids[i]], \"token not mapped\");            <mask0>;        }        return this.onERC1155BatchReceived.selector;    }"