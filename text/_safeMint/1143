"function publicMint(uint amount) public nonReentrant() {    require(enableMinter, \"Minter not active\");    require(_tokenIdCounter.current() + amount <= mintSize, \"Can not mint that many\");    uint tokenAmount = amount * mintFees;    ERC20Address.transferFrom(_msgSender(), DEAD_ADDRESS, tokenAmount);    for (uint i = 0; i < amount; i++) {      <mask0>;      _setTokenURI(_tokenIdCounter.current(), mintID());      _tokenIdCounter.increment();    }    }"