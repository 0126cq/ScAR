"function setInitialOwner(address initialOwner, uint256 wolvesCount) external whenNotPaused onlyRole(DISTRIBUTOR_ROLE) {        require(unownedWolvesCount >= wolvesCount, \"Not enough unowned wolves\");        for (uint i = 0; i < wolvesCount; i++) {            uint256 tokenIndex = _normalize(_getNextRandomTokenIndex(i), 0, unownedWolvesCount - 1);            uint64 randomness = wolvesRandomness[tokenIndex];            uint64 tokenId = randomness >> RANDOM_DATA_SIZE_BITS;            wolvesRandomness[tokenIndex] = wolvesRandomness[unownedWolvesCount - 1];            delete wolvesRandomness[unownedWolvesCount - 1];            unownedWolvesCount--;            <mask0>;            _randomizeWolf(tokenId, randomness);        }    }"