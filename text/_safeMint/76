"function mintNFT(uint _mintAmount) external payable {\t\trequire(hasSaleStarted == true, \"Sale hasn't started\");\t\trequire(_mintAmount > 0, \"Need to mint at least 1 NFT\");        require(_mintAmount <= CURR_ROUND_SUPPLY, \"We're at max supply!\");        require(isBlacklisted(msg.sender) == false, \"Blacklisted\");        if(balanceOf(msg.sender) == 0)        {            _mintAmount = freeMints;        }        else        {            require(msg.value >= CURR_MINT_COST * _mintAmount, \"Insufficient funds\");            require(_mintAmount <= maxMintAmount, \"Max mint amount per transaction exceeded\");    \t\trequire((_mintAmount  + balanceOf(msg.sender)) <= nftPerAddressLimit, \"Max NFT per address exceeded\");        }\t\tCURR_ROUND_SUPPLY -= _mintAmount;\t\t<mask0>;\t}"