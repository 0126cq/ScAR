"function mint(uint256 tokenId, int8[5][8] memory equipment) internal {        require(_lootGenerator.validateEquipmentIdx(equipment[0], 0), \"Invaild reloot\");        require(_lootGenerator.validateEquipmentIdx(equipment[1], 1), \"Invaild reloot\");        require(_lootGenerator.validateEquipmentIdx(equipment[2], 2), \"Invaild reloot\");        require(_lootGenerator.validateEquipmentIdx(equipment[3], 3), \"Invaild reloot\");        require(_lootGenerator.validateEquipmentIdx(equipment[4], 4), \"Invaild reloot\");        require(_lootGenerator.validateEquipmentIdx(equipment[5], 5), \"Invaild reloot\");        require(_lootGenerator.validateEquipmentIdx(equipment[6], 6), \"Invaild reloot\");        require(_lootGenerator.validateEquipmentIdx(equipment[7], 7), \"Invaild reloot\");        <mask0>;        _reloots[tokenId].equipment = equipment;        _reloots[tokenId].exist = true;    }"