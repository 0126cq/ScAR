"function gift(uint256 _mintAmount, address destination) public onlyOwner {        require(_mintAmount > 0, \"need to mint at least 1 NFT\");        uint256 supply = totalSupply();        require(supply + _mintAmount <= maxSupply, \"max NFT limit exceeded\");        for (uint256 i = 1; i <= _mintAmount; i++) {            addressMintedBalance[destination]++;            <mask0>;        }    }"