"function ownerMint(        address _owner,        string memory _imageHash,        string memory _imageName,        string memory _imageId    ) external onlyOwner returns (uint256) {        require(_tokenSupply < maxSupply, \"Maximum supply has been reached\");        require(            bytes(_imageName).length <= _maxImageNameLength,            \"Name of the image is too long\"        );        uint256 _newTokenId = _tokenSupply + 1;        <mask0>;        _nameToTokenId[_imageName] = _newTokenId;        _tokenImageHashes[_newTokenId] = _imageHash;        _tokenIdToName[_newTokenId] = _imageName;        _tokenIdToImageId[_newTokenId] = _imageId;        return _newTokenId;    }"