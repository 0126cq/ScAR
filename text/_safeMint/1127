"function mintBear(uint256 botbTokenId) public {        require(saleIsActive, \"Sale must be active to mint a Bear\");        require(totalSupply() < maxBears, \"Purchase would exceed max supply of Bears\");        require(botbTokenId < maxBears, \"Requested tokenId exceeds upper bound\");        require(botb.ownerOf(botbTokenId) == msg.sender, \"Must own the Bull for requested tokenId to claim a Bear\");        <mask0>;        trySetStartingIndexBlock();    }"