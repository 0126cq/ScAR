"function mintToMultiple(address _to, uint256 _amount) public payable {        require(_amount >= 1, \"Must mint at least 1 token\");        require(_amount <= maxBatchSize, \"Cannot mint more than max mint per transaction\");        require(mintingOpen == true, \"Minting is not open right now!\");        require(publicDropTimePassed() == true, \"Public drop time has not passed!\");        require(currentTokenId() + _amount <= collectionSize, \"Cannot mint over supply cap of 3000\");        require(msg.value == getPrice(_amount), \"Value below required mint fee for amount\");        <mask0>;    }"