"function mintAllowList(    bytes32 messageHash,    bytes calldata signature,    uint amount  ) public payable {    require(!paused, \"s\");    require(!mintEnded, \"m\");    require(hashMessage(msg.sender, address(this)) == messageHash, \"i\");    require(verifyAddressSigner(messageHash, signature), \"f\");    require(allowListMintPrice * amount <= msg.value, \"a\");    <mask0>;  }"