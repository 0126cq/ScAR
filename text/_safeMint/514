"function mintGiveawayNFT(bytes32[] memory proof, uint8 numTokens) public {        require(block.timestamp >= saleStartsAt, 'Sale not active');        require(!giveawayRedeemed[msg.sender], 'Already redeemed');        require((numTokens + redeemedGiveaways) <= RESERVED_GIVEAWAYS, 'All Giveaways Redeemed');        require(MerkleProof.verify(proof, redeemMerkleRoot, keccak256(abi.encodePacked(msg.sender,numTokens))), 'Restricted Access');        giveawayRedeemed[msg.sender] = true;        for (uint8 i = 0; i < numTokens; i++) {            _tokenIds.increment();            redeemedGiveaways++;            <mask0>;        }        emit TokenMinted(msg.sender, numTokens);    }"