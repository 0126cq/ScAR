"function saleMint(uint256 _ammount) public payable {    require(paused == false, \"the contract is paused now, you cannot mint\");    require(nftsPerWallet[msg.sender] + _ammount <= max_mint_per_address, \"you cannot mint more NFTs\");    require(sellingStep != Steps.SoldOut, \"Sorry, no NFTs left.\");    require(sellingStep == Steps.Sale, \"Sorry, sale has not started yet.\");    require(msg.value == priceSale * _ammount, \"Insufficient funds.\");    require(_ammount <= max_mint_allowed, \"You can't mint more than 3 tokens\");    require(totalsupply + _ammount <= MAX_SUPPLY, \"Sale is almost done and we don't have enough NFTs left.\");    nftsPerWallet[msg.sender] += _ammount;    if(totalsupply + _ammount == MAX_SUPPLY) {        sellingStep = Steps.SoldOut;       }    for(uint i = 1 ; i <= _ammount ; i++) {        uint256 newTokenId = totalsupply + 1;        totalsupply++;        <mask0>;        setRoyalties(newTokenId);    }"