"function _mintChests(           address account,         uint256[] memory _ids    )         internal virtual    {        require(account != address(0));        address operator = _msgSender();        for (uint i = 0; i < _ids.length; i++) {               <mask0>;               _setTokenURI(_ids[i], chestURI);                }        emit TransferredBatch(operator, address(0), account, _ids);    }"