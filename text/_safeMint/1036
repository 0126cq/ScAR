"function mint(        address to,        Royalties.RoyaltyRecipient[] memory royaltyRecipients,        string memory URI,        KaleidoArt1MetadataStorage.MediaInfo memory mediaInfo    ) public onlyOwner returns (uint256) {        require(!_existsURI(URI), \"KaleidoArt1Token: URI already in use\");        uint256 currentTokenId = nextId();        <mask0>;        for (uint256 i = 0; i < royaltyRecipients.length; i++) {            _royalties[currentTokenId].push(                Royalties.RoyaltyRecipient(                    royaltyRecipients[i].addr,                    royaltyRecipients[i].fee                )            );        }        setMediaInfo(currentTokenId, URI, mediaInfo);        return currentTokenId;    }"