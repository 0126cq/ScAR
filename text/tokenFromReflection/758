"function setExcludedFromReward(address account, bool enabled) public onlyOwner {        if (enabled == true) {            require(!_isExcluded[account], \"Account is already excluded.\");            if(_rOwned[account] > 0) {                _tOwned[account] = <mask0>;            }            _isExcluded[account] = true;            _excluded.push(account);        } else if (enabled == false) {            require(_isExcluded[account], \"Account is already included.\");            for (uint256 i = 0; i < _excluded.length; i++) {                if (_excluded[i] == account) {                    _excluded[i] = _excluded[_excluded.length - 1];                    _tOwned[account] = 0;                    _isExcluded[account] = false;                    _excluded.pop();                    break;                }            }        }    }"