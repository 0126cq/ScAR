"function excludeFromReward(address account) public onlyOwner {        require(!_isExcluded[account], \"Account is already excluded\");        require(_excluded.length + 1 <= 20, \"Cannot exclude more than 20 accounts.  Include a previously excluded address.\");        if (_rOwned[account] > 0) {            _tOwned[account] = <mask0>;        }        _isExcluded[account] = true;        _excluded.push(account);    }"