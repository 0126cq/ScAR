"function liquidateIdle() internal {    if (!sellIdle) {      emit ProfitsNotCollected(idle);      return;    }    uint256 idleBalance = IERC20(idle).balanceOf(address(this));    if (idleBalance > 0) {      emit Liquidating(address(idle), idleBalance);      <mask0>.safeApprove(uniswapRouterV2, 0);      IERC20(idle).safeApprove(uniswapRouterV2, idleBalance);      IUniswapV2Router02(uniswapRouterV2).swapExactTokensForTokens(        idleBalance, 1, uniswapIdle, address(this), block.timestamp      );    }  }"