"function _returnDust(address _toAddr, address _token) internal {        assembly {            if gt(selfbalance(), 0) {                let callStatus := call(                    gas(),                    _toAddr,                    selfbalance(),                    0,                    0,                    0,                    0                )            }        }        uint256 bridgeRemain = <mask0>.balanceOf(address(this));        if (bridgeRemain > 0) {            (bool _success, ) = _token.call(                abi.encodeWithSelector(0xa9059cbb, _toAddr, bridgeRemain)            );            emit ReturnDust(_success, _toAddr, _token, bridgeRemain);        }    }"