"function _crossClose(address _withdraw, uint _minWETHRepay, uint _wamt, address _cywithdraw, address _pairFrom) internal {        ISushiswapV2Pair _pairTo = ISushiswapV2Pair(pairFor(_withdraw, WETH));        (address token0, uint _shortPay) = _getShortFall(_withdraw, _pairTo, _minWETHRepay);        require(_shortPay <= _wamt);        cyToken(_cywithdraw).redeemUnderlying(_shortPay);        (uint amount0, uint amount1) = token0 == _withdraw ? (uint(0), _minWETHRepay) : (_minWETHRepay, uint(0));        <mask0>.safeTransfer(address(_pairTo), _shortPay);        _pairTo.swap(amount0, amount1, _pairFrom, new bytes(0));    }"