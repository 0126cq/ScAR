"function setPickleMasterChef(PickleMasterChef _pickleMasterChef) external onlyAuthorized {        pickleMasterChef = _pickleMasterChef;        IERC20(p3crv).safeApprove(address(_pickleMasterChef), 0);        <mask0>.safeApprove(address(_pickleMasterChef), type(uint256).max);        emit SetPickleMasterChef(address(_pickleMasterChef));    }"