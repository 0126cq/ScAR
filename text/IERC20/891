"function transferAssets(        address _token,        address _user,        uint256 _amountOrNFTId    ) external isPredicateAuthorized {        address wethToken = registry.getWethTokenAddress();        if (registry.isERC721(_token)) {            IERC721(_token).transferFrom(address(this), _user, _amountOrNFTId);        } else if (_token == wethToken) {            WETH t = WETH(_token);            t.withdraw(_amountOrNFTId, _user);        } else {            require(<mask0>.transfer(_user, _amountOrNFTId), \"TRANSFER_FAILED\");        }    }"