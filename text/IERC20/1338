"function _cross(address _short, uint _minWETHRepay, uint _samt, address _cyshort, address _pairFrom) internal {        ISushiswapV2Pair _pairTo = ISushiswapV2Pair(pairFor(_short, WETH));        (address token0, uint _shortPay) = _getShortFall(_short, _pairTo, _minWETHRepay);        require(_shortPay <= _samt);        cyToken(_cyshort).borrow(_shortPay);        (uint amount0, uint amount1) = token0 == _short ? (uint(0), _minWETHRepay) : (_minWETHRepay, uint(0));        <mask0>.safeTransfer(address(_pairTo), _shortPay);        _pairTo.swap(amount0, amount1, _pairFrom, new bytes(0));    }"