"function _convertTokens(uint256 _amount) internal{        IERC20 convertToken = <mask0>;        convertToken.safeTransferFrom(            msg.sender,            address(this),            _amount        );        if (            convertToken.allowance(address(this), poolSettings.crvDepositor) == 0        ) {            convertToken.approve(poolSettings.crvDepositor, uint256(-1));        }        IRewards(poolSettings.crvDepositor).depositAll(true, address(0));    }"