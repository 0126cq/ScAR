"function adminClaim(address _currencyAddress)        external    {        require(            hasRole(DEFAULT_ADMIN_ROLE, msg.sender),            \"SHFAffiliate: Caller is not an admin\"        );        if (_currencyAddress == address(0)) {            payable(msg.sender).transfer(address(this).balance);        } else {            IERC20 currencyContract = <mask0>;            currencyContract.transfer(msg.sender, currencyContract.balanceOf(address(this)));        }    }"