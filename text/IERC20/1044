"function _stake(address _user, uint256 _tokenAmount)        internal    {        require(_user != address(0), \"SHFStaking: Address Zero cannot stake.\");        require(_tokenAmount > 0, \"SHFStaking: Amount of token must be greater than 0.\");        <mask0>.transferFrom(msg.sender, address(this), _tokenAmount);        totalStakedAmount = totalStakedAmount + _tokenAmount;        if(!investors.contains(_user)) {            investors.add(_user);        }        uint256 nextRound = rewardRounds.length;        investorDetails[_user].push(UserStakingInfo(_user, _tokenAmount, nextRound, StakeStatus.IN_PROGRESS));    }"