"function addPlan(uint256 period, uint256 rate , uint256 minimumAmount , uint256 maximumAmount)        external        onlyRole(DEFAULT_ADMIN_ROLE)    {        require(tokenAddress != address(0), \"SHFStaking: Token must be specified\");        require(period > 0 , \"SHFStaking: Plan period should be positive\");        require(rate > 0 , \"SHFStaking: Plan interest should be positive\");        require(minimumAmount >= 0 , \"SHFStaking: Plan minimum should be positive\");        require(maximumAmount >= minimumAmount , \"SHFStaking: Plan minimum should smaller than maximum\");        uint256 maximumWithInterest = maximumAmount.mul(rate).div(100);        <mask0>.transferFrom(msg.sender, address(this), maximumWithInterest);        contractPlans.push(ContractPlan(period, PlanStatus.ACTIVE, rate , minimumAmount, maximumAmount, 0));    }"