"function createOption(address token, uint amount, uint optionType) public {        uint _cost = cost(token, amount);        uint _fee = fees(token, _cost);        uint _strike = _cost.div(amount);        <mask0>.transferFrom(msg.sender, address(this), _fee);        options.push(option(token, amount, _strike, block.timestamp.add(PERIOD), optionType));        contracts.mint(msg.sender, optionsCount);        emit Created(optionsCount++, msg.sender, token, amount, _strike, block.timestamp, block.timestamp.add(PERIOD));    }"