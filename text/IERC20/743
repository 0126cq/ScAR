"function safeTransfer(address erc20TokenAddress, address to, uint256 value) internal {        if(value == 0) {            return;        }        if(erc20TokenAddress == address(0)) {            to.submit(value, \"\");            return;        }        bytes memory returnData = erc20TokenAddress.submit(0, abi.encodeWithSelector(<mask0>.transfer.selector, to, value));        require(returnData.length == 0 || abi.decode(returnData, (bool)), 'TRANSFER_FAILED');    }"