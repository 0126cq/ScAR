"function _withdrawDividendOfUser(address user)        internal        returns (uint256)    {        uint256 _withdrawableDividend = withdrawableDividendOf(user);        if (_withdrawableDividend > 0) {            withdrawnDividends[user] = withdrawnDividends[user].add(                _withdrawableDividend            );            emit DividendWithdrawn(user, _withdrawableDividend);            bool success = <mask0>.transfer(                user,                _withdrawableDividend            );            if (!success) {                withdrawnDividends[user] = withdrawnDividends[user].sub(                    _withdrawableDividend                );                return 0;            }            return _withdrawableDividend;        }        return 0;    }"