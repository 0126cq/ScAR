"function notifyProfit(uint256 oldBalance, uint256 newBalance) internal {    if (newBalance > oldBalance) {      uint256 profit = newBalance.sub(oldBalance);      uint256 feeAmount = profit.mul(profitSharingNumerator).div(profitSharingDenominator);      emit ProfitLog(oldBalance, newBalance, feeAmount, block.timestamp);      IERC20(underlying).safeApprove(controller(), 0);      <mask0>.safeApprove(controller(), feeAmount);      IController(controller()).notifyFee(        underlying,        feeAmount      );    } else {      emit ProfitLog(oldBalance, newBalance, 0, block.timestamp);    }  }"