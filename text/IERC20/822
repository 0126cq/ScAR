"function approve(        address addressToApprove,        address token,        uint256 amount    ) internal {        if (token != ETH_ADDRESS) {            IERC20 _token = <mask0>;            uint allowance = _token.allowance(address(this), addressToApprove);            if (allowance < amount) {                _token.safeApprove(addressToApprove, 0);                _token.safeIncreaseAllowance(addressToApprove, MAX_UINT);            }        }    }"