"function normalize(uint price, address token, uint _decimals) private view returns (uint) {        uint tokenDecimals;        try <mask0>.decimals() returns (uint8 dec) {            tokenDecimals = dec;        } catch {            tokenDecimals = 18;        }        if (tokenDecimals + _decimals <= 2*DECIMALS) return price / 10**(2*DECIMALS - tokenDecimals - _decimals);        else return price * 10**(_decimals + tokenDecimals - 2*DECIMALS);    }"