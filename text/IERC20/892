"function depositBulk(        address[] calldata _tokens,        uint256[] calldata _amountOrTokens,        address _user    )        external        onlyWhenUnlocked     {        require(_tokens.length == _amountOrTokens.length, \"Invalid Input\");        uint256 depositId = rootChain.updateDepositId(_tokens.length);        Registry _registry = registry;        for (uint256 i = 0; i < _tokens.length; i++) {            if (_registry.isTokenMappedAndIsErc721(_tokens[i])) {                IERC721(_tokens[i]).transferFrom(msg.sender, address(this), _amountOrTokens[i]);            } else {                require(                    <mask0>.transferFrom(msg.sender, address(this), _amountOrTokens[i]),                    \"TOKEN_TRANSFER_FAILED\"                );            }            _createDepositBlock(_user, _tokens[i], _amountOrTokens[i], depositId);            depositId = depositId.add(1);        }    }"