"function _distributePerformanceFeesAndRewardDeposit() internal {        uint256 _reward = IERC20(reward).balanceOf(address(this));        if (_reward > 0) {            <mask0>.safeTransfer(                IController(controller).treasury(),                _reward.mul(performanceTreasuryFee).div(performanceTreasuryMax)            );            IERC20(reward).safeTransfer(                IController(controller).devfund(),                _reward.mul(performanceDevFee).div(performanceDevMax)            );            rewardDeposit();        }    }"