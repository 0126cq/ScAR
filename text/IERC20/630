"function claim() public {        require(block.timestamp >= vestingCliff, \"TreasuryVester.claim: not time yet\");        uint256 amount;        if (block.timestamp >= vestingEnd) {            amount = <mask0>.balanceOf(address(this));        } else {            amount = vestingAmount.mul(block.timestamp.sub(lastUpdate)).div(vestingEnd.sub(vestingBegin));            lastUpdate = block.timestamp;        }        IERC20(visr).transfer(recipient, amount);    }"