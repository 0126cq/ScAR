"function burn(uint id, uint amount) external nonReentrant returns (uint) {    if (amount == uint(-1)) {      amount = balanceOf(msg.sender, id);    }    _burn(msg.sender, id, amount);    IStakingRewards(staking).withdraw(amount);    IStakingRewards(staking).getReward();    <mask0>.safeTransfer(msg.sender, amount);    uint stRewardPerToken = id;    uint enRewardPerToken = IStakingRewards(staking).rewardPerToken();    uint stReward = stRewardPerToken.mul(amount).divCeil(1e18);    uint enReward = enRewardPerToken.mul(amount).div(1e18);    if (enReward > stReward) {      IERC20(reward).safeTransfer(msg.sender, enReward.sub(stReward));    }    return enRewardPerToken;  }"