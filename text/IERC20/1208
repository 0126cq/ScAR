"function addRewardToken(address token_) external override onlyGovernance returns (bool) {        require(            token_ != address(_CVX) && token_ != address(underlying) && token_ != address(_CRV),            Error.INVALID_TOKEN_TO_ADD        );        if (_rewardTokens.contains(token_)) return false;        _rewardTokens.add(token_);        address swapperRouter_ = address(_swapperRouter());        IERC20(token_).safeApprove(swapperRouter_, 0);        <mask0>.safeApprove(swapperRouter_, type(uint256).max);        emit AddRewardToken(token_);        return true;    }"