"function _calculateRewards(        address account,        uint256 amount,        uint256 amountfToken    ) internal view returns (uint256) {        uint256 rewards = userInfo[account].rewards;        uint256 farmBalance = <mask0>.balanceOf(address(this));        if (amount == 0) {            if (rewards < farmBalance) {                return rewards;            }            return farmBalance;        }        return (amount.mul(rewards)).div(amountfToken);    }"