"function withdrawAll() external returns (uint256 balance) {        require(msg.sender == controller, \"!controller\");        _withdrawAll();        _withdrawAllReward();        balance = <mask0>.balanceOf(address(this));        address _jar = IController(controller).jars(address(want));        require(_jar != address(0), \"!jar\");         IERC20(want).safeTransfer(_jar, balance);        IERC20(reward).safeTransfer(            _jar,            IERC20(reward).balanceOf(address(this))        );    }"