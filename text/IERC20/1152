"function enterPosition(    uint256 _principal,    uint256 _iterations,    uint256 _ltv,    address _stableAsset  ) public {    require(_principal > 0, Errors.LS_SWAP_AMOUNT_NOT_GT_0);    require(ENABLED_STABLE_COINS[_stableAsset], Errors.LS_STABLE_COIN_NOT_SUPPORTED);    require(IERC20(COLLATERAL).balanceOf(msg.sender) >= _principal, Errors.LS_SUPPLY_NOT_ALLOWED);    <mask0>.safeTransferFrom(msg.sender, address(this), _principal);    _supply(_principal);    uint256 suppliedAmount = _principal;    uint256 borrowAmount = 0;    uint256 stableAssetDecimals = IERC20Detailed(_stableAsset).decimals();    for (uint256 i = 0; i < _iterations; i++) {      borrowAmount = _calcBorrowableAmount(suppliedAmount, _ltv, _stableAsset, stableAssetDecimals);      if (borrowAmount > 0) {        _borrow(_stableAsset, borrowAmount);        suppliedAmount = _swapTo(_stableAsset, borrowAmount);        _supply(suppliedAmount);      }    }    emit EnterPosition(_principal, _iterations, _ltv, _stableAsset);  }"