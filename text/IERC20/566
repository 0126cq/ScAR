"function initialize(        address _pool,        uint256 _debtLimit,        uint256 _targetAllocation,        uint256 _bound    ) external virtual override initializer {        _initialize(_pool, _debtLimit, _targetAllocation, _bound);        address underlying_ = ILiquidityPool(pool).getUnderlying();        require(underlying_ != address(0), Error.ZERO_ADDRESS_NOT_ALLOWED);        IERC20(underlying_).safeApprove(address(reserve), type(uint256).max);        <mask0>.safeApprove(_pool, type(uint256).max);    }"