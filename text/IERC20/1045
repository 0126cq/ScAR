"function _sendToken(address _user)        internal    {        require(_user != address(0), \"SHFStaking: Cannot send token to address zero.\");        uint amountToken = _calculateStakedToken(_user);        require(amountToken > 0, \"SHFStaking: Amount of token too small.\");        <mask0>.transfer(_user, amountToken);        totalStakedAmount = totalStakedAmount - amountToken;    }"