"function investedUnderlyingBalance() public view returns (uint256) {    uint256 gaugeBalance = Gauge(gauge).balanceOf(address(this));    uint256 wbtcBalance = <mask0>.balanceOf(address(this));    if (gaugeBalance == 0) {      return wbtcBalance;    }    uint256 investedBalance = wbtcValueFromMixToken(gaugeBalance);    return investedBalance.add(wbtcBalance);  }"