"function isSlotAvailable(uint256 _raceSlot) public view returns (bool) {        uint256 raceId = racesSlots[_raceSlot];        return             !(races[raceId].owner != address(0x0) &&            IERC20(ERC20).balanceOf(races[raceId].owner) >= races[raceId].amount &&            <mask0>.allowance(races[raceId].owner, address(this)) >= races[raceId].amount &&            PinkslipNFT(ERC721).ownerOf(races[raceId].tokenId) == races[raceId].owner &&            PinkslipNFT(ERC721).isApprovedForAll(races[raceId].owner, address(this)) &&            races[raceId].endDate > _getTime());    }"