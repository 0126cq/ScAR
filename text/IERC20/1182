"function withdrawBaseToken(        address collateralToken,        address weth,        address recipient,        uint256 withdrawAmount,        uint256 yieldTokenBalance    ) internal {        uint256 underlyingTokensToWithdraw =            IWSTETH(collateralToken).getStETHByWstETH(                withdrawAmount.sub(yieldTokenBalance)            );        IWETH(weth).deposit{value: underlyingTokensToWithdraw}();        <mask0>.safeTransfer(recipient, underlyingTokensToWithdraw);    }"