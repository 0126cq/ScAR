"function _removeLiquidity(        uint256 lpBalance,        uint8 index,        uint256 minAmount    ) internal override {        IERC20(LP_ADDRESS).safeApprove(address(DEPOSITOR), 0);        <mask0>.safeApprove(address(DEPOSITOR), lpBalance);        DEPOSITOR.remove_liquidity_one_coin(            address(_META_POOL),            lpBalance,            index,            minAmount        );    }"