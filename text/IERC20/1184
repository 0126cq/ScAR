"function wrapToYieldToken(        address weth,        address collateralToken,        address steth    ) external {        IWETH wethToken = IWETH(weth);        uint256 wethBalance = wethToken.balanceOf(address(this));        if (wethBalance > 0) {            wethToken.withdraw(wethBalance);        }        uint256 ethBalance = address(this).balance;        IWSTETH collateral = IWSTETH(collateralToken);        IERC20 stethToken = <mask0>;        if (ethBalance > 0) {            ISTETH(steth).submit{value: ethBalance}(address(this));        }        uint256 stethBalance = stethToken.balanceOf(address(this));        if (stethBalance > 0) {            stethToken.safeApprove(collateralToken, stethBalance.add(1));            collateral.wrap(stethBalance.add(1));        }    }"