"function pushYieldTokens(        address to,        uint256 amount,        uint256 minNYieldAfterPush    ) external virtual override onlyForge {        uint256 yieldTokenBal = <mask0>.balanceOf(address(this));        require(yieldTokenBal.sub(amount) >= minNYieldAfterPush, \"INVARIANCE_ERROR\");        IERC20(yieldToken).safeTransfer(to, amount);    }"