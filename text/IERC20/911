"function rewardDeposit() public override {        uint256 _reward = IERC20(reward).balanceOf(address(this));        if (_reward > 0) {            <mask0>.safeApprove(stakingPool, 0);            IERC20(reward).safeApprove(stakingPool, _reward);            IStakingPools(stakingPool).deposit(alcxPoolId, _reward);         }    }"