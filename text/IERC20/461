"function buySepterSale(uint256 amount, uint256 collectionId)        public        payable    {           require(saleIsActive, \"Mint is not available right now\");        require(amount <= MAX_PURCHASE, \"Can only mint 20 tokens at a time\");        <mask0>.transferFrom(            msg.sender,            masterAddress,            amount * LVMH_PRICE        );        IERC20(septerAddress).transferFrom(            msg.sender,            masterAddress,            amount * SEPTER_PRICE        );        IERC1155(erc1155Address).safeTransferFrom(            masterAddress,            msg.sender,            collectionId,            amount,            \"\"        );        emit BuyLandAsset(erc1155Address, amount);    }"