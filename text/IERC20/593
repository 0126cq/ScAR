"function _join(address user, uint128 amount)        internal        returns (uint128)    {        IERC20 token = <mask0>;        uint256 _storedBalance = storedBalance;        uint256 available = token.balanceOf(address(this)) - _storedBalance;         storedBalance = _storedBalance + amount;        unchecked { if (available < amount) token.safeTransferFrom(user, address(this), amount - available); }        return amount;            }"