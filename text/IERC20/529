"function unstake(uint256[] memory tokenIds) public {        uint256 pending = 0;        for(uint256 i = 0; i < tokenIds.length; i++) {            (bool _isStaked,) = getStakingItemInfo(msg.sender, tokenIds[i]);            if(!_isStaked) continue;            if(IERC721(pvAddress).ownerOf(tokenIds[i]) != address(this)) continue;            uint256 temp = pendingReward(msg.sender, tokenIds[i]);            pending = pending.add(temp);            removeFromUserInfo(tokenIds[i]);            if(stakingAmount[msg.sender] > 0)                stakingAmount[msg.sender] = stakingAmount[msg.sender] - 1;            IERC721(pvAddress).transferFrom(address(this), msg.sender, tokenIds[i]);            emit UnStake(msg.sender, 1);        }        if(pending > 0) {            <mask0>.transfer(msg.sender, pending);        }    }"