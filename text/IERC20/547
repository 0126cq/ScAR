"function _deposit(uint256 amount) internal {        ILendingPool lp = _lendingPool();        if (want.allowance(address(this), address(lp)) < amount) {            IERC20(address(want)).safeApprove(address(lp), 0);            <mask0>.safeApprove(address(lp), type(uint256).max);        }        uint16 referral;        uint16 __customReferral = _customReferral;        if (__customReferral != 0) {            referral = __customReferral;        } else {            referral = _DEFAULT_REFERRAL;        }        lp.deposit(address(want), amount, address(this), referral);    }"