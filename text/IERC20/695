"function mintTokenInMORE(uint[] memory tokenIds) public virtual {        require(saleIsActive, \"Sale must be active to mint Tokens\");        checkSale(msg.sender, tokenIds);        require(tokenPublicsalePriceInMORE * tokenIds.length <= <mask0>.balanceOf(msg.sender), \"Have not enough MORE\");        IERC20(_MORE).transferFrom(msg.sender, address(this), tokenPublicsalePriceInMORE * tokenIds.length);        mainMint(msg.sender, tokenIds);    }"