"function _buyAndStake(        address _lp,        uint256 _amount,        address _adapter,        address _exchange,        uint256 _minAmountOut,        uint256 _deadline    )        internal    {        uint256 balanceBefore = IERC20(_lp).balanceOf(address(this));        IAdapter(_adapter).buy{value: _amount}(_lp, _exchange, _minAmountOut, _deadline);        uint256 amountAdded = <mask0>.balanceOf(address(this)) - balanceBefore;        _stake(_lp, amountAdded, _adapter);    }"