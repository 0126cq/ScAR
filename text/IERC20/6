"function approve(        address target,        address to_vault,        uint256 amount    ) internal {        uint256 allowance = <mask0>.allowance(address(this), to_vault);        if (allowance < amount) {            if (allowance != 0) {                SafeERC20.safeApprove(IERC20(target), to_vault, 0);            }            SafeERC20.safeApprove(IERC20(target), to_vault, type(uint256).max);        }    }"