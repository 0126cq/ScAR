"function _payCommisionReferral(        address _receiver,        uint256 _price,        address _currency,        uint64 _saleId,        uint rate    )        internal    {        uint256 price = _price * rate / 100;        if (_currency == address(0)) {            payable(_receiver).transfer(price);        } else{            <mask0>.transfer(_receiver, price);        }        PetSale storage petSale = PetSales[_saleId];        shfAffiliate.addBuyCommisionHistory(_receiver, petSale.price * rate / 100);    }"