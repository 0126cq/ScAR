"function notifyProfitInRewardToken(uint256 _rewardBalance) internal {    if( _rewardBalance > 0 ){      uint256 feeAmount = _rewardBalance.mul(profitSharingNumerator()).div(profitSharingDenominator());      emit ProfitLogInReward(_rewardBalance, feeAmount, block.timestamp);      <mask0>.safeApprove(controller(), 0);      IERC20(rewardToken()).safeApprove(controller(), feeAmount);      IController(controller()).notifyFee(        rewardToken(),        feeAmount      );    } else {      emit ProfitLogInReward(0, 0, block.timestamp);    }  }"