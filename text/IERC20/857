"function _pullFrom(        PoolStorage.Layout storage l,        address from,        uint256 amount,        bool isCallPool,        bool creditMessageValue    ) internal {        uint256 credit;        if (creditMessageValue) {            credit = _creditMessageValue(amount, isCallPool);        }        if (amount > credit) {            credit += _creditReservedLiquidity(                from,                amount - credit,                isCallPool            );        }        if (amount > credit) {            require(                <mask0>.transferFrom(                    from,                    address(this),                    amount - credit                ),                \"ERC20 transfer failed\"            );        }    }"