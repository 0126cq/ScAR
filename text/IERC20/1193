"function totalBalance() public view returns (uint256) {        uint256 wethBalance = <mask0>.balanceOf(address(this));        uint256 ethBalance = address(this).balance;        uint256 stethFromWsteth =            collateralToken.getStETHByWstETH(                collateralToken.balanceOf(address(this))            );        uint256 stEthBalance = IERC20(STETH).balanceOf(address(this));        return            wethBalance                .add(vaultState.lockedAmount)                .add(ethBalance)                .add(stethFromWsteth)                .add(stEthBalance);    }"