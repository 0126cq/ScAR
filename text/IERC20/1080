"function add(address _uToken, bool _withUpdate) public {        require(Converter(_uToken).issuer() == msg.sender ||         isShopAdmin[_uToken][msg.sender] || isPublic[_uToken], \"PointShop: Only shop admin can add shop to farm\");        require(!shopExists[_uToken], \"PointShop: Already added\");        PointFarm(farm).add(<mask0>, _withUpdate);        shopExists[_uToken] = true;    }"