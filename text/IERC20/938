"function stakeOrSend( address _recipient, bool _stake, uint _amount ) internal returns ( uint ) {        if ( !_stake ) {             IERC20( GWS ).transfer( _recipient, _amount );         } else {             if ( useHelper ) {                 <mask0>.approve( stakingHelper, _amount );                IStakingHelper( stakingHelper ).stake( _amount, _recipient );            } else {                IERC20( GWS ).approve( staking, _amount );                IStaking( staking ).stake( _amount, _recipient );            }        }        return _amount;    }"