"function settleLong(        address gammaController,        address oldOption,        address asset    ) external returns (uint256) {        IController controller = IController(gammaController);        uint256 oldOptionBalance = IERC20(oldOption).balanceOf(address(this));        if (controller.getPayout(oldOption, oldOptionBalance) == 0) {            return 0;        }        uint256 startAssetBalance = IERC20(asset).balanceOf(address(this));        IController.ActionArgs[] memory actions =            new IController.ActionArgs[](1);        actions[0] = IController.ActionArgs(            IController.ActionType.Redeem,            address(0),             address(this),             oldOption,             0,             oldOptionBalance,             0,             \"\"         );        controller.operate(actions);        uint256 endAssetBalance = <mask0>.balanceOf(address(this));        return endAssetBalance.sub(startAssetBalance);    }"