"function _tryDeposit(        address asset,        uint amount,        address user    ) internal returns(uint) {        uint256 amountInBase = uint256(LibUnitConverter.decimalToBaseUnit(asset, amount));        if (<mask0>.balanceOf(user) >= amountInBase && IERC20(asset).allowance(user, address(this)) >= amountInBase) {            SafeERC20.safeTransferFrom(IERC20(asset), user, address(this), amountInBase);            return amount;        } else {            return 0;        }    }"