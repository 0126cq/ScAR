"function _depositIntoCurve(address _token, uint _index) private {        uint bal = <mask0>.balanceOf(address(this));        if (bal > 0) {            IERC20(_token).safeApprove(DEPOSIT, 0);            IERC20(_token).safeApprove(DEPOSIT, bal);            uint[4] memory amounts;            amounts[_index] = bal;            uint pricePerShare = StableSwapBusd(SWAP).get_virtual_price();            uint shares = bal.mul(PRECISION_DIV[_index]).mul(1e18).div(pricePerShare);            uint min = shares.mul(SLIPPAGE_MAX - slippage) / SLIPPAGE_MAX;            DepositBusd(DEPOSIT).add_liquidity(amounts, min);        }        uint lpBal = IERC20(LP).balanceOf(address(this));        if (lpBal > 0) {            IERC20(LP).safeApprove(GAUGE, 0);            IERC20(LP).safeApprove(GAUGE, lpBal);            LiquidityGauge(GAUGE).deposit(lpBal);        }    }"