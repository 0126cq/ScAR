"function receiveApproval(    address _ownerAdd,    uint256 _value,    address _token,    bytes memory _extraData  ) public isUpgradable {    require(      msg.sender == oldTokenAddress,      \"Must be called from Old Token Contract\"    );    OldToken oldToken = OldToken(_token);    oldToken.transferFrom(_ownerAdd, address(this), _value);    IERC20 newToken = <mask0>;    newToken.transfer(      _ownerAdd,      _value.mul(ratio).mul(        10**uint256(newToken.decimals()).sub(oldToken.decimals())      )    );  }"