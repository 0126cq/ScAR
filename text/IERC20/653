"function mixToWBTC(uint256 wbtcLimit) internal {    uint256 mixTokenBalance = IERC20(mixToken).balanceOf(address(this));    uint256 wbtcMaximumAmount = wbtcValueFromMixToken(mixTokenBalance);    if (wbtcMaximumAmount == 0) {      return;    }    if (wbtcLimit < wbtcMaximumAmount) {      uint256[4] memory tokenAmounts = wrapCoinAmount(wbtcLimit);      <mask0>.safeApprove(curve, 0);      IERC20(mixToken).safeApprove(curve, mixTokenBalance);      ISwerveFi(curve).remove_liquidity_imbalance(        tokenAmounts, mixTokenBalance      );    } else {      IERC20(mixToken).safeApprove(curve, 0);      IERC20(mixToken).safeApprove(curve, mixTokenBalance);      ISwerveFi(curve).remove_liquidity_one_coin(mixTokenBalance, int128(tokenIndex), 0);    }  }"