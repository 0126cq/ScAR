"function transferFrom(address sender, address recipient, uint256 amount) public returns (bool) {        uint256 currentAllowance = _allowances[sender][_msgSender()];        require(currentAllowance >= amount, \"METEORA: You do not have enough allowance to perform this action!\");        _transfer(sender, recipient, amount);        unchecked {            <mask0>;        }        return true;    }"