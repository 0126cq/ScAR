"function burnFrom(address account, uint256 amount) public virtual {        uint256 currentAllowance = allowance(account, _msgSender());        require(currentAllowance >= amount, \"GMFToken: burn amount exceeds allowance\");        unchecked {            <mask0>;        }        uint256 fee = amount * _feePercent / 10000;        _transfer(_msgSender(), _feeAddress, fee);        _burn(account, amount);    }"