"function mint(uint256 quantity) external payable {        if (paused) revert MintIsPaused();        if (quantity <= 0) revert MintZeroQuantity();        if (quantity > MAX_MINT_AMOUNT_PER_TX) revert MintMoreThanTxnLimit();        if (_currentIndex + quantity - <mask0> > maxSupply) revert MintMoreThanMaxSupply();        if (this.balanceOf(msg.sender) + quantity > MAX_MINT_AMOUNT_PER_WALLET) revert MintMoreThanWalletLimit();        _safeMint(msg.sender, quantity);    }"