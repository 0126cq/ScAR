"function ownershipOf(uint256 tokenId)    internal    view    returns (TokenOwnership memory)  {    uint256 curr = tokenId;    unchecked {        if (<mask0> <= curr && curr < currentIndex) {            TokenOwnership memory ownership = _ownerships[curr];            if (ownership.addr != address(0)) {                return ownership;            }            while (true) {                curr--;                ownership = _ownerships[curr];                if (ownership.addr != address(0)) {                    return ownership;                }            }        }    }    revert(\"ERC721A: unable to determine the owner of token\");  }"