"function _tradeUniswap(        IERC20TokenV06 buyToken,        uint256 sellAmount,        bytes memory bridgeData    )        internal        returns (uint256 boughtAmount)    {        (IERC20TokenV06 sellToken) = abi.decode(bridgeData, (IERC20TokenV06));        IUniswapExchange exchange = _getUniswapExchangeForTokenPair(            sellToken,            buyToken        );        if (sellToken == WETH) {            WETH.<mask0>;            boughtAmount = exchange.ethToTokenTransferInput{ value: sellAmount }(                1,                block.timestamp,                address(this)            );        } else if (buyToken == WETH) {            sellToken.approveIfBelow(                address(exchange),                sellAmount            );            boughtAmount = exchange.tokenToEthSwapInput(                sellAmount,                1,                block.timestamp            );            WETH.deposit{ value: boughtAmount }();        } else {            sellToken.approveIfBelow(                address(exchange),                sellAmount            );            boughtAmount = exchange.tokenToTokenSwapInput(                sellAmount,                1,                1,                block.timestamp,                buyToken            );        }        return boughtAmount;    }"