"function exitPool(uint256 amount) external isAllowExit notInLup notAllowContract {        address investor = msg.sender;        require(balanceOf(investor) >= amount && amount > 0, \"Insufficient balance\");        uint256 exitFee = _takeExitFee(investor, amount);        uint256 exitAmount = amount.sub(exitFee);        takeOutstandingPerformanceFee(investor);        uint256 balance = balanceOf(investor);        exitAmount = balance < exitAmount ? balance : exitAmount;        uint256 scale = exitAmount.bdiv(totalSupply());        uint256 cashAmount = convertToCash(exitAmount);        takeOutstandingManagementFee();        IAssetManager(AM()).<mask0>;        _burn(investor, exitAmount);        emit PoolExited(investor, exitAmount);    }"