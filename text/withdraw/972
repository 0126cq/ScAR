"function _withdraw(uint256 _shares) internal virtual {        uint256 r = (balance().mul(_shares)).div(totalSupply());        _burn(msg.sender, _shares);        uint256 b = token.balanceOf(address(this));        if (b < r) {            uint256 _toWithdraw = r.sub(b);            IController(controller).<mask0>;            uint256 _after = token.balanceOf(address(this));            uint256 _diff = _after.sub(b);            if (_diff < _toWithdraw) {                r = b.add(_diff);            }        }        token.safeTransfer(msg.sender, r);    }"