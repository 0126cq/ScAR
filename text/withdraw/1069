"function deallocate(uint256[] memory amounts) public override {        _onlyGuardian();        for (uint256 i; i < amounts.length; i++) {            uint256 amount = amounts[i];            if (amount > 0) {                IERC20 token = _tokens[i];                IERC20 aToken = _aTokens[i];                aToken.approve(address(pool), amount);                pool.<mask0>;            }        }    }"