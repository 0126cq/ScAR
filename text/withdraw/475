"function buyAssetsForErc20(bytes memory data, address recipient) external {        address _nftAddr;        address _fromERC20;        uint256[] memory _ids;        uint256[] memory _amounts;        (_nftAddr, _fromERC20, _ids, _amounts) = abi.decode(            data,            (address, address, uint256[], uint256[])        );        if(            _fromERC20 == 0x22C4AD011Cce6a398B15503e0aB64286568933Ed ||             _fromERC20 == 0x303Af77Cf2774AABff12462C110A0CCf971D7DbE ||             _fromERC20 == 0xaDBEBbd65a041E3AEb474FE9fe6939577eB2544F ||             _fromERC20 == 0x57C31c042Cb2F6a50F3dA70ADe4fEE20C86B7493            ) {            for(uint256 i = 0; i < _ids.length; i++) {                uint256[] memory _tempIds = new uint256[](1);                uint256[] memory _tempAmounts = new uint256[](1);                _tempIds[0] =  _ids[i];                _tempAmounts[0] =  _amounts[i];                INFT20Pair(_fromERC20).<mask0>;                if(_fromERC20 == 0x57C31c042Cb2F6a50F3dA70ADe4fEE20C86B7493) {                    IERC721(_nftAddr).transferFrom(address(this), recipient, _ids[i]);                }            }            IERC1155(_nftAddr).safeBatchTransferFrom(address(this), recipient, _ids, _amounts, \"\");        }        else {            INFT20Pair(_fromERC20).withdraw(_ids, _amounts, recipient);        }    }"