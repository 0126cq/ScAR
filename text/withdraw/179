"function withdrawToSafeAddress(        address pcvDeposit,        address safeAddress,        uint256 amount,        bool pauseAfter,        bool depositAfter    ) external override hasAnyOfThreeRoles(TribeRoles.GOVERNOR, TribeRoles.PCV_SAFE_MOVER_ROLE, TribeRoles.GUARDIAN) {        require(isSafeAddress(safeAddress), \"Provided address is not a safe address!\");        pcvDeposit._ensureUnpaused();        IPCVDeposit(pcvDeposit).<mask0>;        if (pauseAfter) {            pcvDeposit._pause();        }        if (depositAfter) {            IPCVDeposit(safeAddress).deposit();        }        emit PCVGuardianWithdrawal(pcvDeposit, safeAddress, amount);    }"