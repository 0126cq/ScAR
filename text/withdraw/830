"function transferAsset(        address weth,        address asset,        address recipient,        uint256 amount    ) public {        if (asset == weth) {            IWETH(weth).<mask0>;            (bool success, ) = payable(recipient).call{value: amount}(\"\");            require(success, \"!success\");            return;        }        IERC20(asset).safeTransfer(recipient, amount);    }"