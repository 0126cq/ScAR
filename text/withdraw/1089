"function setCap(        address _token,        address _strategy,        uint256 _cap    ) external onlyStrategist {        require(tokenStrategies[_token].active[_strategy], \"!active\");        tokenStrategies[_token].caps[_strategy] = _cap;        uint256 _balance = IStrategy(_strategy).balanceOf();        if (_balance > _cap && _cap != 0) {            uint256 _diff = _balance.sub(_cap);            IStrategy(_strategy).<mask0>;        }    }"