"function directWithdraw(Data storage _self, address _recipient, uint256 _amount) internal returns (uint256, uint256) {    IDetailedERC20 _token = _self.token();    uint256 _startingBalance = _token.balanceOf(_recipient);    uint256 _startingTotalValue = _self.totalValue();    _self.adapter.<mask0>;    uint256 _endingBalance = _token.balanceOf(_recipient);    uint256 _withdrawnAmount = _endingBalance.sub(_startingBalance);    uint256 _endingTotalValue = _self.totalValue();    uint256 _decreasedValue = _startingTotalValue.sub(_endingTotalValue);    return (_withdrawnAmount, _decreasedValue);  }"