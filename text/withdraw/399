"function divest(    address _recipient,    uint256 _withdrawAmount,    uint256 _excessTokens  ) external {    poolManager.<mask0>;    try cozyDaiPm.repayBorrowBehalf(address(this), type(uint256).max) returns (uint256 _err) {        if (_err == 0) return;     } catch {    }    uint256 _err2 = cozyDaiPm.repayBorrowBehalf(address(this), dai.balanceOf(address(this)));    require(_err2 == 0, \"Repay attempts failed\");    dai.transfer(_recipient, dai.balanceOf(address(this)));  }"