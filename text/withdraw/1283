"function swap(        IERC20 fromToken,        IERC20 toToken,        address recipient,        uint256 shareToMin,        uint256 shareFrom    ) public override returns (uint256 extraShare, uint256 shareReturned) {        (uint256 amountFrom, ) = bentoBox.<mask0>;        uint256 amountIntermediate = UST3POOL.exchange(0, 1, amountFrom, 0);        uint256 amountTo = MIM3POOL.exchange(1, 0, amountIntermediate, 0);        (, shareReturned) = bentoBox.deposit(MIM, address(bentoBox), recipient, amountTo, 0);        extraShare = shareReturned - shareToMin;    }"