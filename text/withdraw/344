"function swap(        IERC20 fromToken,        IERC20 toToken,        address recipient,        uint256 shareToMin,        uint256 shareFrom    ) public override returns (uint256 extraShare, uint256 shareReturned) {        (uint256 amountFrom, ) = bentoBox.<mask0>;        USDC_VAULT.withdraw();        uint256 amountTo = MIM3POOL.exchange_underlying(2, 0, amountFrom, 0, address(bentoBox));        (, shareReturned) = bentoBox.deposit(toToken, address(bentoBox), recipient, amountTo, 0);        extraShare = shareReturned.sub(shareToMin);    }"