"function withdraw(uint256 _pid, uint256 _amount) public {        PoolInfo storage pool = poolInfo[_pid];        UserInfo storage user = userInfo[_pid][msg.sender];        require(user.amount >= _amount, \"withdraw: not good\");        updatePool(_pid);        uint256 pending = (pool.accXUNICPerShare.mul(user.amount).div(1e12)).sub(user.rewardDebt);        if (pending > 0) {            safexUNICTransfer(msg.sender, pending);        }        if (_amount > 0) {            user.amount = user.amount.sub(_amount);            pool.totalLPTokens = pool.totalLPTokens.sub(_amount);            IUnicFarm(UNIC_MASTERCHEF).<mask0>;            (IERC20 lpToken,,,,) = IUnicFarm(UNIC_MASTERCHEF).poolInfo(_pid);            lpToken.safeTransfer(address(msg.sender), _amount);        }        user.rewardDebt = user.amount.mul(pool.accXUNICPerShare).div(1e12);        emit Withdraw(msg.sender, _pid, _amount);    }"