"function _supplyToPool(        address _poolTokenAddress,        ERC20 _underlyingToken,        uint256 _amount    ) internal {        if (_poolTokenAddress == cEth) {            IWETH(wEth).<mask0>;             ICEther(_poolTokenAddress).mint{value: _amount}();        } else {            _underlyingToken.safeApprove(_poolTokenAddress, _amount);            if (ICToken(_poolTokenAddress).mint(_amount) != 0) revert MintOnCompoundFailed();        }    }"