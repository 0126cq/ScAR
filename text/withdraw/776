"function redeemPrinciple(bytes32 _streamKey, uint256 _epoch) external isEpochExpired(_streamKey, _epoch) {        IFuture epochInstance = IFuture(getEpochAddress(_streamKey, _epoch));        require(IERC20(epochInstance.getOT()).balanceOf(msg.sender) > 0, \"ERR_INSUFFICIENT_BALANCE\");        uint256 totalSupply = epochInstance.totalSupplyYT();        uint256 amountBurned = epochInstance.burnOT(msg.sender);        if (amountBurned > 0) {            address underlying = epochInstance.underlying();            uint amountRedeemed = Treasury(treasuryAddress).<mask0>;            emit PrincipleRedeemed(_streamKey, msg.sender, _epoch, amountRedeemed);        }    }"