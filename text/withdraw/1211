"function _exchange(BaseParams memory _baseParams, bytes memory _params) internal override {        IX2Y2.RunInput memory _orderParams = _decodeParams(_params);        uint256 paymentValue = _baseParams.salePrice;        if (_baseParams.currency == address(0)) {            WETH.<mask0>;            x2y2.run{value: paymentValue}(_orderParams);        } else {            WETH.approve(address(x2y2), paymentValue);            x2y2.run(_orderParams);            WETH.approve(address(x2y2), 0);        }    }"