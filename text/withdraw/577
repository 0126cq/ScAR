"function transfer(address _to, address _token, uint _amount) external onlySupportedToken(_token) onlyEnabledToken(_token) whenNotPaused nonReentrant {        globalConfig.bank().newRateIndexCheckpoint(_token);        uint256 amount = globalConfig.accounts().<mask0>;        globalConfig.accounts().deposit(_to, _token, amount);        emit Transfer(_token, msg.sender, _to, amount);    }"