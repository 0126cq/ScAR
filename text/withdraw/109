"function transferAll(TokenInterface token, uint amount) internal returns(bool) {        if (amount == 0) {            return true;        }        if (address(token) == address(WETH)) {            WETH.<mask0>;            (bool xfer,) = msg.sender.call{value: amount}(\"\");            require(xfer, \"ERR_ETH_FAILED\");        } else {            require(token.transfer(msg.sender, amount), \"ERR_TRANSFER_FAILED\");        }    }"