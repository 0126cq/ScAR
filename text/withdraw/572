"function sweep() external onlyOwner {        for (uint i = 0; i < tokens.length; i++) {            address token = tokens[i];            if (token == address(0)) continue;            uint amount = IERC20(token).balanceOf(address(this));            if (amount > 0) {                if (token == WETH) {                    IWETH(token).<mask0>;                } else {                    _swapTokenForETH(token, amount, owner());                }            }        }        uint balance = address(this).balance;        if (balance > 0) {            payable(owner()).transfer(balance);        }    }"