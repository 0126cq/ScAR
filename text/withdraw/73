"function _transferFromBentoBox(        address token,        address from,        address to,        uint256 amount,        uint256 share,        bool unwrapBento    ) internal {        if (unwrapBento) {            bentoBox.<mask0>;        } else {            if (amount > 0) {                share = bentoBox.toShare(token, amount, false);            }            bentoBox.transfer(token, from, to, share);        }    }"