"function buyForStrategy(\t\tuint256 amount,        address rewardToken,        address recipient    ) public onlyStrategy{    \taddress[] memory path;        uint256[] memory amounts;        uint256 tokenAmount = amount;        if(amount < minBuyFromAMM){            path = new address[](3);        \tpath[0] = address(weth);        \tpath[1] = DEUS;        \tpath[2] = rewardToken;        }else{            path = new address[](2);            path[0] = DEUS;            path[1] = rewardToken;            weth.<mask0>;            tokenAmount = AMM.calculatePurchaseReturn(amount);            AMM.buy{value: amount}(tokenAmount);        }        amounts = uniswapRouter.swapExactTokensForTokens(            tokenAmount, 1, path, recipient, block.timestamp        );    \tIStakeAndYield(recipient).notifyRewardAmount(    \t\tamounts[amounts.length-1],     \t\t2     \t);\t}"