"function ragequit(address[] calldata tokens, uint256 share) external returns (uint256[] memory amounts) {        require(!lock);        lock = true;        uint256 totalShare = _711.totalSupply();        _711.burnFrom(msg.sender, share);        amounts = new uint256[](tokens.length);        for(uint256 i = 0; i < tokens.length; i++) {            if(i > 0) require(uint256(tokens[i-1]) < uint256(tokens[i]), \"bad order\");            uint256 tokenInBank;            if(tokens[i] == etherAddr) {                address payable _bank = address(bank);                tokenInBank = _bank.balance;            }            else {                tokenInBank = ERC20(tokens[i]).balanceOf(address(bank));            }            uint256 amount = share.mul(tokenInBank).div(totalShare);            amounts[i] = amount;            require(bank.<mask0>, \"fail to withdraw\");        }        lock = false;    }"