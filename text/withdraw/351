"function _aaveWithdraw(uint256 amount) internal {    address lendPool = lendingPool();    IERC20(aTokenAddress).safeApprove(lendPool, 0);    IERC20(aTokenAddress).safeApprove(lendPool, amount);    uint256 maxAmount = IERC20(aTokenAddress).balanceOf(address(this));    uint256 amountWithdrawn = ILendingPool(lendPool).<mask0>;    require(      amountWithdrawn == amount ||      (amount == uint256(-1) && maxAmount == IERC20(aaveUnderlying).balanceOf(address(this))),      \"Did not withdraw requested amount\"    );  }"