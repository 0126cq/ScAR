"function withdrawAll(address _token) external onlySupportedToken(_token) whenNotPaused nonReentrant {        require(globalConfig.accounts().getDepositPrincipal(msg.sender, _token) > 0, \"Token depositPrincipal must be greater than 0\");        globalConfig.bank().newRateIndexCheckpoint(_token);        uint amount = globalConfig.accounts().getDepositBalanceCurrent(_token, msg.sender);        uint256 actualAmount = globalConfig.bank().<mask0>;        if(actualAmount != 0) {            SavingLib.send(globalConfig, actualAmount, _token);        }        emit WithdrawAll(_token, msg.sender, actualAmount);    }"