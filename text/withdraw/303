"function remarkRewards(address[] calldata _oldCrvPools, uint256[] calldata _pids) public {        require(_oldCrvPools.length >= _pids.length, \"!_oldCrvPools\");        for (uint256 i = 0; i < _pids.length; i++) {            PoolInfo storage pool = poolInfo[_pids[i]];            address currentOldCrvPools = _oldCrvPools[_pids[i]];            if (pool.rewardCrvPool != currentOldCrvPools) {                if (IConvexRewardPool(currentOldCrvPools).earned(msg.sender) > 0) {                    IConvexRewardPool(currentOldCrvPools).getReward(msg.sender);                }                IConvexRewardPool(currentOldCrvPools).<mask0>;                IConvexRewardPoolV2(pool.rewardCrvPool).stake(msg.sender);            }        }    }"