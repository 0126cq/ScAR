"function swap(        IERC20,        IERC20,        address recipient,        uint256 shareToMin,        uint256 shareFrom    ) public override returns (uint256 extraShare, uint256 shareReturned) {        DEGENBOX.withdraw(IERC20(address(YVMIM3CRV)), address(this), address(this), 0, shareFrom);        uint256 mim3CrvAmount = YVMIM3CRV.<mask0>;        uint256 mimAmount = MIM3CRV.remove_liquidity_one_coin(mim3CrvAmount, 0, 0, address(DEGENBOX));        (, shareReturned) = DEGENBOX.deposit(MIM, address(DEGENBOX), recipient, mimAmount, 0);        extraShare = shareReturned - shareToMin;    }"