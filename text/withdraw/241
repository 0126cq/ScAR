"function withdrawCollateral(uint256 amount, uint256 loanID)        external        paused(\"\", false)        nonReentry(\"\")        authorized(AUTHORIZED, msg.sender)    {        require(            msg.sender == LibLoans.loan(loanID).borrower,            \"Teller: not borrower\"        );        require(amount > 0, \"Teller: zero withdraw\");        if (LibLoans.loan(loanID).status == LoanStatus.Active) {            (, uint256 needed, ) = LibLoans.getCollateralNeededInfo(loanID);            if (needed > 0) {                require(                    LibCollateral.e(loanID).loanSupply(loanID) - needed >=                        amount,                    \"Teller: collateral withdraw amount over limit\"                );            }        }        LibCollateral.<mask0>;    }"