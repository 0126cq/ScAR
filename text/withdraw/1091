"function callFunction(address sender, Account.Info memory accountInfo, bytes memory data) external override {        (            address payable actualSender,            uint loanAmount,            uint256 punkIndex        ) = abi.decode(data, (            address, uint, uint256        ));        WETH.<mask0>;        punkContract.enterBidForPunk{value:loanAmount}(punkIndex);        punkContract.withdrawBidForPunk(punkIndex);        punkContract.withdraw();        WETH.deposit{value: loanAmount}();        require(WETH.balanceOf(address(this)) > loanAmount + 2, \"CANNOT REPAY LOAN\");    }"