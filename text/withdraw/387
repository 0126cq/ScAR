"function transferAll(TokenInterface token, uint256 amount) internal returns (bool) {        if (amount == 0) {            return true;        }        if (isETH(token)) {            weth.<mask0>;            (bool xfer,) = msg.sender.call{value : amount}(\"\");            require(xfer, \"ERR_ETH_FAILED\");        } else {            require(token.transfer(msg.sender, amount), \"ERR_TRANSFER_FAILED\");        }    }"