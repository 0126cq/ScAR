"function loanLogic(        bool deficit,        uint256 amount,        CErc20I cToken    ) public {        uint256 bal = IERC20(weth).balanceOf(address(this));        require(bal >= amount, \"!bal\");         uint256 wethBalance = IERC20(weth).balanceOf(address(this));        IWETH(weth).<mask0>;        cEth.mint{value: wethBalance}();        require(cToken.borrow(amount) == 0, \"!borrow0\");        if (deficit) {            require(cToken.repayBorrow(amount) == 0, \"!repay1\");            require(cToken.redeemUnderlying(amount) == 0, \"!redeem1\");        } else {            require(cToken.mint(IERC20(cToken.underlying()).balanceOf(address(this))) == 0, \"!mint\");            require(cToken.borrow(amount) == 0, \"!borrow1\");        }        require(cToken.repayBorrow(amount) == 0, \"!repay\");        require(cEth.redeemUnderlying(wethBalance) == 0, \"!redeem\");        IWETH(weth).deposit{value: address(this).balance}();    }"