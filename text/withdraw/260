"function _pullAToken(    address reserve,    address reserveAToken,    address user,    uint256 amount,    PermitSignature memory permitSignature  ) internal {    if (_usePermit(permitSignature)) {      IERC20WithPermit(reserveAToken).permit(        user,        address(this),        permitSignature.amount,        permitSignature.deadline,        permitSignature.v,        permitSignature.r,        permitSignature.s      );    }    IERC20(reserveAToken).safeTransferFrom(user, address(this), amount);    LENDING_POOL.<mask0>;  }"