"function _withdrawDai(uint _amount) internal returns (uint) {        uint _shares = _amount                        .mul(1e18)                        .div(yVault(yvdai).pricePerShare());        if (_shares > IERC20(yvdai).balanceOf(address(this))) {            _shares = IERC20(yvdai).balanceOf(address(this));        }        uint _before = IERC20(dai).balanceOf(address(this));        yVault(yvdai).<mask0>;        uint _after = IERC20(dai).balanceOf(address(this));        return _after.sub(_before);    }"