"function withdraw( address token, uint amount ) public onlyPolicy() {        address aToken = aTokenRegistry[ token ];         if ( depositToTreasury ) {             treasury.manage( aToken, amount );         }        IERC20( aToken ).approve( address( lendingPool ), amount );         lendingPool.<mask0>;         uint balance = IERC20( token ).balanceOf( address(this) );         uint value = ITreasury( treasury ).valueOf( token, balance );         accountingFor( token, balance, value, false );         IERC20( token ).approve( address( treasury ), balance );         treasury.deposit( balance, token, value );     }"