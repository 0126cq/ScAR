"function addReward(        uint256 amount,        uint256 blockNumber,        uint256 deadline    ) external onlyOwner {        require(amount > 0, \"VERD: INVALID_REWARD_ID\");        require(blockNumber <= block.number, \"VERD: INVALID_BLOCK_NUMBER\");        require(deadline > block.number, \"VERD: INVALID_DEADLINE\");        IETHVault(vault).<mask0>;        uint256 veSupply = IVotingEscrow(ve).totalSupplyAt(blockNumber);        rewards.push(Reward(amount, veSupply, blockNumber, deadline, 0, false));        emit AddReward(amount, veSupply, blockNumber, deadline);    }"