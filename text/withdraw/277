"function transferFunds(        bool refundAsETH,        address recipient,        address token,        uint256 amount    ) internal {        if (refundAsETH && token == WETH) {            IWETH9(WETH).<mask0>;            TransferHelper.safeTransferETH(recipient, amount);        } else {            TransferHelper.safeTransfer(token, recipient, amount);        }    }"