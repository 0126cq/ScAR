"function drawMaxCollateral(uint _cdpId, address _joinAddr, uint _amount) internal returns (uint) {        manager.frob(_cdpId, -toPositiveInt(_amount), 0);        manager.flux(_cdpId, address(this), _amount);        uint joinAmount = _amount;        if (Join(_joinAddr).dec() != 18) {            joinAmount = _amount / (10 ** (18 - Join(_joinAddr).dec()));        }        Join(_joinAddr).exit(address(this), joinAmount);        if (isEthJoinAddr(_joinAddr)) {            Join(_joinAddr).gem().<mask0>;         }        return joinAmount;    }"