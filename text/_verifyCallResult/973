"function redeem(    address nftAsset,    uint256 nftTokenId,    uint256 amount,    uint256 bidFine  ) external override nonReentrant whenNotPaused returns (uint256) {    address poolLiquidator = _addressesProvider.getLendPoolLiquidator();    (bool success, bytes memory result) = poolLiquidator.delegatecall(      abi.encodeWithSignature(\"redeem(address,uint256,uint256,uint256)\", nftAsset, nftTokenId, amount, bidFine)    );    bytes memory resultData = <mask0>;    uint256 repayAmount = abi.decode(resultData, (uint256));    return (repayAmount);  }"