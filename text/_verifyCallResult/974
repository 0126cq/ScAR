"function liquidate(    address nftAsset,    uint256 nftTokenId,    uint256 amount  ) external override nonReentrant whenNotPaused returns (uint256) {    address poolLiquidator = _addressesProvider.getLendPoolLiquidator();    (bool success, bytes memory result) = poolLiquidator.delegatecall(      abi.encodeWithSignature(\"liquidate(address,uint256,uint256)\", nftAsset, nftTokenId, amount)    );    bytes memory resultData = <mask0>;    uint256 extraAmount = abi.decode(resultData, (uint256));    return (extraAmount);  }"