"function addReferrer(address _user, uint32 _code)        external    {        require(            hasRole(STORE_ROLE, msg.sender) ||                <mask0>,            \"SHFAffiliate: Caller is not a Store\"        );        if( referralDetails[_code].user != address(0) && referralDetails[_code].isActive == true) {            if(referralLists[_user] == 0) {                _createNewReferralInformation(_user);            }            if(referralLists[_user] != _code && !_hasReferrerTier1(_user)) {                referralDetails[referralLists[_user]].referrerCode = _code;                 }        }    }"