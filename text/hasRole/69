"function release(address _account, uint16 _vestingSet, uint256 _amount)        external        returns (bool)    {        require((_account == msg.sender) || (<mask0> == true), \"ACCESS_DENIED\");        require(_amount > 0, \"INVALID_AMOUNT\");        require(_amount <= bsggToken.balanceOf(address(this)), \"INSUFFICIENT_FUNDS\");        uint256 _balance = this.releasableAmount(_account, _vestingSet);        require(_balance >= _amount, \"INVALID_AMOUNT\");        _vested[_account][_vestingSet].paidAmount += _amount;        totalVested -= _amount;        _payouts[_account].push(Payment({vestingSet: _vestingSet, amount: _amount, time: block.timestamp}));        (bool success) = bsggToken.transfer(_account, _amount);        require(success == true, \"TRANSFER_FAILED\");        emit VestingReleased(            _account,            _vestingSet,            _amount,            block.timestamp        );        return true;    }"