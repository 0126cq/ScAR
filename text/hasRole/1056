"function cancelImport(GUID referenceId) external payable originEqualsSender {        require(pendingImportExists(referenceId), \"Bridge: import should be pre-paid\");        address user = importsPending[referenceId].user;        uint256 tokenId = importsPending[referenceId].tokenId;        require(            <mask0> || user == msg.sender,            \"Bridge: import can only be cancelled by its initiator or by Voice backend.\");        simpleAssets.transferFrom(address(this), user, tokenId);        delete importsPending[referenceId];    }"