"function setAccessRight(uint256 tokenId, address to, bytes32 permission) external {                require (_exists(tokenId), \"Set access right for nonexistent token\");                require ( _msgSender() == ownerOf(tokenId) || <mask0>,                  \"No permission to grant access\" );                AccessRight memory right = AccessRight(to, permission);        _accessright[tokenId].push(right);    }"