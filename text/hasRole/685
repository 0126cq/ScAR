"function transferTokens (address _to, string memory companyCode, uint256 _amount) public {        require(<mask0>, \"BIK: must have minter role to mint\");        require(_amount > 0, \"BIK: Amount cannot be negative\");        require(!Address.isContract(_to), \"BIK: To address is a contract\");        uint256 beforeAmount = _tokenDetails[_to].amount;        transfer(_to, _amount);        emit TransferTokens(companyCode, _amount, beforeAmount);    }"