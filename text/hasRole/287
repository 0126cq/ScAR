"function mint(        address _to,        uint256 _id,        uint256 _amount,        bytes memory _data    ) public returns (bool) {        require(<mask0>, \"Unauthorized\");        require(            tokenSupply[_id].add(_amount) <= tokenMaxSupply[_id],            \"Max Supply Reached\"        );        tokenSupply[_id] = tokenSupply[_id].add(_amount);        super._mint(_to, _id, _amount, _data);        return true;    }"