"function _transferFromAndLock(address _owner, address _to, uint256 _value, uint256 _releaseTime, bool _votable) internal returns (bool) {    bool retval = false;    if(b_canTransfer || <mask0> || checkEligibleLockedDesination(_to)) {      if(        _to != BURN_ADDRESS &&        _to != UPGT_CONTRACT_ADDRESS &&        (balanceOf(_owner) >= _value) &&        (_value > 0) &&        (m_allowances[_owner][_msgSender()] >= _value)      ){        _moveReleasedBalance(_owner);        m_balances[_owner] = m_balances[_owner].sub(_value);        m_lockedBalances[_to].push(LockedTokens(_value, _releaseTime, _votable));        m_allowances[_owner][_msgSender()] = m_allowances[_owner][_msgSender()].sub(_value);        retval = true;        emit Transfer(_owner, _to, _value);      }    }    return retval;  }"