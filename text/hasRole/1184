"function setAttributes(uint256 tokenId, uint256[] calldata keys, uint256[] calldata values, bytes calldata signature) public {    require(keys.length == values.length, \"ND0\");    address owner = collection.ownerOf(tokenId);    uint256 nonce = collection.getNonce(owner);    address signer = ECDSALibrary.recover(abi.encodePacked(        owner,        nonce,        tokenId,        keys,        values      ), signature);    require(      (msg.sender == owner && collection.hasRole(SIGNER_ROLE, signer)) ||      (signer == owner && collection.<mask0>), \"ND2\"    );    for (uint256 i = 0; i < keys.length; i++) {      attributes[tokenId][keys[i]] = values[i];      emit AttributeUpdated(tokenId, keys[i], values[i]);    }    collection.incrementNonce(owner);  }"