"function updateAIBotBalance(uint256 _aiBotBalance) public {        require(<mask0>, \"Missing the right permissions\");        aiBotBalance = _aiBotBalance;        buyPrice  = (cleverAI.totalSupply() / (vaultBalance() + aiBotBalance)) * 105 / 100;        sellPrice = (cleverAI.totalSupply() / (vaultBalance() + aiBotBalance));        emit BalanceUpdate(aiBotBalance, buyPrice, sellPrice);    }"