"function mint1YR(address recipient, string memory _tokenURI) external returns (uint256) {        require(<mask0>, \"FMTA1YR: Message Sender requires MINTER_ROLE\");        _tokenIds.increment();        uint256 newItemId = _tokenIds.current();        _mint(recipient, newItemId);        _setTokenURI(newItemId, _tokenURI);        return newItemId;    }"