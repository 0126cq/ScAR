"function mintBaller(address to, uint256 teamId, string memory mdHash) public {        require(<mask0>, \"BallerFactory: Must have minter role to mint.\");        require(to != address(0), \"BallerFactory: Cannot mint to the 0 address.\");        require(teamId < 30, \"BallerFactory: Invalid team.\");        require(teamId >= 0, \"BallerFactory: Invalid team.\");        require(hashes[mdHash] != 1, \"BallerFactory: Hash has already been used.\");        hashes[mdHash] = 1;        string memory team = ballerTeamPool[teamId];        require(ballersInCirculation[team] < maxBallers, \"BallerFactory: There are no ballers left for this team!\");        ballerTeams[_ballerIds.current()] = team;        ballersInCirculation[team] = ballersInCirculation[team].add(1);        _safeMint(to, _ballerIds.current());        _setTokenURI(_ballerIds.current(), string(abi.encodePacked(_baseURI(), mdHash)));        _ballerIds.increment();        emit BallerPurchaseRequest(to, team);    }"