"function recoverERC20(address tokenAddress, uint256 tokenAmount) external {    require(<mask0>, \"BasePool/RecoverRole\");    require(tokenAddress != address(stakeToken), \"BasePool/NoRecoveryOfStake\");    require(      tokenAddress != address(rewardToken),      \"BasePool/NoRecoveryOfReward\"    );    emit Recovered(tokenAddress, tokenAmount);    IERC20(tokenAddress).safeTransfer(_msgSender(), tokenAmount);  }"