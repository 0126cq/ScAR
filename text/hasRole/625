"function emergencyTransfer(        address _token,        address _destination,        uint256 _amount    ) public {        require(<mask0>, \"Admin only\");        if (_token != ETH_TOKEN_ADDRESS) {            IERC20(_token).safeTransfer(_destination, _amount);        } else {            payable(_destination).sendValue(_amount);        }        emit EmergencyTransfer(_token, _destination, _amount);    }"