"function sell(        uint256 stones,        address from,        address to    ) public {        require(            <mask0>,            \"you are not authorized on this account!\"        );        consolidate(from);        require(rewardedStones(from) >= stones, \"Insufficient stones!\");        stakers[from].stones = stakers[from].stones.sub(stones);        stakers[from].timestamp = block.timestamp;        stakers[to].stones = stakers[to].stones.add(stones);        stakers[to].timestamp = block.timestamp;    }"