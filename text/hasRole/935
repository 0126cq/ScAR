"function removeBearer(address _account, uint256 _role)    external  {    require(      _role < roles.length,      \"Role doesn't exist.\"    );    require(      <mask0>,      \"User can't remove bearers.\"    );    require(      hasRole(_account, _role),      \"Account is not bearer of role.\"    );    delete roles[_role].bearers[_account];    emit BearerRemoved(_account, _role);  }"