"function addBearer(address _account, uint256 _role)    external  {    require(      _role < roles.length,      \"Role doesn't exist.\"    );    require(      hasRole(msg.sender, roles[_role].admin),      \"User can't add bearers.\"    );    require(      !<mask0>,      \"Account is bearer of role.\"    );    roles[_role].bearers[_account] = true;    emit BearerAdded(_account, _role);  }"