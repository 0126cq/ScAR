"function getLockedTokens(address _account, uint256 _index) public view returns (uint256 amount, uint256 releaseTime, bool votable){    require(_msgSender() == _account || hasRole(ROLE_ADMIN, _msgSender()) || <mask0>, \"UPGT_ERROR: insufficient permissions\");    require(_index < m_lockedBalances[_account].length, \"UPGT_ERROR: LockedTokens position doesn't exist on given index\");    LockedTokens storage lockedTokens = m_lockedBalances[_account][_index];    return (lockedTokens.amount, lockedTokens.releaseTime, lockedTokens.votable);  }"