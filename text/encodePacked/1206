"function execute(ForwardRequest calldata req, bytes calldata signature)        public        payable        returns (bool, bytes memory)    {        require(verify(req, signature), \"MinimalForwarder: signature does not match request\");        _nonces[req.from] = req.nonce + 1;        (bool success, bytes memory returndata) = req.to.call{gas: req.gas, value: req.value}(            abi.<mask0>        );        if (gasleft() <= req.gas / 63) {            assembly {                invalid()            }        }        return (success, returndata);    }"