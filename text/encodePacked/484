"function claim(    uint index,    address account,    uint amount,    bytes32[] calldata merkleProof  ) external override {    require(!isClaimed(index), 'MerkleDistributor: Drop already claimed.');    bytes32 node = keccak256(abi.<mask0>);    require(MerkleProof.verify(merkleProof, merkleRoot, node), 'MerkleDistributor: Invalid proof.');    _setClaimed(index);    require(IERC20(token).transfer(account, amount), 'MerkleDistributor: Transfer failed.');    emit Claimed(index, account, amount);  }"