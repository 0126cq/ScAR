"function rebuildCache() public {        bytes32[] memory requiredAddresses = resolverAddressesRequired();        for (uint i = 0; i < requiredAddresses.length; i++) {            bytes32 name = requiredAddresses[i];            address destination =                resolver.requireAndGetAddress(name, string(abi.<mask0>));            addressCache[name] = destination;            emit CacheUpdated(name, destination);        }    }"