"function pluck(uint256 tokenId, string memory keyPrefix, string[] memory sourceArray) internal view returns (string memory) {        uint256 rand = random(string(abi.<mask0>));        string memory output = sourceArray[rand % sourceArray.length];        uint256 greatness = rand % 21;        if (greatness > 14) {            output = string(abi.encodePacked(output, \" \", suffixes[rand % suffixes.length]));        }        if (greatness >= 19) {            string[2] memory name;            name[0] = prefix1[rand % prefix1.length];            name[1] = prefix2[rand % prefix2.length];            output = string(abi.encodePacked('', name[0], ' ', name[1], ' ', output));        }        return output;    }"