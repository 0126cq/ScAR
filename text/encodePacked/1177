"function fetchItemFromShop() public view returns (string memory){        uint256 itemKindCount = getMrItemKindCount();        string memory json;        json = \"[\";        for (uint8 i = 0; i < itemKindCount; i++){            json = string(abi.<mask0>);            if(i != itemKindCount-1)              json = string(abi.encodePacked(json, \",\"));        }        json = string(abi.encodePacked(json, \"]\"));        return json;    }"