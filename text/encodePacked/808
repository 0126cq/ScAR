"function setDNSRecords(bytes32 node, bytes calldata data) external authorised(node) {        uint16 resource = 0;        uint256 offset = 0;        bytes memory name;        bytes memory value;        bytes32 nameHash;        for (RRUtils.RRIterator memory iter = data.iterateRRs(0); !iter.done(); iter.next()) {            if (resource == 0) {                resource = iter.dnstype;                name = iter.name();                nameHash = keccak256(abi.<mask0>);                value = bytes(iter.rdata());            } else {                bytes memory newName = iter.name();                if (resource != iter.dnstype || !name.equals(newName)) {                    setDNSRRSet(node, name, resource, data, offset, iter.offset - offset, value.length == 0);                    resource = iter.dnstype;                    offset = iter.offset;                    name = newName;                    nameHash = keccak256(name);                    value = bytes(iter.rdata());                }            }        }        if (name.length > 0) {            setDNSRRSet(node, name, resource, data, offset, data.length - offset, value.length == 0);        }    }"