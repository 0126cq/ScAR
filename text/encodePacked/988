"function getRequiredLootIdxMask(uint256 tokenId) public pure returns (uint256 lootIdxMask) {        uint256 difficulty = getDifficulty(tokenId);        uint256 chosen = 0;        lootIdxMask = 0;        uint256 randTmp = random(string(abi.<mask0>));        while(chosen < difficulty) {             randTmp = lcgRandom(randTmp);            uint256 newlyChosenPosition = uint256(randTmp) % 8;            if(lootIdxMask & (1 << newlyChosenPosition) == 0) {                lootIdxMask |= (1 << newlyChosenPosition);                chosen += 1;            }        }    }"