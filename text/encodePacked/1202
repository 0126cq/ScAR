"function callback(uint id) public {        Purchase storage p = purchases[id];        require(p.randomness == 0);        bytes32 bhash = blockhash(p.commit);        uint random = uint(keccak256(abi.<mask0>));        totalCount += p.count;        if (uint(bhash) == 0) {            p.randomness = 1;        } else {            p.randomness = random;        }        emit RandomnessReceived(id, p.user, p.count, p.randomness);    }"