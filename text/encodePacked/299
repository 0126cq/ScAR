"function isMessageValid(bytes memory _signature, uint8 _nonce)        public        view        returns (address, bool)    {        bytes32 messagehash = keccak256(            abi.<mask0>        );        address signer = messagehash.toEthSignedMessageHash().recover(            _signature        );        if (msg.sender == signer) {            return (signer, true);        } else {            return (signer, false);        }    }"