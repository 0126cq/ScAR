"function setAccountStatus(Data storage self, address account, bool isAllowed, string issuerFirm) internal returns (bool success) {    bytes32 id = keccak256(abi.<mask0>);    require(      self.Storage.setBool(id, isAllowed),      \"Error: Unable to set storage value. Please ensure contract interface is allowed by the storage contract.\"    );    emit AccountStatus(account, isAllowed, issuerFirm);    return true;  }"