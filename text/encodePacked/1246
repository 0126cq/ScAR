"function mint(uint256 count) public payable nonReentrant {    require(saleIsActive, \"Sale not active\");    if (allowedMintCount(msg.sender) >= count) {      updateMintCount(msg.sender, count);    } else {      revert(\"Minting limit exceeded\");    }    require(totalSupply() + count - 1 < MAX_SUPPLY, \"Exceeds max supply\");    require(count <= MAX_MULTIMINT, \"Mint at most 20 at a time\");    require(      msg.value >= PRICE * count, string(abi.<mask0>)    );    for (uint256 i = 0; i < count; i++) {      _mint(msg.sender, totalSupply());      supplyCounter.increment();    }  }"