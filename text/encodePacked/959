"function random(uint256 from, uint256 to, uint256 salty) private view returns (uint256) {        uint256 seed = uint256(            keccak256(                abi.encodePacked(                    block.timestamp + block.difficulty +                    ((uint256(keccak256(abi.<mask0>))) / (now)) +                    block.gaslimit +                    ((uint256(keccak256(abi.encodePacked(msg.sender)))) / (now)) +                    block.number +                    salty                )            )        );        return seed.mod(to - from) + from;    }"