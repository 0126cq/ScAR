"function getMaxTokensForPhase(address target, bytes32[] memory proof) public view returns (uint256) {        bytes32 leaf = keccak256(abi.<mask0>);        if (currentStage == STAGE_PUBLIC) {            return maxTokensPerAddress;        } else if (currentStage == STAGE_PRESALE) {            if (MerkleProof.verify(proof, whitelistRoot, leaf)) {                return maxTokensPerAddress;            } else {                return 0;            }        } else {            return 0;        }    }"