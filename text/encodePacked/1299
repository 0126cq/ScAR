"function getFullTextElem(uint256 tokenId, string memory keyPrefix, string[] memory sourceArray, string[] memory legendsArray, string memory yVal) internal pure returns (string memory) {        uint256 rand = random(string(abi.encodePacked(keyPrefix, toString(tokenId))));        bool isRare = (rand % 21) >= 19;        string memory textOpen = '<text x=\"30\" y=';        string memory classType = isRare ? 'class=\"rare\">' : 'class=\"base\">';        string memory player = pluck(tokenId, keyPrefix, sourceArray, legendsArray);        string memory closingTag = '</text>';        return string(abi.<mask0>);    }"