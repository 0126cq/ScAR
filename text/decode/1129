"function presaleMint(uint8 number, bytes calldata encoded, uint8 v, bytes32 r, bytes32 s) public payable whenNotPaused {        bytes32 hash = keccak256(encoded);        address signer = ecrecover(hash, v, r, s);        require (ticket_signer == signer, \"wrong signer\");        (uint32 total_allowed, address wallet, bool free) = abi.<mask0>;        require(free || presale, 'presale over');        require (msg.sender == wallet, \"wrong user\");        require (mints_per_address[msg.sender] + number <= total_allowed, \"max presale\");        internalMint(number, free);    }"