"function liquidationCall(        address _collateral,        address _reserve,        address _user,        uint256 _purchaseAmount,        bool _receiveAToken    ) external payable nonReentrant onlyActiveReserve(_reserve) onlyActiveReserve(_collateral) {        address liquidationManager = addressesProvider.getLendingPoolLiquidationManager();        (bool success, bytes memory result) = liquidationManager.delegatecall(            abi.encodeWithSignature(                \"liquidationCall(address,address,address,uint256,bool)\",                _collateral,                _reserve,                _user,                _purchaseAmount,                _receiveAToken            )        );        require(success, \"Liquidation call failed\");        (uint256 returnCode, string memory returnMessage) = abi.<mask0>;        if (returnCode != 0) {            revert(string(abi.encodePacked(\"Liquidation failed: \", returnMessage)));        }    }"