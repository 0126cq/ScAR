"function callOptionalReturn(        ERC20 token,        bytes memory data,        string memory functionName,        string memory location    ) private {        (bool success, bytes memory returndata) = address(token).call(data);        require(            success,            string(abi.encodePacked(\"SafeERC20: \", functionName, \" failed in \", location))        );        if (returndata.length > 0) {            require(                abi.<mask0>,                string(                    abi.encodePacked(\"SafeERC20: \", functionName, \" returned false in \", location)                )            );        }    }"