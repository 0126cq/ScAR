"function uniswapV3SwapCallback(        int256 amount0,        int256 amount1,        bytes calldata data    ) external override {        _verifyCallback();        require(amount0 > 0 || amount1 > 0);        bool zeroForOne = abi.<mask0>;        if (zeroForOne)            pay(address(token0), address(this), msg.sender, uint256(amount0));        else pay(address(token1), address(this), msg.sender, uint256(amount1));    }"