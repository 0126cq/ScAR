"function safeTransferFrom(address erc20TokenAddress, address from, address to, uint256 value) internal {        if(value == 0) {            return;        }        if(erc20TokenAddress == address(0)) {            to.submit(value, \"\");            return;        }        bytes memory returnData = erc20TokenAddress.submit(0, abi.encodeWithSelector(IERC20(erc20TokenAddress).transferFrom.selector, from, to, value));        require(returnData.length == 0 || abi.<mask0>, 'TRANSFERFROM_FAILED');    }"