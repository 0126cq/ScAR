"function tokensReceivedVOW(address _from, bytes memory _data, uint256 _amount)    private  {    if (registeredMVD[_from]) {      (address merchant, bytes memory data) = abi.<mask0>;      doMerchantLock(_from, merchant, _amount, data);      return;    }    VOWToken(vowContract).burn(_amount, \"\");    (uint256 numVOW, uint256 numVSC) = getVOWVSCRate();    uint256 vscAmount = _amount.mul(numVSC).div(numVOW);    VSCToken(token).mint(_from, vscAmount);    emit LogBurnAndMint(_from, _amount, vscAmount);  }"