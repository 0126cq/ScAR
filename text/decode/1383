"function safeTransferFrom(IERC20 token, address from, address to, uint256 amount) internal {        (bool success, bytes memory data) = address(token).call(abi.encodeWithSelector(0x23b872dd, from, to, amount));        require(success && (data.length == 0 || abi.<mask0>), \"BoringERC20: TransferFrom failed\");    }"