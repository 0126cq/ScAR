"function reconstruct(uint256[] calldata traitTokenIds, bytes calldata encoded, uint8 v, bytes32 r, bytes32 s) public whenNotPaused {        require(phase == PHASE.DECONSTRUCT, \"wrong phase\");        bytes32 hash = keccak256(encoded);        address signer = ecrecover(hash, v, r, s);        require (ticket_signer == signer, \"wrong signer\");        (uint256 signedId, uint256[] memory signedTraitIds, string memory tokenURIPath, uint256 version)  = abi.<mask0>;        require(ownerOf(signedId)==msg.sender, \"not owner\");        require(dolls[signedId].status == DOLL_STATUS.DECONSTRUCTED, \"not deconstructed\");        require(version==(dolls[signedId].version)+1, \"wrong version\");        dolls[signedId].status = DOLL_STATUS.RECONSTRUCTED;        dolls[signedId].version = dolls[signedId].version + 1;        dolls[signedId].reconstructedLink = tokenURIPath;        traits_contract.burnTraits(msg.sender, traitTokenIds, signedTraitIds);    }"