"function transactTokenApprove(        address wallet,        address token,        address spender,        uint    amount        )        internal    {        require(token != address(0), \"INVALID_TOKEN\");        bytes memory txData = abi.encodeWithSelector(            ERC20.approve.selector,            spender,            amount        );        bytes memory returnData = transactCall(wallet, token, 0, txData);        bool success = returnData.length == 0 ? true :  abi.<mask0>;        require(success, \"ERC20_APPROVE_FAILED\");    }"