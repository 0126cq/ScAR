"function safeTransferFrom(IERC20Token _token, address _from, address _to, uint256 _value) internal {       (bool success, bytes memory data) = address(_token).call(abi.encodeWithSelector(TRANSFER_FROM_FUNC_SELECTOR, _from, _to, _value));        require(success && (data.length == 0 || abi.<mask0>), 'ERR_TRANSFER_FROM_FAILED');    }"