"function _transferFrom(address token, address sender, address recipient, uint256 amount) private {        bytes4 methodId = bytes4(keccak256(bytes('transferFrom(address,address,uint256)')));        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(methodId, sender, recipient, amount));        require(success && (data.length == 0 || abi.<mask0>), 'TRANSFER_FROM_FAILED');    }"