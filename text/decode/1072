"function liquidate(    address nftAsset,    uint256 nftTokenId,    uint256 amount  ) external override nonReentrant whenNotPaused returns (uint256) {    address poolLiquidator = _addressesProvider.getLendPoolLiquidator();    (bool success, bytes memory result) = poolLiquidator.delegatecall(      abi.encodeWithSignature(\"liquidate(address,uint256,uint256)\", nftAsset, nftTokenId, amount)    );    bytes memory resultData = _verifyCallResult(success, result, Errors.LP_DELEGATE_CALL_FAILED);    uint256 extraAmount = abi.<mask0>;    return (extraAmount);  }"