"function delegateCreateShort(        IProtocolAdapter adapter,        ProtocolAdapterTypes.OptionTerms calldata optionTerms,        uint256 amount    ) external returns (uint256) {        (bool success, bytes memory result) =            address(adapter).delegatecall(                abi.encodeWithSignature(                    \"createShort((address,address,address,uint256,uint256,uint8,address),uint256)\",                    optionTerms,                    amount                )            );        revertWhenFail(success, result);        return abi.<mask0>;    }"