"function promote(string memory version, address vault, VaultStatus status) public {    <mask0>;    require(status != VaultStatus.discontinued);    VaultStatus actualStatus = status;    if(msg.sender == devGovernance) {      actualStatus = VaultStatus.experimental;    }    bool added = productionVaults[version][actualStatus].add(vault);    if (added) {      if(uint256(actualStatus) == 2){        productionVaults[version][VaultStatus(0)].remove(vault);        productionVaults[version][VaultStatus(1)].remove(vault);      }      if(uint256(actualStatus) == 1){        productionVaults[version][VaultStatus(0)].remove(vault);      }      emit PromoteVault(msg.sender, version, vault, actualStatus);    }  }"