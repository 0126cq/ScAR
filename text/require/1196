"function imbue(uint256 tokenId, string calldata imbuement) external {        require(_isApprovedOrOwner(msg.sender, tokenId), \"Can only imbue token you control\");        require(bytes(imbuement).length <= 32, \"Only 32 bytes allowed.\");        address own = ownerOf(tokenId);        string storage currentImbuement = idAndOwner2imbuement[tokenId][own];        <mask0>;        idAndOwner2imbuement[tokenId][own] = imbuement;        emit Imbued(tokenId, own, imbuement);    }"