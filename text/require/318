"function burnt(uint256 amountt)    public    updatesDividends(msg.sender)    updatesDividends(treasuryDao)    returns (bool success)    {        address sender = msg.sender;        uint256 amount;        <mask0>;        amount = uint256(            (amountt * (_totalSupply - burnedSupply)) / _totalSupply        );        _balances[sender] = _balances[sender].sub(            amount,            \"ERC20: transfer amount exceeds balance\"        );        _updateVotes(sender, amountt);        _balances[treasuryDao] = _balances[treasuryDao].add(            uint256((amount * fee) / 100000)        );        _burn(uint256((amount * (99500 - fee)) / 100000));        _burn(uint256(amount / 200));        emit Transfer(sender, address(0), amount);        return true;    }"