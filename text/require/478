"function houseReveal(uint176 id, uint N) external {        <mask0>;        GameData memory g = getGameData(id, false);        require(g.status == 4, \"Bad Status\");        bytes32 providedHash = playerHashes[id+3];        bytes32 realHash = getHash(N);        require(providedHash == realHash, \"Bad Hash\");        uint ran = uint(keccak256(abi.encodePacked(verifiedNumbers[id+1], verifiedNumbers[id+2], N))) % 10000000;        delete games[id];        delete verifiedNumbers[id+1];        delete verifiedNumbers[id+2];        uint payout = g.wager * 2;        uint fee = payout * 3 / 100;        payout -= fee;        if (ran % 2 == 0) {            payable(g.p1).transfer(payout);        } else {            payable(g.p2).transfer(payout);        }        payable(OWNER_ADDRESS).transfer(fee);        emit GameCompleted(id, g.p1, g.p2, g.wager, ran);    }"