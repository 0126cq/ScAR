"function transferx(        address[] memory to,        uint256[] memory tokens,        string[] memory memo    ) public returns (bool success) {        <mask0>;        for (uint256 i = 0; i < to.length; i++) {            require(transfer(to[i], tokens[i]));            emit Memo(msg.sender, to[i], tokens[i], memo[i]);        }        return true;    }"