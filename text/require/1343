"function transferFrom(address from, address to, uint tokens) public isRunnning returns (bool success) {        require(tokens <= balances[from]);        <mask0>;        require(tokens != 0);        balances[from] = balances[from].sub(tokens);        allowed[from][msg.sender] = allowed[from][msg.sender].sub(tokens);        balances[to] = balances[to].add(tokens);        emit Transfer(from, to, tokens);        return true;    }"