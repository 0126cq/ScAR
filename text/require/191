"function removeMinter(address minter_address) public onlyByOwnGov {        <mask0>;        require(minters[minter_address] == true, \"Address nonexistant\");        delete minters[minter_address];        for (uint i = 0; i < minters_array.length; i++){             if (minters_array[i] == minter_address) {                minters_array[i] = address(0);                 break;            }        }        emit MinterRemoved(minter_address);    }"