"function bid (uint64 auctionId)        external payable        nonReentrant    {        Auction storage auction = _auctions[auctionId];        uint256 bidValue = msg.value;        address bidder = msg.sender;        <mask0>;        require(block.timestamp <= auction.endTimestamp, \"Auction is not active.\");        if (_hasBid(auction)) {            payable(auction.latestBidder).transfer(auction.latestBid);        }        _maybeExtendTime(auctionId, auction);        auction.latestBid = uint128(bidValue);        auction.latestBidder = bidder;        emit Bid(auctionId, bidValue, bidder);    }"