"function safeDecreaseAllowance(        IERC20 token,        address spender,        uint256 value    ) internal {        unchecked {            uint256 oldAllowance = token.allowance(address(this), spender);            <mask0>;            uint256 newAllowance = oldAllowance - value;            _callOptionalReturn(token, abi.encodeWithSelector(token.approve.selector, spender, newAllowance));        }    }"