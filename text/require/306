"function newproposal(        uint256 fnd,        uint16 fam,        uint256 mint,        uint256 lockmn,        address burntarget,        uint256 burnam,        uint256 lockxp_    ) public {        require(lockxp_ >= 6 hours ||lockxp_ == 0);        require(lockmn >= 3 days || lockmn == 0);        <mask0>;        prop += 1;        uint256 proposal = prop;        proposals[proposal].proposer = msg.sender;        proposals[proposal].lock = now + tlock;        proposals[proposal].pfee = fam;        proposals[proposal].burnaddress = burntarget;        proposals[proposal].burnamount = burnam;        proposals[proposal].mintam = fnd;        proposals[proposal].inflate = mint;        proposals[proposal].lockmin = lockmn;        proposals[proposal].lockx = lockxp_;        emit Newproposal(proposal);    }"