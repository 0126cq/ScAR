"function reveal(uint176 id, uint N) external {        GameData memory g = getGameData(id, false);        require(g.status == 3, \"Bad Status\");        if (g.p1 == msg.sender) {            bytes32 providedHash = playerHashes[id+1];            bytes32 realHash = getHash(N);            <mask0>;            delete playerHashes[id+1];            verifiedNumbers[id+1] = N;            if (verifiedNumbers[id+2] != 0) {                games[id] = encodeGameData(4, g.p2);                emit GameReady(id);            }            return;        } else if (g.p2 == msg.sender) {            bytes32 providedHash = playerHashes[id+2];            bytes32 realHash = getHash(N);            require(providedHash == realHash, \"Bad Hash\");            delete playerHashes[id+2];            verifiedNumbers[id+2] = N;            if (verifiedNumbers[id+1] != 0) {                games[id] = encodeGameData(4, g.p2);                emit GameReady(id);            }            return;        }        require(false, \"Wrong Sender\");    }"