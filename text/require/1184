"function cancelOrder(uint id) external {        <mask0>;        require(orders[id].creator == msg.sender);        require(orders[id].amount > 0);        if (orders[id].buy) {            uint amountEther = calcAmountEther(orders[id].token, orders[id].price, orders[id].amount);            balanceAdd(0x0, msg.sender, amountEther);            emit BalanceChanged(0x0, msg.sender, balanceOf[0x0][msg.sender]);        } else {            balanceAdd(orders[id].token, msg.sender, orders[id].amount);            emit BalanceChanged(orders[id].token, msg.sender, balanceOf[orders[id].token][msg.sender]);        }        orders[id].amount = 0;        emit CancelOrder(id);    }"