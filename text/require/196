"function _transfer(        address from,        address to,        uint256 amount    ) internal virtual {        require(from != address(0), \"ERC20: transfer from the zero address\");        require(to != address(0), \"ERC20: transfer to the zero address\");        _beforeTokenTransfer(from, to, amount);        uint256 fromBalance = _balances[from];        <mask0>;        unchecked {            _balances[from] = fromBalance - amount;        }        _balances[to] += amount;        emit Transfer(from, to, amount);        _afterTokenTransfer(from, to, amount);    }"