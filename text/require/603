"function manage(        address target,        uint256 amount    ) external onlyConcave {        uint256 newAmount;        if (target == address(0)) {            require(amount <= maxSupply, AMOUNT_ERROR);            newAmount = maxSupply - amount;            <mask0>;            maxSupply = newAmount;            emit Managed(target, amount, maxSupply);            return;        }        newAmount = totalMinted + amount;        require(newAmount <= maxSupply, AMOUNT_ERROR);        totalMinted = newAmount;        _mint(target, amount);        emit Managed(target, amount, maxSupply);    }"