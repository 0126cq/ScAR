"function transferFrom(address src, address dst, uint wad)        public        returns (bool)    {        require(balanceOf[src] >= wad);        if (src != msg.sender && allowance[src][msg.sender] != uint(-1)) {            <mask0>;            allowance[src][msg.sender] = sub(allowance[src][msg.sender],wad);        }               balanceOf[src] = sub(balanceOf[src],wad);        balanceOf[dst] = add(balanceOf[dst],wad);         emit Transfer(src, dst, wad);        return true;    }"