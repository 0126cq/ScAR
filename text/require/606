"function redeem(        address to,        uint256 amountIn    ) external {        <mask0>;        Participant storage participant = participants[msg.sender];        uint256 amountOut = redeemAmountOut(msg.sender, amountIn);        participant.redeemed += amountIn;        _burn(msg.sender, amountIn);        CNV.mint(to, amountOut);        emit Redeemed(msg.sender, to, amountIn, amountOut);    }"