"function withdrawEther(uint amount) external {        <mask0>;        balanceSub(0x0, msg.sender, amount);        msg.sender.transfer(amount);        emit Withdraw(0x0, msg.sender, amount);        emit BalanceChanged(0x0, msg.sender, balanceOf[0x0][msg.sender]);        emit FundTransfer(msg.sender, amount, false);    }"