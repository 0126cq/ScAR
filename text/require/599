"function deposit(uint256 _alcxAmount) external returns (uint256 shares) {        <mask0>;        if (totalSupply() == 0) {            shares = _alcxAmount;        } else {            shares = _alcxAmount * totalSupply() / _totalDiscountedAssets();        }        _mint(msg.sender, shares);        alcx.safeTransferFrom(msg.sender, address(this), _alcxAmount);        alcxStakingPool.deposit(POOL_ID, _alcxAmount);     }"