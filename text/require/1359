"function purge(address vault) public {    <mask0>;    VaultInfo memory existedVaultInfo = productionVaultInfoByVault[vault];    require(existedVaultInfo.vault != address(0), \"BadgerRegistry: Vault does not exist\");    bool removedFromVersionStatusSet = productionVaults[existedVaultInfo.version][existedVaultInfo.status].remove(      vault    );    bool deletedFromVaultInfoByVault = productionVaultInfoByVault[vault].vault != address(0);    if (removedFromVersionStatusSet || deletedFromVaultInfoByVault) {      delete productionVaultInfoByVault[vault];      emit PurgeVault(msg.sender, existedVaultInfo.version, existedVaultInfo.metadata, vault, existedVaultInfo.status);    }  }"