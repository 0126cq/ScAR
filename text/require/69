"function _safeTransferFrom(        address from,        address to,        uint256 id,        uint256 amount,        bytes memory data    ) internal virtual {        require(to != address(0), \"ERC1155: transfer to the zero address\");        address operator = _msgSender();        _beforeTokenTransfer(operator, from, to, _asSingletonArray(id), _asSingletonArray(amount), data);        uint256 fromBalance = _balances[id][from];        <mask0>;        unchecked {            _balances[id][from] = fromBalance - amount;        }        _balances[id][to] += amount;        emit TransferSingle(operator, from, to, id, amount);        _doSafeTransferAcceptanceCheck(operator, from, to, id, amount, data);    }"