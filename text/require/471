"function joinGame(uint176 id, bytes32 hash) payable external {        GameData memory g = getGameData(id, false);        require(g.status == 1, \"Bad Status\");        <mask0>;        games[id] = encodeGameData(2, msg.sender);        playerHashes[id+2] = hash;        emit GameJoined(id, g.p1, msg.sender, g.wager);    }"