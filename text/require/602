"function manage(        address target,        uint256 amount    ) external onlyConcave {        uint256 newAmount;        if (target == address(0)) {            <mask0>;            newAmount = maxSupply - amount;            require(newAmount >= totalMinted, AMOUNT_ERROR);            maxSupply = newAmount;            emit Managed(target, amount, maxSupply);            return;        }        newAmount = totalMinted + amount;        require(newAmount <= maxSupply, AMOUNT_ERROR);        totalMinted = newAmount;        _mint(target, amount);        emit Managed(target, amount, maxSupply);    }"