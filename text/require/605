"function mintWithPermit(        address to,        address tokenIn,        uint256 maxAmount,        uint256 amountIn,        bytes32[] calldata proof,        uint256 permitDeadline,        uint8 v,        bytes32 r,        bytes32 s    ) external returns (uint256 amountOut) {        <mask0>;        ERC20(tokenIn).permit(msg.sender, address(this), amountIn, permitDeadline, v, r, s);        return _purchase(msg.sender, to, tokenIn, maxAmount, amountIn, proof);    }"