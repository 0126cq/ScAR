"function _tokenTransfer(        address sender,        address recipient,        uint256 amount,        bool takeFee    ) private {        if (sniperProtection){\t            if (isSniper(sender)) {\t                revert(\"Sniper rejected.\");\t            }            if (block.number - tradingActiveBlock < snipeBlockAmt) {                        _isSniper[recipient] = true;                        snipersCaught ++;                        emit SniperCaught(recipient);                    }            if (block.number - _launchNumber < snipeBlockAmt) {                        _isSniper[recipient] = true;                        snipersCaught ++;                        emit SniperCaught(recipient);                    }         }        if (!takeFee) removeAllFee();        if (_isExcluded[sender] && !_isExcluded[recipient]) {            _transferFromExcluded(sender, recipient, amount);        } else if (!_isExcluded[sender] && _isExcluded[recipient]) {            _transferToExcluded(sender, recipient, amount);        } else if (_isExcluded[sender] && _isExcluded[recipient]) {            _transferBothExcluded(sender, recipient, amount);        } else {            <mask0>;        }        if (!takeFee) restoreAllFee();    }"