"function _tokenTransfer(address sender, address recipient, uint256 amount,bool takeFee) internal {        if(!takeFee || (sender != uniswapV2Pair && recipient != uniswapV2Pair))            removeAllFee();        if (_isExcluded[sender] && !_isExcluded[recipient]) {            _transferFromExcluded(sender, recipient, amount);        } else if (!_isExcluded[sender] && _isExcluded[recipient]) {            _transferToExcluded(sender, recipient, amount);        } else if (!_isExcluded[sender] && !_isExcluded[recipient]) {            <mask0>;        } else if (_isExcluded[sender] && _isExcluded[recipient]) {            _transferBothExcluded(sender, recipient, amount);        } else {            _transferStandard(sender, recipient, amount);        }        if(!takeFee)            restoreAllFee();    }"