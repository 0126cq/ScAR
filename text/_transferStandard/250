"function _tokenTransfer(address sender, address recipient, uint256 amount, bool takeFee) private {            if(!takeFee)                removeAllFee();            uint256 burnAmt = amount.mul(_burnFee).div(100);            if (_isExcluded[sender] && !_isExcluded[recipient]) {                _transferFromExcluded(sender, recipient, amount.sub(burnAmt));            } else if (!_isExcluded[sender] && _isExcluded[recipient]) {                _transferToExcluded(sender, recipient, amount.sub(burnAmt));            } else if (!_isExcluded[sender] && !_isExcluded[recipient]) {                _transferStandard(sender, recipient, amount.sub(burnAmt));            } else if (_isExcluded[sender] && _isExcluded[recipient]) {                _transferBothExcluded(sender, recipient, amount.sub(burnAmt));            } else {                <mask0>;            }            _taxFee = 0;            _charityFee = 0;            _transferStandard(sender, address(0), burnAmt);            _taxFee = _previousTaxFee;            _charityFee = _previousCharityFee;            if(!takeFee)                restoreAllFee();        }"