"function mint(address to, uint256 amount) external override onlyRole(MINTER_ROLE) {        require(<mask0> < MAX_SUPPLY, \"YuGiOhCard: all have been minted\");        require(totalSupply() + amount <= MAX_SUPPLY, \"YuGiOhCard: exceeds MAX_SUPPLY\");        for (uint i = 0; i < amount; i++) {            uint256 tokenId = _tokenIdCounter.current();            _tokenIdCounter.increment();            _safeMint(to, tokenId);        }    }"