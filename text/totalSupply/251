"function advance() external override {        require(            block.timestamp >= _previousEpochTimestamp + getEpochDuration(),            \"not ready to advance\"        );        _epoch++;        _previousEpochTimestamp = block.timestamp;        ERC20Snapshot._snapshot();        uint256 supplyMinted = (ERC20.<mask0> * getInflationBps()) / 10000;        ERC20._mint(getRecipient(), supplyMinted);        emit Advanced(_epoch, supplyMinted);    }"