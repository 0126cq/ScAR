"function mintComics(uint256 amount) external payable whenSaleActive {      require(<mask0> + amount <= MAX_COMICS, \"TheLostGlitchesComic: Purchase would exceed cap\");      require(amount <= maxPerMint, \"TheLostGlitchesComic: Amount exceeds max per mint\");      if (tlg.balanceOf(msg.sender) > 0) {        _mintWithDiscount(amount);        return;      }      _mintRegular(amount);    }"