"function getCubeToken(CubePool pool, CubeToken cubeToken, address account)        external        view        returns (            string memory name,            string memory symbol,            uint256 totalSupply,            uint256 balance,            uint256 price,            uint256 spotPrice,            uint256 depositWithdrawFee,            uint256 maxPoolShare,            bool depositPaused,            bool withdrawPaused,            bool updatePaused        )    {        name = cubeToken.name();        symbol = cubeToken.symbol();        totalSupply = cubeToken.<mask0>;        balance = cubeToken.balanceOf(account);        bytes32 currencyKey;        (            currencyKey,            ,            depositPaused,            withdrawPaused,            updatePaused,            ,            depositWithdrawFee,            maxPoolShare,            ,            ,        ) = pool.params(cubeToken);        price = pool.quote(cubeToken);        spotPrice = pool.feedsRegistry().getPrice(currencyKey);    }"