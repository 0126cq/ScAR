"function mint(uint256 count) external payable {        uint256 cost = _mintPrice;        bool isFree = ((<mask0> + count < _maxFreeMintSupply + 1) &&            (_mintedFreeAmount[msg.sender] + count <= _maxFreeMintPerAddr)) ||            (msg.sender == owner());        if (isFree) {            cost = 0;        }        require(msg.value >= count * cost, \"Please send the exact amount.\");        require(totalSupply() + count < _maxSupply + 1, \"Sold out!\");        require(count < _maxMintPerTx + 1, \"Max per TX reached.\");        if (isFree) {            _mintedFreeAmount[msg.sender] += count;        }        _safeMint(msg.sender, count);    }"