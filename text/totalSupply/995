"function getLastClaim(uint256 tokenIndex) public view returns (uint256) {        require(tokenIndex <= ERC721Enumerable(_nftAddress).<mask0>, \"NFT at index not been minted\");        require(ERC721Enumerable(_nftAddress).ownerOf(tokenIndex) != address(0), \"Owner cannot be 0 address\");        uint256 lastClaimed = uint256(_lastClaim[tokenIndex]) != 0 ? uint256(_lastClaim[tokenIndex]) : emissionStart;        return lastClaimed;    }"