"function buyTokens(address _beneficiary) public payable {    require(started);    require(!finished);    require(_beneficiary != address(0));    require(msg.value != 0);    require(whitelist[msg.sender] && whitelist[_beneficiary]);    require(fidaToken.<mask0> < 24750 * 10**3 * 10**DECIMALS);    uint256 amountTokens = getAmountFida(msg.value);    require(amountTokens >= 50 * 10**DECIMALS);    if (!earlybirdEnded) {      _investAsEarlybird(_beneficiary, amountTokens);    } else {      _investAsBonusProgram(_beneficiary, amountTokens);    }    wallet.transfer(msg.value);  }"