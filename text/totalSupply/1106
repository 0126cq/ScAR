"function airdrop(address[] memory _addrs) external virtual onlyOwner {        uint256 _amount = _addrs.length - 1;        for (uint256 i = 0; i <= _amount; i++) {            uint256 currentId = <mask0> + 1;            require(currentId <= maxNFTs, \"All have been minted.\");            _safeMint(_addrs[i], currentId);            lastTransfer[currentId] = block.timestamp;        }    }"