"function mint(uint256 numberOfTokens) external payable {        require(activated, \"Inactive\");        require(<mask0> + numberOfTokens <= maxSupply, \"All minted\");        require(numberOfTokens <= maxPerTx, \"Too many for Tx\");        require(            _numberMinted(msg.sender) + numberOfTokens <= maxPerWallet,            \"Too many for address\"        );        _safeMint(msg.sender, numberOfTokens);    }"