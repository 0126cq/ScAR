"function distributeRewards() public {        require(stakingDerivative.<mask0> > 0, \"Cannot distribute when nothing is staked\");        uint256 toDistribute = rewardsToken.balanceOf(address(this)).sub(distributedRewards);        distributedRewards += toDistribute;        _updateRewardPerToken(toDistribute);        emit RewardDistributed(msg.sender, stakingDerivative.totalSupply(), toDistribute);    }"