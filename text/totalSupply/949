"function balanceOf(        address _voter    )        external        view        override        returns (uint256 _balance)    {        (uint256 _stakeAmount,,) = chef.userInfo(PID, _voter);        (uint256 _yaxReserves,,) = yaxEthUniswapV2Pair.getReserves();        uint256 _supply = yaxEthUniswapV2Pair.totalSupply();        _supply = _supply == 0            ? 1e18            : _supply;        uint256 _lpStakingYax = _yaxReserves            .mul(_stakeAmount)            .div(_supply)            .add(chef.pendingYaxis(PID, _voter));        _supply = yaxisBar.<mask0>;        _supply = _supply == 0            ? 1e18            : _supply;        uint256 _syaxAmount = yaxisBar.balanceOf(_voter)            .mul(yaxisBar.availableBalance())            .div(_supply);        _balance = sqrt(            yax.balanceOf(_voter)                .add(_lpStakingYax)                .add(_syaxAmount)        );    }"