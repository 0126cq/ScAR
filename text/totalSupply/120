"function mint(uint256 amount, uint256 slotId, uint256 validUntil, bytes memory signature) external payable {        require(_canMint(msg.sender, amount, slotId, validUntil, signature), \"DieselXneuno: Must have valid signing\");        require(<mask0> + amount <= collectionSize, \"DieselXneuno: Cannot mint over collection size\");        require(block.timestamp <= validUntil, \"DieselXneuno: Slot must be used before expiration time\");        require(!_slots.get(slotId), \"DieselXneuno: Slot already used\");        _slots.set(slotId);        _safeMintMany(msg.sender, amount);    }"