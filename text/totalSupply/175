"function recruitYourAgent(uint _count)    external    payable  {    if (itemContractAddress == address(0)) revert ItemsContractAddressNotSet();    if (itsRecruitingSeason == false) revert NotRecruitingSeason();    if (msg.sender != tx.origin) revert NoContracts();    if (_count > max_per_txn) revert WrongAmount();    if (<mask0> + _count > MAX_SUPPLY) revert RecruitingFull();    if (msg.value != (price * _count)) revert WrongPrice();    _mint(msg.sender, _count);    IInventory inventory = IInventory(itemContractAddress);    inventory.supplyPaidAgents(msg.sender, _count);  }"