"function whitelistMint(uint256 _mintAmount) public payable {    uint256 supply = <mask0>;    require(!paused, 'The contract is paused!');    require(isDropActive, \"The whitelist sale is not enabled!\");    require(!whitelistClaimed[_msgSender()], \"Address already claimed!\");    require(_mintAmount > 0, \"You need to mint at least 1 NFT\");    require(_mintAmount == 1, \"You can not claim more than one free mint\");    require(supply + _mintAmount <= maxSupply, \"Max NFT limit supply exceeded\");    require(isWhitelistedUser(_msgSender()), \"You are not eligable for the whitelist\");    whitelistClaimed[_msgSender()] = true;    _safeMint(_msgSender(), _mintAmount);  }"