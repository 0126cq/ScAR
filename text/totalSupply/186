"function gift(uint256[] calldata _tokenIds, address _to) public onlyOwner {    require(!paused, \"the contract is paused\");    require(_to != address(0), \"invalid address\");    for(uint i=0;i<_tokenIds.length;i++){      uint256 supply = <mask0>;      require(supply + 1 <= maxSupply, \"max NFT limit exceeded\");      require(minted[_tokenIds[i]] == false, \"NFT already minted\");      addressMintedBalance[_to]++;      minted[_tokenIds[i]] = true;      _safeMint(_to, _tokenIds[i]);    }  }"