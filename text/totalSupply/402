"function Mint(uint256 numTokens) public payable {        require(!paused, \"Paused\");        require(numTokens > 0 && numTokens <= MAX_MINTS);        require(<mask0> + numTokens <= MAX_SUPPLY);        require(MAX_MINTS >= numTokens, \"Excess max per paid tx\");        require(msg.value >= numTokens * NFT_PRICE, \"Invalid funds provided\");        _safeMint(msg.sender, numTokens);    }"