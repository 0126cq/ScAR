"function mint(uint8 numNFTs) external payable {        require(numNFTs > 0 && numNFTs <= MAX_MINTS_PER_ADDRESS,            MAX_MINTS_PER_ADDRESS_TEXT        );        address msgSender = _msgSender();        require(            numMinted[msgSender] + numNFTs <= MAX_MINTS_PER_ADDRESS,            MAX_MINTS_PER_ADDRESS_TEXT        );        uint count = <mask0>;        require(            count + numNFTs <= MAX_SUPPLY,            \"<numNFTs> NFTs not available\"        );        require(            msg.value == MINT_PRICE * numNFTs,            \"Price per NFT: 0.042 ETH\"        );        numMinted[msgSender] += numNFTs;        _mint(msgSender, count+1);        if (numNFTs == 2) {            _mint(msgSender, count+2);        }    }"