"function preSalemint() public payable{     require(isPreSalePaused == false, \"Sale is not active at the moment\");      require(<mask0><maxSupply,\"Quantity is greater than remaining Supply\");        require(price== msg.value, \"Sent ether value is incorrect\");        require(_addressExist[msg.sender]==true,\"Address not Found in whitelist\");        SpecificAddresses storage myaddress = _whiteList[msg.sender];        require(myaddress.counter==1,\"The Amount Selected is Greater than the Remaining Amount of the Token at this Address\");            _safeMint(msg.sender, totalsupply());            tokenId++;        myaddress.counter-=1;"