"function updateState(uint256 _assetID, uint256 newBalance) external override onlyPermit {    uint256 total = <mask0>;    if (newBalance > 0 && total > 0 && newBalance > total) {      uint256 diff = newBalance.sub(total);      uint256 amountToIndexRatio = (diff.wadToRay()).rayDiv(total.wadToRay());      uint256 result = amountToIndexRatio.add(WadRayMath.ray());      result = result.rayMul(indexes[_assetID]);      require(result <= type(uint128).max, Errors.VL_INDEX_OVERFLOW);      indexes[_assetID] = uint128(result);    }  }"