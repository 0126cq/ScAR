"function publicMint(uint256 quantity) external payable callerIsUser {        uint256 _salePrice = saleConfig.publicSalePrice;        uint256 _totalCost = _salePrice * quantity;        require(isPublicSaleOn(), \"Public sale is not active\");        require(msg.value == _totalCost, \"Value cannot be lower than total cost\");        require(totalSupply() < MaxSupply, \"Max supply reached, collection sold out\");        require(<mask0> + quantity <= MaxSupply, \"Cannot mint this many\");        require(publicMinted[msg.sender] + quantity <= publicSaleMax, \"Reached maximum mint amount\");        publicMinted[msg.sender] += quantity;        _mintRandom(msg.sender, quantity);    }"