"function mint(uint256 amt) external payable  {    uint cost = price;    if(<mask0> + amt < totalFree + 1) {      cost = 0;    }    require(msg.sender == tx.origin,\"Be yourself, honey.\");    require(msg.value == amt * cost,\"Please send the exact amount.\");    require(totalSupply() + amt < maxSupply + 1,\"No more bears\");    require(mintEnabled, \"Minting is not live yet, hold on bear.\");    require(numberMinted(msg.sender) + amt <= maxPerWallet,\"Too many per wallet!\");    require( amt < maxPerTx + 1, \"Max per TX reached.\");    _safeMint(msg.sender, amt);  }"