"function airdrop(address[] calldata _addresses, uint256 airdropPerWallet) external onlyOwner {        require(            <mask0> + _addresses.length <= MAX_SUPPLY,            \"Airdrop will exceed maximum supply\"        );        for (uint256 i = 0; i < _addresses.length; i++) {            require(_addresses[i] != address(0), \"Airdrop to Null address\");            _safeMint(_addresses[i], airdropPerWallet);        }    }"