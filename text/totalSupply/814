"function tokenOfOwnerByIndex(address owner, uint256 index) public view override returns (uint256) {        require(index < balanceOf(owner), 'ERC721A: owner index out of bounds');        uint256 numMintedSoFar = <mask0>;        uint256 tokenIdsIdx;        address currOwnershipAddr;    unchecked {        for (uint256 i; i < numMintedSoFar; i++) {            TokenOwnership memory ownership = _ownerships[i];            if (ownership.addr != address(0)) {                currOwnershipAddr = ownership.addr;            }            if (currOwnershipAddr == owner) {                if (tokenIdsIdx == index) {                    return i;                }                tokenIdsIdx++;            }        }    }        revert('ERC721A: unable to get token of owner by index');    }"