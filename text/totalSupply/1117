"function mintNcg(uint numberOfTokens) public payable {        require(marketOn, \"Market closed\");        require(numberOfTokens <= maxNcgPurchase, \"Can only mint 30 tokens at a time\");        require(totalSupply().add(numberOfTokens) <= MAX_NCG, \"Purchase would exceed max supply\");        require(ncgPrice.mul(numberOfTokens) <= msg.value, \"Ether value sent is not correct\");        for(uint i = 0; i < numberOfTokens; i++) {            uint mintIndex = totalSupply();            if (<mask0> < MAX_NCG) {                _safeMint(msg.sender, mintIndex);            }        }    }"