"function Airdrop(uint8 _amountPerAddress, address[] calldata addresses) external onlyOwner {     uint16 totalSupply = uint16(<mask0>);     uint totalAmount =   _amountPerAddress * addresses.length;    require(totalSupply + totalAmount <= maxSupply, \"Excedes max supply.\");     for (uint256 i = 0; i < addresses.length; i++) {            _safeMint(addresses[i], _amountPerAddress);        }     delete _amountPerAddress;     delete totalSupply;  }"