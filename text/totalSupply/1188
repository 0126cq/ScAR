"function getAmount(uint mintCount) external view returns(uint){        require(<mask0> + mintCount <= mintTotal, 'Maximum mint quantity exceeded');        address minter = _msgSender();        uint balanceMiner = balanceOf(minter);        uint balanceWill = balanceMiner + mintCount;        require(0 < mintCount && balanceWill <= mintMax, 'Invalid quantity');        if(balanceWill > mintFee){            if(balanceMiner < mintFee){                uint feeCount = mintFee - balanceMiner;                return feeCount * mintEveryAmount;            }else{                return 0;            }        }else{            return mintCount * mintEveryAmount;        }    }"