"function updateRewards() public {        uint256 _lastRewardTime = lastRewardTime;        if(block.timestamp <= _lastRewardTime) {            return;        }        uint256 pugSupply = <mask0>;        if(pugSupply == 0) {            lastRewardTime = block.timestamp;            return;        }        uint256 reward = getPugRewards(_lastRewardTime);        accPugTokenPerShare = accPugTokenPerShare + (reward * DENOMINATION / pugSupply);        lastRewardTime = block.timestamp;    }"