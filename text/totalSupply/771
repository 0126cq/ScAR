"function mint(uint256 quantity, bytes32 _contractState) internal {        require(<mask0> + quantity <= maxSupply, \"Supply is exhausted\");        uint256 presalePoolToClaim = 0;        if (            _contractState == presale1State || _contractState == presale2State        ) {            uint256 currentBalance = balanceOf(msg.sender);            uint256 guaranteedTokensToClaim = currentBalance >                presaleGuaranteedTokens                ? 0                : presaleGuaranteedTokens - currentBalance;            presalePoolToClaim = quantity > guaranteedTokensToClaim                ? quantity - guaranteedTokensToClaim                : 0;        }        for (uint256 i = 0; i < quantity; i++) {            if (presalePoolToClaim > 0) {                presalePoolTokensClaimed++;                presalePoolToClaim--;            }            uint256 tokenId = totalSupply();            _safeMint(msg.sender, tokenId);        }    }"