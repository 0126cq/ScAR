"function mintToken(uint numberOfTokens) public payable {    require(saleIsActive, \"Sale must be active to mint\");    require(numberOfTokens < MAX_TOKEN_PER_TXN, \"The max mint per transaction is 5\");    require(totalSupply().add(numberOfTokens) < MAX_TOKENS, \"Purchase would exceed max supply\");    require(msg.value == tokenPrice.mul(numberOfTokens), \"Ether value sent is not correct\");    for(uint i; i < numberOfTokens; i++) {      _safeMint(_msgSender(), <mask0>);    }  }"