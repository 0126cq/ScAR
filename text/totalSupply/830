"function rebaseWhenTotalSupplyChange() public {        uint256 newTotalSupply_ = ShardedWallet(payable(wallet)).<mask0>;        require (newTotalSupply_ != recordedTotalSupply);        curve.k = curve.k * newTotalSupply_ / recordedTotalSupply * newTotalSupply_ / recordedTotalSupply;         curve.x = curve.x * newTotalSupply_ / recordedTotalSupply;         recordedTotalSupply = newTotalSupply_;        assert(curve.k > 0);        assert(curve.x > 0);        assert(recordedTotalSupply > 0);    }"