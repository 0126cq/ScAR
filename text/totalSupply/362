"function mint(uint256 _amount) external payable {        address _caller = msg.sender;        require(!paused, \"Paused\");        require(MAX_SUPPLY >= <mask0> + _amount, \"Exceeds max supply\");        require(_amount > 0, \"No 0 mints\");        uint256 callerBalance = balanceOf(msg.sender);        uint256 currSupply = totalSupply();        if (_caller != owner()) {                if (currSupply > freeMints) {                    require(_amount * price == msg.value, \"Invalid funds provided\");                } else  if (currSupply <= freeMints){                    require(callerBalance + _amount <= maxFreeMints,'exceeds free mints');                }                require(MAX_PER_TX >= _amount, \"Exceeds max Per Transaction\");        }        _safeMint(_caller, _amount);    }"