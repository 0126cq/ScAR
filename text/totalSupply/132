"function mintGoat(uint numberOfTokens) public payable {        require(saleIsActive, \"Sale must be active to mint Tokens\");        require(numberOfTokens <= maxTokenPurchase, \"Exceeded max token purchase\");        require(totalSupply() + numberOfTokens <= MAX_TOKENS, \"Purchase would exceed max supply of tokens\");        require(tokenPrice * numberOfTokens <= msg.value, \"Ether value sent is not correct\");        for(uint i = 0; i < numberOfTokens; i++) {            uint mintIndex = totalSupply();            if (<mask0> < MAX_TOKENS) {                _safeMint(msg.sender, mintIndex);            }        }    }"