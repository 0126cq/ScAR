"function mint(uint256 tokens) public payable {    require(!paused, \"Alienmfers: contract is paused\");    uint256 supply = <mask0>;    require(tokens > 0, \"Alienmfers: need to mint at least 1 NFT\");    require(tokens <= maxsize, \"Alienmfers: max mint amount per tx exceeded\");    require(supply + tokens <= maxSupply, \"Alienmfers: We are sold out\");    if(supply<500){    require(msg.value >= lowercost * tokens, \"Alienmfers: insufficient funds\");    }    else{      require(msg.value >= cost * tokens, \"Alienmfers: insufficient funds\");    }      _safeMint(_msgSender(), tokens);  }"