"function canMint(uint256 _optionId) override public view returns (bool) {        if (_optionId >= NUM_OPTIONS) {            return false;        }        UselessNFT uselessNFTinstance = UselessNFT(nftAddress);        uint256 uselessNFTsSupply = uselessNFTinstance.<mask0>;        uint256 numItemsAllocated = 0;        if (_optionId == SINGLE_NFT_OPTION) {            numItemsAllocated = 1;        } else if (_optionId == TWO_NFT_OPTION) {            numItemsAllocated = 2;        } else if (_optionId == FIVE_NFT_OPTION) {            numItemsAllocated = 5;        } else if (_optionId == TEN_NFT_OPTION) {            numItemsAllocated = 10;        } else if (_optionId == TWENTY_NFT_OPTION) {            numItemsAllocated = 20;        } else if (_optionId == HUNDRED_NFT_OPTION) {            numItemsAllocated = 100;        }         return uselessNFTsSupply < (USELESSNFTS_SUPPLY - numItemsAllocated);    }"