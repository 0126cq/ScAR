"function accountVaultBalance(address account)        external        view        returns (uint256)    {        uint256 _decimals = vaultParams.decimals;        uint256 assetPerShare =            ShareMath.pricePerShare(                <mask0>,                totalBalance(),                vaultState.totalPending,                _decimals            );        return            ShareMath.sharesToAsset(shares(account), assetPerShare, _decimals);    }"