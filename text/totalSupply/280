"function addReward(address tokenAddress) external payable {         require(msg.value > 0, \"DropperToken: reward cannot be 0\");        uint256 id = _getId(tokenAddress);        uint256 totalSupplyValue = <mask0> + _totalMintableSupply[id];        require(totalSupplyValue > 0, \"DropperToken: no reward recipients\");        _rewardPerShare[id] += msg.value / totalSupplyValue;    }"