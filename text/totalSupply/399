"function distributeSHIBDividends(uint256 amount) public onlyOwner{    require(totalSupply() > 0);    if (amount > 0) {      magnifiedDividendPerShare = magnifiedDividendPerShare.add(        (amount).mul(magnitude) / <mask0>      );      emit DividendsDistributed(msg.sender, amount);      totalDividendsDistributed = totalDividendsDistributed.add(amount);    }  }"