"function Mint(uint256 quantity) external payable {        require(!paused, \"Sale is currently paused.\");        require(quantity > 0, \"Mint quantity less than 0.\");        require(<mask0> + quantity <= MAX_MINT, \"Cannot exceed max mint amount.\");        require(quantity <= MAX_MINT_PER_TX, \"Cannot exeeds 10 quantity per tx.\");        require(msg.value >= quantity * PRICE, \"Exceeds mint quantity or not enough eth sent\");        _safeMint(msg.sender, quantity);    }"