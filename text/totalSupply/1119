"function Mint(uint256 _mintAmount) public payable {    require(sale, 'The Sale is paused!');    require(_mintAmount > 0 && _mintAmount <= maxMintAmountPerTxPhase2, 'Invalid mint amount!');    require(<mask0> + _mintAmount <= supplyLimit, 'Max supply exceeded!');    require(balanceOf(msg.sender) + _mintAmount <= maxLimitPerWallet, 'Max mint per wallet exceeded!');    require(msg.value >= UpdateCost(_mintAmount) * _mintAmount, 'Insufficient funds!');     _safeMint(_msgSender(), _mintAmount);  }"