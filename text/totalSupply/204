"function mintComicsPresale(uint256 amount) external payable whenPresaleActive {      require(tlg.balanceOf(msg.sender) > 0, \"TheLostGlitchesComic: The presale is only for Glitch Owners.\");      require(<mask0> + amount <= MAX_COMICS, \"TheLostGlitchesComic: Purchase would exceed cap\");      require(amount <= maxPerMint, \"TheLostGlitchesComic: Amount exceeds max per mint\");      _mintWithDiscount(amount);    }"