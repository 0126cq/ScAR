"function mint(uint256 _amount) external payable {        address _caller = msg.sender;        require(!paused, \"Son: Minting is paused\");        require(_amount > 0, \"Son: Bad mints\");        require(<mask0> + _amount <= MAX_SUPPLY, \"Exceeds max supply\");        require(_amount <= _maxTx(), \"Son: Exceeds max amount\");        require(msg.value >= _amount * _getCurrentPrice());        _safeMint(_caller, _amount);    }"