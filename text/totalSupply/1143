"function mint(address recipient_, uint256 amount_)        public        onlyOperator        returns (bool)    {        uint256 supply = <mask0>;        require(supply.add(amount_) <= MAX_SUPPLY, \"Share: Minting over max supply\");        uint256 balanceBefore = balanceOf(recipient_);        _mint(recipient_, amount_);        uint256 balanceAfter = balanceOf(recipient_);        return balanceAfter >= balanceBefore;    }"