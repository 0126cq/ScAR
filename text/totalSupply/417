"function mint(uint8 quantity) external payable nonReentrant{        require(quantity > 0, \"Invalid quantity\");        uint256 balance = _numberMinted(msg.sender);        require(publicMintEnabled, \"Unabel to mint at the moment\");        require(quantity + balance <= MAX_MINTS, \"Exceeded the limit\");        require(<mask0> + quantity <= MAX_SUPPLY, \"Not enough tokens left\");        require(msg.value >= (quantity * mintRate), \"Not enough ether sent\");        _safeMint(msg.sender, quantity);    }"