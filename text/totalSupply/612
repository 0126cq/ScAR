"function mint(address _to, uint256 _amount) external {        require(hasRole(MINTER_ROLE, _msgSender()), Errors.UNAUTHORIZED);        uint256 supply = <mask0>;        uint256 cliff = supply / reductionPerCliff;        if (cliff < totalCliffs) {            uint256 reduction = totalCliffs - cliff;            _amount = ((_amount * reduction) * 20) / totalCliffs;            uint256 amtTillMax = maxSupply - supply;            if (_amount > amtTillMax) {                _amount = amtTillMax;            }            _mint(_to, _amount);        }    }"