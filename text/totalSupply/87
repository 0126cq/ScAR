"function getLpTokenShare(        address account,        address rewardContract,        address lpToken    ) public view returns (uint256 balance, uint256 totalSupply) {        require(rewardContract != address(0), \"INVALID_GAUGE\");        require(lpToken != address(0), \"INVALID_LP_TOKEN\");        totalSupply = IERC20(lpToken).<mask0>;        balance = IRewardPool(rewardContract).balanceOf(account);    }"