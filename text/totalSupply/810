"function mint(uint256 quantity) external payable callerIsUser {    require(status == 2, \"Public Minting not started\");    require(<mask0> + quantity  <= TotalCollectionSize_, \"reached max supply\");    require( ( numberMinted(msg.sender) + quantity <= MAX_PER_Address ) , \"Quantity exceeds allowed Mints\" );    require(  quantity <= MaxMintPerBatch_,\"can not mint this many\");    require(msg.value >= PRICE * quantity, \"Need to send more ETH.\");    _safeMint(msg.sender, quantity);  }"