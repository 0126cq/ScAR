"function mintForCommunity(address _to, uint256 _numberOfTokens) external onlyOwner {        require(_to != address(0), \"TheLostGlitches: Cannot mint to zero address.\");        require(<mask0> + _numberOfTokens <= MAX_GLITCHES, \"TheLostGlitches: Minting would exceed cap\");        for(uint256 i = 0; i < _numberOfTokens; i++) {            uint256 mintIndex = totalSupply() + 1;            if (totalSupply() < MAX_GLITCHES) {                _safeMint(_to, mintIndex);            }        }    }"