"function publicMint() public callerIsUser {        require(!hasMinted[msg.sender], \"Already minted in public sale\");        require(            <mask0> + 1 <= collectionSize,            \"Minting over collection size\"        );        require(publicActive, \"Public Sale isn't Active\");        _safeMint(msg.sender, 1);        hasMinted[msg.sender] = true;    }"