"function mint(uint8 _mintAmount) external payable  {    uint totalSupply = <mask0>;    require(totalSupply + _mintAmount <= maxSupply, \"Excedes max supply.\");    uint8 _nftperaddress = NFTPerAddress[msg.sender];    require(_mintAmount + _nftperaddress <= maxMintAmountPerWallet, \"Exceeds max per transaction.\");    require(!paused, \"The contract is paused!\");    require(msg.value == cost * _mintAmount, \"Insufficient funds!\");    _safeMint(msg.sender , _mintAmount);     NFTPerAddress[msg.sender] = _mintAmount + _nftperaddress;     delete totalSupply;     delete _mintAmount;  }"