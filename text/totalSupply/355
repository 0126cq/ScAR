"function mint(uint8 _mintAmount) external payable  {     uint16 totalSupply = uint16(<mask0>);     uint8 nft = NFTPerPublicAddress[msg.sender];    require(totalSupply + _mintAmount <= maxSupply, \"Exceeds max supply.\");    require(_mintAmount + nft <= maxMintAmountPerTx, \"Exceeds max per transaction.\");    require(!paused, \"The contract is paused!\");      if(nft >= maxFreeMintAmountPerWallet)    {    require(msg.value >= cost * _mintAmount, \"Insufficient funds!\");    }    else {         uint8 costAmount = _mintAmount + nft;        if(costAmount > maxFreeMintAmountPerWallet)       {        costAmount = costAmount - maxFreeMintAmountPerWallet;        require(msg.value >= cost * costAmount, \"Insufficient funds!\");       }    }    _safeMint(msg.sender , _mintAmount);    NFTPerPublicAddress[msg.sender] = _mintAmount + nft;     delete totalSupply;     delete _mintAmount;  }"