"function finishMinting() public onlyOwner saleFinished overSoftcap {        require(status == 1 || (status != 2 && now < start + period * 1 days && balanceTotal < hardcap));        uint issuedTokenSupply = token.<mask0>;        uint restrictedTokens = issuedTokenSupply.mul(restricted).div(100 - restricted);        mintAndTransfer(owner, restrictedTokens);        token.finishMinting();        owner.transfer(this.balance);    }"