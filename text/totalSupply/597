"function deposit(uint256 _amount) external returns (uint){    uint _pool = balance();    uint _before = token.balanceOf(address(this));    token.safeTransferFrom(msg.sender, address(this), _amount);    uint _after = token.balanceOf(address(this));    _amount = _after.sub(_before);     uint _shares = 0;    if (<mask0> == 0) {      _shares = _amount;    } else {      _shares = (_amount.mul(totalSupply())).div(_pool);    }    _mint(msg.sender, _shares);  }"