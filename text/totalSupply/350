"function publicSaleMint() external payable nonReentrant {        require(!hasSaleEnded, \"Sale ended\");        require(isPublicSale, \"Public sale is not enabled\");        require(totalSupply() < MAX_SUPPLY, \"Max supply reached\");        require(!hasMinted[msg.sender], \"Max one mint per address\");        require(msg.value >= PUBLIC_SALE_MINT_COST, \"Not enough ETH\");        if (<mask0> + 1 == MAX_SUPPLY) {            isPresale = false;            isPublicSale = false;            hasSaleEnded = true;            emit SaleEnded();        }        hasMinted[msg.sender] = true;        if (msg.value > PUBLIC_SALE_MINT_COST) {            payable(msg.sender).transfer(msg.value - PUBLIC_SALE_MINT_COST);        }        _safeMint(msg.sender, totalSupply() + 1);        emit TokenMinted();    }"