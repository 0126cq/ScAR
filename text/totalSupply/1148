"function mintToken(uint numberOfTokens) public payable {        require(saleIsActive, \"Sale must be active\");        require(numberOfTokens <= maxTokensPerTx, \"Exceed max tokens per tx\");        require(numberOfTokens > 0, \"Must mint at least one\");        require(<mask0> + numberOfTokens <= MAX_TOKENS, \"Exceed max supply\");        if(whitelistMintRemains > 0 && numberMinted(msg.sender) <= 0){            if(numberOfTokens >= whitelistMintRemains){                numberOfTokens = whitelistMintRemains;            }            _safeMint(msg.sender, numberOfTokens);            whitelistMintRemains = whitelistMintRemains - numberOfTokens;        } else{            if(_numberMinted(msg.sender) > 0){                require(msg.value >= numberOfTokens * tokenPrice, \"Not enough ether\");            } else  if(numberOfTokens > maxTokensWithFeePerTx){                require(msg.value >= maxTokensWithFeePerTx * tokenPrice, \"Not enough ether\");            } else if(numberOfTokens <= maxTokensWithFeePerTx){                require(msg.value >= numberOfTokens * tokenPrice, \"Not enough ether\");            }            _safeMint(msg.sender, numberOfTokens);        }    }"