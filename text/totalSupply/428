"function mint(uint256 _count) public payable {        require(publicSaleOpen, \"public mint not open\");        require(mintPerAddress[msg.sender] + _count <= maxMintPerAddress, \"max mint per address\");        require(msg.value == _count * price, \"invalid price\");        require(totalSupply() + _count <= MAX_PIXELS, \"max pixels reached\");        mintPerAddress[msg.sender] += _count;        for (uint256 i = 1; i <= _count; i++) {            _safeMint(msg.sender, <mask0>+1);        }    }"