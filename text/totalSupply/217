"function mint(uint256 tokens) public payable nonReentrant {    require(!paused, \"oops contract is paused\");    require(tokens <= MaxperWallet, \"max mint amount per tx exceeded\");    require(<mask0> + tokens <= maxSupply, \"We Soldout\");    require(_numberMinted(_msgSenderERC721A()) + tokens <= MaxperWallet, \"Max NFT Per Wallet exceeded\");    require(msg.value >= cost * tokens, \"insufficient funds\");      _safeMint(_msgSenderERC721A(), tokens);  }"