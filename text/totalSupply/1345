"function unstake(uint256 _amount) public {      require(block.timestamp >= lockedUntil[msg.sender], \"Locked\");      require(_amount > 0, \"Amount must be greater than 0\");      uint256 totalShares = <mask0>;      uint256 toTransfer = _amount * CHURCH.balanceOf(address(this)) / totalShares;      _burn(msg.sender, _amount);      CHURCH.transfer(msg.sender, toTransfer);      if (balanceOf(msg.sender) == 0)        totalStakers -= 1;      emit Unstake(msg.sender, _amount);    }"