"function mint(uint256 amt) external payable {    require(mintEnabled, \"Minting is not live yet.\");    require(msg.sender == tx.origin,\"No bots, only true WAAAI!\");    require(<mask0> + amt < maxSupply + 1, \"Not enough WAAAI left.\");    require(amt < maxPerTx + 1, \"That's enough WAAAI this tx!\");    require(msg.value >= amt * price,\"Please send the exact amount.\");    _walletMints[_msgSender()] += amt;    _safeMint(msg.sender, amt);  }"