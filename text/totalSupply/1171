"function mint(uint256 tokensNumber) public payable {        require(saleStarted, \"Sale is not active\");        require(totalSupply() < MAX_TOKENS, \"Sale has already ended\");        require(tokensNumber > 0 && tokensNumber <= 20, \"Tokens amount must be between 1 and 20\");        require(totalSupply().add(tokensNumber) <= MAX_TOKENS, \"Exceeds MAX_TOKENS\");        require(msg.value == TOKEN_PRICE.mul(tokensNumber), \"Ether value sent is not correct\");        for (uint i = 0; i < tokensNumber; i++) {            uint mintIndex = <mask0>;            _safeMint(msg.sender, mintIndex);            creationDates[mintIndex] = block.number;        }    }"