"function mint(uint256 quantity) external payable nonReentrant {        if (!normalSale) revert NormalSaleNotActive();        if (quantity + _numberMinted(msg.sender) > maxMints) {            revert ExceededLimit();        }        if (<mask0> + quantity > maxSupply) {            revert NotEnoughTokensLeft();        }        if (mintRate * quantity != msg.value) {            revert WrongEther();        }        _safeMint(msg.sender, quantity);    }"