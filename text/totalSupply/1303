"function ownerMint(uint256 _amount) external onlyOwner {        require(            ownerMinted + _amount <= OWNER_CAP,            \"OwnerMint: exceeding quota\"        );        require(            totalSupply() + _amount <= MAX_SUPPLY,            \"OwnerMint: exceeding max supply\"        );        ownerMinted += _amount;        _mint(msg.sender, _amount);        emit Mint(msg.sender, <mask0> - _amount + 1, _amount, 0);    }"