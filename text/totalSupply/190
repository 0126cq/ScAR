"function airdrop(address receiver, uint256 amountOfArts)        external        onlyOwner {        uint256 _nextTokenId = <mask0>;        for (uint256 i = 0; i < amountOfArts; i++) {            _safeMint(receiver, _nextTokenId++);        }        yieldToken.updateRewardOnMint(receiver);        emit AirdropMint(receiver, amountOfArts);    }"