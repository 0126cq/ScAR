"function mintBear(uint256 botbTokenId) public {        require(saleIsActive, \"Sale must be active to mint a Bear\");        require(<mask0> < maxBears, \"Purchase would exceed max supply of Bears\");        require(botbTokenId < maxBears, \"Requested tokenId exceeds upper bound\");        require(botb.ownerOf(botbTokenId) == msg.sender, \"Must own the Bull for requested tokenId to claim a Bear\");        _safeMint(msg.sender, botbTokenId);        trySetStartingIndexBlock();    }"