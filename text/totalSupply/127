"function distributeDividends() public onlyOwner override payable {        require(<mask0> > 0);        if (msg.value > 0) {            magnifiedDividendPerShare = magnifiedDividendPerShare.add(                (msg.value).mul(magnitude) / totalSupply()            );            emit DividendsDistributed(msg.sender, msg.value);            totalDividendsDistributed = totalDividendsDistributed.add(msg.value);        }    }"