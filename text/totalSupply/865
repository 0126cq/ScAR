"function mintNFTDuringPresale(        uint256 _numOfTokens,        bytes32[] memory _proof    )         public         payable    {        require(isActive, 'Sale is not active');        require(isPresaleActive, 'Whitelist is not active');        require(verify(_proof, bytes32(uint256(uint160(msg.sender)))), \"Not whitelisted\");            require(<mask0> < MAX_NFT_PUBLIC, 'All public tokens have been minted');            require(_numOfTokens <= WHITELIST_MAX_MINT, 'Cannot purchase this many tokens');            require(totalSupply().add(_numOfTokens) <= MAX_NFT_PUBLIC, 'Purchase would exceed max public supply of NFTs');            require(whiteListClaimed[msg.sender].add(_numOfTokens) <= WHITELIST_MAX_MINT, 'Purchase exceeds max whitelisted');            require(NFTPrice.mul(_numOfTokens) == msg.value, \"Ether value sent is not correct\");            whiteListClaimed[msg.sender] += _numOfTokens;            _safeMint(msg.sender, _numOfTokens);    }"