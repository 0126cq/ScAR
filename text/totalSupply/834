"function mint(uint256 count) public payable nonReentrant {    require(saleIsActive, \"Sale not active\");    if (allowedMintCount(msg.sender) >= count) {      updateMintCount(msg.sender, count);    } else {      revert(\"Minting limit exceeded\");    }    require(<mask0> + count - 1 < MAX_SUPPLY, \"Max Supply Reached\");    require(count <= MAX_MULTIMINT, \"Maximum mint per txn is 10\");    require(      msg.value == PRICE * count, \"Incorrect ETH amount\"    );    for (uint256 i = 0; i < count; i++) {      _mint(msg.sender, totalSupply());      supplyCounter.increment();    }  }"