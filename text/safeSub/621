"function transfer(address _to, uint _value) {        uint codeLength;        bytes memory empty;        assembly {            codeLength := extcodesize(_to)        }        balances[msg.sender] = <mask0>;        balances[_to] = safeAdd(balances[_to], _value);        if(codeLength>0) {            ContractReceiver receiver = ContractReceiver(_to);            receiver.tokenFallback(msg.sender, _value, empty);        }        Transfer(msg.sender, _to, _value, empty);    }"