"function decreaseApproval(address _spender, uint _subtractedValue) public returns (bool) {            uint oldValue = allowances[msg.sender][_spender];            if (_subtractedValue > oldValue) {              allowances[msg.sender][_spender] = 0;            } else {              allowances[msg.sender][_spender] = <mask0>;            }            emit Approval(msg.sender, _spender, allowances[msg.sender][_spender]);            return true;    }"