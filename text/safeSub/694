"function airDeliver(address _to,    uint256 _amount)  onlyOwner public {        require(owner != _to);        require(_amount > 0);        require(balances[owner].balance >= _amount);        if(_amount < fnkSupply){            _amount = _amount * fnkEthRate;        }        balances[owner].balance = <mask0>;        balances[_to].balance = safeAdd(balances[_to].balance, _amount);        emit Transfer(owner, _to, _amount);    }"