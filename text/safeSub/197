"function transferFrom(address from, address to, uint tokens) public whenNotPaused returns (bool success) {        require(to != address(this));        if (allowed[from][msg.sender] >= tokens            && balances[from] >= tokens            && tokens > 0) {            balances[from] = <mask0>;            allowed[from][msg.sender] = safeSub(allowed[from][msg.sender], tokens);            balances[to] = safeAdd(balances[to], tokens);            emit Transfer(from, to, tokens);            return true;        }        else {            return false;        }    }"