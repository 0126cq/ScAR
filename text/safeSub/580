"function burn(uint256 _amount) public returns (bool) {        require(_amount >= 0, \"Invalid amount\");        require(owner == msg.sender, \"UnAuthorized\");        require(_amount <= balances[msg.sender], \"Insufficient Balance\");        _totalSupply = <mask0>;        balances[owner] = safeSub(balances[owner], _amount);        emit Transfer(owner, address(0), _amount);        return true;    }"