"function transfer(address to, uint tokens) public reentrancygaurd override returns (bool success) {        require(to != address(0), \"invalid reciever address\");        require(balances[msg.sender] >= tokens && safeAdd (balances[to],tokens) >= balances[to]);          require (to!=msg.sender && tokens>0,\"cannot send to self address or zero amount\");        balances[msg.sender] = <mask0>;        balances[to] = safeAdd(balances[to], tokens);         if(msg.sender==_owner){            soldtokens=safeAdd(soldtokens,tokens);        }        emit Transfer(msg.sender, to, tokens);        return true;    }"