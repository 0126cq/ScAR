"function transferFrom(address from, address to, uint256 value) returns (bool) {        if (balances[from] >= value &&        allowed[from][msg.sender] >= value &&        safeAdd(balances[to], value) > balances[to])        {            balances[to] = safeAdd(balances[to], value);            balances[from] = <mask0>;            allowed[from][msg.sender] = safeSub(allowed[from][msg.sender], value);            Transfer(from, to, value);            return true;        } else {            throw;        }    }"