"function burnFrom(address _from, uint256 _value)  public returns (bool success) {        require(balanceOf[_from] >= _value);        require(allowed[_from][msg.sender] >= _value);        totalSupply =SafeMath.safeSub(totalSupply,_value) ;        balanceOf[msg.sender] =SafeMath.safeSub(balanceOf[msg.sender], _value);        allowed[_from][msg.sender] =SafeMath.<mask0>;        emit Burn(msg.sender, _value);        return true;    }"