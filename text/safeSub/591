"function burn(uint256 _value) onlyOwner returns (bool success){        if (balanceOf[msg.sender] < _value) throw;            \t\tif (_value <= 0) throw;         balanceOf[msg.sender] = SafeMath.<mask0>;                              totalSupply = SafeMath.safeSub(totalSupply,_value);                                        Burn(msg.sender, _value);        return true;    }"