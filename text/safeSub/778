"function transfer(address _to, uint256 _value) public validAddress(_to) transfersAllowed returns (bool success){        require(balanceOf[msg.sender] >= _value && balanceOf[_to] + _value > balanceOf[_to]);         balanceOf[msg.sender] = <mask0>;        balanceOf[_to] = safeAdd(balanceOf[_to], _value);        Transfer(msg.sender, _to, _value);        return true;    }"