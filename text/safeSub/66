"function transferFrom(        address from_,        address to_,        uint256 amount_    ) public returns(bool success) {        if(balances[from_] >= amount_ && allowed[from_][msg.sender] >= amount_ && amount_ > 0) {            balances[to_] = safeAdd(balances[to_], amount_);            balances[from_] = <mask0>;            allowed[from_][msg.sender] = safeSub(allowed[from_][msg.sender], amount_);            emit Transfer(from_, to_, amount_);            return true;        } else {            return false;        }    }"