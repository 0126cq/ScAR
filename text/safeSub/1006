"function burn(uint256 _value) returns (bool success) {        if (balances[msg.sender] < _value) throw;            \t\tif (_value <= 0) throw;         balances[msg.sender] = Token.safeSub(balances[msg.sender], _value);                              totalSupply = Token.<mask0>;                                        Burn(msg.sender, _value);        return true;    }"