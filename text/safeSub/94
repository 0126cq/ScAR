"function IMDEXwithdrawToken(address token, uint256 amount) public returns (bool) {    require(<mask0> > inactivityReleasePeriod);    require(tokens[token][msg.sender] > amount);    tokens[token][msg.sender] = safeSub(tokens[token][msg.sender], amount);    if (token == address(0)) {      msg.sender.transfer(amount);    } else {      require(IMDEX(token).transfer(msg.sender, amount));    }    emit Withdraw(token, msg.sender, amount, tokens[token][msg.sender]);  }"