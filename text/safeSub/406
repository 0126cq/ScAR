"function _harvest(uint dropPrice) internal {        require((cdpink() > 0), \"no-profit-to-harvest\");        uint lockedCollateralDAI = rmul(cdpink(), dropPrice);        uint requiredLocked = calcOvercollAmount(debt());        if(lockedCollateralDAI < requiredLocked) {            return;        }        uint profitDAI = <mask0>;        uint profitDROP = safeDiv(safeMul(profitDAI, ONE), dropPrice);        mgr.exit(profitDROP);        collateral.burn(address(this), profitDROP);        updateSeniorAsset(profitDAI, 0);    }"