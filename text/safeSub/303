"function transferFrom(address _from, address _to, uint256 _value) onlyActive public returns (bool success) {        if (balances[_from] >= _value && allowed[_from][msg.sender] >= _value && _value > 0) {            balances[_to] = safeAdd(balances[_to], _value);            balances[_from] = <mask0>;            allowed[_from][msg.sender] = safeSub(allowed[_from][msg.sender], _value);            emit Transfer(_from, _to, _value);            return true;        } else {            return false;        }    }"