"function transferFrom(address _from, address _to, uint _value) returns (bool success) {      if (_value < 0) {          revert();      }    uint _allowance = allowed[_from][msg.sender];    balances[_to] = safeAdd(balances[_to], _value);    balances[_from] = safeSub(balances[_from], _value);    allowed[_from][msg.sender] = <mask0>;    Transfer(_from, _to, _value);    return true;  }"