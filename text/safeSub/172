"function transferFrom(address from, address to, uint256 value) returns (bool) {        if (balances[from] >= value &&        allowed[from][msg.sender] >= value &&        safeAdd(balances[to], value) > balances[to])        {            balances[to] = safeAdd(balances[to], value);            balances[from] = safeSub(balances[from], value);            allowed[from][msg.sender] = <mask0>;            Transfer(from, to, value);            return true;        } else {            throw;        }    }"