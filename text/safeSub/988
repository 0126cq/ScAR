"function _transfer(address _from,address _to, uint256 _value) internal {    require(_to != 0x0);    uint256 previousBalances = balanceOf[_from].safeAdd(balanceOf[_to]);    balanceOf[_from] = balanceOf[_from].<mask0>;    balanceOf[_to]   = balanceOf[_to].safeAdd(_value);    emit Transfer(_from,_to,_value);    assert(balanceOf[_from].safeAdd(balanceOf[_to]) == previousBalances);   }"