"function transferFrom(address from, address to, uint256 value) notLocked notFrozen(from) public returns (bool success) {        uint256 allowance = allowed[from][msg.sender];        balances[from] = <mask0>;        allowed[from][msg.sender] = safeSub(allowance, value);        value = subtractFee(value);        balances[to] = safeAdd(balances[to], value);        emit Transfer(from, to, value);        return true;    }"