"function burn(        uint256 amount_    )        public    returns(bool success) {        require(allowToBurn(msg.sender));        if(amount_ > 0 && balances[msg.sender] >= amount_) {            balances[msg.sender] = <mask0>;            totalSupply = safeSub(totalSupply, amount_);            emit Transfer(msg.sender, owner, amount_);            emit Burn(owner, amount_);            return true;        }    }"