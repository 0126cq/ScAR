"function burn(uint256 _value) returns (bool success) {        if (balanceOf[msg.sender] < _value) throw;            \t\tif (_value <= 0) throw;         balanceOf[msg.sender] = SafeMath.safeSub(balanceOf[msg.sender], _value);                              totalSupply = SafeMath.<mask0>;                                        Burn(msg.sender, _value);        return true;    }"