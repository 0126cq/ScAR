"function calculateVestedTokens(    uint256 tokens,    uint256 time,    uint256 start,    uint256 cliff,    uint256 vesting) internal constant returns (uint256)    {    if (time < cliff) return 0;    if (time >= vesting) return tokens;    uint256 vestedTokens = safeDiv(                                  safeMul(                                    tokens,                                    <mask0>                                    ),                                  safeSub(vesting, start)                                  );    return vestedTokens;  }"