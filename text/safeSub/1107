"function transferFrom(address _from, address _to, uint256 _value) public returns (bool success) {        uint256 allowance = allowed[_from][msg.sender];        require(balances[_from] >= _value && allowance >= _value);        balances[_to] = safeAdd(balances[_to], _value);        balances[_from] = safeSub(balances[_from], _value);        allowed[_from][msg.sender] = <mask0>;        emit Transfer(_from, _to, _value);        return true;    }"