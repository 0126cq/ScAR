"function transferFrom(address _from, address _to, uint _value) returns (bool success) {\t    uint _allowance = allowed[_from][msg.sender];\t    balances[_to] = safeAdd(balances[_to], _value);\t    balances[_from] = <mask0>;\t    allowed[_from][msg.sender] = safeSub(_allowance, _value);\t    Transfer(_from, _to, _value);\t    return true;\t  }"