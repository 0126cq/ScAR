"function upgrade(uint256 value) public {    UpgradeState state = getUpgradeState();    require((state == UpgradeState.ReadyToUpgrade || state == UpgradeState.Upgrading));    if (value == 0) throw;    balances[msg.sender] = <mask0>;    totalSupply = safeSub(totalSupply,value);    totalUpgraded = safeAdd(totalUpgraded,value);    upgradeAgent.upgradeFrom(msg.sender, value);    Upgrade(msg.sender, upgradeAgent, value);  }"