"function transferFrom(address _from, address _to, uint256 _value) stoppable public returns (bool success) {\t\trequire(frozenCheck(_from,_to));\t\trequire(_to!= address(0));\t\tif (balances[_from] >= _value && allowed[_from][msg.sender] >= _value && _value > 0) {\t\t\tbalances[_to]  = safeAdd(balances[_to],_value);\t\t\tbalances[_from] = <mask0>;\t\t\tallowed[_from][msg.sender] = safeSub(allowed[_from][msg.sender],_value);\t\t\temit Transfer(_from, _to, _value);\t\t\treturn true;\t\t} else { return false; }\t}"