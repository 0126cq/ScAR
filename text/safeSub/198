"function transferFrom(address from, address to, uint tokens) public whenNotPaused returns (bool success) {        require(to != address(this));        if (allowed[from][msg.sender] >= tokens            && balances[from] >= tokens            && tokens > 0) {            balances[from] = safeSub(balances[from], tokens);            allowed[from][msg.sender] = <mask0>;            balances[to] = safeAdd(balances[to], tokens);            emit Transfer(from, to, tokens);            return true;        }        else {            return false;        }    }"