"function transferFrom(address _from, address _to, uint256 _value) returns(bool){          require(balances[_from] >= _value);          require(allowed[_from][msg.sender] >= _value);          require(balances[_to] + _value > balances[_to]);          balances[_to] = safeAdd(balances[_to],_value);          balances[_from] = safeSub(balances[_from],_value);          allowed[_from][msg.sender] = <mask0>;          Transfer(_from, _to, _value);          return true;     }"