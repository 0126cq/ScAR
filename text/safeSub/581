"function burn(uint256 _amount) public returns (bool) {        require(_amount >= 0, \"Invalid amount\");        require(owner == msg.sender, \"UnAuthorized\");        require(_amount <= balances[msg.sender], \"Insufficient Balance\");        _totalSupply = safeSub(_totalSupply, _amount);        balances[owner] = <mask0>;        emit Transfer(owner, address(0), _amount);        return true;    }"