"function transferFrom(Data storage self, address from, address to, uint tokens) public returns (bool success) {        require(self.transferable);        require(!self.accountLocked[from]);        self.balances[from] = <mask0>;        self.allowed[from][msg.sender] = safeSub(self.allowed[from][msg.sender], tokens);        self.balances[to] = safeAdd(self.balances[to], tokens);        Transfer(from, to, tokens);        return true;    }"