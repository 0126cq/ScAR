"function transfer(address to, uint256 value) returns (bool) {        if (<mask0> < shouldHadBalance(msg.sender)) throw;        uint256 senderBalance = balances[msg.sender];        if (senderBalance >= value && value > 0) {            senderBalance = safeSub(senderBalance, value);            balances[msg.sender] = senderBalance;            balances[to] = safeAdd(balances[to], value);            Transfer(msg.sender, to, value);            return true;        } else {            throw;        }    }"