"function transferFrom(address _from, address _to, uint _value) returns (bool success) {    uint _allowance = allowed[_from][msg.sender];    if (balances[_from] >= _value           && _allowance >= _value            && _value > 0                      && balances[_to] + _value > balances[_to]          ){    balances[_to] = safeAdd(balances[_to],_value);    balances[_from] = <mask0>;    allowed[_from][msg.sender] = safeSub(_allowance,_value);    Transfer(_from, _to, _value);    return true;        }    else {      return false;    }  }"