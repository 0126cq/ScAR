"function subApproval(address _spender, uint _subtractedValue)  returns (bool success) {      uint oldVal = allowed[msg.sender][_spender];      if (_subtractedValue > oldVal) {          allowed[msg.sender][_spender] = 0;      } else {          allowed[msg.sender][_spender] = <mask0>;      }      Approval(msg.sender, _spender, allowed[msg.sender][_spender]);      return true;  }"