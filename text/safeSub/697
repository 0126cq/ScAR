"function buyTokens() payable isRunning isBuyable onlyWhitelist  public {        uint256 weiVal = msg.value;        address investor = msg.sender;        require(investor != address(0) && weiVal >= minInvEth && weiVal <= maxInvEth);        require(safeAdd(weiVal,whitelistLimit[investor]) <= maxInvEth);        uint256 amount = 0;        if(now > sellDeadline1)            amount = safeMul(msg.value, ethFnkRate2);        else            amount = safeMul(msg.value, ethFnkRate1);           whitelistLimit[investor] = safeAdd(weiVal, whitelistLimit[investor]);        balances[owner].balance = <mask0>;        balances[investor].balance = safeAdd(balances[investor].balance, amount);        emit Transfer(owner, investor, amount);    }"