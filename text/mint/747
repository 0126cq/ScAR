"function claimRewards(address user) external notHalted {        Raider storage raider = raiders[user];        (uint32 _roundId, uint256 rewards) = _fetchRewards(raider);        raider.startRound = _roundId;        raider.pendingRewards = 0;        raider.startBlock = uint32(block.number);        raider.startSnapshot = uint32(snapshots.length + 1);        if (rewards > 0) {            confetti.<mask0>;        }    }"