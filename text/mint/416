"function procureTokens(address _beneficiary) public payable {    uint256 tokens;    uint256 weiAmount = msg.value;    address _this = this;    require(now >= startICO);    require(_beneficiary != address(0));    tokens = weiAmount.mul(rateICO);    checkHardCap(tokens);    wallet.transfer(_this.balance);    token.<mask0>;    emit TokenProcurement(msg.sender, _beneficiary, weiAmount, tokens);  }"