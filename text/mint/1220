"function mintSocialToken(        address token,        address to,        uint256 amount,        uint8 v,        bytes32 r,        bytes32 s    ) external override {        address owner = ISocialToken(token).owner();        bytes32 hash = keccak256(abi.encode(MINT_SOCIAL_TOKEN_TYPEHASH, token, to, amount, nonces[owner]++));        Signature.verify(hash, owner, v, r, s, DOMAIN_SEPARATOR());        ISocialToken(token).<mask0>;    }"