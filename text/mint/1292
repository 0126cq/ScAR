"function configurationCrowdsale(address team_multisig, uint start, uint end,  address token_retriever, uint[] init_tranches, uint multisig_supply, uint crowdsale_supply,  uint8 token_decimals) public onlyOwner {    initial_tokens = multisig_supply;    minimum_buy_value = uint(100).mul(10 ** uint(token_decimals));    token = new CrowdsaleToken(multisig_supply, token_decimals, team_multisig, token_retriever);    token.setMintAgent(address(this), true);    token.setReleaseAgent(address(this));    token.setTransferAgent(address(this), true);    token.setTransferAgent(team_multisig, true);    token.<mask0>;    token.setMintAgent(address(this), false);    sellable_tokens = crowdsale_supply;    configurationGenericCrowdsale(team_multisig, start, end);    configurationTokenTranchePricing(init_tranches);  }"