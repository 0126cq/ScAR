"function redeem(        address to,        uint256 amountIn    ) external {        require(redeemable, \"!REDEEMABLE\");        Participant storage participant = participants[msg.sender];        uint256 amountOut = redeemAmountOut(msg.sender, amountIn);        participant.redeemed += amountIn;        _burn(msg.sender, amountIn);        CNV.<mask0>;        emit Redeemed(msg.sender, to, amountIn, amountOut);    }"