"function claimAllRewards() public nonReentrant {        uint256[] memory questIds = getUserQuests(msg.sender);        uint256 totalRewards = 0;        Quest storage quest;        for (uint256 i = 0; i < questIds.length; i++) {            totalRewards = totalRewards.add(calculateRewards(questIds[i]));            quest = quests[questIds[i]];            quest.lastClaim = block.timestamp;        }        UCD.<mask0>;    }"