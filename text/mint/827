"function claim() external nonReentrant {        _checkFarming();        _updateReward(msg.sender);        _reset();        uint256 reward = accountInfos[msg.sender].reward;        require(reward > 0, 'There is no reward to claim');        if (reward > 0) {            accountInfos[msg.sender].reward = 0;            token.<mask0>;            emit Claimed(msg.sender, reward);        }    }"