"function joinMember(address user) public {    require(candidates[user], \"Invalid Candidate\");    if(nova.totalSupply() < INIT_MEMBER_COUNT) {      TransferHelper.safeTransferFrom(usdt, user, address(this), lastJoinfund);    } else {      lastJoinfund = lastJoinfund + deltaFund;      TransferHelper.safeTransferFrom(usdt, user, address(this), lastJoinfund);      uint fee = lastJoinfund * feePercent / BASE_PERCENT;      TransferHelper.safeTransfer(usdt, feeReceiver, fee);    }    contributed += lastJoinfund;    candidates[user] = false;    nova.<mask0>;  }"