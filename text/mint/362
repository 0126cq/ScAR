"function mintGiveaway(bytes32[] calldata proof, uint256 authorizedAmount, uint256 mintAmount)  public  {    require(_verify(proof, root_giveaway,_leaf(msg.sender, authorizedAmount)), \"Invalid merkle proof\");    require(mintedFromGiveAway[msg.sender] + mintAmount <= authorizedAmount, \"Already minted max amount\");    require(totalMintedGiveaway + mintAmount <= 200, \"Max minted for giveaway\");    totalMintedGiveaway += mintAmount;    mintedFromGiveAway[msg.sender] += mintAmount;    for (uint256 i = 0; i < mintAmount; i++)    {      myNFT.<mask0>;    }    _checkPriceBrackets();  }"