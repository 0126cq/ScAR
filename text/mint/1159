"function mintX (address _to, uint256 _amount) public onlyOwner{        if(time==0){            time = now;        }        else{            require(time+14 days<=now,\"Minting is allowed after atleast 2 weeks\");            time = now;        }        Token tok = Token(TOKEN_X_ADDRESS);        require(SafeMath.div(SafeMath.mul(96446000000000000000000,10),100)>=_amount,\"Minting 10% Limit exceeds\");        require(totalXmint<=96446000000000000000000,\"Maximum Minting Limit Reached\");        tok.<mask0>;        totalXmint = totalXmint + _amount;    }"