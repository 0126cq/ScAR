"function deposit(uint256 _amount) public override returns (uint256 _shares) {    userCooldown[msg.sender] = now.add(withdrawCooldown);    uint256 _pool = totalUnderlying();    uint256 _before = unusedUnderlyingBalance();    token.safeTransferFrom(msg.sender, address(this), _amount);    uint256 _after = unusedUnderlyingBalance();    _amount = _after.sub(_before);     if (zToken.totalSupply() == 0) {      _shares = _amount;    } else {      _shares = (_amount.mul(zToken.totalSupply())).div(_pool);    }    zToken.<mask0>;    emit Deposited(msg.sender, _amount, _shares);  }"