"function stakeSushiToCreamToBento(address to, uint256 amount) external returns (uint256 amountOut, uint256 shareOut) {        sushiToken.safeTransferFrom(msg.sender, address(this), amount);         ISushiBarBridge(sushiBar).enter(amount);         ICompoundBridge(crXSushiToken).<mask0>;         (amountOut, shareOut) = bento.deposit(IERC20(crXSushiToken), address(this), to, IERC20(crXSushiToken).safeBalanceOfSelf(), 0);     }"