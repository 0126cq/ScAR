"function vest(        address _beneficiary,        uint256 _releaseDate,        uint256 _amount    )        public onlyOwner canMint returns (address)    {        address vestingContract = new BITVesting(            this,            _beneficiary,            _releaseDate        );        assert (vestingContract != 0x0);        require(<mask0>);        emit Vested(_beneficiary, address(vestingContract), _releaseDate, _amount);        return vestingContract;    }"