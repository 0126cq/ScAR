"function buyTokens() public payable {    require(msg.value != 0);    require(msg.sender != 0);    require(isICOActive() || postICOSale);    require(token.whitelist(msg.sender));    uint256 tokensPurchased;    uint256 weiChange;    (tokensPurchased, weiChange) = token.weiToTokens(msg.value);    uint256 weiExactAmount = msg.value.sub(weiChange);    require(tokensPurchased >= minimumPurchase);    if (isICOActive() && token.totalSupply().add(tokensPurchased) > icoTokenCap) revert();    weiRaised = weiRaised.add(weiExactAmount);    token.<mask0>;     emit TokenPurchase(msg.sender, weiExactAmount, tokensPurchased);    wallet.transfer(weiExactAmount);    msg.sender.transfer(weiChange);  }"