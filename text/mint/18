"function airdropFor(address _beneficiary) public {    require(_beneficiary != 0x0);    require(contributorList[_beneficiary].airdrop == true);    require(contributorList[_beneficiary].tokensIssued == 0);    require(contributorList[_beneficiary].allowance > 0);    setCrowdsaleState();    require(crowdsaleState == state.crowdsaleEnded);    uint256 amount = contributorList[_beneficiary].allowance.mul(rate);    require(token.<mask0>);    emit TokenAllocated(_beneficiary, amount);    contributorList[_beneficiary].tokensIssued = contributorList[_beneficiary].tokensIssued.add(amount);  }"