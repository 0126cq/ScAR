"function _createAuction() internal {        try nouns.<mask0> returns (uint8 nounId) {            uint256 startTime = block.timestamp;            uint256 endTime = startTime + duration;            auction = Auction({                nounId: nounId,                amount: 0,                startTime: startTime,                endTime: endTime,                bidder: payable(0),                settled: false            });            emit AuctionCreated(nounId, startTime, endTime);        } catch Error(string memory) {            _pause();        }    }"