"function updatePool(uint256 _pid) public {        PoolInfo storage pool = poolInfo[_pid];        if (block.number <= pool.lastRewardBlock) {            return;        }        uint256 lpSupply = pool.lpToken.balanceOf(address(this));        if (lpSupply == 0) {            pool.lastRewardBlock = block.number;            return;        }        uint256 multiplier = getMultiplier(            pool.lastRewardBlock,            block.number,            pool.rewardEndBlock        );        uint256 xpixulReward = multiplier        .mul(xpixulPerBlock)        .mul(pool.allocPoint)        .div(totalAllocPoint);        xpixul.<mask0>;         xpixul.mint(address(this), xpixulReward);        pool.accxPixulPerShare = pool.accxPixulPerShare.add(            xpixulReward.mul(1e12).div(lpSupply)        );        pool.lastRewardBlock = block.number;    }"