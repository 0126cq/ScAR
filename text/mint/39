"function buy(uint256 numberOfTokens) external payable {    SaleConfig memory _saleConfig = saleConfig;    require(      block.timestamp >= _saleConfig.publicSaleStartTime,      \"Sale is not active\"    );    require(      numberOfTokens <= _saleConfig.txLimit,      \"Transaction limit exceeded\"    );    require(msg.value == (mintPrice * numberOfTokens), \"Incorrect payment\");    <mask0>;  }"