"function deposit(uint lusdAmount) external {                uint lusdValue = SP.getCompoundedLUSDDeposit(address(this));        uint ethValue = SP.getDepositorETHGain(address(this)).add(address(this).balance);        uint price = fetchPrice();        require(ethValue == 0 || price > 0, \"deposit: chainlink is down\");        uint totalValue = lusdValue.add(ethValue.mul(price) / PRECISION);        require(totalValue > 0 || total == 0, \"deposit: system is rekt\");        uint newShare = PRECISION;        if(total > 0) newShare = total.mul(lusdAmount) / totalValue;        require(LUSD.transferFrom(msg.sender, address(this), lusdAmount), \"deposit: transferFrom failed\");        SP.provideToSP(lusdAmount, frontEndTag);        <mask0>;        emit UserDeposit(msg.sender, lusdAmount, newShare);            }"