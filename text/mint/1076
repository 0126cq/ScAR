"function buyTokens(address beneficiary) payable {    require(beneficiary != 0x0);    require(validPurchase());    uint256 weiAmount = msg.value;    uint256 tokens = weiAmount.mul(rate);    uint256 bonusTokens = 0;    if(now < (startTime + bonusPeriod * 1 days)) {      bonusTokens = tokens.div(5);     }    tokens += bonusTokens;    weiRaised = weiRaised.add(weiAmount);    token.<mask0>;    TokenPurchase(msg.sender, beneficiary, weiAmount, tokens);    forwardFunds();  }"