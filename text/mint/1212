"function buyAsset(string memory optionID,uint256 quantity) public {        uint256 totalPrice = _assetprice[optionID]*quantity;        require(totalPrice>0,\"ERC721: asset not created\");        require(_spay.allowance(_msgSender(),address(this))>=totalPrice,\"ERC20: must approve enough SPAY\");        require(_spay.transferFrom(_msgSender(),airdropPool,totalPrice), \"ERC20: SPAY transfer failed\");        uint mineShareAmount = 0;        if (minesharePerMillion > 0 ) {            mineShareAmount = totalPrice.mul(minesharePerMillion).div(1000000);            emit SpayMining(optionID,quantity,_msgSender(),totalPrice,mineShareAmount);            if(shouldMint) {                require(_spay.hasRole(MINTER_ROLE,address(this)), \"ERC721PresetMinterPauserAutoId:contract must have minter role to mint Spay\");                require(miningPool!=address(0), \"Invalid MiningPool Address\");                _spay.<mask0>;            }         }        emit AssetPurchased(_msgSender(),optionID,quantity);           }"