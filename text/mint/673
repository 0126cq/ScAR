"function finishMinting() public onlyOwner {      if(this.balance > softcap) {        multisig.transfer(this.balance);        uint issuedTokenSupply = token.totalSupply();        uint restrictedTokens = issuedTokenSupply.mul(restrictedPercent).div(100 - restrictedPercent);        token.<mask0>;        token.finishMinting();      }    }"