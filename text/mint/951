"function mint(uint256 id, uint256 amount)        external        payable        virtual        override    {        if (!mintEnabled) {            revert NotOnSale();        }        if (prices[id] == 0) {            revert NotOnSale();        }        if (amount * prices[id] > msg.value) {            revert NotEnoughFunds();        }        _safeDecreaseAvailableStock(id, amount);        zetaERC1155.<mask0>;    }"