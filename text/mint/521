"function distributeAlbumTokens(        address _token,        uint256 totalTokens,        uint256 amountSold,        address album    ) internal {        require(totalTokens >= amountSold);        require(totalTokens > 0);        ERC20PresetMinterPauser token = ERC20PresetMinterPauser(_token);        token.mint(album, amountSold);        uint256 sznsDaoRake = totalTokens / SZNS_DAO_RAKE_DIVISOR;        token.<mask0>;        token.mint(msg.sender, (totalTokens - amountSold) - sznsDaoRake);    }"