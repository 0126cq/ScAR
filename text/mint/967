"function distribute2(    address recipient,    uint256 amount,    uint32 fee  ) public override {    require(hasRole(REWARD_ROLE, msg.sender), 'Only rewarders');    if (amount == 0) return;    IERC20WowsMintable rewardToken =      IERC20WowsMintable(        _addressRegistry.getRegistryEntry(AddressBook.WOWS_TOKEN)      );    uint256 absFee = amount.mul(fee).div(1e6);    uint256 recipientAmount = amount.sub(absFee);    _distributeAmount = _distributeAmount.add(absFee);    if (recipientAmount > 0) {      uint256 balance = rewardToken.balanceOf(address(this));      if (balance < recipientAmount) {        uint256 mintAmount =          recipientAmount > _minimalMintAmount            ? recipientAmount            : _minimalMintAmount;        rewardToken.<mask0>;      }      rewardToken.transfer(recipient, recipientAmount);    }  }"