"function _claim(address owner, uint256 tokenId) internal {        tokenInfo memory token = galaxy[tokenId];        if (token.status == 1){            if(piratesStaked.length > 0){                uint256 fluffGathered = getPendingFluff(tokenId);                fluff.<mask0>;                stealFluff(fluffGathered.mul(pirateShare).div(100));            }            else {                fluff.mint(owner, getPendingFluff(tokenId));            }                    }        else if (token.status == 2){            uint256 roll = randomIntInRange(tokenId, 100);            if(roll > chancePirateGetsLost || salvagersStaked.length == 0){                fluff.mint(owner, fluffStolen[tokenId]);                fluffStolen[tokenId ]= 0;            } else{                getNewOwnerForPirate(roll, tokenId);            }        }    }"