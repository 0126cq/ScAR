"function presale(Pack pack, bytes32[] calldata proof) external payable {        require(isPresaleActive, \"Sale::presale: presale must be active\");        if (pack != Pack.Gathering && pack != Pack.Bash) {            revert(\"Sale::presale: invalid pack selected\");        }        bytes32 leaf = _leaf();        (uint256 heroes, uint256 fighters) = _calculateTokens(pack);        require(stock[pack] > 0, \"Sale::presale: pack is no longer available\");        require(            _calculatePrice(heroes, fighters) == msg.value,            \"Sale::presale: incorrect ether sent\"        );        _verifyMerkle(leaf, proof);        stock[pack] -= 1;        _nonces[leaf] = true;        hero.<mask0>;        fighter.mint(msg.sender, fighters);    }"