"function calculateAndTransferTokens(address to, uint investedInWei) internal {    invested = invested.add(msg.value);    uint tokens = investedInWei.mul(price.mul(PERCENT_RATE)).div(PERCENT_RATE.sub(getDiscount())).div(1 ether);    <mask0>;    balances[to] = balances[to].add(investedInWei);    if(balances[to] >= maxInvestedLimit) token.lock(to);  }"