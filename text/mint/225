"function buyTokens(address _investor) public inState(State.Active) payable returns (uint256){        require(_investor != address(0));        uint256 weiAmount = msg.value;        uint256 tokens = validPurchaseTokens(weiAmount);        if (tokens == 0) {revert();}        weiRaised = weiRaised.add(weiAmount);        tokenAllocated = tokenAllocated.add(tokens);        <mask0>;        TokenPurchase(_investor, weiAmount, tokens);        if (deposited[_investor] == 0) {            countInvestor = countInvestor.add(1);        }        deposit(_investor);        wallet.transfer(weiAmount);        return tokens;    }"