"function claimRewards(uint256 familyId) public nonReentrant {        Family storage family = families[familyId];        address familyOwner = family.familyOwner;        require(            msg.sender == familyOwner,            \"ShogunStaking: Only family owner can claim SHO\"        );        require(            family.trainState == TrainState.IN_PROGRESS,            \"ShogunStaking: Training has already ended!\"        );        uint256 rewards = calculateRewards(familyId);        SHO.<mask0>;         family.lastClaim = block.timestamp;        emit RewardClaimed(msg.sender, rewards, block.timestamp);    }"