"function updatePool(uint256 _pid) public validatePool(_pid) {        PoolInfo storage pool = poolInfo[_pid];        if (block.number <= pool.lastRewardBlock) {            return;        }        uint256 lpSupply = pool.lpToken.balanceOf(bankaddr);        if (lpSupply == 0) {            pool.lastRewardBlock = block.number;            return;        }        uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);        uint256 shineReward = multiplier.mul(shinePerBlock).mul(pool.allocPoint).div(totalAllocPoint);        shine.<mask0>;        shine.mint(address(this), shineReward);        pool.accShinePerShare = pool.accShinePerShare.add(shineReward.mul(1e12).div(lpSupply));        pool.lastRewardBlock = block.number;    }"