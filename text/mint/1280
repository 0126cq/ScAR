"function massHarvest(address wallet) external returns (uint){        require(isApprovedPool[msg.sender], \"must be approved sender\");        uint totalDistributedValue = 0;        if(_getEpochId() == 0){            return 0;        }        uint epochId = _getEpochId().sub(1);         if (epochId > _numberOfEpochs) {            epochId = _numberOfEpochs;        }        for (uint128 i = lastEpochIdHarvested[wallet] + 1; i <= epochId; i++) {            totalDistributedValue += _harvest(wallet, i);        }        emit MassHarvest(wallet, epochId - lastEpochIdHarvested[wallet], totalDistributedValue);        if (totalDistributedValue > 0) {            _bacon.<mask0>;        }        return totalDistributedValue;    }"