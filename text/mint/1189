"function mintNewBridgeTokens(        address payable _intendedRecipient,        address _bridgeTokenAddress,        string memory _symbol,        uint256 _amount    ) internal {        require(            controlledBridgeTokens[_symbol] == _bridgeTokenAddress,            \"Token must be a controlled bridge token\"        );        require(            BridgeToken(_bridgeTokenAddress).<mask0>,            \"Attempted mint of bridge tokens failed\"        );        emit LogBridgeTokenMint(            _bridgeTokenAddress,            _symbol,            _amount,            _intendedRecipient        );    }"