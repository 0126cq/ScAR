"function _createAuction() internal {        try sunrise.<mask0> returns (uint256 sunriseId) {            uint256 startTime = block.timestamp;            uint256 endTime = startTime + duration;            auction = Auction({                sunriseId: sunriseId,                amount: 0,                startTime: startTime,                endTime: endTime,                bidder: payable(0),                settled: false,                duration: duration            });            emit AuctionCreated(sunriseId, startTime, endTime);        } catch Error(string memory) {            _pause();        }    }"