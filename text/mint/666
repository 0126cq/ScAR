"function _investAllUnderlying() internal {        if (!investActivated) {            return;        }        uint256 underlyingBalance = IERC20(underlying).balanceOf(address(this));        if (underlyingBalance > 0) {            IERC20(underlying).safeApprove(cToken, 0);            IERC20(underlying).safeApprove(cToken, underlyingBalance);            uint256 mintResult = ICToken(cToken).<mask0>;            require(mintResult == 0, \"Error calling mint on Compound\");        }    }"