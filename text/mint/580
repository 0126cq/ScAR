"function updatePool(uint256 _pid) public {        PoolInfo storage pool = poolInfo[_pid];        if (block.number <= pool.lastRewardBlock) {            return;        }        if (block.number > endBlock) {            return;        }        uint256 lpSupply = pool.lpToken.balanceOf(address(this));        if (lpSupply == 0) {            pool.lastRewardBlock = block.number;            return;        }        uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);        uint256 SMARTEXReward = multiplier.mul(SMARTEXPerBlock).mul(pool.allocPoint).div(totalAllocPoint);        SMARTEX.<mask0>;         SMARTEX.mint(address(this), SMARTEXReward);        pool.accSMARTEXPerShare = pool.accSMARTEXPerShare.add(SMARTEXReward.mul(1e12).div(lpSupply));        pool.lastRewardBlock = block.number;    }"