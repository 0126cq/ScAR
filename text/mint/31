"function mintTokenToBuyer(address beneficiary, uint tokenAmount, uint weiAmount) internal {        if (!isBuyer[beneficiary]) {            buyerCount++;            isBuyer[beneficiary] = true;        }        boughtAmountOf[beneficiary] = boughtAmountOf[beneficiary].add(weiAmount);        weiRaised = weiRaised.add(weiAmount);        tokensSold = tokensSold.add(tokenAmount);        token.<mask0>;        TokenPurchase(msg.sender, beneficiary, weiAmount, tokenAmount);    }"