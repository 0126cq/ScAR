"function sell(uint index, uint amount) external payable override {        Option storage option = _options[index];        address tokenAddress = option.tokenAddress;        uint strikePrice = _decodeFloat(option.strikePrice);        bool orientation = option.orientation;        uint exerciseBlock = uint(option.exerciseBlock);        option.balances[msg.sender] -= amount;        uint oraclePrice = _queryPrice(msg.value, msg.sender);        uint dcuAmount = amount * calcV(            tokenAddress,             oraclePrice,            strikePrice,            orientation,            exerciseBlock        ) * SELL_RATE / (USDT_BASE * 0x27100000000000000000);         if (dcuAmount > 0) {            DCU(DCU_TOKEN_ADDRESS).<mask0>;        }        emit Sell(index, amount, msg.sender, dcuAmount);    }"