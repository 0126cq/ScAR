"function saveViaMint(        address _mAsset,        address _save,        address _vault,        address _bAsset,        uint256 _amount,        uint256 _minOut,        bool _stake    ) external {        require(_mAsset != address(0), \"Invalid mAsset\");        require(_save != address(0), \"Invalid save\");        require(_vault != address(0), \"Invalid vault\");        require(_bAsset != address(0), \"Invalid bAsset\");        IERC20(_bAsset).safeTransferFrom(msg.sender, address(this), _amount);        uint256 massetsMinted = IMasset(_mAsset).<mask0>;        _saveAndStake(_save, _vault, massetsMinted, _stake);    }"