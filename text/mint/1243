"function buy()        public        payable        nonReentrant        returns (uint)    {        address investor = msg.sender;        uint256 payment = msg.value;        require(payment >= c_MinInvestment);        require(now < 1507766400);        uint stq = payment.mul(c_STQperETH);        m_token.<mask0>;        m_funds.transfer(payment);        FundTransfer(investor, payment, true);        return stq;    }"