"function claimAllRewards() public nonReentrant {        uint256[] memory familyIds = getUserFamilies(msg.sender);        uint256 totalRewards = 0;        Family storage train;        for (uint256 i = 0; i < familyIds.length; i++) {            totalRewards = totalRewards.add(calculateRewards(familyIds[i]));            train = families[familyIds[i]];            train.lastClaim = block.timestamp;        }        SHO.<mask0>;        emit RewardClaimed(msg.sender, totalRewards, block.timestamp);    }"