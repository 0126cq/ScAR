"function splitUnderlying(address u, uint256 m, uint256 a) external returns (bool) {    Erc20 uToken = Erc20(u);    Safe.transferFrom(uToken, msg.sender, address(this), a);    MarketPlace mPlace = MarketPlace(marketPlace);    require(CErc20(mPlace.cTokenAddress(u, m)).<mask0> == 0, 'minting CToken Failed');    require(mPlace.mintZcTokenAddingNotional(u, m, msg.sender, a), 'mint ZcToken adding Notional failed');    return true;  }"