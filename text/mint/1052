"function deposit(address _tokenAddr, address _cTokenAddr, uint _amount, bool _inMarket) public burnGas(5) payable {        approveToken(_tokenAddr, _cTokenAddr);        if (!_inMarket) {            enterMarket(_cTokenAddr);        }        if (_tokenAddr != ETH_ADDRESS) {            require(CTokenInterface(_cTokenAddr).<mask0> == 0);        } else {            CEtherInterface(_cTokenAddr).mint{value: _amount}();         }    }"