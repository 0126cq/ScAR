"function deposit(address _tokenAddr, address _cTokenAddr, uint _amount, bool _inMarket) public burnGas(5) payable {        if (_tokenAddr != ETH_ADDR) {            ERC20(_tokenAddr).safeTransferFrom(msg.sender, address(this), _amount);        }        approveToken(_tokenAddr, _cTokenAddr);        if (!_inMarket) {            enterMarket(_cTokenAddr);        }        if (_tokenAddr != ETH_ADDR) {            require(CTokenInterface(_cTokenAddr).<mask0> == 0);        } else {            CEtherInterface(_cTokenAddr).mint{value: msg.value}();         }    }"