"function buyPresale(bytes calldata signature, uint256 mintAmount)        external        payable    {        PresaleConfig memory _config = presaleConfig;        uint256 ownerTokenCount = balanceOf(msg.sender);        require(            (!msg.sender.isContract() && msg.sender == tx.origin),            \"Contract buys not allowed\"        );        require(            block.timestamp >= _config.startTime &&                block.timestamp < _config.endTime,            \"Presale is not active\"        );        require(            (totalSupply + mintAmount) <= whitelistLimit,            \"Not enough CWS remaining\"        );        require(mintAmount > 0, \"Not enough Mint Amount\");        require(mintAmount <= mintLimit, \"Mint Amount is too high\");        require(            (mintAmount + ownerTokenCount) <= mintLimit,            \"You have reached the limit\"        );        require(            msg.value == (_config.price * mintAmount),            \"Insufficient payment\"        );        bytes32 digest = keccak256(            abi.encodePacked(                \"\\x19\\x01\",                DOMAIN_SEPARATOR,                keccak256(abi.encode(TYPEHASH, msg.sender))            )        );        address signer = digest.recover(signature);        require(            signer != address(0) && signer == whitelistSigner,            \"Invalid signature\"        );        <mask0>;    }"