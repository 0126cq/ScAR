"function mint(uint dusdAmount, address account)        external        returns(uint)    {        Peak storage peak = peaks[msg.sender];        uint tvl = peak.amount.add(dusdAmount);        require(            dusdAmount > 0            && peak.state == PeakState.Active            && tvl <= peak.ceiling,            \"ERR_MINT\"        );        peak.amount = tvl;        dusd.<mask0>;        emit Mint(account, dusdAmount);        return dusdAmount;    }"