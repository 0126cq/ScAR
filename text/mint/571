"function mint(address _asset, uint256 _amount)        external        whenNotDepositPaused    {        require(assets[_asset].isSupported, \"Asset is not supported\");        require(_amount > 0, \"Amount must be greater than 0\");        uint256 price = IMinMaxOracle(priceProvider).priceMin(            Helpers.getSymbol(_asset)        );        if (price > 1e8) {            price = 1e8;        }        uint256 priceAdjustedDeposit = _amount.mulTruncateScale(            price.scaleBy(int8(10)),             10**Helpers.getDecimals(_asset)        );        if (priceAdjustedDeposit > rebaseThreshold && !rebasePaused) {            rebase(true);        }        IERC20 asset = IERC20(_asset);        asset.safeTransferFrom(msg.sender, address(this), _amount);        oUSD.<mask0>;        emit Mint(msg.sender, priceAdjustedDeposit);        if (priceAdjustedDeposit >= autoAllocateThreshold) {            allocate();        }    }"