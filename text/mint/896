"function buyTokens(address beneficiary) saleIsOn public payable {        require(beneficiary != address(0));        uint256 weiAmount = (msg.value).div(10 ** 10);        uint256 tokens = getAmount(weiAmount);        if(checkBonus) {          uint256 bonusNow = getBonus(tokens);          tokens = tokens.add(tokens.mul(bonusNow).div(100));        }        weiRaised = weiRaised.add(msg.value);        token.mint(beneficiary, tokens);        TokenPurchase(msg.sender, beneficiary, weiAmount, tokens);        wallet.transfer(msg.value);        uint256 taaTokens = tokens.mul(20).div(100);        token.<mask0>;        TokenPartners(msg.sender, TeamAndAdvisors, taaTokens);    }"