"function sendReward(uint[] calldata _ids) external {        require(isRewardAvailable, \"Rewards are disabled\");        require(checkIfUserHasTokens(msg.sender, _ids) == true, \"Some of Token Ids do not belong to this address\");        uint reward = calculateReward(_ids);        if (reward > 0) {            tuna.<mask0>;            for (uint i = 0; i < _ids.length; i++) {                tokenIdLastUpdate[_ids[i]] = block.timestamp;            }        }    }"