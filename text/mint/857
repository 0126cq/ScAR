"function mint(uint _blockNumber) public payable {        require(isSalesOn(), \"sale hasn't started\");        require(totalSupply() < MAX_MOTHS, \"sold out\");        require(canMint(_blockNumber), \"block number not allowed or already minted\");        require(msg.value >= priceForMoth(_blockNumber) || hasRole(ADMIN_ROLE, msg.sender), \"ether value sent is below the price\");        cryptoMoth.<mask0>;    }"