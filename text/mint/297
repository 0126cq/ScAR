"function finalizeMint(uint256 _index) public mintNotPaused {        MintOperation memory op = mintOperations[_index];        address to = op.to;        uint256 value = op.value;        if (msg.sender != owner) {            require(canFinalize(_index));            _subtractFromMintPool(value);        }        delete mintOperations[_index];        token.<mask0>;        emit FinalizeMint(to, value, _index, msg.sender);    }"