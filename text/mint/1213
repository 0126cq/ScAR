"function startReward(uint256 startTime)        external        onlyGovernance        updateReward(address(0))    {        require(_hasStart == false, \"has started\");        _hasStart = true;        _startTime = startTime;        update_initreward();        _rewardRate = _initReward.div(DURATION);         _BDM.<mask0>;        _lastUpdateTime = _startTime;        _periodFinish = _startTime.add(DURATION);        emit RewardAdded(_initReward);    }"