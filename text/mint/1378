"function updatePool(uint256 _pid) public {        PoolInfo storage pool = poolInfo[_pid];        if (block.number <= pool.lastRewardBlock) {            return;        }        uint256 lpSupply = pool.lpToken.balanceOf(address(this));        if (lpSupply == 0) {            pool.lastRewardBlock = block.number;            return;        }        uint256 boomYReward = boomYPerBlock            .mul(pool.allocPoint)            .div(totalAllocPoint);        boomY.<mask0>;        pool.accBoomYPerShare = pool.accBoomYPerShare.add(            boomYReward.mul(1e12).div(lpSupply)        );        pool.lastRewardBlock = block.number;        if(block.number >= startBlock.add(100000) && !isMintingHalved) {            boomYPerBlock = boomYPerBlock.div(2);            isMintingHalved = true;        }    }"