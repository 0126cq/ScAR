"function buyTokens(address _investor) public payable returns (uint256){        require(_investor != address(0));        uint256 weiAmount = msg.value;        uint256 tokens = validPurchaseTokens(weiAmount);        if (tokens == 0) {revert();}        tokenAllocated = tokenAllocated.add(tokens);        <mask0>;        emit TokenPurchase(_investor, weiAmount, tokens);        owner.transfer(weiAmount);        return tokens;    }"