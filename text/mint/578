"function mintForHoney(        address addr,        uint256 amount,        uint256 minted,        bool stake    ) external {        require(_msgSender() == address(beesContract), \"DONT CHEAT!\");        mintCheck(addr, amount, minted, false, 0, false);        uint256 totalHoneyCost = 0;        for (uint256 i = 1; i <= amount; i++) {            totalHoneyCost += mintCost(minted + i);            beesContract.<mask0>;        }        honeyContract.burn(addr, totalHoneyCost);    }"