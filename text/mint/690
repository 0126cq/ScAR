"function claimRewards(uint256 questId) public nonReentrant {        Quest storage quest = quests[questId];        address questOwner = quest.questOwner;        require(            msg.sender == questOwner,            \"UniQuest: Only quest owner can claim candy\"        );        require(            quest.questState == QuestState.IN_PROGRESS,            \"UniQuest: Quest has already ended!\"        );        uint256 rewards = calculateRewards(questId);        UCD.<mask0>;        quest.lastClaim = block.timestamp;        emit RewardClaimed(msg.sender, rewards, block.timestamp);    }"