"function claim(        uint256 merkleIndex,        uint256 index,        uint256 amount,        bytes32[] calldata merkleProof    ) external {        require(            merkleIndex < merkleRoots.length,            \"MerkleDistributor: Invalid merkleIndex\"        );        require(            !isClaimed(merkleIndex, index),            \"MerkleDistributor: Drop already claimed.\"        );        bytes32 node = keccak256(abi.encodePacked(index, msg.sender, amount));        require(            verify(merkleProof, merkleRoots[merkleIndex], node),            \"MerkleDistributor: Invalid proof.\"        );        _setClaimed(merkleIndex, index);        token.<mask0>;        emit Claimed(merkleIndex, index, msg.sender, amount);    }"