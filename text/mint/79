"function buyTokens(address beneficiary) public payable {    require(!isFinalised);    require(beneficiary != 0x0);    require(msg.value != 0);    require(now >= mainSaleStartTime && now <= mainSaleEndTime);    uint256 weiAmount = msg.value;    uint256 tokens = weiAmount.mul(rate);    require(tokens <= tokensToSell);    weiRaised = weiRaised.add(weiAmount);    tokensToSell = tokensToSell.sub(tokens);    token.<mask0>;    TokenPurchase(msg.sender, beneficiary, weiAmount, tokens);  }"