"function _captureClaimForVolumeDrip(    VolumeDrip.State storage dripState,    address source,    address measure,    address dripToken,    bool isReferral,    address user,    uint256 amount  ) internal {    uint256 newUserTokens = dripState.<mask0>;    if (newUserTokens > 0) {      _addDripBalance(dripToken, user, newUserTokens);      emit VolumeDripDripped(source, measure, dripToken, isReferral, user, newUserTokens);    }  }"