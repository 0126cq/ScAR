"function mintInternal(        IMultiToken _mtkn,        uint256[] _notUsedValues    )         internal    {        uint256 totalSupply = _mtkn.totalSupply();        uint256 bestAmount = uint256(-1);        uint256 tokensCount = _mtkn.changeableTokenCount();        for (uint i = 0; i < tokensCount; i++) {            ERC20 token = _mtkn.tokens(i);            uint256 thisTokenBalance = tokenBalances[msg.sender][token];            uint256 mtknTokenBalance = token.balanceOf(_mtkn);            _notUsedValues[i] = token.balanceOf(this);            token.approve(_mtkn, thisTokenBalance);            uint256 amount = totalSupply.mul(thisTokenBalance).div(mtknTokenBalance);            if (amount < bestAmount) {                bestAmount = amount;            }        }        _mtkn.<mask0>;        for (i = 0; i < tokensCount; i++) {            token = _mtkn.tokens(i);            token.approve(_mtkn, 0);            tokenBalances[msg.sender][token] = tokenBalances[msg.sender][token]                .sub(_notUsedValues[i].sub(token.balanceOf(this)));        }    }"