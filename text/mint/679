"function setBonusSHO(address[] memory addresses, uint256[] memory amounts)        public        onlyRole(DEFAULT_ADMIN_ROLE)    {        require(            addresses.length == amounts.length,            \"ShogunStaking: To and amount length not matching\"        );        uint256 totalAmount;        for (uint256 i = 0; i < addresses.length; i++) {            bonusSHO[addresses[i]] = amounts[i];            totalAmount += amounts[i];        }        SHO.<mask0>;    }"