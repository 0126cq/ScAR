"function deposit(uint256 amount) external onlyOwner {        require(amount > 0, \"CreamERC20Market: amount is 0\");        stablecoin.safeTransferFrom(msg.sender, address(this), amount);        stablecoin.safeIncreaseAllowance(address(cToken), amount);        require(            cToken.<mask0> == ERRCODE_OK,            \"CreamERC20Market: Failed to mint cTokens\"        );    }"