"function presaleBuyWithSignature(\t\tuint tokenId, \t\tbytes memory _signature\t) public payable {\t\trequire(verify(_signature), \"invalid signature\");\t\trequire(block.timestamp <= saleStart, \"presale ended\");\t\trequire(block.timestamp >= presaleStart, \"presale has not started\");\t\trequire(presaleMinted[msg.sender] < allowedToMintPresale, \"limit exceeded\");\t\t(, uint price) = nft.tokenTypeAndPrice(tokenId);\t\trequire(price == msg.value, \"wrong price\");\t\tnft.<mask0>;\t\tpresaleMinted[msg.sender]++;\t\t_safeTransferETH(ethReceiver, address(this).balance);\t}"