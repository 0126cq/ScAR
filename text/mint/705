"function _supplyToPool(        address _poolTokenAddress,        ERC20 _underlyingToken,        uint256 _amount    ) internal {        if (_poolTokenAddress == cEth) {            IWETH(wEth).withdraw(_amount);             ICEther(_poolTokenAddress).<mask0>;        } else {            _underlyingToken.safeApprove(_poolTokenAddress, _amount);            if (ICToken(_poolTokenAddress).mint(_amount) != 0) revert MintOnCompoundFailed();        }    }"