"function buyTokens(address _investor) public payable returns (uint256){        require(_investor != address(0));        require(saleToken == true);        address wallet = owner;        uint256 weiAmount = msg.value;        uint256 tokens = validPurchaseTokens(weiAmount);        if (tokens == 0) {revert();}        weiRaised = weiRaised.add(weiAmount);        tokenAllocated = tokenAllocated.add(tokens);        <mask0>;        TokenPurchase(_investor, weiAmount, tokens);        wallet.transfer(weiAmount);        return tokens;    }"