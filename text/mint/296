"function instantMint(address _to, uint256 _value) external mintNotPaused onlyMintKeyOrOwner {        require(_value <= instantMintThreshold, \"over the instant mint threshold\");        require(_value <= instantMintPool, \"instant mint pool is dry\");        instantMintPool = instantMintPool.sub(_value);        emit InstantMint(_to, _value, msg.sender);        token.<mask0>;    }"