"function buyTokens(address contributor) payable {        uint256 amount = calcAmount();        require(contributor != 0x0) ;        require(minimal_token_sell < amount);        require((token.totalSupply() + amount) <= maximumCap);        require(validPurchase());        token.<mask0>;        TokenPurchase(0x0, contributor, msg.value, amount);        Transfer(0x0, contributor, amount);        wallet.transfer(msg.value);    }"