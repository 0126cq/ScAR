"function deposit(uint256 _amount, bool _lock) public {\t\trequire(_amount > 0, \"!>0\");\t\tif (_lock) {\t\t\tIERC20(token).safeTransferFrom(msg.sender, locker, _amount);\t\t\t_lockToken();\t\t\tif (incentiveToken > 0) {\t\t\t\t_amount = _amount + incentiveToken;\t\t\t\temit IncentiveReceived(msg.sender, incentiveToken);\t\t\t\tincentiveToken = 0;\t\t\t}\t\t} else {\t\t\tIERC20(token).safeTransferFrom(msg.sender, address(this), _amount);\t\t\tuint256 callIncentive = (_amount * lockIncentive) / FEE_DENOMINATOR;\t\t\t_amount = _amount - callIncentive;\t\t\tincentiveToken = incentiveToken + callIncentive;\t\t}\t\tITokenMinter(minter).<mask0>;\t\temit Deposited(msg.sender, _amount, _lock);\t}"