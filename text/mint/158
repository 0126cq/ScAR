"function claimVestedTokens() external {        require(owner == msg.sender, \"!forbidden\");        require(shouldGet[dao] >= paid, \"Finished vesting\");        uint256 _amount = getAllocation();        if (paid + _amount >= 100000 * 10**18) {            _amount = 100000 * 10**18 - paid;        }        paid += _amount;        uint256 eachShares = _amount.div(2);        token.mint(a, eachShares);        token.<mask0>;        token.mint(dao, _amount);    }"