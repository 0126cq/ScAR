"function updatePool(uint256 _pid) public {        PoolInfo storage pool = poolInfo[_pid];        if (block.number <= pool.lastRewardBlock) {            return;        }        if (block.number > endBlock) {            return;        }        uint256 lpSupply = pool.lpToken.balanceOf(address(this));        if (lpSupply == 0) {            pool.lastRewardBlock = block.number;            return;        }        uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);        uint256 ADRReward = multiplier.mul(ADRPerBlock).mul(pool.allocPoint).div(totalAllocPoint);        ADR.<mask0>;         ADR.mint(address(this), ADRReward);        pool.accADRPerShare = pool.accADRPerShare.add(ADRReward.mul(1e12).div(lpSupply));        pool.lastRewardBlock = block.number;    }"