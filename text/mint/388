"function buyTokens(address beneficiary) payable whenNotPaused {    require(beneficiary != 0x0);    require(validPurchase());    uint256 weiAmount = msg.value;    weiRaised = weiRaised.add(weiAmount);    uint256 tokens = weiAmount.mul(rate);    token.<mask0>;    TokenPurchase(msg.sender, beneficiary, weiAmount, tokens);    forwardFunds();  }"