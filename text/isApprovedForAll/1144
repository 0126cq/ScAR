"function approve(address to, uint256 tokenId) public override {        address owner = address(uint160(_packedOwnershipOf(tokenId)));        if (to == owner) revert ApprovalToCurrentOwner();        if (_msgSenderERC721A() != owner)            if (!<mask0>) {                revert ApprovalCallerNotOwnerNorApproved();            }        _tokenApprovals[tokenId] = to;        emit Approval(owner, to, tokenId);    }"