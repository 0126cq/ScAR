"function burn(        address _owner,        uint256 _tokenId,        uint256 _amount    ) public returns (bool) {        require(            _owner == _msgSender() ||                <mask0> == true,            \"Need operator approval for 3rd party burns.\"        );        require(transferBalance(_tokenId, _owner) >= _amount);        _burn(_owner, _tokenId, _amount);        return true;    }"