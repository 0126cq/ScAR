"function approve(address to, uint256 tokenId) public override {        address owner = ERC721A.ownerOf(tokenId);        require(to != owner, 'ERC721A: approval to current owner');        require(            _msgSender() == owner || <mask0>,            'ERC721A: approve caller is not owner nor approved for all'        );        _approve(to, tokenId, owner);    }"