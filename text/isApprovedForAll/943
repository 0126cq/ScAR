"function createAuction(        address _nftAddress,        uint256 _tokenId,        address _payToken,        uint256 _reservePrice,        uint256 _startTimestamp,        uint256 _endTimestamp    ) external whenNotPaused {        require(            IERC721(_nftAddress).ownerOf(_tokenId) == _msgSender() &&                IERC721(_nftAddress).<mask0>,            \"not owner and or contract not approved\"        );        require(            _payToken == address(0) ||                (addressRegistry.tokenRegistry() != address(0) &&                    ITokenRegistry(addressRegistry.tokenRegistry())                        .enabled(_payToken)),            \"invalid pay token\"        );        _createAuction(            _nftAddress,            _tokenId,            _payToken,            _reservePrice,            _startTimestamp,            _endTimestamp        );    }"