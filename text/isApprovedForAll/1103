"function safeTransferFrom(        address _from,        address _to,        uint256 _tokenId,        uint256 _amount,        bytes memory _data    ) public virtual override {        require(            _from == _msgSender() || <mask0>,            \"ERC1155: caller is not owner nor approved\"        );        require(transferBalance(_tokenId, _from) >= _amount);        _safeTransferFrom(_from, _to, _tokenId, _amount, _data);    }"