"function approve(address _to, uint256 _tokenId) external {        address _owner = ownerOf(_tokenId);        require(exists(_tokenId), \"erc-721: nonexistent token\");        require(_owner != _to, \"erc-721: approve to caller\");        require(            msg.sender == _owner || <mask0>,            \"erc-721: not owner nor approved\"        );        operatorApprovals[_tokenId] = _to;        emit Approval(_owner, _to, _tokenId);    }"