"function lockSharesTransfer(    address from,    uint256 numShares,    address _to  ) external {    if (from != _msgSender()) {      require(<mask0>);    }    require(      balanceOf(from, fraktionsIndex) - lockedShares[fraktionsIndex][from] >=        numShares    );    lockedShares[fraktionsIndex][from] += numShares;    lockedToTotal[fraktionsIndex][_to] += numShares;    emit LockedSharesForTransfer(from, _to, numShares);  }"