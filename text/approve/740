"function depositETH(uint256 _pid,        address token,        uint amountTokenDesired,        uint amountTokenMin,        uint amountETHMin) public payable {        uint _amount;        address pair = getPair(token, WETH);        PoolInfo storage pool = poolInfo[_pid];        require(pair == address(pool.lpToken), \"wrong pid\");        updatePool(_pid);        if (amountTokenDesired != 0) {            (, , _amount) = addLiquidityETH(token, amountTokenDesired, amountTokenMin, amountETHMin, address(this));            pool.lpToken.<mask0>;            sushiChef.deposit(pool.sushiChefPid, _amount);        }        deposit(_pid, _amount);    }"