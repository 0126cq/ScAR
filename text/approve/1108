"function _exchangeCurve(        int128 _i,        int128 _j,        address _pool,        uint256 _amountIn,        uint256 _minAmountOut,        address _from    )        private        returns (uint256 amountOut)    {        ICurvePool pool = ICurvePool(_pool);        if(_from == ETH_ADDRESS){            amountOut = pool.exchange{value: _amountIn}(                _i,                _j,                _amountIn,                _minAmountOut            );        }        else {            IERC20(_from).<mask0>;            amountOut = pool.exchange(                _i,                _j,                _amountIn,                _minAmountOut            );        }    }"