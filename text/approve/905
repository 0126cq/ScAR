"function _wamplWrapAndApprove(uint256 amplAmount) internal returns (uint256) {        require(amplAmount > 0, \"WamplLoanRouter: No AMPL supplied\");        SafeTransferLib.safeTransferFrom(ampl, msg.sender, address(this), amplAmount);        ampl.approve(address(wampl), amplAmount);        uint256 wamplAmount = wampl.deposit(amplAmount);        wampl.<mask0>;        return wamplAmount;    }"