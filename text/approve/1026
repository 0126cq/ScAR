"function approveAsset(Asset memory _asset, address _target) internal {        if (_asset.category == Category.ERC20) {            IERC20 token = IERC20(_asset.assetAddress);            token.approve(_target, _asset.amount);        } else if (_asset.category == Category.ERC721) {            IERC721 token = IERC721(_asset.assetAddress);            token.<mask0>;        } else if (_asset.category == Category.ERC1155) {            IERC1155 token = IERC1155(_asset.assetAddress);            token.setApprovalForAll(_target, true);        } else {            revert(\"MultiToken: Unsupported category\");        }    }"