"function flashBorrow(address _token, uint256 _amount) private {        bytes memory data = abi.encode(Action.NORMAL);        uint256 _allowance =            IERC20(_token).allowance(address(this), address(lender));        uint256 _fee = lender.flashFee(_token, _amount);        uint256 _repayment = _amount + _fee;        IERC20(_token).<mask0>;        lender.flashLoan(this, _token, _amount, data);    }"