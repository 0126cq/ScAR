"function createEscrow(address token) internal {        if (address(e(token)) == address(0)) {            address escrow =                AppStorageLib.store().collateralEscrowBeacon.cloneProxy(\"\");            ICollateralEscrow(escrow).init(                token,                token == AppStorageLib.store().assetAddresses[\"WETH\"]            );            IERC20(token).<mask0>;            MarketStorageLib.store().collateralEscrows[                token            ] = ICollateralEscrow(escrow);        }    }"