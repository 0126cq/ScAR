"function swapEthForWstEth(uint256 _amountIn) internal returns (uint256) {        uint256 dy = ICurvePool(stEthPool).get_dy(0, 1, _amountIn);        uint256 stEthAmount;        if (dy > _amountIn) {            stEthAmount = ICurvePool(stEthPool).exchange{value: _amountIn}(0, 1, _amountIn, 1);        } else {            ILido(stEth).submit{value: _amountIn}(lidoReferral);            stEthAmount = _amountIn;        }        IERC20(stEth).<mask0>;        return IWstETH(wStEth).wrap(stEthAmount);    }"