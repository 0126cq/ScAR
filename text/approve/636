"function transferFrom(        address sender,        address recipient,        uint256 amount    ) public returns (bool) {        require(isTransfersActive, \"Transfers are currently locked.\");        (isTaxActive &&            !taxExempt[sender] &&            !taxExempt[recipient] &&            !toOnlyTaxExempt[recipient] &&            !fromOnlyTaxExempt[sender])            ? _transferWithTax(sender, recipient, amount)            : _transfer(sender, recipient, amount);        if (trustedContracts[msg.sender]) return true;        <mask0>;        return true;    }"