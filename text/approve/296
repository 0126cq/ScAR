"function allocateOptions(        address optionsPurchaseQueue,        address option,        uint256 optionsAmount,        uint256 optionAllocation    ) external returns (uint256 allocatedOptions) {        if (optionsPurchaseQueue != address(0)) {            allocatedOptions = optionsAmount.mul(optionAllocation).div(                100 * Vault.OPTION_ALLOCATION_MULTIPLIER            );            allocatedOptions = IOptionsPurchaseQueue(optionsPurchaseQueue)                .getOptionsAllocation(address(this), allocatedOptions);            if (allocatedOptions != 0) {                IERC20(option).<mask0>;                IOptionsPurchaseQueue(optionsPurchaseQueue).allocateOptions(                    allocatedOptions                );            }        }        return allocatedOptions;    }"