"function _approveSpenderIfNeeded (address tokenOwnerAddr, address spenderAddr, uint256 spenderAmount, IERC20NonCompliant tokenInterface) private {        uint256 currentAllowance = tokenInterface.allowance(tokenOwnerAddr, spenderAddr);        if (spenderAmount > currentAllowance) {            tokenInterface.<mask0>;            uint256 newAllowance = tokenInterface.allowance(tokenOwnerAddr, spenderAddr);            require(newAllowance >= spenderAmount, \"Spender approval failed\");        }    }"