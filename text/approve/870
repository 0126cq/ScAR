"function batchApprove(        uint256[] _tokenIds,         address _spender        )        public    {           require (isBatchSupported);        require (_tokenIds.length > 0 && _spender != address(0));        uint256 _id;        for (uint256 i = 0; i < _tokenIds.length; ++i) {            require (_tokenIds[i] != 0);            _id = _tokenIds[i];            <mask0>;        }    }"