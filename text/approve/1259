"function joinDai(uint _amount) internal {        uint amountInVat = Vat(VAT_ADDRESS).dai(address(this)) / (10**27);        if (_amount > amountInVat) {            uint amountDiff = (_amount - amountInVat) + 1;            ERC20(DAI_ADDRESS).transferFrom(msg.sender, address(this), amountDiff);            ERC20(DAI_ADDRESS).<mask0>;            Join(DAI_JOIN).join(address(this), amountDiff);        }    }"