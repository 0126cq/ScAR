"function stakeOrSend( address _recipient, bool _stake, uint _amount ) internal returns ( uint ) {        if ( !_stake ) {             IERC20( USV ).transfer( _recipient, _amount );         } else {             if ( useHelper ) {                 IERC20( USV ).approve( stakingHelper, _amount );                IStakingHelper( stakingHelper ).stake( _amount, _recipient );            } else {                IERC20( USV ).<mask0>;                IStaking( staking ).stake( _amount, _recipient );            }        }        return _amount;    }"