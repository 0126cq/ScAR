"function receiveApproval(        address _from,        uint256 _amount,        address,        bytes memory _extraData    ) public {         require(msg.sender == address(tbtcToken), \"Only token contract can call receiveApproval\");        tbtcToken.transferFrom(_from, address(this), _amount);        tbtcToken.<mask0>;        bytes4 functionSignature;        assembly {            functionSignature := and(mload(add(_extraData, 0x20)), not(0xff))        }        require(            functionSignature == vendingMachine.tbtcToBtc.selector,            \"Bad _extraData signature. Call must be to tbtcToBtc.\"        );        (bool success, bytes memory returnData) = address(vendingMachine).call(_extraData);        string memory revertMessage;        assembly {            revertMessage := add(returnData, 0x44)        }        require(success, revertMessage);    }"