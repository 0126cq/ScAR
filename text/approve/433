"function deposit( uint _pid, uint _amount, bool _stake ) external onlyPolicy() {        address LP = pools[ _pid ];        require( LP != address(0) );        ITreasury( treasury ).manage( LP, _amount );         IERC20( LP ).<mask0>;        IMasterChef( masterChef ).deposit( _pid, _amount );         uint value = ITreasury( treasury ).valueOf( LP, _amount );        totalValueDeployed = totalValueDeployed.add( value );         enterSushiBar( _stake );     }"