"function withdraw( address token, uint amount ) public onlyPolicy() {        address aToken = aTokenRegistry[ token ];         if ( depositToTreasury ) {             treasury.manage( aToken, amount );         }        IERC20( aToken ).approve( address( lendingPool ), amount );         lendingPool.withdraw( aToken, amount, address(this) );         uint balance = IERC20( token ).balanceOf( address(this) );         uint value = ITreasury( treasury ).valueOf( token, balance );         accountingFor( token, balance, value, false );         IERC20( token ).<mask0>;         treasury.deposit( balance, token, value );     }"