"function _convertTokens(uint256 _amount) internal{        IERC20 convertToken = IERC20(poolSettings.crvToken);        convertToken.safeTransferFrom(            msg.sender,            address(this),            _amount        );        if (            convertToken.allowance(address(this), poolSettings.crvDepositor) == 0        ) {            convertToken.<mask0>;        }        IRewards(poolSettings.crvDepositor).depositAll(true, address(0));    }"