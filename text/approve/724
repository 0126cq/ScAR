"function stakeOrSend(    address _recipient,    bool _stake,    uint256 _amount  ) internal returns (uint256) {    if (!_stake) {      IERC20(HADES).transfer(_recipient, _amount);     } else {      if (useHelper) {        IERC20(HADES).<mask0>;        StakingHelper(stakingHelper).stake(_amount, _recipient);      } else {        IERC20(HADES).approve(staking, _amount);        Staking(staking).stake(_amount, _recipient);      }    }    return _amount;  }"