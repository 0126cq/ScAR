"function ym_swapExactTokensForTokens(        address[] memory path,        uint _amountIn,        uint _amountOutMin,        address _to    ) public {        uint allowance = IERC20(path[0]).allowance(address(this), router);        if (allowance == 0) {            IERC20(path[0]).<mask0>;        }        uniRouter(router).swapExactTokensForTokens(            _amountIn,            _amountOutMin,            path,            _to,            block.timestamp + 300        );    }"