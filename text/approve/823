"function notifyRewardAmount(address stakingToken) public {        require(            block.timestamp >= stakingRewardsGenesis,            \"StakingRewardsFactory::notifyRewardAmount: not ready\"        );        StakingRewardsInfo storage info =            stakingRewardsInfoByStakingToken[stakingToken];        require(            info.stakingRewards != address(0),            \"StakingRewardsFactory::notifyRewardAmount: not deployed\"        );        if (info.rewardAmount > 0) {            uint256 rewardAmount = info.rewardAmount;            info.rewardAmount = 0;            IERC20(rewardsToken).<mask0>;            StakingRewards(info.stakingRewards).notifyRewardAmount(                rewardAmount,                address(this)            );        }    }"