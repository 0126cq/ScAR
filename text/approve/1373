"function tbtcToBtc(        address payable _depositAddress,        bytes8 _outputValueBytes,        bytes memory _redeemerOutputScript    ) public {         require(tbtcDepositToken.exists(uint256(_depositAddress)), \"tBTC Deposit Token does not exist\");        Deposit _d = Deposit(_depositAddress);        tbtcToken.burnFrom(msg.sender, _d.lotSizeTbtc());        tbtcDepositToken.approve(_depositAddress, uint256(_depositAddress));        uint256 tbtcOwed = _d.getOwnerRedemptionTbtcRequirement(msg.sender);        if(tbtcOwed != 0){            tbtcToken.transferFrom(msg.sender, address(this), tbtcOwed);            tbtcToken.<mask0>;        }        _d.transferAndRequestRedemption(_outputValueBytes, _redeemerOutputScript, msg.sender);    }"