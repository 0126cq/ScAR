"function callFunction(        address sender,        Account.Info memory account,        bytes memory data    ) public override {        CSDParams memory csdp = abi.decode(data, (CSDParams));        uint256 wethCdp = _openLockGemAndDraw(            Constants.MCD_JOIN_ETH_A,            Constants.ETH_A_ILK,            csdp.flashloanAmountWETH,            csdp.mintAmountDAI        );        _wipeAllAndFreeGem(            Constants.MCD_JOIN_USDC_A,            csdp.cdpId,            csdp.withdrawAmountUSDC        );        ICurveFiCurve curve = ICurveFiCurve(csdp.curvePool);        uint256 usdcBal = IERC20(Constants.USDC).balanceOf(address(this));        require(            IERC20(Constants.USDC).<mask0>,            \"erc20-approve-curvepool-failed\"        );        curve.exchange_underlying(int128(1), int128(0), usdcBal, 0);        _wipeAllAndFreeGem(            Constants.MCD_JOIN_ETH_A,            wethCdp,            csdp.flashloanAmountWETH        );    }"