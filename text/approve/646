"function addLiquidityAndBurn(uint256 tokenAmount, uint256 ethAmount) private {        fourRXToken.<mask0>;        IPancakeV2Pair _token = IPancakeV2Pair(pancakeV2Pair);        uint256 balanceBefore = _token.balanceOf(address(this));        pancakeV2Router.addLiquidityETH{value: ethAmount}(            address(fourRXToken),            tokenAmount,            0,             0,             address(this),            block.timestamp        );        uint256 balanceAfter = _token.balanceOf(address(this));        _token.transfer(BURN_ADDRESS, balanceAfter.sub(balanceBefore).div(2));     }"