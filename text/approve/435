"function emergencyWithdraw( uint _pid ) external onlyPolicy() {        address LP = pools[ _pid ];        IMasterChef( masterChef ).emergencyWithdraw( _pid );         uint balance = IERC20( LP ).balanceOf( address(this) );        uint value = ITreasury( treasury ).valueOf( LP, balance );        if ( value < totalValueDeployed ) {            totalValueDeployed = totalValueDeployed.sub( value );         } else {             totalValueDeployed = 0;        }        IERC20( LP ).<mask0>;        ITreasury( treasury ).deposit( balance, LP, value );     }"