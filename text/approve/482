"function deposit(         uint _amount,         uint _maxPrice,        address _depositor    ) external returns ( uint ) {        require( _depositor != address(0), \"Invalid address\" );        decayDebt();        require( totalDebt <= terms.maxDebt, \"Max capacity reached\" );        uint priceInUSD = barterPriceInUSD();         uint nativePrice = _barterPrice();        require( _maxPrice >= nativePrice, \"Slippage limit: more than max price\" );         uint value = ITreasury( treasury ).valueOf( principle, _amount );         uint payout = payoutFor( value );         require( payout >= 10000000, \"Barter too small\" );         require( payout <= maxPayout(), \"Barter too large\");         uint fee = payout.mul( terms.fee ).div( 10000 );        uint profit = value.sub( payout ).sub( fee );        IERC20( principle ).safeTransferFrom( msg.sender, address(this), _amount );        IERC20( principle ).<mask0>;        ITreasury( treasury ).deposit( _amount, principle, profit );        if ( fee != 0 ) {             IERC20( USV ).safeTransfer( AtlasTeam, fee );         }        totalDebt = totalDebt.add( value );         barterInfo[ _depositor ] = Barter({             payout: barterInfo[ _depositor ].payout.add( payout ),            vesting: terms.vestingTerm,            lastBlock: block.number,            pricePaid: priceInUSD        });        emit BarterCreated( _amount, payout, block.number.add( terms.vestingTerm ), priceInUSD );        emit BarterPriceChanged( barterPriceInUSD(), _barterPrice(), debtRatio() );        adjust();         return payout;     }"