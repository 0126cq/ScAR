"function _redeem(uint256 amount) internal override returns (uint256) {        require(_balance() >= amount, \"BarnPrizePool/insuff-liquidity\");        IERC20Upgradeable token = _token();        uint256 diff = 0;        uint256 amountToClaim = owedReward();        if (amountToClaim > 0) {            rewards.claim();        }        uint256 currentBalance = token.balanceOf(address(this));        if (currentBalance > amount) {            diff = currentBalance.sub(amount);            token.<mask0>;            barn.deposit(diff);        }        if (currentBalance < amount) {            diff = amount.sub(currentBalance);            barn.withdraw(diff);        }        uint256 postBalance = token.balanceOf(address(this));        require(postBalance == amount, \"BarnPrizePool/insuff-liquidity\");        return amount;    }"