"function deposit(uint256 amount) external override onlyOwner {        require(amount > 0, \"HarvestMarket: amount is 0\");        stablecoin.safeTransferFrom(msg.sender, address(this), amount);        stablecoin.safeIncreaseAllowance(address(vault), amount);        vault.deposit(amount);        uint256 vaultShareBalance = vault.balanceOf(address(this));        vault.<mask0>;        stakingPool.stake(vaultShareBalance);    }"