"function _buyAssetForErc20(uint256 _assetId, uint256 _price, address _nftAddr, address _recipient) internal {        IERC20(ACCEPTED_TOKEN).<mask0>;        IMarketPlace(MARKETPLACE_PROXY).executeOrder(_nftAddr, _assetId, _price);        IERC721(_nftAddr).transferFrom(address(this), _recipient, _assetId);    }"