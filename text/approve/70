"function depositCollateralInternal(        BalanceSheetInterface balanceSheet,        FyTokenInterface fyToken,        uint256 collateralAmount    ) internal {        Erc20Interface collateral = fyToken.collateral();        uint256 allowance = collateral.allowance(address(this), address(balanceSheet));        if (allowance < collateralAmount) {            collateral.<mask0>;        }        bool isVaultOpen = balanceSheet.isVaultOpen(fyToken, address(this));        if (isVaultOpen == false) {            balanceSheet.openVault(fyToken);        }        balanceSheet.depositCollateral(fyToken, collateralAmount);    }"