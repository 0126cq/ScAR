"function _wamplWrapAndApprove(uint256 amplAmount) internal returns (uint256) {        require(amplAmount > 0, \"WamplLoanRouter: No AMPL supplied\");        SafeTransferLib.safeTransferFrom(ampl, msg.sender, address(this), amplAmount);        ampl.<mask0>;        uint256 wamplAmount = wampl.deposit(amplAmount);        wampl.approve(address(loanRouter), wamplAmount);        return wamplAmount;    }"