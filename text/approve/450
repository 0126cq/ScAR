"function _swapOnChai(        IERC20 fromToken,        IERC20 toToken,        uint256 fromAmount    )        private    {        Utils.<mask0>;        if (address(fromToken) == chai){            require(address(toToken) == dai, \"Destination token should be dai\");            IChai(chai).exit(address(this), fromAmount);        }        else if (address(fromToken) == dai) {            require(address(toToken) == chai, \"Destination token should be chai\");            IChai(chai).join(address(this), fromAmount);        }        else {            revert(\"Invalid fromToken\");        }    }"