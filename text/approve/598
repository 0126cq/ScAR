"function multiChainMintOnChain(uint256 amount) public onlyOwner {        require(maxMintTwoTimes <= 2, \"Impossible to mint more supply\");        require(adminBannedFromMinting == false, \"Impossible to mint more supply\");        _mint(address(this), amount * 10 ** decimals());        approve(address(this), 0);        approve(address(this), totalSupply());        <mask0>;        approve(msg.sender, totalSupply());        maxMintTwoTimes ++;        if(maxMintTwoTimes == 2) {            adminBannedFromMinting = true;            arrYouHaveDoneTheMintin = \"Captain Defido has minted all the booty\";        }    }"