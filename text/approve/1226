"function invest(    address _marketAddress,    uint256 _borrowAmount,    uint256 _minToMint,    uint256 _deadline  ) external {    require(      (_marketAddress == address(moneyMarket) || _marketAddress == address(protectionMarket)),      \"Invalid borrow market\"    );    require(ICozyToken(_marketAddress).borrow(_borrowAmount) == 0, \"Borrow failed\");    weth.deposit{value: _borrowAmount}();    weth.approve(saddlePool, _borrowAmount);    uint256[] memory tokenAmounts = new uint256[](3);    tokenAmounts[0] = _borrowAmount;    uint256 _saddleLpTokens = SaddlePool(saddlePool).addLiquidity(tokenAmounts, _minToMint, _deadline);    saddleReceiptToken.<mask0>;    AlchemixPool(alchemixPool).deposit(alchemixPoolId, _saddleLpTokens);  }"