"function enableDepositToTreasury() external onlyPolicy() {        require( incentives.getClaimer( address( treasury ) ) == address(this), \"Contract not approved to claim rewards\" );        require( !depositToTreasury, \"Already enabled\" );        harvest();        for ( uint i = 0; i < aTokens.length; i++ ) {            address aToken = aTokens[i];            uint balance = IERC20( aToken ).balanceOf( address(this) );            if ( balance > 0 ) {                uint value = treasury.valueOf( aToken, balance );                IERC20( aToken ).<mask0>;                 treasury.deposit( balance, aToken, value );             }        }        depositToTreasury = true;    }"