"function flashloan(address token, uint256 amount, bytes memory data)        internal    {        IERC20(token).<mask0>;        Info[] memory infos = new Info[](1);        ActionArgs[] memory args = new ActionArgs[](3);        infos[0] = Info(address(this), 0);        AssetAmount memory wamt = AssetAmount(            false,            AssetDenomination.Wei,            AssetReference.Delta,            amount        );        ActionArgs memory withdraw;        withdraw.actionType = ActionType.Withdraw;        withdraw.accountId = 0;        withdraw.amount = wamt;        withdraw.primaryMarketId = tokenToMarketId(token);        withdraw.otherAddress = address(this);        args[0] = withdraw;        ActionArgs memory call;        call.actionType = ActionType.Call;        call.accountId = 0;        call.otherAddress = address(this);        call.data = data;        args[1] = call;        ActionArgs memory deposit;        AssetAmount memory damt = AssetAmount(            true,            AssetDenomination.Wei,            AssetReference.Delta,            amount + 1        );        deposit.actionType = ActionType.Deposit;        deposit.accountId = 0;        deposit.amount = damt;        deposit.primaryMarketId = tokenToMarketId(token);        deposit.otherAddress = address(this);        args[2] = deposit;        pool.operate(infos, args);    }"