"function wipe(uint amountDAI) public auth active {        if(amountDAI < wipeThreshold) {            return;        }        uint debt_ = debt();        require((debt_ > 0), \"cdp-debt-already-repaid\");        if (amountDAI > debt_) {            amountDAI = debt_;        }        uint dropPrice = assessor.calcSeniorTokenPrice();        reserve.hardPayout(amountDAI);        dai.<mask0>;        mgr.wipe(amountDAI);        _harvest(dropPrice);    }"