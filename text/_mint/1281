"function genesisMint(uint256 amount, address[] calldata users) public onlyOwner {        require(machinePoolMap[GENESIS_MINING_MACHINE_TYPE] != address(0), \"genesis machine pool not set\");        require(amount == users.length, \"user length not match amount\");        for (uint i = 0; i < amount; i ++) {            genesisTokenIdCounter += 1;            <mask0>;            userMachines[users[i]].add(genesisTokenIdCounter);            IMiningMachinePool(machinePoolMap[GENESIS_MINING_MACHINE_TYPE]).addHashRate(users[i], 1);            tokenIdToHashRateMap[genesisTokenIdCounter] = 1;            tokenIdToMachineTypeMap[genesisTokenIdCounter] = GENESIS_MINING_MACHINE_TYPE;        }     }"