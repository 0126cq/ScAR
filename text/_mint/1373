"function _mintAvatar(uint8[] calldata genders) internal {\t\trequire(_publicTokenIdTracker + genders.length <= MINT_MAX, \"Exceed max supply\");\t\trequire(genders.length > 0 && genders.length <= 5, \"Can only mint 1 to 5 avatar at a time\");\t\trequire(!_isContract(_msgSender()), \"Caller cannot be contract\");\t\tfor (uint8 i = 0; i < genders.length; i++){\t\t\trequire(genders[i] == 0 || genders[i] == 1, \"Gender is 0 or 1\");\t\t\t<mask0>;\t\t\t_avatarAttributes[_publicTokenIdTracker] = _createAvatarAttributes(genders[i], i);\t\t\t_publicTokenIdTracker += 1;\t\t}\t}"