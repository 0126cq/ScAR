"function publicMint(uint amount) external payable {        require(isPublicMint == true, \"Minting isn't public\");        require(paused == false, \"Minting is paused\");        require(amount + numPublicMinted <= maxPublicMint, \"Trying to mint more than max\");        require(amount < maxMintPerTxn, \"Cant mint as much per txn\");        require(msg.value == price * amount, \"Invalid amount sent\");        numPublicMinted += amount;        <mask0>;    }"