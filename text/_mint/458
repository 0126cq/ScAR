"function _deposit(        address fromAccount,        address toAccount,        uint256 amount    ) internal {        require(amount > 0, \"INVALID_AMOUNT\");        require(toAccount != address(0), \"INVALID_ADDRESS\");        <mask0>;        underlyer.safeTransferFrom(fromAccount, address(this), amount);        bytes32 eventSig = \"Deposit\";        encodeAndSendData(eventSig, toAccount);    }"