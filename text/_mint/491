"function _mintShortTokenInterval(        PoolStorage.Layout storage l,        address underwriter,        address longReceiver,        uint256 shortTokenId,        Interval memory interval,        bool isCallPool    ) internal {        _reserveApyFee(l, underwriter, shortTokenId, interval.apyFee);        bool isManualUnderwrite = interval.payment ==            interval.tokenAmount + interval.apyFee;        if (!isManualUnderwrite) {            _burn(                underwriter,                _getFreeLiquidityTokenId(isCallPool),                interval.tokenAmount + interval.apyFee - interval.payment            );        }        <mask0>;        _addUserTVL(            l,            underwriter,            isCallPool,            interval.payment - interval.apyFee        );        emit Underwrite(            underwriter,            longReceiver,            shortTokenId,            interval.contractSize,            isManualUnderwrite ? 0 : interval.payment,            isManualUnderwrite        );    }"