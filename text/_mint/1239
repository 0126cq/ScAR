"function mint(uint256 _mintAmount)  external payable {        require(!paused, \"the contract is paused\");        require(_mintAmount > 0, \"need to mint at least 1 NFT\");        require(_mintAmount <= phaseData[phaseId].maxMintAmount, \"max mint amount per session exceeded\");        require(phaseData[phaseId].totalSupply + _mintAmount <= phaseData[phaseId].maxSupply, \"max NFT limit exceeded\");        require(msg.value >= phaseData[phaseId].cost * _mintAmount, \"insufficient funds\");        if(onlyWhitelisted == true) {            require(phaseData[phaseId].whitelistUserAmount[msg.sender] != 0, \"user is not whitelisted\");            require(_mintAmount <= phaseData[phaseId].whitelistUserAmount[msg.sender], \"max NFT per address exceeded\");        }        if(onlyWhitelisted == true) {            phaseData[phaseId].whitelistUserAmount[msg.sender] -= _mintAmount;        }        phaseData[phaseId].totalSupply += _mintAmount;        <mask0>;    }"