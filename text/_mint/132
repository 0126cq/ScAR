"function mint() public payable {        require(mintBalance[msg.sender] < 1, \"Only 1 per account\");        require(msg.sender == tx.origin, \"no.\");        require(balanceOf(msg.sender) != MAX_PER_WALLET, \"Max amount minted\");        require(msg.value == PRICE, \"Price incorrect\");        uint256 tokenId = _tokenIdCounter.current();        _tokenIdCounter.increment();        mintBalance[msg.sender] = mintBalance[msg.sender] + 1;        <mask0>;    }"