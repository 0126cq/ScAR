"function updateSale(uint256 _saleRate, uint256 _saleSupply, bool _burnToken, bool _forSale) external onlyManager {        saleRate = _saleRate;        forSale = _forSale;        if (_saleSupply > 0 && _burnToken) {_burn(address(this), _saleSupply);}        if (_saleSupply > 0 && !_burnToken) {<mask0>;}        if (_forSale) {require(_saleRate > 0, \"_saleRate = 0\");}        emit UpdateSale(_saleRate, _saleSupply, _burnToken, _forSale);    }"