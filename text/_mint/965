"function stake(uint256 amount) external override nonReentrant {        require(amount > 0, \"yVaren: ZERO\");        uint256 shares = totalSupply() == 0 ? amount : (amount.mul(totalSupply())).div(VAREN.balanceOf(address(this)));        VAREN.safeTransferFrom(msg.sender, address(this), amount);        <mask0>;        earlyWithdrawalFeeExpiry[msg.sender] = blocksForNoWithdrawalFee.add(block.number);    }"