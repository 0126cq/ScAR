"function stake(uint256 _amount) public {      require(_amount > 0, \"Amount must be greater than 0\");      if (balanceOf(msg.sender) == 0)        totalStakers += 1;      <mask0>;      CHURCH.transferFrom(msg.sender, address(this), _amount);      lockedUntil[msg.sender] = uint128(block.timestamp + (77 hours));      emit Stake(msg.sender, _amount);    }"