"function enter(uint256 _pspAmount) public whenNotPaused {        uint256 pspBalanceAvailable = psp.balanceOf(address(this)) - pspsLocked;        uint256 totalSPSP = totalSupply();        if (totalSPSP == 0 || pspBalanceAvailable == 0) {            <mask0>;        } else {            uint256 sPSPAmount = (_pspAmount * totalSPSP) / pspBalanceAvailable;            _mint(msg.sender, sPSPAmount);        }        psp.transferFrom(msg.sender, address(this), _pspAmount);        emit Entered(msg.sender, _pspAmount);    }"