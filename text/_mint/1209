"function mintTo(uint16[] calldata quantity, address[] calldata recipient) external payable onlyDelegates{    require(quantity.length == recipient.length, \"must provide equal quantities and recipients\" );    uint totalQuantity;    for(uint i; i < quantity.length; ++i){      totalQuantity += quantity[i];    }    uint supply = totalSupply();    require( supply + totalQuantity < MAX_SUPPLY, \"mint/order exceeds supply\" );    for(uint i; i < recipient.length; ++i){      if( quantity[i] > 0 ){        owners[recipient[i]].balance += quantity[i];        for(uint j; j < quantity[i]; ++j){          <mask0>;        }      }    }  }"