"function    mint() external payable callerIsUser    {        require(block.timestamp              >= salesDate,          \"Sales did not start yet\");        require(msg.value                    >= salesPrice,         \"Send exact Amount to claim your Nft\");        require(walletMintCounts[msg.sender] <= maxMintPerWallet,   \"You cannot claim more tokens\");        require(leftTokenCount               >  0,                  \"No tokens left to be claimed\");        walletMintCounts[msg.sender]++;        mintedTokenCount++;        leftTokenCount--;        <mask0>;    }"