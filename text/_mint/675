"function createClan(uint256 colonyId) public nonReentrant {    require(featureFlagCreateClan, \"feature not enabled\");    require(colonyId > 0 && colonyId < 4, \"only 3 colonies ever\");    require(!isClanLeader(_msgSender()), \"clan leader can't create new clan\");    if (!admins[_msgSender()]) {      require(isEntity(_msgSender()), \"must be in a clan\");      uint256 cost = getCreateClanCost();      if (cost > 0) {        oxgnToken.burn(_msgSender(), cost);      }    }    uint256 clanId = clanIdTracker.current();    clanToColony[clanId] = colonyId;    clanIdTracker.increment();    emit ClanCreated(_msgSender(), clanId, colonyId);    clanStructs[_msgSender()].clanId = clanId;    clanStructs[_msgSender()].updateClanTimestamp = block.timestamp;    <mask0>;  }"