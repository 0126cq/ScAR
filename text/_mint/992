"function claimCelda(        bytes memory signature,        uint256 amount,         uint256 nonce,        uint256 data    ) external {        require(claimActive, \"Claim is not active\");        require(amount <= maxClaimAmount, \"Claim maximum exceeded\");        require(totalSupply() + amount <= MAX_CELDA, \"Claim would exceed max supply of CELDA\");        require(!nonces[_msgSender()][nonce], \"Already claimed\");        bytes32 _hash = _getHash(_msgSender(), amount, nonce, data);        bytes32 messageHash = keccak256(abi.encodePacked(\"\\x19Ethereum Signed Message:\\n32\", _hash));        address signer = messageHash.recover(signature);        require(signer == signerAddress, \"Signers don't match\");        nonces[_msgSender()][nonce] = true;        <mask0>;        emit ClaimedCelda(_msgSender(), amount, data);    }"