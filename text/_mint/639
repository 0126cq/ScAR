"function _buyShares(        uint256 _amount,        uint256 _minSharesToMint,        address user    ) private onlyWhenUnlocked returns (uint256) {        require(delegation, \"Delegation is disabled\");        uint256 rate = exchangeRate();        uint256 precision = _getRatePrecision();        uint256 shares = _amount.mul(precision).div(rate);        require(shares >= _minSharesToMint, \"Too much slippage\");        require(unbonds[user].shares == 0, \"Ongoing exit\");        <mask0>;        _amount = rate.mul(shares).div(precision);        stakeManager.updateValidatorState(validatorId, int256(_amount));        activeAmount = activeAmount.add(_amount);        StakingInfo logger = stakingLogger;        logger.logShareMinted(validatorId, user, _amount, shares);        logger.logStakeUpdate(validatorId);        return _amount;    }"