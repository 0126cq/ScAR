"function freeMint() public gate {        require(_claimer[msg.sender] + totalSupply() <= 3333, \"HRF: maximum tokens minted\");        require(_freeMintPaused != true, \"HRF: minting is paused\");        require(_claimer[msg.sender] >= 1, \"HRF: not a claimer or already claimed\");        uint256 _totalAmount = _claimer[msg.sender];        for (uint256 i=0; i < _totalAmount; i++) {            _claimer[msg.sender] -= 1;            <mask0>;        }    }"