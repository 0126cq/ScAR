"function mint(bytes32[] calldata _merkleProof) external {        require(sale, \"Mint not started\");        require(max_supply >= tokenIdCounter, \"Sold out\");        require(!allowlistClaimed[msg.sender], \"Address already claimed\");        require(            MerkleProof.verify(_merkleProof, merkleRoot, keccak256(abi.encodePacked(msg.sender))),            \"Not in allowlist\"        );        allowlistClaimed[msg.sender] = true;        ++tokenIdCounter;        <mask0>;    }"