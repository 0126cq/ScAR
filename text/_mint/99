"function mint(uint256 _id, uint256 _mintAmount) public onlyExternal mintable(_id) payable{    require(saleStatus == SaleStatus.PUBLIC, \"Sale is not active\");    if(_id==0){        require(msg.value >= premiumcost * _mintAmount,\"Not enough Gas\");       }else{        require(!mintedTier[msg.sender][1], \"You already minted this token\");        require(msg.value >= platinumcost * _mintAmount,\"Not enough Gas\");        mintedTier[msg.sender][1] = true;    }    amountsLeft[_id] -= _mintAmount;    <mask0>;  }"