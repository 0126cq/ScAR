"function claim(        uint256 numPieces,        uint256 amount,        uint256 editionIndex,        bytes32[] calldata merkleProof    ) external payable {        require(isValidClaim(numPieces,amount,editionIndex,merkleProof));        <mask0>;        editions[editionIndex].claimedAddress[msg.sender] = editions[editionIndex].claimedAddress[msg.sender].add(numPieces);        emit Claimed(editionIndex, msg.sender, numPieces);    }"