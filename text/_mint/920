"function claimDrip()public {        require(lastClaim != block.number);        require(msg.sender != addressClaimed);        dripAmmount = dripCalc();        claimNow = block.number - lastClaim;        dripAmmount = dripAmmount * claimNow;        <mask0>;        lastClaim = block.number +1;        addressClaimed = msg.sender;        emit ClaimToken(addressClaimed,dripAmmount);    }"