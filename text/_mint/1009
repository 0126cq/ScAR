"function discover(string calldata fragmentSecret) external override {        bytes32 secret = keccak256(abi.encodePacked(fragmentSecret));        bytes16 groupId = _fragmentGroupIds[secret];        _checkFragmentGroupExists(groupId);        FragmentGroup storage fg = _fragmentGroups[groupId];        if (fg.fragmentCount[msg.sender] >= fg.accountCap) revert MintingCapReached(msg.sender, groupId, fg.accountCap);        uint256 sourceOfRandomness = uint256(            keccak256(abi.encodePacked(blockhash(block.number - 1), block.timestamp, msg.sender))        );        uint128 index = uint128(sourceOfRandomness % fg.size);        for (uint128 i = 0; i < fg.size; i++) {            index = (index + 1) % fg.size;            (uint128 supplyLeft, uint128 totalSupply) = fg.supply[index].deconstructFragmentSupply();            if (supplyLeft == 0) continue;            fg.supply[index] = FragmentMiniGameUtils.toFragmentSupply(supplyLeft - 1, totalSupply);            fg.fragmentCount[msg.sender] += 1;            uint256 tokenId = groupId.toFragmentTokenId(index);            <mask0>;            emit FragmentMinted(msg.sender, tokenId);            return;         }        revert AllItemsDiscovered(groupId);    }"