"function mint(uint256 amount) external returns (uint) {    uint256 newCTokens;    if (totalSupply() == 0) {      newCTokens = amount;    } else {      uint256 fractionOfCredit = FixedPoint.calculateMantissa(amount, underlying.balanceOf(address(this)));      newCTokens = FixedPoint.multiplyUintByMantissa(totalSupply(), fractionOfCredit);    }    <mask0>;    require(underlying.transferFrom(msg.sender, address(this), amount), \"could not transfer tokens\");    return 0;  }"