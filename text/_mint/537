"function mintOnDeposit(address _account, uint256 _amount) external onlyLendingPool {        (,        ,        uint256 balanceIncrease,        uint256 index) = cumulateBalanceInternal(_account);        updateRedirectedBalanceOfRedirectionAddressInternal(_account, balanceIncrease.add(_amount), 0);        <mask0>;        emit MintOnDeposit(_account, _amount, balanceIncrease, index);    }"