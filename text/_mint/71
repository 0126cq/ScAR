"function mintCrystalHolders(uint _count, uint _id) external payable returns (uint oldIndex, uint newIndex) {        require(crystal != address(0), \"presale: disabled\");        PresaleConfig memory _cfg = presaleConfig;        require(msg.value == _cfg.price * _count, \"presale: invalid payment amount\");        require(_count > 0 && _count < _cfg.tokensPerCrystal, \"presale: invalid count\");        IERC1155(crystal).safeTransferFrom(msg.sender, address(this), _id, 1, \"\");        return <mask0>;    }"