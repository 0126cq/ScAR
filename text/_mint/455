"function    batchMint(uint256 quantity) external payable callerIsUser    {        require(block.timestamp                         >= salesDate,               \"Sales did not start yet\");        require(msg.value                               == salesPrice * quantity,   \"Send exact Amount to claim your Nfts\");        require(walletMintCounts[msg.sender] + quantity <= maxMintPerWallet,        \"You cannot claim that much tokens\");        require(leftTokenCount                          >= quantity,                \"No tokens left to be claimed\");        for (uint256 i=0; i < quantity; i++)        {            walletMintCounts[msg.sender]++;            mintedTokenCount++;            leftTokenCount--;            <mask0>;        }    }"