"function deposit(uint256 _amount) public returns (uint256) {        uint256 underlyingTotal = balance();        uint256 _before = balance();        underlying.safeTransferFrom(msg.sender, address(this), _amount);        uint256 _after = balance();        _amount = _after.sub(_before);        uint256 shares;        if (totalSupply() == 0) {            shares = _amount;        } else {            shares = (_amount.mul(totalSupply())).div(underlyingTotal);        }        <mask0>;        return shares;    }"