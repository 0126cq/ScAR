"function mint(uint256 amount, uint256 price, uint256 priceusd, string memory trackLink)        external    {        require(msg.sender == admin, \"Not Correct Admin\");        require(amount>0, \"Minimumn quantity allowed is 1\");        require(amount<5000, \"Maximum quantity allowed is 5000\");        nftId=nftId.add(1);        nfts[nftId]._id = nftId;        nfts[nftId]._price = price;        nfts[nftId]._priceUSD = priceusd;        nfts[nftId]._num_records = nfts[nftId]._num_records.add(amount);        nfts[nftId]._purchaseValue = 0;        _tokenURI[nftId] = trackLink;        <mask0>;    }"