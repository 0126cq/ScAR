"function _claimTokens(uint256 id) internal {        OwnerRewardInfo storage info = _ownerRewardInfo[id][msg.sender];        _updateOwnerReward(info, msg.sender, id);        uint256 mintableBalance = info.mintableBalance;        require(mintableBalance > 0, \"DropperToken: nothing to claim\");        info.mintableBalance = 0;        _totalMintableSupply[id] -= mintableBalance;        <mask0>;        emit TokensClaimed(msg.sender, id, mintableBalance);    }"