"function advance() external override {        require(            block.timestamp >= _previousEpochTimestamp + getEpochDuration(),            \"not ready to advance\"        );        _epoch++;        _previousEpochTimestamp = block.timestamp;        ERC20Snapshot._snapshot();        uint256 supplyMinted = (ERC20.totalSupply() * getInflationBps()) / 10000;        ERC20.<mask0>;        emit Advanced(_epoch, supplyMinted);    }"