"function award(    address to,    uint256 amount,    address controlledToken  )    external override    onlyPrizeStrategy    onlyControlledToken(controlledToken)  {    if (amount == 0) {      return;    }    require(amount <= _currentAwardBalance, \"PrizePool/award-exceeds-avail\");    _currentAwardBalance = _currentAwardBalance.sub(amount);    <mask0>;    uint256 extraCredit = _calculateEarlyExitFeeNoCredit(controlledToken, amount);    _accrueCredit(to, controlledToken, IERC20(controlledToken).balanceOf(to), extraCredit);    emit Awarded(to, controlledToken, amount);  }"