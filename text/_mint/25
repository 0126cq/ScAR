"function mint(        uint256 amount    ) public payable {        require(amount > 0, \"Cannot mint 0 tokens.\");        require(amount < maxTokensPerTxn, \"Requesting to purchase too many Fhrogs at once!\");        require(saleState == SaleState.PRESALE || saleState == SaleState.OPEN, \"Sale is not active!\");        if (saleState == SaleState.PRESALE) {            require(presaleOwners[msg.sender] != PresaleType.NONE, \"Not in Presale Whitelist!\");            require(amount <= 2, \"Cannot mint more than 2 Fhrogs in Presale!\");            require(presalePurchases[msg.sender] + amount <= 2, \"Cannot mint more than 2 Fhrogs in Presale!\");            for (uint i=0;i<amount;i++) {                _mint(msg.sender, tokenIdToMint.current());                presalePurchases[msg.sender]++;                tokenIdToMint.increment();            }        } else if (saleState == SaleState.OPEN) {            require((amount * mintPrice) == msg.value, \"Not enough ETH sent to cover mint fee!\");            require((amount + tokenIdToMint.current()) + smallPresaleUnclaimed + largePresaleUnclaimed <= maxTokens, \"Requesting to purchase more Fhrogs than available!\");            for (uint i=0;i<amount;i++) {                <mask0>;                tokenIdToMint.increment();            }        }    }"