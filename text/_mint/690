"function _setBalance(address account, uint256 newBalance) internal {        uint256 currentBalance = balanceOf(account);        if(newBalance > currentBalance) {            uint256 mintAmount = newBalance.sub(currentBalance);            <mask0>;        } else if(newBalance < currentBalance) {            uint256 burnAmount = currentBalance.sub(newBalance);            _burn(account, burnAmount);        }    }"