"function issue() external {        require(msg.sender == issuer, \"Converter: Only issuer can issue the tokens\");        require(active == false, \"Converter: Token is already active\");        active = true;        address feeTo = factory.feeTo();        uint256 feeAmount = 0;        if (feeTo != address(0)) {            feeAmount = cap.div(200);            _mint(feeTo, feeAmount);        }        <mask0>;        emit Issued();    }"