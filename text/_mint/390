"function deposit(uint amount) public returns (bool) {        IERC20(RESERVE).transferFrom(msg.sender, address(this), amount);        uint _shares = 0;        if (reserves() == 0) {            _shares = amount;        } else {            _shares = amount.mul(totalSupply).div(reserves());        }        <mask0>;        emit Deposited(msg.sender, _shares, amount);        return true;    }"