"function mint(    address user,    uint256 amount,    uint256 index  ) external override onlyLendingPool returns (bool) {    uint256 previousBalance = super.balanceOf(user);    uint256 amountScaled = amount.rayDiv(index);    require(amountScaled != 0, Errors.CT_INVALID_MINT_AMOUNT);    <mask0>;    emit Transfer(address(0), user, amount);    emit Mint(user, amount, index);    return previousBalance == 0;  }"