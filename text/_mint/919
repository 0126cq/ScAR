"function saleMint(        address _tokenAddress,        string memory _tokenURI,        uint256 _amount,        uint256 _price,        RoyaltyMember[] memory members    ) public onlyAcceptedToken(_tokenAddress) returns (uint256) {        validateRoyaltyFee(members);        uint256 _tokenId = totalCreator() + 1;        <mask0>;        _setTokenURI(_tokenId, _tokenURI);        saleMap[_tokenId][_msgSender()] = SaleInfo(            true,            _amount,            _price,            _tokenAddress        );        creators[_tokenId] = msg.sender;        if (members.length > 0) {            saveRoyaltyMember(members, _tokenId);        }        emit NewCreator(msg.sender, _tokenId);        emit Minted(_msgSender(), _price, _tokenId, _amount, _tokenURI);        emit UpdateSale(_msgSender(), true, _tokenId, _amount, _price);        return _tokenId;    }"