"function mintOne(bytes calldata _signature, uint256 spotId)        external        payable    {        uint256 _nextTokenId = nextTokenId;        if (!saleActive) revert SaleInactive();        if (_nextTokenId > SUPPLY) revert SoldOut();        if (msg.value != price) revert InvalidPrice();        _claimAllowlistSpot(_signature, spotId);        <mask0>;        unchecked {            _nextTokenId++;        }        nextTokenId = _nextTokenId;    }"