"function claimGenesis(uint256[] memory avatarIds) public {        for (uint i = 0; i < avatarIds.length; i ++) {            require(unClaimedAvatar[avatarIds[i]], \"avatar claimed\");            require(IERC721Upgradeable(avatar).ownerOf(avatarIds[i]) == msg.sender, \"msg.sender is not the owner of avatar\");            genesisTokenIdCounter += 1;            <mask0>;            userMachines[msg.sender].add(genesisTokenIdCounter);            IMiningMachinePool(machinePoolMap[GENESIS_MINING_MACHINE_TYPE]).addHashRate(msg.sender, 1);            unClaimedAvatar[avatarIds[i]] = false;            claimedAvatar[avatarIds[i]] = true;            tokenIdToHashRateMap[genesisTokenIdCounter] = 1;            tokenIdToMachineTypeMap[genesisTokenIdCounter] = GENESIS_MINING_MACHINE_TYPE;        }    }"