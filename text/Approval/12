"function decreaseApproval(address _spender, uint _subtractedValue) public returns (bool success) {        uint oldValue = allowed[msg.sender][_spender];        if (_subtractedValue > oldValue) {            allowed[msg.sender][_spender] = 0;        } else {            allowed[msg.sender][_spender] = oldValue - _subtractedValue;        }        <mask0>;        return true;    }"