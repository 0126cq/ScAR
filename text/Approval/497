"function approve(address _to, uint256 _itemId) public {    require(msg.sender != _to);    require(tokenExists(_itemId));    require(ownerOf(_itemId) == msg.sender);    if (_to == 0) {      if (approvedOfItem[_itemId] != 0) {        delete approvedOfItem[_itemId];        <mask0>;      }    } else {      approvedOfItem[_itemId] = _to;      Approval(msg.sender, _to, _itemId);    }  }"