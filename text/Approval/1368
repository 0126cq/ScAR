"function approve(address _spender, uint256 _amount) public returns (bool success) {        require(transfersEnabled);        require((_amount == 0) || (allowed[msg.sender][_spender] == 0));        if (isContract(controller)) {            require(ITokenController(controller).onApprove(msg.sender, _spender, _amount) == true);        }        allowed[msg.sender][_spender] = _amount;        <mask0>;        return true;    }"