"function approve(address _spender, uint256 _amount) returns (bool success) {        if (!transfersEnabled) throw;        if ((_amount!=0) && (allowed[msg.sender][_spender] !=0)) throw;        if (isContract(controller)) {            if (!TokenController(controller).onApprove(msg.sender, _spender, _amount))            throw;        }        allowed[msg.sender][_spender] = _amount;        <mask0>;        return true;    }"