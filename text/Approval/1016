"function approve(address _spender, uint256 _amount) returns (bool success) {        require(transfersEnabled);        require((_amount == 0) || (allowed[msg.sender][_spender] == 0));        if (isContract(controller)) {            require(TokenController(controller).onApprove(msg.sender, _spender, _amount));        }        allowed[msg.sender][_spender] = _amount;        <mask0>;        return true;    }"