"function updateCoinageFinanceRouter(address _router) public onlyOperator {        coinageFinanceRouter = IUniswapV2Router02(_router);        coinageFinancePair = IUniswapV2Factory(coinageFinanceRouter.factory()).getPair(address(this), coinageFinanceRouter.<mask0>);        require(coinageFinancePair != address(0), \"COINAGE::updateCoinageFinanceRouter: Invalid pair address.\");        emit coinageFinanceRouterUpdated(msg.sender, address(coinageFinanceRouter), coinageFinancePair);    }"