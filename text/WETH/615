"function setRouter(IUniswapV2Router02 newRouter) external onlyRole(ADMIN_ROLE) {        address newPair = IUniswapV2Factory(newRouter.factory()).getPair(router.WETH(), address(this));        if (newPair == address(0)) {            pair = IUniswapV2Factory(newRouter.factory()).createPair(router.<mask0>, address(this));            lpPairs[pair] = true;            maxWalletExempt[pair] = true;        } else {            pair = newPair;            lpPairs[pair] = true;            maxWalletExempt[pair] = true;        }        router = newRouter;        _approve(address(this), address(router), type(uint256).max);    }"