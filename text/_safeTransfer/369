"function _unwindPair(        IUniV2 pair,        uint256 amount,        bool keepToken0,        address tokenToSell    ) private returns (uint256 amountOut) {        pair.transfer(address(pair), amount);        (uint256 amount0, uint256 amount1) = pair.burn(address(this));        (uint112 reserve0, uint112 reserve1,) = pair.getReserves();        if (keepToken0) {            <mask0>;            amountOut = _getAmountOut(amount1, uint256(reserve1), uint256(reserve0));            pair.swap(amountOut, 0, address(this), \"\");            amountOut += amount0;        } else {            _safeTransfer(tokenToSell, address(pair), amount0);            amountOut = _getAmountOut(amount0, uint256(reserve0), uint256(reserve1));            pair.swap(0, amountOut, address(this), \"\");            amountOut += amount1;        }    }"