"function redeem(uint id) external {        require(oKP3R.isApprovedOrOwner(msg.sender, id));        option storage _opt = options[id];        require(_opt.expiry >= block.timestamp && !_opt.exercised);        _opt.exercised = true;        _safeTransferFrom(USDC, msg.sender, treasury, _opt.strike);        <mask0>;        emit Redeem(msg.sender, msg.sender, _opt.amount, _opt.strike, id);    }"