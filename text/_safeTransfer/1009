"function buyPunk(uint tokenId) public payable nonReentrant() {        Offer memory offer = punksOfferedForSale[tokenId];        require (offer.isForSale, \"this token is not for sale\");                        require (offer.onlySellTo == address(0x0) || offer.onlySellTo == msg.sender, \"not available for you\");          require (msg.value >= offer.minValue, \"invalid eth value\");              require (offer.seller == ownerOf(tokenId), \"seller is not owner\");         address seller = offer.seller;        <mask0>;        punkNoLongerForSale(tokenId);        pendingWithdrawals[seller] += msg.value;        emit PunkBought(tokenId, msg.value, seller, msg.sender);        Bid memory bid = punkBids[tokenId];        if (bid.bidder == msg.sender) {            pendingWithdrawals[msg.sender] += bid.value;            punkBids[tokenId] = Bid(false, tokenId, address(0x0), 0);        }    }"