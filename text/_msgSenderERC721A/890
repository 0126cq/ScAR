"function approve(address to, uint256 tokenId) public override {        address owner = ERC721A.ownerOf(tokenId);        if (to == owner) revert ApprovalToCurrentOwner();        if (_msgSenderERC721A() != owner) if(!isApprovedForAll(owner, <mask0>)) {            revert ApprovalCallerNotOwnerNorApproved();        }        _tokenApprovals[tokenId] = to;        emit Approval(owner, to, tokenId);    }"