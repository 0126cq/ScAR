"function approve(address to, uint256 tokenId) public override {        address owner = address(uint160(_packedOwnershipOf(tokenId)));        if (_msgSenderERC721A() != owner)            if (!isApprovedForAll(owner, <mask0>)) {                revert ApprovalCallerNotOwnerNorApproved();            }        _tokenApprovals[tokenId] = to;        emit Approval(owner, to, tokenId);    }"