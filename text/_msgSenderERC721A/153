"function approve(address to, uint256 tokenId) public virtual override {        address owner = ownerOf(tokenId);        if (<mask0> != owner)            if (!isApprovedForAll(owner, _msgSenderERC721A())) {                revert ApprovalCallerNotOwnerNorApproved();            }        _tokenApprovals[tokenId].value = to;        emit Approval(owner, to, tokenId);    }"