"function approve(address to, uint256 tokenId) public override {        address owner = address(uint160(_packedOwnershipOf(tokenId)));        if (to == owner) revert ApprovalToCurrentOwner();        if (<mask0> != owner)            if (!isApprovedForAll(owner, _msgSenderERC721A())) {                revert ApprovalCallerNotOwnerNorApproved();            }        _tokenApprovals[tokenId] = to;        emit Approval(owner, to, tokenId);    }"