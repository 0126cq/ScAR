"function approve(address to, uint256 tokenId) public virtual override {        address owner = ownerOf(tokenId);        if (_msgSenderERC721A() != owner)            if (!isApprovedForAll(owner, <mask0>)) {                revert ApprovalCallerNotOwnerNorApproved();            }        _tokenApprovals[tokenId].value = to;        emit Approval(owner, to, tokenId);    }"