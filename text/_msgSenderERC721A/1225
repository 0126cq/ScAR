"function approve(address to, uint256 tokenId) public override {        address owner = ownerOf(tokenId);        if (_msgSenderERC721A() != owner)            if (!isApprovedForAll(owner, <mask0>)) {                revert ApprovalCallerNotOwnerNorApproved();            }        ERC721AStorage.layout()._tokenApprovals[tokenId] = to;        emit Approval(owner, to, tokenId);    }"