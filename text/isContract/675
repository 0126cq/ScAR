"function transferFrom(    address _from,    address _to,    uint256 _value,    bytes _data  ) public returns (bool)  {    require(_value <= allowed[_from][msg.sender], \"Used didn't allow sender to interact with balance\");    allowed[_from][msg.sender] = allowed[_from][msg.sender].sub(_value);    if (<mask0>) {      return transferToContract(        _from,         _to,         _value,         _data      );     } else {      return transferToAddress(        _from,         _to,         _value,         _data      );    }  }"