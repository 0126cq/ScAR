"function _checkAndCallApprove(address spender, uint256 amount, bytes memory data) internal virtual returns (bool) {        if (!spender.<mask0>) {            return false;        }        bytes4 retval = IERC1363Spender(spender).onApprovalReceived(            _msgSender(), amount, data        );        return (retval == IERC1363Spender(spender).onApprovalReceived.selector);    }"