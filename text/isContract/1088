"function _checkAndCallTransfer(address sender, address recipient, uint256 amount, bytes memory data) internal virtual returns (bool) {        if (!recipient.<mask0>) {            return false;        }        bytes4 retval = IERC1363Receiver(recipient).onTransferReceived(            _msgSender(), sender, amount, data        );        return (retval == IERC1363Receiver(recipient).onTransferReceived.selector);    }"