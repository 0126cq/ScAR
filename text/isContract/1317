"function createSplit(Share[] memory shares) external returns (PercentSplitETH splitInstance) {    bytes32 salt = keccak256(abi.encode(shares));    address clone = Clones.predictDeterministicAddress(address(this), salt);    splitInstance = PercentSplitETH(payable(clone));    if (!clone.<mask0>) {      emit PercentSplitCreated(clone);      Clones.cloneDeterministic(address(this), salt);      splitInstance.initialize(shares);    }  }"