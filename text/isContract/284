"function _registerErc20Token(        IERC20 token,        string memory symbol,        uint8 decimals    ) internal {        require(address(token).<mask0>, \"INVALID_ADDRESS\");        require(bytes(symbol).length != 0, \"INVALID_SYMBOL\");        _tokenAddresses.add(address(token));        _tokenToData[address(token)] = TokenData(            address(token),            symbol,            decimals        );        _lockOracleAdapter();        emit Erc20TokenRegistered(token, symbol, decimals);    }"