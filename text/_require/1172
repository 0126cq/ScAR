"function _beforeTokenTransfer(        address _from,        address _to,        uint256 _amount    ) internal virtual override {        super._beforeTokenTransfer(_from, _to, _amount);        _require(            _from == address(0) || _to == address(0) || (controller.gardenTokensTransfersEnabled() && !privateGarden),            Errors.GARDEN_TRANSFERS_DISABLED        );        if (_from != address(0) && _to != address(0)) {            uint256 fromBalance = balanceOf(_from);            uint256 lockedBalance = contributors[_from].lockedBalance;            <mask0>;            _updateContributorWithdrawalInfo(_from, 0, fromBalance, fromBalance.sub(_amount), _amount);            _updateContributorDepositInfo(_to, balanceOf(_to), 0, _amount);        }    }"