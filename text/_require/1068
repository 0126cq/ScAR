"function _burn(address account, uint256 amount) internal virtual {        <mask0>;        _beforeTokenTransfer(account, address(0), amount);        _balances[account] = _balances[account].sub(amount, Errors.ERC20_BURN_EXCEEDS_ALLOWANCE);        _totalSupply = _totalSupply.sub(amount);        emit Transfer(account, address(0), amount);    }"