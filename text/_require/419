"function _startGradualWeightChange(        uint256 startTime,        uint256 endTime,        uint256[] memory startWeights,        uint256[] memory endWeights    ) internal virtual {        bytes32 newPoolState = _poolState;        uint256 normalizedSum = 0;        for (uint256 i = 0; i < endWeights.length; i++) {            uint256 endWeight = endWeights[i];            _require(endWeight >= _MIN_WEIGHT, Errors.MIN_WEIGHT);            newPoolState = newPoolState                .insertUint31(startWeights[i].compress31(), _START_WEIGHT_OFFSET + i * 31)                .insertUint16(endWeight.compress16(), _END_WEIGHT_OFFSET + i * 16);            normalizedSum = normalizedSum.add(endWeight);        }        <mask0>;        _poolState = newPoolState.insertUint32(startTime, _START_TIME_OFFSET).insertUint32(endTime, _END_TIME_OFFSET);        emit GradualWeightUpdateScheduled(startTime, endTime, startWeights, endWeights);    }"