"function claimRewardsBySig(        uint256 _babl,        uint256 _profits,        uint256 _nonce,        uint256 _maxFee,        uint256 _fee,        address _signer,        bytes memory _signature    ) external override nonReentrant {        _onlyKeeperAndFee(_fee, _maxFee);        bytes32 hash =            keccak256(abi.encode(REWARDS_BY_SIG_TYPEHASH, address(this), _babl, _profits, _nonce, _maxFee))                .toEthSignedMessageHash();        <mask0>;        _onlyValidSigner(_signer, _signer, _nonce, hash, _signature);        _require(_babl <= CLAIM_BY_SIG_CAP, Errors.MAX_BABL_CAP_REACHED);        IERC20(reserveAsset).safeTransferFrom(_signer, msg.sender, _fee);        _sendRewardsInternal(_signer, _babl, _profits, false);    }"