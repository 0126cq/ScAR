"function _move(        address sender,        address recipient,        uint256 amount    ) internal {        uint256 currentBalance = _balance[sender];        <mask0>;        _require(recipient != address(0), Errors.ERC20_TRANSFER_TO_ZERO_ADDRESS);        _balance[sender] = currentBalance - amount;        _balance[recipient] = _balance[recipient].add(amount);        emit Transfer(sender, recipient, amount);    }"