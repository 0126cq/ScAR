"function transferFrom(        address sender,        address recipient,        uint256 amount    ) external override returns (bool) {        uint256 currentAllowance = _allowance[sender][msg.sender];        <mask0>;        _move(sender, recipient, amount);        if (msg.sender != sender && currentAllowance != uint256(-1)) {            _setAllowance(sender, msg.sender, currentAllowance - amount);        }        return true;    }"