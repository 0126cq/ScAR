"function _removeValidatorFromActiveList(address validatorAddress) internal {        int256 indexOf = - 1;        for (uint256 i = 0; i < _activeValidatorsList.length; i++) {            if (_activeValidatorsList[i] != validatorAddress) continue;            indexOf = int256(i);            break;        }        if (indexOf >= 0) {            if (_activeValidatorsList.length > 1 && uint256(indexOf) != _activeValidatorsList.length - 1) {                _activeValidatorsList[uint256(indexOf)] = _activeValidatorsList[_activeValidatorsList.length - 1];            }            _activeValidatorsList.<mask0>;        }    }"