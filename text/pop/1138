"function removeGen0Token(uint16 token) internal {   uint x=   stakeOwners[ msg.sender].gen0.tokens.length  ;   if (token == stakeOwners[ msg.sender].gen0.tokens[x-1])   {        stakeOwners[ msg.sender].gen0.tokens.<mask0>;   }   else{    for (uint i ; i < stakeOwners[ msg.sender].gen0.tokens.length ; i ++)    {      if(token == stakeOwners[ msg.sender].gen0.tokens[i] )      {        uint16 temp = stakeOwners[ msg.sender].gen0.tokens[x-1];        stakeOwners[ msg.sender].gen0.tokens[x-1]   =  stakeOwners[ msg.sender].gen0.tokens[i];        stakeOwners[ msg.sender].gen0.tokens[i] = temp;        stakeOwners[ msg.sender].gen0.tokens.pop();      }    }   }   }"