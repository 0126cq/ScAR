"function removeCosigner(address cosaddr) public override onlyOwner {        require(cosaddr != address(0), \"BCM: ZERO_ADDRESS\");        Cosigner memory cosigner = _cosigners[cosaddr];        require(cosigner.active, \"BCM: NOT_EXIST\");        address[] storage addrs = _cosaddrs[cosigner.chainId];        if (addrs.length > 1) {            addrs[cosigner.index] = _cosaddrs[cosigner.chainId][                addrs.length - 1            ];            addrs.<mask0>;            address cosaddrLast = addrs[cosigner.index];            _cosigners[cosaddrLast].index = cosigner.index;        } else {            addrs.pop();        }        delete _cosigners[cosaddr];        emit CosignerRemoved(cosigner.addr, cosigner.chainId);    }"