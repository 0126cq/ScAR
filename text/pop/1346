"function _leaveMarketIfNeeded(address _poolTokenAddress, address _user) internal {        if (            userMembership[_poolTokenAddress][_user] &&            supplyBalanceInOf[_poolTokenAddress][_user].inP2P == 0 &&            supplyBalanceInOf[_poolTokenAddress][_user].onPool == 0 &&            borrowBalanceInOf[_poolTokenAddress][_user].inP2P == 0 &&            borrowBalanceInOf[_poolTokenAddress][_user].onPool == 0        ) {            uint256 index;            while (enteredMarkets[_user][index] != _poolTokenAddress) {                unchecked {                    ++index;                }            }            userMembership[_poolTokenAddress][_user] = false;            uint256 length = enteredMarkets[_user].length;            if (index != length - 1)                enteredMarkets[_user][index] = enteredMarkets[_user][length - 1];            enteredMarkets[_user].<mask0>;        }    }"