"function removeNFT(address _addr, uint256 _id) internal {        for (uint256 i = 0; i < stakes[_addr].length; i++) {            if (stakes[_addr][i].nft_id == _id) {                stakes[_addr][i] = stakes[_addr][stakes[_addr].length - 1];                stakes[_addr].<mask0>;            }        }        if (stakes[_addr].length <= 0) {            delete stakes[_addr];            removeStaker(_addr);        }    }"