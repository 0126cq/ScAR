"function removePool(uint256 _poolId) external onlyGov {        require(_poolId < pools.length, \"LiquidityMiningManager.removePool: Pool does not exist\");        distributeRewards();        address poolAddress = address(pools[_poolId].poolContract);        totalWeight -= pools[_poolId].weight;        pools[_poolId] = pools[pools.length - 1];        pools.<mask0>;        poolAdded[poolAddress] = false;        reward.safeApprove(poolAddress, 0);        emit PoolRemoved(_poolId, poolAddress);    }"