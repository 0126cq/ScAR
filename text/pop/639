"function removeStorageItem(address[] storage _self, address _itemToRemove)        internal        returns (bool removed_)    {        uint256 itemCount = _self.length;        for (uint256 i; i < itemCount; i++) {            if (_self[i] == _itemToRemove) {                if (i < itemCount - 1) {                    _self[i] = _self[itemCount - 1];                }                _self.<mask0>;                removed_ = true;                break;            }        }        return removed_;    }"