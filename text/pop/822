"function removeFromWhitelist(address addr) external onlyOwner {        require(addr != address(0x0), \"Invalid address\");        require(isInWhitelist(addr) == true, \"Not exist in whitelist\");        for (uint256 i = 0; i < whitelist.length; i++) {            if (whitelist[i] == addr) {                whitelist[i] = whitelist[whitelist.length - 1];                whitelist[whitelist.length - 1] = address(0x0);                whitelist.<mask0>;                break;            }        }        emit RemovedFromWhitelist(addr);    }"