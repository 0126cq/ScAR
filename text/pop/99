"function addOracle(address _oracle , bool add) external onlyAdmin {        if (add) {            require(!isOracle[_oracle] , \"already an oracle\");            emit OracleAdded(_oracle);            oracles.push(_oracle);        } else {        require(isOracle[_oracle] , \"not an oracle\");        uint256 oracleLength = oracles.length;          for (uint256 index; index < oracleLength ; index++) {            if (oracles[index] == _oracle) {                oracles[index] = oracles[oracleLength - 1];                oracles.<mask0>;            }         }         emit OracleRemoved(_oracle);        }        isOracle[_oracle] = add;    }"