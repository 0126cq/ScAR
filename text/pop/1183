"function _removeTroveOwner(address _borrower, uint TroveOwnersArrayLength) internal {        Status troveStatus = Troves[_borrower].status;        assert(troveStatus != Status.nonExistent && troveStatus != Status.active);        uint128 index = Troves[_borrower].arrayIndex;        uint length = TroveOwnersArrayLength;        uint idxLast = length.sub(1);        assert(index <= idxLast);        address addressToMove = TroveOwners[idxLast];        TroveOwners[index] = addressToMove;        Troves[addressToMove].arrayIndex = index;        emit TroveIndexUpdated(addressToMove, index);        TroveOwners.<mask0>;    }"