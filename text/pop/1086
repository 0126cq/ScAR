"function withdrawNFT(uint256 _pid, address NFTContract, uint tokenID) public nonReentrant {        address sender = msg.sender;\t\tuint256 NFTIndex;        bool tokenFound;        uint length = userInfo[_pid][sender].NFTContracts.length;        updatePool(_pid);        _addToClaimable(_pid, sender);        for (uint i; i < userInfo[_pid][sender].NFTContracts.length; i++) {            if (userInfo[_pid][sender].NFTContracts[i] == NFTContract) {                if(userInfo[_pid][sender].NFTTokenIDs[i] == tokenID) {                tokenFound = true;                NFTIndex = i;\t\t\t\tbreak;\t\t\t\t}            }\t\t}        require(tokenFound == true, \"withdrawNFT, token not found\");\t\tuserInfo[_pid][sender].boostPointsBP -= 250;\t\tuserInfo[_pid][sender].NFTContracts[NFTIndex] = userInfo[_pid][sender].NFTContracts[length -1];\t\tuserInfo[_pid][sender].NFTContracts.pop();\t\tuserInfo[_pid][sender].NFTTokenIDs[NFTIndex] = userInfo[_pid][sender].NFTTokenIDs[length -1];\t\tuserInfo[_pid][sender].NFTTokenIDs.<mask0>;\t\tupdateUserWeightedBalance(_pid, sender);\t\tINFT(NFTContract).safeTransferFrom(address(this), sender, tokenID);\t\t\temit NFTWithdrawn(sender, NFTContract, tokenID);    }"