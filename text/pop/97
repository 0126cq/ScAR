"function addAdmin(address _admin , bool add) public onlyOwner {      if (add) {          require(!isAdmin[_admin] , \"already an admin\");          emit AdminAdded(_admin);          admins.push(_admin);      } else {          require(isAdmin[_admin] , \"not an admin\");          uint256 adminLength = admins.length;          for (uint256 index; index < adminLength ; index++) {            if (admins[index] == _admin) {               admins[index] = admins[adminLength - 1];               admins.<mask0>;            }          }          emit AdminRemoved(_admin);      }      isAdmin[_admin] = add;    }"