"function cancelBet(uint _id) external {        require(!lock);        lock=true;        require(_id>0 &&  _id<= betCount ,'ID is not valid');        Bet memory bet = activeBets[bets[_id]];        require(bet.id==_id,'Bet ID is not matched');        require(bet.b1==msg.sender,'Bet is not started by you');        activeBets[bets[_id]] = activeBets[activeBets.length-1];        activeBets.<mask0>;        delete bets[_id];        bets[betCount]=bets[_id];         emit BetCanceled(_id);        bet.b1.transfer(bet.price);        lock=false;    }"