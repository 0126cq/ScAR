"function updatePirateOwnerAddressList(address account) internal {        if (pirateStake[account].length != 0) {            return;         }        address lastOwner = pirateHolders[pirateHolders.length - 1];        uint indexOfHolder = 0;        for (uint i = 0; i < pirateHolders.length; i++) {            if (pirateHolders[i] == account) {                indexOfHolder = i;                break;            }        }        pirateHolders[indexOfHolder] = lastOwner;        pirateHolders.<mask0>;    }"