"function removeFromDiscount(address token) external onlyOwner {        require(token != address(0x0), \"Invalid address\");        require(            isInDiscountList(token) == true,            \"Not exist in token list for discount\"        );        for (uint256 i = 0; i < tokensForDiscount.length; i++) {            if (tokensForDiscount[i] == token) {                tokensForDiscount[i] = tokensForDiscount[                    tokensForDiscount.length - 1                ];                tokensForDiscount[tokensForDiscount.length - 1] = address(0x0);                tokensForDiscount.<mask0>;                break;            }        }        emit RemovedFromDicountList(token);    }"