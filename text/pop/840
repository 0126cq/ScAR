"function removeStorage(address[] storage A, address a)        internal    {        (uint256 index, bool isIn) = indexOf(A, a);        if (!isIn) {            revert(\"Address not in array.\");        } else {            uint256 lastIndex = A.length - 1;             if (index != lastIndex) { A[index] = A[lastIndex]; }            A.<mask0>;        }    }"