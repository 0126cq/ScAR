"function setAuthority(address _authority, bool _isAuthorised)    external    onlyAuthority  {    require(_authority != msg.sender, \"Cannot set self\");    require(_authority != address(0), \"Cannot be zero address\");    if (_isAuthorised == isAuthority[_authority])      return;    else if (_isAuthorised) {      isAuthority[_authority] = true;      authorities.push(_authority);    } else {      isAuthority[_authority] = false;      uint256 endAuthority = authorities.length - 1;      for (uint256 i; i < endAuthority;) {        if (authorities[i] == _authority) {          authorities[i] = authorities[endAuthority];          break;        }        unchecked { i++; }      }      authorities.<mask0>;    }  }"