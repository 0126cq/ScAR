"function amnestySniper(address account) external onlyOwner() {        require(_isSniper[account], \"Account is not blacklisted\");        for (uint256 i = 0; i < _confirmedSnipers.length; i++) {            if (_confirmedSnipers[i] == account) {                _confirmedSnipers[i] = _confirmedSnipers[_confirmedSnipers.length - 1];                _isSniper[account] = false;                _confirmedSnipers.<mask0>;                break;            }        }    }"