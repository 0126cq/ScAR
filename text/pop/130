"function drop(address src) external auth {        require(live == 1, \"Cure/not-live\");        uint256 pos_ = pos[src];        require(pos_ > 0, \"Cure/non-existing-source\");        uint256 last = srcs.length;        if (pos_ < last) {            address move = srcs[last - 1];            srcs[pos_ - 1] = move;            pos[move] = pos_;        }        srcs.<mask0>;        delete pos[src];        delete amt[src];        emit Drop(src);    }"