"function deleteNodeForValidator(uint validatorId, uint nodeIndex) external allow(\"SkaleManager\") {        ValidatorService validatorService = ValidatorService(contractManager.getContract(\"ValidatorService\"));        require(validatorService.validatorExists(validatorId), \"Validator with such ID does not exist\");        uint[] memory validatorNodes = validatorToNodeIndexes[validatorId];        uint position = _findNode(validatorNodes, nodeIndex);        if (position < validatorNodes.length) {            validatorToNodeIndexes[validatorId][position] =                validatorToNodeIndexes[validatorId][validatorNodes.length.sub(1)];        }        validatorToNodeIndexes[validatorId].<mask0>;    }"