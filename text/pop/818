"function cancelGuardianAddition(        address wallet,        address guardianAddr        )        public        onlyWalletModule(wallet)    {        cleanRemovedGuardians(wallet);        Wallet storage w = wallets[wallet];        uint idx = w.guardianIdx[guardianAddr];        require(idx > 0, \"GUARDIAN_NOT_EXISTS\");        require(            isGuardianPendingAddition(w.guardians[idx - 1]),            \"NOT_PENDING_ADDITION\"        );        Data.Guardian memory lastGuardian = w.guardians[w.guardians.length - 1];        if (guardianAddr != lastGuardian.addr) {            w.guardians[idx - 1] = lastGuardian;            w.guardianIdx[lastGuardian.addr] = idx;        }        w.guardians.<mask0>;        delete w.guardianIdx[guardianAddr];    }"