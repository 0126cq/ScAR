"function burnMint(uint256[] memory burnTokenIds, uint256 allowedAmount, bytes32[] calldata merkleProof)        external        payable        whenNotPaused        isNotOverMaxSupply(burnTokenIds.length)        isNotOverMaxAmount(burnTokenIds.length)        enoughEth(burnTokenIds.length)        hasRightToMint(burnTokenIds.length, allowedAmount, merkleProof)        whenBurnAndMintSale    {        for (uint256 i = 0; i < burnTokenIds.length; i++) {            uint256 tokenId = burnTokenIds[i];            require(_msgSender() == ownerOf(tokenId));            <mask0>;            emit Burned(tokenId);        }        _mintCore(uint64(burnTokenIds.length));    }"