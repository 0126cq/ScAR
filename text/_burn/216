"function burn(    address user,    address receiverOfUnderlying,    uint256 amount,    uint256 index  ) external override onlyLendingPool {    uint256 amountScaled = amount.rayDiv(index);    require(amountScaled != 0, Errors.CT_INVALID_BURN_AMOUNT);    <mask0>;    IERC20(UNDERLYING_ASSET_ADDRESS).safeTransfer(receiverOfUnderlying, amount);    emit Transfer(user, address(0), amount);    emit Burn(user, receiverOfUnderlying, amount, index);  }"