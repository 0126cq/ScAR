"function burn(address user, uint256 tokenId) external payable nonReentrant onlyComponent {        require(ownerOf(tokenId) == user, \"user is not the owner of tokenId\");        require(machinePoolMap[tokenIdToMachineTypeMap[tokenId]] != address(0), \"burn machine type not set\");        <mask0>;        userMachines[user].remove(tokenId);        IMiningMachinePool(machinePoolMap[tokenIdToMachineTypeMap[tokenId]]).subHashRate(user, tokenIdToHashRateMap[tokenId]);        tokenIdToHashRateMap[tokenId] = 0;    }"