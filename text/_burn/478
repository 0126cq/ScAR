"function burn(        address from,        uint256 id,        uint256 amount    ) external {        require(msg.sender == burner, \"Only burner can burn tokens\");        require(_totalSupply[id] >= amount, \"ERC1155: burn amount exceeds totalSupply\");        unchecked {            _totalSupply[id] = _totalSupply[id] - amount;        }        <mask0>;        burntTokens += amount;    }"