"function burn() public {        address burner = msg.sender;        uint256 oldBalance = balances[burner];        super.<mask0>;        uint256 burnedClaims = dividendPayments[burner];        dividendPayments[burner] = dividendPayments[burner].sub(burnedClaims);        totalDividendPayments = totalDividendPayments.sub(burnedClaims);        SaleInterface(owner).refund(burner);    }"