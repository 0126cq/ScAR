"function bridgeOut(        uint256 _tokenId,        uint16 _chainId,        string memory _destination    ) public payable {        require(msg.sender == ownerOf(_tokenId), \"is not owner\");        (bool sent, ) = payable(bridge).call{value: msg.value}(\"\");        require(sent);        <mask0>;        emit BridgeOut(_chainId, _tokenId, _destination, msg.value);    }"