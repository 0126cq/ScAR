"function _annihilate(        address account,        uint64 maturity,        int128 strike64x64,        bool isCall,        uint256 contractSize    ) internal {        uint256 longTokenId = PoolStorage.formatTokenId(            _getTokenType(isCall, true),            maturity,            strike64x64        );        uint256 shortTokenId = PoolStorage.formatTokenId(            _getTokenType(isCall, false),            maturity,            strike64x64        );        <mask0>;        _burn(account, shortTokenId, contractSize);        emit Annihilate(shortTokenId, contractSize);    }"