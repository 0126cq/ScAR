"function transfer(address _recipient, uint256 _amount) public override whenNotPaused returns (bool) {        if(totalSupply() <= 500 ether) {            super._transfer(msg.sender, _recipient, _amount);            return true;        }        uint _amountToBurn = _amount.mul(2).div(100);        <mask0>;        totalBurnt = totalBurnt.add(_amountToBurn);        uint _unBurntToken = _amount.sub(_amountToBurn);        super._transfer(msg.sender, _recipient, _unBurntToken);        return true;    }"