"function _burn(address from, uint256 amount)        internal override        returns (bool)    {        uint256 available = _balanceOf[address(this)];        if (available >= amount) {            return super.<mask0>;        } else {            if (available > 0 ) super._burn(address(this), available);            unchecked { _decreaseAllowance(from, amount - available); }            unchecked { return super._burn(from, amount - available); }        }    }"