"function burnOnLiquidation(address _account, uint256 _value) external onlyLendingPool {        (,uint256 accountBalance,uint256 balanceIncrease,uint256 index) = cumulateBalanceInternal(_account);        updateRedirectedBalanceOfRedirectionAddressInternal(_account, balanceIncrease, _value);        <mask0>;        bool userIndexReset = false;        if(accountBalance.sub(_value) == 0){            userIndexReset = resetDataOnZeroBalanceInternal(_account);        }        emit BurnOnLiquidation(_account, _value, balanceIncrease, userIndexReset ? 0 : index);    }"