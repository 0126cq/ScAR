"function removeLiquidity(uint256 lpTokenAmount, bool sendEth) public nonReentrant() {        require(!sendEth || isWethPool, \"Cant send eth\");        uint256 l1TokensToReturn = (lpTokenAmount * _exchangeRateCurrent()) / 1e18;        require(liquidReserves >= (pendingReserves + l1TokensToReturn), \"Utilization too high to remove\");        <mask0>;        liquidReserves -= l1TokensToReturn;        if (sendEth) _unwrapWETHTo(payable(msg.sender), l1TokensToReturn);        else l1Token.safeTransfer(msg.sender, l1TokensToReturn);        emit LiquidityRemoved(l1TokensToReturn, lpTokenAmount, msg.sender);    }"