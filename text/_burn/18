"function redeem(        address to,        uint256 amountIn    ) external {        require(redeemable, \"!REDEEMABLE\");        Participant storage participant = participants[msg.sender];        uint256 amountOut = redeemAmountOut(msg.sender, amountIn);        participant.redeemed += amountIn;        <mask0>;        CNV.mint(to, amountOut);        emit Redeemed(msg.sender, to, amountIn, amountOut);    }"