"function burn(address account, uint256 amount)        external        onlyRole(DEFAULT_ADMIN_ROLE)    {        <mask0>;        if (block.timestamp >= lastInterestCompound + updateTimeLimit) {            updateRatio();        }        uint256 assetValue = (amount * growingRatio) / multiplier;        emit TransferAssetValue(msg.sender, address(0), amount, assetValue, growingRatio);    }"