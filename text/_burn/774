"function _redeem(        address _account,        uint256 _shares,        uint256 _price    ) internal returns (uint256 redeemed) {        redeemed = (_shares * _price) / ONE_CURVE_LP_TOKEN;        totalLpTokensStaked -= redeemed;        IERC20Detailed _curveLpToken = IERC20Detailed(curveLpToken);        <mask0>;        IBaseRewardPool(rewardPool).withdraw(redeemed, false);        IBooster(BOOSTER).withdraw(poolID, redeemed);        _curveLpToken.safeTransfer(_account, redeemed);    }"