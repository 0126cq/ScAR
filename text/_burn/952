"function deleteToken(uint256 _tokenId, uint256 _amount) public onlyTokenOwner(_tokenId) {        bool activeBid = marketAuction.hasTokenActiveBid(_tokenId, msg.sender);        uint256 balance = balanceOf(msg.sender, _tokenId);        if (activeBid == true)            require(balance.sub(_amount) > 0, \"you have the active bid\");        <mask0>;        DeleteTokens(msg.sender, _tokenId, _amount);    }"