"function _burnLPTokens(address to, uint256 liquidityTokenAmount) internal {        uint256 redeemableUnderlyingTokens = calculateLiquidityTokenValue(liquidityTokenAmount);        require(redeemableUnderlyingTokens > 0, 'INSUFFICIENT_LIQUIDITY_BURNED');        <mask0>;        IERC20(underlier).safeTransfer(to, _convertToUnderlierDecimal(redeemableUnderlyingTokens));        emit Mint(to, redeemableUnderlyingTokens, liquidityTokenAmount);    }"