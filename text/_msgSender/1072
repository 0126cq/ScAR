"function _burn(        address account,        uint256 id,        uint256 amount    ) internal virtual {        require(account != address(0), \"ERC1155: burn from the zero address\");        address operator = <mask0>;        _beforeTokenTransfer(operator, account, address(0), _asSingletonArray(id), _asSingletonArray(amount), \"\");        uint256 accountBalance = _balances[id][account];        require(accountBalance >= amount, \"ERC1155: burn amount exceeds balance\");        unchecked {            _balances[id][account] = accountBalance - amount;        }        emit TransferSingle(operator, account, address(0), id, amount);    }"