"function authorize(bool approved, uint equity, address who) public {        require(hasRole(DEFAULT_ADMIN_ROLE, <mask0>), \"INVALID_ROLE\");        require(equity <= TOTAL_CAP, \"INVALID EQUITY (0-10000)\");        require(equity >= 0, \"INVALID EQUITY (0-10000)\");        FounderAuthorization[] storage auths = _addressToFounderAuthorization[who];        bool exists = false;        for (uint i = 0; i < auths.length; i++) {            if (auths[i].founder == msg.sender) {                exists = true;                auths[i].equity = equity;                auths[i].approved = approved;                auths[i].isRemoval = false;            }        }        if (!exists) {            auths.push(FounderAuthorization({founder: msg.sender, equity: equity, approved: approved, isRemoval: false}));        }    }"