"function decreaseAllowance(address spender, uint256 subtractedValue) public virtual returns (bool) {        address owner = <mask0>;        uint256 currentAllowance = _allowances[owner][spender];        require(currentAllowance >= subtractedValue, \"ERC20: decreased allowance below zero\");        unchecked {            _approve(owner, spender, currentAllowance - subtractedValue);        }        return true;    }"