"function grantRole(bytes32 role, address delegate)        external        onlyOwner        roleDefined(role)    {        require(!_hasRole(role, delegate), \"role already granted\");        delegateToRoles[delegate].add(role);        if (delegateSet.add(delegate)) {            emit DelegateAdded(delegate, <mask0>);        }        emit RoleGranted(role, delegate, _msgSender());    }"