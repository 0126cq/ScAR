"function _mint(        address account,        uint256 id,        uint256 amount,        bytes memory data    ) internal virtual {        require(account != address(0), \"ERC1155: mint to the zero address\");        address operator = <mask0>;        _beforeTokenTransfer(operator, address(0), account, _asSingletonArray(id), _asSingletonArray(amount), data);        _balances[id][account] += amount;        emit TransferSingle(operator, address(0), account, id, amount);        _doSafeTransferAcceptanceCheck(operator, address(0), account, id, amount, data);    }"