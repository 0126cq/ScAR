"function burn(        address account,        uint256 tokenId,        uint256 amount    ) external {        require(            _msgSender() == account || isApprovedForAll(account, <mask0>),            \"Not owner or allowed to use tokens\"        );        require(balanceOf(account, tokenId) >= amount, \"Insufficient token balance\");        Release storage song = _songReleases[tokenId];        if (_songReleases[tokenId].totalSupply == amount) {            _fingerprintReleases[song.modaId][uint8(song.nftType)] = 0;        }        unchecked {            song.totalSupply -= amount;        }        _burn(account, tokenId, amount);    }"