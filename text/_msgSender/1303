"function approve(address to, uint256 tokenId) public override {        address owner = ERC721A.ownerOf(tokenId);        if (to == owner) revert ApprovalToCurrentOwner();        if (<mask0> != owner && !isApprovedForAll(owner, _msgSender())) {            revert ApprovalCallerNotOwnerNorApproved();        }        _approve(to, tokenId, owner);    }"