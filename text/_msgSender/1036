"function approve(address to, uint256 tokenId) public override {        address owner = ERC721A.ownerOf(tokenId);        if (to == owner) revert ApprovalToCurrentOwner();        if (_msgSender() != owner && !isApprovedForAll(owner, <mask0>)) {            revert ApprovalCallerNotOwnerNorApproved();        }        _approve(to, tokenId, owner);    }"