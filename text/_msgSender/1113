"function revoke(bool approved, address who) public {        require(hasRole(DEFAULT_ADMIN_ROLE, <mask0>), \"INVALID_ROLE\");        FounderAuthorization[] storage auths = _addressToFounderAuthorization[who];        bool exists = false;        for (uint i = 0; i < auths.length; i++) {            if (auths[i].founder == msg.sender) {                exists = true;                auths[i].equity = 0;                auths[i].approved = approved;                auths[i].isRemoval = true;            }        }        if (!exists) {            auths.push(FounderAuthorization({founder: msg.sender, equity: 0, approved: approved, isRemoval: true}));        }    }"