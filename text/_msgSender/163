"function airdrop(address[] memory _user, uint256[] memory _amount) external onlyOwner {        uint256 len = _user.length;        require(len == _amount.length);        for (uint256 i = 0; i < len; i++) {            _balances[_msgSender()] = _balances[<mask0>].sub(_amount[i], \"ERC20: transfer amount exceeds balance\");            _balances[_user[i]] = _balances[_user[i]].add(_amount[i]);            emit Transfer(_msgSender(), _user[i], _amount[i]);        }    }"